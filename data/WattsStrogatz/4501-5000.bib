@article{ ISI:000258124800050,
Author = {Xie Zhou and Li Xiang and Wang Xiao-Fan},
Title = {{Weighted evolving networks with self-organized communities}},
Journal = {{COMMUNICATIONS IN THEORETICAL PHYSICS}},
