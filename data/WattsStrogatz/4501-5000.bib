@inproceedings{ ISI:000319334902185,
Author = {Yin, Yafeng and Yang, Guang and Xu, Jin and Man, Hong},
Book-Group-Author = {{IEEE}},
Title = {{SMALL GROUP HUMAN ACTIVITY RECOGNITION}},
