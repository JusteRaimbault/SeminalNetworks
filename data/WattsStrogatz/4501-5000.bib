@article{ ISI:000403438900002,
Author = {Chu, Chia-Chi and Iu, Herbert Ho-Ching},
Title = {{Complex Networks Theory For Modern Smart Grid Applications: A Survey}},
Journal = {{IEEE JOURNAL ON EMERGING AND SELECTED TOPICS IN CIRCUITS AND SYSTEMS}},
Year = {{2017}},
Volume = {{7}},
Number = {{2, SI}},
Pages = {{177-191}},
Month = {{JUN}},
Abstract = {{This paper provides a survey of studying complex network theory for
   modern smart grid applications. A brief overview of complex network
   theory will be explored first. Topological characteristics, statistic
   characteristics, such as self-organized criticality and critically slow
   down, and dynamical characteristics, including synchronizations,
   consensus control, and pinning control, will be briefly addressed. Then,
   we will illustrate how complex network theory can be applied to modern
   smart grids in structural vulnerability assessment, cascading blackouts,
   grid synchronization, network reconfigurations, distributed droop
   control, pinning control for microgrid autonomous operations, and
   effective grid expansions. Some emerging topics and future perspectives
   are also addressed.}},
