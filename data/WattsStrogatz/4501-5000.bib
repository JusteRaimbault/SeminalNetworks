@article{ ISI:000284585400004,
Author = {Park, Christopher Y. and Hess, David C. and Huttenhower, Curtis and
   Troyanskaya, Olga G.},
Title = {{Simultaneous Genome-Wide Inference of Physical, Genetic, Regulatory, and
   Functional Pathway Components}},
