@article{ ISI:000300412100002,
Author = {Garnerone, Silvano and Giorda, Paolo and Zanardi, Paolo},
Title = {{Bipartite quantum states and random complex networks}},
Journal = {{NEW JOURNAL OF PHYSICS}},
Year = {{2012}},
Volume = {{14}},
Month = {{JAN 10}},
Abstract = {{We introduce a mapping between graphs and pure quantum bipartite states
   and show that the associated entanglement entropy conveys non-trivial
   information about the structure of the graph. Our primary goal is to
   investigate the family of random graphs known as complex networks. In
   the case of classical random graphs, we derive an analytic expression
   for the averaged entanglement entropy (S) over bar while for general
   complex networks we rely on numerics. For a large number of nodes n we
   find a scaling (S) over bar similar to c log n + g(e) where both the
   prefactor c and the sub-leading O(1) term g(e) are characteristic of the
   different classes of complex networks. In particular, g(e) encodes
   topological features of the graphs and is named network topological
   entropy. Our results suggest that quantum entanglement may provide a
   powerful tool for the analysis of large complex networks with
   non-trivial topological properties.}},
