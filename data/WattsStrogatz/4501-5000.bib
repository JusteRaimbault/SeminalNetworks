@article{ ISI:000171086500002,
Author = {Ozana, M},
Title = {{Incipient spanning cluster on small-world networks}},
Journal = {{EUROPHYSICS LETTERS}},
Year = {{2001}},
Volume = {{55}},
Number = {{6}},
Pages = {{762-766}},
Month = {{SEP}},
Abstract = {{We analyze the scaling properties of the largest cluster size for the
   site percolation problem on small-world graphs. It is shown how the
   presence of the extra length-scale, the small-world crossover length xi,
   influences the fractal dimension D of the spanning cluster. Using the
   results for dimension d = 2 we nd the critical exponent governing the
   cluster size distribution tau similar or equal to5/2. This implies that
   is universal and independent of d in agreement with the conjecture by
   Moore and Newman.}},
