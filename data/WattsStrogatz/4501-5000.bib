@article{ ISI:000265885900029,
Author = {Abe, Sumiyoshi and Suzuki, Norikazu},
Title = {{Scaling relation for earthquake networks}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2009}},
Volume = {{388}},
Number = {{12}},
Pages = {{2511-2514}},
Month = {{JUN 15}},
Abstract = {{The scaling relation, 2 gamma - delta = 1, for the exponents of the
   power-law connectivity distribution, gamma, and the power-law eigenvalue
   distribution of the adjacency matrix,, is theoretically predicted to be
   fulfilled by a locally treelike scale-free network in the.,effective
   medium approximation{''} (i.e., all analog of the mean field
   approximation). Here, it is Shown that Such a relation holds well for
   the reduced simple earthquake networks (i.e., the network without
   tadpole-loops and multiple edges) constructed from the seismic data
   taken from California and Japan. This validates the goodness of the
   effective medium approximation in the earthquake networks and is
   consistent with the hierarchical organization of the networks. The
   present result may be useful for modeling seismicity on complex
   networks. (C) 2009 Elsevier B.V. All rights reserved.}},
