@article{ ISI:000330084000006,
Author = {Cafieri, Sonia and Hansen, Pierre and Liberti, Leo},
Title = {{Improving heuristics for network modularity maximization using an exact
   algorithm}},
Journal = {{DISCRETE APPLIED MATHEMATICS}},
Year = {{2014}},
Volume = {{163}},
Number = {{1, SI}},
Pages = {{65-72}},
Month = {{JAN 30}},
Abstract = {{Heuristics are widely applied to modularity maximization models for the
   identification of communities in complex networks. We present an
   approach to be applied as a post-processing to heuristic methods in
   order to improve their performances. Starting from a given partition, we
   test with an exact algorithm for bipartitioning if it is worthwhile to
   split some communities or to merge two of them. A combination of merge
   and split actions is also performed. Computational experiments show that
   the proposed approach is effective in improving heuristic results. (C)
   2012 Elsevier B.V. All rights reserved.}},
