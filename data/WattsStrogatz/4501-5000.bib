@article{ ISI:000303133700017,
Author = {Estrada, Ernesto},
Title = {{The communicability distance in graphs}},
Journal = {{LINEAR ALGEBRA AND ITS APPLICATIONS}},
Year = {{2012}},
Volume = {{436}},
Number = {{11}},
Pages = {{4317-4328}},
Month = {{JUN 1}},
Abstract = {{Let G be a simple connected graph with adjacency matrix A. The
   communicability G(pq) between two nodes p and q of the graph is defined
   as the pq-entry of G = exp(A). We prove here that xi(p.q) = (G(pp) +
   G(pq) - 2G(pq))(1/2) is a Euclidean distance and give expressions for it
   in paths, cycles, stars and complete graphs with n nodes. The sum of all
   communicability distances in a graph is introduced as a new graph
   invariant gamma(G). We compare this index with the Wiener and Kirchhoff
   indices of graphs and conjecture about the graphs with maximum and
   minimum values of this index. (C) 2012 Elsevier Inc. All rights
   reserved.}},
