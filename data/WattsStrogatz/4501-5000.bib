@article{ ISI:000285423200004,
Author = {Bullock, S. and Barnett, L. and Di Paolo, E. A.},
Title = {{Spatial Embedding and the Structure of Complex Networks}},
Journal = {{COMPLEXITY}},
Year = {{2010}},
Volume = {{16}},
Number = {{2}},
Pages = {{20-28}},
Month = {{NOV-DEC}},
Abstract = {{We review and discuss the structural consequences of embedding a random
   network within a metric space such that nodes distributed in this space
   tend to be connected to those nearby. We find that where the spatial
   distribution of nodes is maximally symmetrical some of the structural
   properties of the resulting networks are similar to those of random
   nonspatial networks. However, where the distribution of nodes is
   inhomogeneous in some way, this ceases to be the case, with consequences
   for the distribution of neighborhood sizes within the network, the
   correlation between the number of neighbors of connected nodes, and the
   way in which the largest connected component of the network grows as the
   density of edges is increased. We present an overview of these findings
   in an attempt to convey the ramifications of spatial embedding to those
   studying real-world complex systems. (C) 2010 Wiley Periodicals, Inc.
   Complexity 16: 20-28, 2010}},
