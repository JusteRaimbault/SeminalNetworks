@article{ ISI:000267599800003,
Author = {Alonso-Sanz, Ramon},
Title = {{Spatial order prevails over memory in boosting cooperation in the
   iterated prisoner's dilemma}},
Journal = {{CHAOS}},
Year = {{2009}},
Volume = {{19}},
Number = {{2}},
Month = {{JUN}},
Abstract = {{This paper presents some results on a spatial version of the iterated
   prisoner's dilemma in which every player imitates in any iteration the
   optimal strategy of its neighbors. Neighbors are defined with different
   degrees of random variation (initial rewiring) based on a square
   lattice, and optimal is defined with different degrees of memory,
   ranging from only the single preceding iteration up to all preceding
   iterations. It is concluded that memory notably stimulates cooperation
   in the iterated prisoner's dilemma played in ordered lattices, but it is
   unable to boost cooperation as the wiring network becomes highly
   disordered. (C) 2009 American Institute of Physics. {[}DOI:
   10.1063/1.3106322]}},
