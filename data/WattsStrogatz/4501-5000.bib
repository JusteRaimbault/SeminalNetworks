@article{ ISI:000233176400008,
Author = {Bunimovich-Mendrazitsky, S and Stone, L},
Title = {{Modeling polio as a disease of development}},
Journal = {{JOURNAL OF THEORETICAL BIOLOGY}},
Year = {{2005}},
Volume = {{237}},
Number = {{3}},
Pages = {{302-315}},
Month = {{DEC 7}},
Abstract = {{Poliomyelitis is a disease which began to appear in epidemic proportions
   in the late 19th century, paradoxically, just at the time when living
   conditions and developments in health were transforming enormously for
   the better. We present a simple age-class model that explains this
   ``disease of development{''} as a threshold phenomenon. Epidemics arise
   when improved conditions in hygiene are able to reduce disease
   transmission of polio amongst children below a critical threshold level.
   This generates a large susceptible adult population in which, under
   appropriate conditions, epidemics can propagate. The polio model is
   analysed in terms of its bifurcation properties and in terms of its
   non-equilibrium outbreak dynamics. (c) 2005 Elsevier Ltd. All rights
   reserved.}},
