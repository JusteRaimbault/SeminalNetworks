@article{ ISI:000331714700025,
Author = {Liu, Zhiliang and Ke, Lining and Liu, Huafeng and Huang, Wenhua and Hu,
   Zhenghui},
Title = {{Changes in Topological Organization of Functional PET Brain Network with
   Normal Aging}},
