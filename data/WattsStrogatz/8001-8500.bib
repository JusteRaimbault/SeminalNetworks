@article{ ISI:000268662800072,
Author = {Sun Rong-Sheng and Hua Da-Yin},
Title = {{Synchronization of Local Oscillations in a Spatial Rock-Scissors-Paper
   Game Model}},
Journal = {{CHINESE PHYSICS LETTERS}},
Year = {{2009}},
Volume = {{26}},
Number = {{8}},
Month = {{AUG}},
Abstract = {{We study a spatial rock-scissors-paper model in a square lattice and a
   quenched small-world network. The system exhibits a global oscillation
   in the quenched small-world network, but the oscillation disappears in
   the square lattice. We find that there is a local oscillation in the
   square lattice the same as in the quenched small-world network. We
   define sigma = 1/N Sigma(i)(d(i) - < d(i)>)(2) (where d(i) is the
   density of a kind of species and < d(i)> is the average value) as the
   variance of the oscillation amplitude in a certain local patch. It is
   found that.. decays in a power law with an increase of the local patch
   size.. in the square lattice sigma proportional to R-delta, but it
   remains constant with an increase of the patch size in the quenched
   small-world network. We can speculate that in the square lattice,
   superposition between the local oscillations in different patches leads
   to global stabilization, while in the quenched small-world network,
   long-range interactions can synchronize the local oscillations, and
   their coherence results in the global oscillation.}},
