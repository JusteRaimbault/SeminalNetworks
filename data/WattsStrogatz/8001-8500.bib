@article{ ISI:000388547800009,
Author = {Drago, Carlo and Ricciuti, Roberto},
Title = {{Communities detection as a tool to assess a reform of the Italian
   interlocking directorship network}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2017}},
Volume = {{466}},
Pages = {{91-104}},
Month = {{JAN 15}},
Abstract = {{Interlocking directorships are important communication channels among
   companies and may have anticompetitive effect. A corporate governance
   reform was introduced in 2011 to prevent interlocking directorships in
   the financial sector. We apply community detection techniques to the
   analysis of the networks in 2009 and 2012 to ascertain the effect of
   such reform on the Italian directorship network. We find that, although
   the number of interlocking directorships decreases in 2012, the
   reduction takes place mainly at the periphery of the network. The
   network core is stable, allowing the most connected companies to keep
   their strategic position. (C) 2016 Elsevier B.V. All rights reserved.}},
