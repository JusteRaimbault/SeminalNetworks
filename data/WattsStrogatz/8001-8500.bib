@article{ ISI:000327164400013,
Author = {Schumm, Phillip and Scoglio, Caterina},
Title = {{Bloom: A stochastic growth-based fast method of community detection in
   networks}},
Journal = {{JOURNAL OF COMPUTATIONAL SCIENCE}},
Year = {{2012}},
Volume = {{3}},
Number = {{5, SI}},
Pages = {{356-366}},
Month = {{SEP}},
Abstract = {{Networks are characterized by a variety of topological features and
   dynamics. Classifying nodes into communities, community structure, is
   important when exploring networks. This paper explores the community
   detection metric called modularity. The theoretical definitions of
   modularity are connected with intuitive insights into the compositions
   of communities. Local modularity costs/benefits are explored and an
   efficient stochastic algorithm, Bloom, is introduced, based on growing
   communities using local improvement measures. Three extensions of Bloom
   are presented that build upon the basic version. A numerical analysis
   compares Bloom with the popular fast-greedy algorithm and demonstrates
   the successful performance of the three modifications of Bloom. (C) 2012
   Elsevier B.V. All rights reserved.}},
