@article{ ISI:000267435200015,
Author = {Preciado, Victor M. and Verghese, George C.},
Title = {{Low-Order Spectral Analysis of the Kirchhoff Matrix for a Probabilistic
   Graph With a Prescribed Expected Degree Sequence}},
