@article{ ISI:000411155700006,
Author = {Politsky, Jeffrey M.},
Title = {{Brain Tumor-Related Epilepsy: a Current Review of the Etiologic Basis
   and Diagnostic and Treatment Approaches}},
Journal = {{CURRENT NEUROLOGY AND NEUROSCIENCE REPORTS}},
Year = {{2017}},
