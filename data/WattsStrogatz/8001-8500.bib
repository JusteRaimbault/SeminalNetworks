@article{ ISI:000276199400020,
Author = {Rodrigues, Francisco Aparecido and Costa, Luciano da Fontoura},
Title = {{Generalized connectivity between any two nodes in a complex network}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2010}},
Volume = {{81}},
Number = {{3, 2}},
Month = {{MAR}},
Abstract = {{This article focuses on the identification of the number of paths with
   different lengths between pairs of nodes in complex networks and how
   these paths can be used for characterization of topological properties
   of theoretical and real-world complex networks. This analysis revealed
   that the number of paths can provide a better discrimination of network
   models than traditional network measurements. In addition, the analysis
   of real-world networks suggests that the long-range connectivity tends
   to be limited in these networks and may be strongly related to network
   growth and organization.}},
