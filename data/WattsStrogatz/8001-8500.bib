@article{ ISI:000304099500012,
Author = {Kim, Yup and Choi, Woosik and Yook, Soon-Hyung},
Title = {{Modified Penna Bit-string Network Evolution Model for Scale-free
   Networks with Assortative Mixing}},
Journal = {{JOURNAL OF THE KOREAN PHYSICAL SOCIETY}},
Year = {{2012}},
Volume = {{60}},
Number = {{4}},
Pages = {{621-624}},
Month = {{FEB}},
Abstract = {{Motivated by biological aging dynamics, we introduce a network evolution
   model for social interaction networks. In order to study the effect of
   social interactions originating from biological and sociological reasons
   on the topological properties of networks, we introduce the
   activity-dependent rewiring process. From the numerical simulations, we
   show that the degree distribution of the obtained networks follows a
   power-law distribution with an exponentially decaying tail, P(k) similar
   to (k + c)(-gamma) exp(-k/k(0)). The obtained value of gamma is in the
   range 2 < gamma < 3, which is consistent with the values for real social
   networks. Moreover, we also show that the degree-degree correlation of
   the network is positive, which is a characteristic of social interaction
   networks. The possible applications of our model to real systems are
   also discussed.}},
