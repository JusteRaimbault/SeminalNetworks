@article{ ISI:000234008100025,
Author = {Yoon, BG and Choi, J and Choi, MY},
Title = {{Connectedness and strength of biological organisms}},
Journal = {{JOURNAL OF THE KOREAN PHYSICAL SOCIETY}},
Year = {{2005}},
Volume = {{47}},
Number = {{6}},
Pages = {{1053-1058}},
Month = {{DEC}},
Abstract = {{We study, via extensive Monte Carlo simulations, a model for failures in
   biological organisms with local load sharing. The qualitative behaviors
   of the model are found to be similar to those of the global load sharing
   case, but the system is weaker in the sense that it has smaller values
   of the critical stress and requires larger healing ability for a
   transition to the healthy state. When shortcuts or pathways along which
   load is transferred are introduced, the critical stress, in general,
   increases with the number of shortcuts present, and the system, thus,
   becomes stronger, manifesting the importance of connectedness in
   biological systems.}},
