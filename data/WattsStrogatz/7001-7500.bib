@inproceedings{ ISI:000245668200006,
Author = {Kirley, Michael and Stewart, Robert},
Editor = {{Obayashi, S and Deb, K and Poloni, C and Hiroyasu, T and Murata, T}},
Title = {{Multiobjective evolutionary algorithms on complex networks}},
