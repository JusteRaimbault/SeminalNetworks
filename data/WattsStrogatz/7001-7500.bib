@article{ ISI:000270336100041,
Author = {Gonzalez, Mario and Dominguez, David and Rodriguez, Francisco B.},
Title = {{Block attractor in spatially organized neural networks}},
Journal = {{NEUROCOMPUTING}},
Year = {{2009}},
Volume = {{72}},
Number = {{16-18}},
Pages = {{3795-3801}},
Month = {{OCT}},
Abstract = {{The model of attractor neural network on the small-world topology (local
   and random connectivity) is investigated. The synaptic weights are
   random, driving the network towards a disordered state for the neural
   activity. An ordered macroscopic neural state is induced by a bias in
   the network weight connections, and the network evolution when
   initialized in blocks of positive/negative activity is studied. The
   retrieval of the block-like structure is investigated. An application to
   the Hebbian learning of a pattern, carrying local information, is
   presented. The block and the global attractor compete according to the
   initial conditions and the change of stability from one to the other
   depends on the long-range character of the network connectivity, as
   shown with a flow-diagram analysis. Moreover, a larger number of blocks
   emerges with the network dilution. (C) 2009 Elsevier B.V. All rights
   reserved.}},
