@article{ ISI:000311070600001,
Author = {Shapiro, Michael and Delgado-Eckert, Edgar},
Title = {{Finding the probability of infection in an SIR network is NP-Hard}},
Journal = {{MATHEMATICAL BIOSCIENCES}},
