@article{ ISI:000432011000006,
Author = {Yu, Haitao and Wu, Xinyu and Cai, Lihui and Deng, Bin and Wang, Jiang},
Title = {{Modulation of Spectral Power and Functional Connectivity in Human Brain
   by Acupuncture Stimulation}},
