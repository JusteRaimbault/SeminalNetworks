@article{ ISI:000267580200008,
Author = {Mori, Fumito and Odagaki, Takashi},
Title = {{Synchronization of coupled oscillators on small-world networks}},
Journal = {{PHYSICA D-NONLINEAR PHENOMENA}},
Year = {{2009}},
Volume = {{238}},
Number = {{14}},
Pages = {{1180-1185}},
Month = {{JUL 1}},
Abstract = {{We investigate the synchronous dynamics of Kuramoto oscillators and van
   der Pol oscillators on Watts-Strogatz type small-world networks. The
   order parameters to characterize macroscopic synchronization are
   calculated by numerical integration. We focus on the difference between
   frequency synchronization and phase synchronization. In both oscillator
   systems, the critical coupling strength of the phase order is larger
   than that of the frequency order for the small-world networks. The
   critical coupling strength for the phase and frequency synchronization
   diverges as the network structure approaches the regular one. For the
   Kuramoto oscillators, the behavior can be described by a power-law
   function and the exponents are obtained for the two synchronizations.
   The separation of the critical point between the phase and frequency
   synchronizations is found only for small-world networks in the
   theoretical models studied. (C) 2009 Elsevier B.V. All rights reserved.}},
