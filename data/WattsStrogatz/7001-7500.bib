@article{ ISI:000311775200029,
Author = {Nocera, Antonino and Ursino, Domenico},
Title = {{PHIS: A system for scouting potential hubs and for favoring their
   ``growth{''} in a Social Internetworking Scenario}},
Journal = {{KNOWLEDGE-BASED SYSTEMS}},
Year = {{2012}},
Volume = {{36}},
Pages = {{288-299}},
Month = {{DEC}},
Abstract = {{In this paper we propose Phi Phi (PHIS), a system for scouting current
   and potential hubs in a Social Internetworking Scenario (SIS) and for
   favoring the ``growth{''} of these last ones in such a way that they can
   become real hubs quickly. Phi Phi operates on a SIS, rather than on a
   single social network. It uses a hypergraph-based model to represent the
   SIS. In order to verify if a user is a (current or potential) hub, it
   considers a range of criteria encompassing her reputation, her
   capability of receiving and delivering information, her position in the
   social networks of the SIS and the wideness of her profile. The same
   criteria are also exploited by Phi Phi to organize a campaign of
   stimulations to ``train{''} a potential hub to become a real one. (C)
   2012 Elsevier B.V. All rights reserved.}},
