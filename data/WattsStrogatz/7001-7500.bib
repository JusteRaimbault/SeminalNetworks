@article{ ISI:000303469900005,
Author = {Abe, Sumiyoshi and Suzuki, Norikazu},
Title = {{Aftershocks in modern perspectives: Complex earthquake network, aging,
   and non-Markovianity}},
Journal = {{ACTA GEOPHYSICA}},
Year = {{2012}},
Volume = {{60}},
Number = {{3}},
Pages = {{547-561}},
Month = {{JUN}},
Abstract = {{The phenomenon of aftershocks is studied in view of science of
   complexity. In particular, three different concepts are examined: (i)
   the complex-network representation of seismicity, (ii) the event-event
   correlations, and (iii) the effects of long-range memory. Regarding (i),
   it is shown that the clustering coefficient of the complex earthquake
   network exhibits a peculiar behavior at and after main shocks. Regarding
   (ii), it is found that aftershocks experience aging, and the associated
   scaling holds. And regarding (iii), the scaling relation to be satisfied
   by a class of singular Markovian processes is violated, implying the
   existence of the long-range memory in processes of aftershocks.}},
