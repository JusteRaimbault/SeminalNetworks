@article{ ISI:000283602700001,
Author = {Wu, Qingchu and Fu, Xinchu and Zhang, Haifeng and Small, Michael},
Title = {{OSCILLATIONS AND PHASE TRANSITION IN THE MEAN INFECTION RATE OF A FINITE
   POPULATION}},
