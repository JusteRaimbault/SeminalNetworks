@article{ ISI:000412613900007,
Author = {Bellingeri, Michele and Cassi, Davide},
Title = {{Robustness of weighted networks}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
