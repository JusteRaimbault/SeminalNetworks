@article{ ISI:000352748900006,
Author = {Sun, Xiaoling and Lin, Hongfei and Xu, Kan},
Title = {{A social network model driven by events and interests}},
Journal = {{EXPERT SYSTEMS WITH APPLICATIONS}},
