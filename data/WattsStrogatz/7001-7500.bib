@article{ ISI:000286927300007,
Author = {Liu, Xiwei and Chen, Tianping and Lu, Wenlian},
Title = {{CLUSTER SYNCHRONIZATION FOR LINEARLY COUPLED COMPLEX NETWORKS}},
