@article{ ISI:000256387900022,
Author = {Lambiotte, R.},
Title = {{Majority rule on heterogeneous networks}},
Journal = {{JOURNAL OF PHYSICS A-MATHEMATICAL AND THEORETICAL}},
Year = {{2008}},
Volume = {{41}},
Number = {{22, SI}},
Month = {{JUN 6}},
Note = {{Workshop on Complex Networks - from Biology to Information Technology,
   Pula, ITALY, JUL, 2007}},
Abstract = {{We focus on the majority rule (MR) applied on heterogeneous networks.
   When the underlying topology is homogeneous, the system is shown to
   exhibit a transition from an ordered regime to a disordered regime when
   the noise is increased. When the network exhibits modular structures, in
   contrast, the system may also exhibit an asymmetric regime, where the
   nodes in each community reach an opposite average opinion. Finally, the
   node degree heterogeneity is shown to play an important role by
   displacing the location of the order-disorder transition and by making
   the system exhibit non-equipartition of the average spin.}},
