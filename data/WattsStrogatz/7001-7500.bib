Title = {{Complex innovation networks, patent citations and power laws}},
Booktitle = {{PICMET `07: PORTLAND INTERNATIONAL CENTER FOR MANAGEMENT OF ENGINEERING
   AND TECHNOLOGY, VOLS 1-6, PROCEEDINGS: MANAGEMENT OF CONVERGING
   TECHNOLOGIES}},
Series = {{Portland International Conference on Management of Engineering and
   Technology}},
Year = {{2007}},
