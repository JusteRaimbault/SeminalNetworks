@inproceedings{ ISI:000364317900049,
Author = {Friedrich, Tobias and Krohmer, Anton},
Editor = {{Halldorsson, MM and Iwama, K and Kobayashi, N and Speckmann, B}},
Title = {{On the Diameter of Hyperbolic Random Graphs}},
