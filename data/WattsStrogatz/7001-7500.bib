   Costa, Antonio C. R. and Mattos, Viviane L. D. and Coelho, Helder},
Editor = {{Dimuro, GP and Adamatti, DF and Coelho, H and Sichman, JS and Balsa, J and Tedesco, P and Costa, ACD}},
Title = {{Analyzing the Evolution of Social Exchange Strategies in Social
   Preference-based MAS through an Evolutionary Spatial Approach of the
   Ultimatum Game}},
Booktitle = {{2012 Brazilian Workshop on Social Simulation (BWSS 2012): Advances in
   Social Simulation II}},
Series = {{Brazilian Workshop on Social Simulation}},
