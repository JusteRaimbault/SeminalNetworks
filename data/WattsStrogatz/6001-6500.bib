@article{ ISI:000167848000048,
Author = {Sun, K and Ouyang, Q},
Title = {{Distance distribution and reliability of small-world networks}},
Journal = {{CHINESE PHYSICS LETTERS}},
Year = {{2001}},
Volume = {{18}},
Number = {{3}},
Pages = {{452-454}},
Month = {{MAR}},
Abstract = {{We have studied the distribution of distances in small-world networks by
   computer simulation. We found that in a small-world network, the longest
   distance between two vertices is just slightly longer than the average
   distance, indicating that the efficiency of the network is absolute
   rather than in average. We also investigated the robustness of a
   small-world network by randomly cutting some long paths in the network.
   The results show that the network is reliable against random cutting.}},
