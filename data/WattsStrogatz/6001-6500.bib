@article{ ISI:000293567600001,
Author = {Sun, Weigang and Zhang, Jingyuan and Li, Changpin},
Title = {{Synchronization Analysis of Two Coupled Complex Networks with Time
   Delays}},
Journal = {{DISCRETE DYNAMICS IN NATURE AND SOCIETY}},
Year = {{2011}},
Abstract = {{This paper studies the synchronized motions between two complex networks
   with time delays, which include individual inner synchronization in each
   network and outer synchronization between two networks. Based on the
   Lyapunov stability theory and the linear matrix equality (LMI), a
   synchronous criterion for inner synchronization inside each network is
   derived. Numerical examples are given which fit the theoretical
   analysis. In addition, the involved numerical results show that the
   delays between two networks have little effect on inner synchronization.
   It is also shown that synchronous motions within each network or between
   two networks are not enhanced if individual intranetwork connections are
   allowed.}},
