@article{ ISI:000368610900002,
Author = {Hedley, Richard W.},
Title = {{Composition and sequential organization of song repertoires in Cassin's
   Vireo (Vireo cassinii)}},
Journal = {{JOURNAL OF ORNITHOLOGY}},
