@article{ ISI:000407011200025,
Author = {Racz, Frigyes Samuel and Mukli, Peter and Nagy, Zoltan and Eke, Andras},
Title = {{Increased prefrontal cortex connectivity during cognitive challenge
   assessed by fNIRS imaging}},
Journal = {{BIOMEDICAL OPTICS EXPRESS}},
