@article{ ISI:000406345900021,
Author = {Hong, Seok-Jun and Bernhardt, Boris C. and Gill, Ravnoor S. and
   Bernasconi, Neda and Bernasconi, Andrea},
Title = {{The spectrum of structural and functional network alterations in
   malformations of cortical development}},
Journal = {{BRAIN}},
Year = {{2017}},
Volume = {{140}},
