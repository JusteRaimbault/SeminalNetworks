@article{ ISI:000374989300009,
Author = {Feng, Minyu and Qu, Hong and Yi, Zhang and Xie, Xiurui and Kurths,
   Jurgen},
Title = {{Evolving Scale-Free Networks by Poisson Process: Modeling and Degree
   Distribution}},
