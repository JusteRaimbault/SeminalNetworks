@article{ ISI:000376693600048,
Author = {Zhou, Guangye and Li, Chengren and Li, Tingting and Yang, Yi and Wang,
   Chen and He, Fangjun and Sun, Jingchang},
Title = {{Outer synchronization investigation between WS and NW small-world
   networks with different node numbers}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2016}},
Volume = {{457}},
Pages = {{506-513}},
Month = {{SEP 1}},
Abstract = {{Some typical dual-ring erbium-doped fiber lasers with hyperchaos
   behaviors are taken as nodes to construct two kinds of small-world
   networks NW and WS networks. Based on Lyapunov stability theorem, the
   appropriate controllers are designed and the outer synchronization
   between the small-world networks with diverse structures and different
   node numbers is further investigated. The simulation results show that
   the perfect synchronization between the complex small-world networks is
   realized, which is of potential application for all optical
   communication network. (C) 2016 Elsevier B.V. All rights reserved.}},
