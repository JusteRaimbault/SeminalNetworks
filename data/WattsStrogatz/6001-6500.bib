@article{ ISI:000344359300007,
Author = {Wilson, A. D. M. and Croft, D. P. and Krause, J.},
Title = {{Social networks in elasmobranchs and teleost fishes}},
Journal = {{FISH AND FISHERIES}},
Year = {{2014}},
Volume = {{15}},
Number = {{4}},
Pages = {{676-689}},
Month = {{DEC}},
Abstract = {{Over the last decade, there has been an exponential increase in studies
   using social network analysis to describe the structure of animal
   societies. In this synthesis, we examine the contribution of social
   network analysis towards developing our understanding of the social
   organization of elasmobranchs and teleost fishes. We review and discuss
   the current state of knowledge of the mechanisms and functions
   underpinning social network structure in fishes with particular emphasis
   on cooperation, familiarity, site fidelity, population structure and the
   welfare of captive populations. We also discuss important methodological
   issues (e.g. how to identify and mark fish) and highlight new
   developments in this area of research and their implications for the
   study of fish behaviour. Finally, we outline promising future research
   areas for the application of social network analysis to teleost fishes
   and elasmobranchs.}},
