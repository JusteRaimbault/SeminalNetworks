@article{ ISI:000255369500029,
Author = {Qi, G. X. and Huang, H. B. and Chen, L. and Wang, H. J. and Shen, C. K.},
Title = {{Fast synchronization in neuronal networks}},
Journal = {{EPL}},
Year = {{2008}},
Volume = {{82}},
Number = {{3}},
Month = {{MAY}},
Abstract = {{We study the fast synchronization in complex networks of coupled
   Hindmarsh-Rose neurons. The relation between the maximal Lyapunov
   exponent corresponding to the least stable transverse mode and the speed
   of synchronization is given, based on which we can obtain an optimal
   value of global coupling strength, with which the network synchronizes
   with the minimal synchronization time. The speed limits of several kinds
   of complex networks (small-world, scale-free, and modular) with
   different eigenratios are studied. Finally, we extend to the modified
   Hodgkin-Huxley neuron case. Copyright (C) EPLA, 2008.}},
