@article{ ISI:000350338800007,
Author = {Zhou, Xue and Hu, Yong and Wu, Yue and Xiong, Xi},
Title = {{Influence analysis of information erupted on social networks based on
   SIR model}},
Journal = {{INTERNATIONAL JOURNAL OF MODERN PHYSICS C}},
Year = {{2015}},
Volume = {{26}},
Number = {{2}},
Month = {{FEB}},
Abstract = {{In this paper, according to the similarity of chain reaction principle
   and the characteristics of information propagation on social network, we
   proposed a new word `information bomb{''}. Based on the complex networks
   and SIR model, dynamical evolution equations were setup. Then methods
   used to evaluate the four indexes of bomb power were given, including
   influence breadth, influence strength, peak time and relaxation time. At
   last, the power of information was ascertained through these indexes.
   The process of information propagation is simulated to illustrate the
   spreading characteristics through the results. Then parameters which
   impact on the power of information bomb are analyzed and some methods
   which control the propagation of information are given.}},
