@article{ ISI:000400353700014,
Author = {Zhu, Yifei and Bai, Lin and Liang, Panpan and Kang, Shan and Gao, Hengbo
   and Yang, Haiqing},
Title = {{Disrupted brain connectivity networks in acute ischemic stroke patients}},
