@article{ ISI:000170493100041,
Author = {Cosenza, MG and Tucci, K},
Title = {{Pattern formation on trees}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2001}},
Volume = {{64}},
Number = {{2, 2}},
Month = {{AUG}},
Abstract = {{Networks having the geometry and the connectivity of trees are
   considered as the spatial support of spatiotemporal dynamical processes.
   A tree is characterized by two parameters: its ramification and its
   depth. The local dynamics at the nodes of a tree is described by a
   nonlinear map, giving rise to a coupled map lattice system. The coupling
   is expressed by a matrix whose eigenvectors constitute a basis on which
   spatial patterns on trees can be expressed by linear combination. The
   spectrum of eigenvalues of the coupling matrix exhibit a nonuniform
   distribution that manifests itself in the bifurcation structure of the
   spatially synchronized modes. These models may describe
   reaction-diffusion processes and several other phenomena occurring on
   heterogeneous media with hierarchical structure.}},
