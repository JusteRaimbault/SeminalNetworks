@article{ ISI:000368589300010,
Author = {Mukherjee, Satyam and Uzzi, Brian and Jones, Ben and Stringer, Michael},
Title = {{A New Method for Identifying Recombinations of Existing Knowledge
   Associated with High-Impact Innovation}},
Journal = {{JOURNAL OF PRODUCT INNOVATION MANAGEMENT}},
