@inproceedings{ ISI:000227501400102,
Author = {Kitani, T and Funabiki, N and Higashino, T},
Editor = {{Pung, HK and BuSungLee, F and Tham, CK and Kuttan, S}},
Title = {{A proposal of hierarchical chordal ring network topology for WDM
   networks}},
