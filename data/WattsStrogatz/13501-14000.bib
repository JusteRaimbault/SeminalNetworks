@inproceedings{ ISI:000286799900024,
Author = {Mungovan, Declan and Howley, Enda and Duggan, Jim},
Editor = {{Coyle, L and Freyne, J}},
Title = {{Norm Convergence in Populations of Dynamically Interacting Agents}},
Booktitle = {{ARTIFICIAL INTELLIGENCE AND COGNITIVE SCIENCE}},
