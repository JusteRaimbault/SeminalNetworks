@inproceedings{ ISI:000189411800045,
Author = {Fuhrmann, T},
Book-Group-Author = {{IEEE}},
Title = {{On the topology of overlay-networks}},
Booktitle = {{ICON 2003: 11TH IEEE INTERNATIONAL CONFERENCE ON NETWORKS}},
