@inproceedings{ ISI:000291559800088,
Author = {Wang, Dan and Li, Yanping and Liu, Wenwei and Dong, Liang and Li, Lin},
Book-Group-Author = {{IEEE}},
Title = {{Routing Strategy with Waiting Time on Scale-free Network}},
Booktitle = {{11TH INTERNATIONAL CONFERENCE ON CONTROL, AUTOMATION, ROBOTICS AND
   VISION (ICARCV 2010)}},
Series = {{International Conference on Control Automation Robotics and Vision}},
Year = {{2010}},
Pages = {{523-526}},
Note = {{11th International Conference on Control, Automation, Robotics and
   Vision (ICARCV 2010), Singapore, SINGAPORE, DEC 07-10, 2010}},
Abstract = {{We study traffic dynamics and propose a new routing strategy based on
   the local topological information in scale-free network. According to
   the new routing strategy with a single tunable parameter, a packet is
   delivered to next node with probabilities that depend not only on the
   degrees of the next node but also the estimated waiting time at the new
   node. The routing algorithm is implemented in BA scale-free networks in
   the case of nodes' delivering ability proportional to its degree.
   Simulations show that the maximal network capacity corresponds to an
   optimal controlled parameter.}},
Publisher = {{IEEE}},
Address = {{345 E 47TH ST, NEW YORK, NY 10017 USA}},
