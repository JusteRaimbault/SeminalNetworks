@article{ ISI:000237495400003,
Author = {Xia, YX and Tse, CK and Lau, FCM and Tam, WM and Shan, XM},
Title = {{Telephone traffic analysis based on scale-free user network and
   scale-free load distribution}},
Journal = {{DYNAMICS OF CONTINUOUS DISCRETE AND IMPULSIVE SYSTEMS-SERIES
   B-APPLICATIONS \& ALGORITHMS}},
Year = {{2006}},
Volume = {{13}},
Number = {{3-4}},
Pages = {{345-352}},
