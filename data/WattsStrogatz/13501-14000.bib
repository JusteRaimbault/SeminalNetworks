@article{ ISI:000251326200001,
Author = {Awazu, Akinori},
Title = {{Network structure formation in pulsating particle populations}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2007}},
Volume = {{76}},
Number = {{5, 2}},
Month = {{NOV}},
Abstract = {{Pattern formations in populations of ``pulsating particles{''} which can
   autonomously vary their volumes were investigated. It was found that
   clusters of pulsating particles whose pulsation frequencies depend on
   the number of other particles in the vicinity form the following
   dynamical network structures: (i) void type networks exhibiting large
   voids, and (ii) branching type networks consisting of individual
   branches. Such global structures are formed through the annihilation and
   creation of local structures in the particle populations induced by
   tug-of-war-like interactions among them.}},
