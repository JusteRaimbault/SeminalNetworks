@article{ ISI:000473250200004,
Author = {Sharma, Saurabh Kumar and Singh, Soibam Shyamchand and Haobijam,
   Dineshchandra and Malik, Md. Zubbair and Singh, R. K. Brojen},
Title = {{Organization in complex brain networks: Energy distributions and phase
   shift}},
Journal = {{JOURNAL OF THEORETICAL BIOLOGY}},
Year = {{2019}},
Volume = {{476}},
Pages = {{30-35}},
Month = {{SEP 7}},
Abstract = {{The Hamiltonian function of a network, derived from the intrinsic
   distributions of nodes and edges, magnified by resolution parameter has
   information on the distribution of energy in the network. In brain
   networks, the Hamiltonian function follows hierarchical features
   reflecting a power-law behavior which can be a signature of
   self-organization. Further, the transition of three distinct phases
   driven by resolution parameter is observed which could correspond to
   various important brain states. This resolution parameter could thus
   reflect a key parameter that controls and balances the energy
   distribution in the brain network. (C) 2019 Published by Elsevier Ltd.}},
