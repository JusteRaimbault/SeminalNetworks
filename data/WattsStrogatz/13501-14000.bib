@article{ ISI:000245671800015,
Author = {Karami, Asieh and Samani, Keivan Aghababaei},
Title = {{Characteristics of `semi-bipartite' networks}},
Journal = {{PHYSICA SCRIPTA}},
Year = {{2007}},
Volume = {{75}},
Number = {{3}},
Pages = {{316-319}},
Month = {{MAR}},
Abstract = {{We study some characteristics of a model for networks called the
   semi-bipartite networks. This model is constructed from two parts. In
   the first part which is called the centre of the network all nodes are
   connected together. The nodes of the second part ( peripheral nodes) are
   not connected to each other, but there are some links between them and
   the nodes of the centre ( central nodes). We show that such a network
   has a high clustering coefficient and a short average path length. In
   this model these two characteristics of the network are independent of
   the size of the network directly, but it depends on the fraction of
   central nodes. In this model the average path length has an upper bound
   and the clustering coefficient has a lower bound regardless of the size
   of the network.}},
