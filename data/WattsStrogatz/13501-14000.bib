Title = {{Resource limitations, transmission costs and critical thresholds in
   scale-free networks}},
Booktitle = {{AGENT AND MULTI-AGENT SYSTEMS: TECHNOLOGIES AND APPLICATIONS,
   PROCEEDINGS}},
