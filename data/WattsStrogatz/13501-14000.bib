@article{ ISI:000280300500003,
Author = {Bi, Qiao and Fang, Jin-qing and Liu, Gui-ping},
Title = {{Quantum information density and network}},
Journal = {{FRONTIERS OF PHYSICS IN CHINA}},
Year = {{2009}},
Volume = {{4}},
Number = {{1}},
Pages = {{38-48}},
Month = {{MAR}},
Abstract = {{We present a quantum information network in which quantum information
   density is used for performing quantum computing or teleportation. The
   photons are entangled in quantum channels and play a role of flying ebit
   to transmit interaction among the nodes. A particular quantum Gaussian
   channel is constructed; it permits photon-encoded information to
   transmit quantum signals with certain quantum parallelism. The
   corresponding quantum dynamical mutual information is discussed, and the
   controlling nodes connectivity by driving the network is studied. With
   regard to different driving functions, the connectivity distribution of
   the network is complicated. They obey positive or negative power law,
   and also influence the assortativity coefficient or the dynamical
   property of the network.}},
