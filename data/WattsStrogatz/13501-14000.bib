@article{ ISI:000251720200007,
Author = {Westra, Ronald L. and Hollanders, Goele and Bex, Geert Jan and Gyssens,
   Marc and Tuyls, Karl},
Title = {{The pattern memory of gene-protein networks}},
Journal = {{AI COMMUNICATIONS}},
