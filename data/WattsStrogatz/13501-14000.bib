@inproceedings{ ISI:000189451400079,
Author = {Helmy, A},
Book-Group-Author = {{IEEE}},
Title = {{TRANSFER: Transactions Routing for Ad-hoc Networks with eFficient EneRgy}},
Booktitle = {{GLOBECOM'03: IEEE GLOBAL TELECOMMUNICATIONS CONFERENCE, VOLS 1-7}},
