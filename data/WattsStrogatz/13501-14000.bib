@inproceedings{ ISI:000395046500017,
Author = {Han, Xiuping and Fu, Xilin},
Editor = {{Luo, ACJ}},
Title = {{Impulsive Control Induced Effects on Dynamics of Complex Networks}},
Booktitle = {{DYNAMICAL SYSTEMS: DISCONTINUITY, STOCHASTICITY AND TIME-DELAY}},
Year = {{2010}},
Pages = {{203-216}},
Note = {{2nd Conference on Dynamics, Vibration and Control (DVC2009), Chengdu,
   PEOPLES R CHINA, AUG 15-29, 2009}},
Abstract = {{Control and synchronization of complex networks have been extensively
   investigated in many research and application fields. Previous works
   focused upon realizing synchronization by varied methods. There has been
   little research on the dynamics of synchronization manifold of complex
   networks by now. It was known that dynamics of the single system can be
   changed very obviously after inputting particular impulse signals. For
   the first time, above impulsive control of complex networks is
   considered in this chapter. Complex networks can realize to synchronize
   with such impulsive control. And dynamics of the synchronous state of
   complex networks can be induced to different orbit. The orbit may be an
   equilibrium point, a periodic orbit,or a chaotic orbit, which is
   determined by a parameter in the outer impulse signal. Strict theories
   are given.}},
