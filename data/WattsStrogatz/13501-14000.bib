@inproceedings{ ISI:000307413400056,
Author = {Hu, M. B. and Jiang, R. and Wu, Y. H. and Wu, Q. S.},
Editor = {{Zhou, J}},
Title = {{The Effects of Link and Node Capacity on Traffic Dynamics in Weighted
   Scale-Free Networks}},
Booktitle = {{COMPLEX SCIENCES, PT 1}},
