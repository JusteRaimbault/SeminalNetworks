@article{ ISI:000270300600082,
Author = {Li Yan-Long and Ma Jun and Zhang Wei and Liu Yan-Jun},
Title = {{Order parameters and synchronization of FitzHugh-Nagumo small-world
   networks}},
Journal = {{CHINESE PHYSICS B}},
Year = {{2009}},
Volume = {{18}},
Number = {{10}},
Pages = {{4598-4602}},
Month = {{OCT}},
Abstract = {{This paper numerically investigates the order parameter and
   synchronisation in the small world connected FitzHugh-Nagumo excitable
   systems. The simulations show that the order parameter continuously
   decreases with increasing D, the quality of the synchronisation worsens
   for large noise intensity. As the coupling intensity goes up, the
   quality of the synchronisation worsens, and it finds that the larger
   rewiring probability becomes the larger order parameter. It obtains the
   complete phase diagram for a wide range of values of noise intensity D
   and control parameter g.}},
