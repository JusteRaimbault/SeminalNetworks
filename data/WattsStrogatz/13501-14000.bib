@inproceedings{ ISI:000225878300001,
Author = {Freeman, WJ},
Editor = {{Pal, NR and Kasabov, N and Mudi, RK and Pal, S and Parui, SK}},
Title = {{Neurobiological foundation for the meaning of information}},
