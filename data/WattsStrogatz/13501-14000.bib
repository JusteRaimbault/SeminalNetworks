@article{ ISI:000241200000054,
Author = {Tam, Wai M. and Lau, Francis C. M. and Tse, Chi K. and Xia, Yongxiang
   and Shan, Xiuming},
Title = {{Effect of clustering in a complex user network on the telephone traffic}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2006}},
Volume = {{371}},
Number = {{2}},
Pages = {{745-753}},
Month = {{NOV 15}},
Abstract = {{Recently, it has been found that the degree distribution of a user
   network has a significant effect on the traffic carried by a telephone
   network. In this paper, we investigate the effect of user clustering on
   the telephone traffic based on computer simulations. Both uniform and
   scale-free user networks have been considered. In a uniform user
   network, it has been found that the carried traffic is not affected by
   the clustering. On the other hand, a scale-free user network with high
   clustering produces more carried traffic compared to one with smaller
   clustering because of the larger call arrival rate and the smaller call
   blocking rate. (c) 2006 Elsevier B.V. All rights reserved.}},
