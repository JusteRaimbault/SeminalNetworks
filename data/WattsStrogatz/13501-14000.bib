Language = {{English}},
Affiliation = {{Hong, DW (Reprint Author), Rutgers State Univ, Ctr Computat \& Integrat Biol, Camden, NJ 08102 USA.
   Hong, Dawei, Rutgers State Univ, Ctr Computat \& Integrat Biol, Camden, NJ 08102 USA.
   Man, Shushuang, SW Minnesota State Univ, Dept Math \& Comp Sci, Marshall, MN 56258 USA.}},
DOI = {{10.1016/j.jtbi.2009.10.019}},
ISSN = {{0022-5193}},
Keywords = {{Signal propagation; Stochastic differential equation; Weak internal
   noise; Small-world network; Measure concentration}},
