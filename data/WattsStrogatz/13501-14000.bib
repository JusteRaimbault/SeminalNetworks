@inproceedings{ ISI:000292494100007,
Author = {Thiriot, Samuel and Kant, Jean-Daniel},
Editor = {{Takadama, K and CioffiRevilla, C and Deffuant, G}},
Title = {{A Naturalistic Multi-Agent Model of Word-of-Mouth Dynamics}},
