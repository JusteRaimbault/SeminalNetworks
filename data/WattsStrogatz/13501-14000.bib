@inproceedings{ ISI:000241472100025,
Author = {Koroutchev, Kostadin and Korutcheva, Elka},
Editor = {{Kollias, S and Stafylopatis, A and Duch, W and Oja, E}},
Title = {{Improved storage capacity of Hebbian learning attractor neural network
   with bump formations}},
