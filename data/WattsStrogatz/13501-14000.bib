@inproceedings{ ISI:000236462500047,
Author = {Huang, Y and Jin, BH and Ca, JN},
Editor = {{Cantarella, JD}},
Title = {{A distributed approach to construction of topology mismatching aware P2P
   overlays in wireless ad hoc networks}},
Booktitle = {{Proceedings of the 14th Euromicro International Conference on Parallel,
   Distributed, and Network-Based Processing}},
Year = {{2005}},
Pages = {{340-347}},
Note = {{14th Euromicro Conference on Parallel, Distributed and Network-Bases
   Processing, Montbeliard Sochaux, FRANCE, FEB 15-17, 2006}},
Organization = {{AMD; sgi; PSA Peugeot Citroen; IEEE Comp Soc; IEEE}},
Abstract = {{Peer-to-Peer computing is mainly based on the virtual overlay network
   constructed in the application layer. Often, there is topology
   mismatching between the overlay network and the physical network, which
   may cause great traffic overhead In this paper, we study the topology
   mismatching problem and its impact on communication in wireless ad hoc
   networks. We present an efficient, fully distributed algorithm, named
   D-TAOC, for constructing the overlay network. By qualitative analysis
   and simulation experiments, we show that D-TAOC can significantly reduce
   the traffic overhead while slightly sacrificing the routing efficiency.
   We also prove that D-TAOC works well in a dynamic peer-to-peer
   environment.}},
