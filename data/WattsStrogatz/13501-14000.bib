@inproceedings{ ISI:000295049108016,
Author = {Schmidt, Gerd S. and Wu, Jingbo and Muenz, Ulrich and Allgoewer, Frank},
Book-Group-Author = {{IEEE}},
Title = {{Consensus in Bistable and Multistable Multi-Agent Systems}},
Booktitle = {{49TH IEEE CONFERENCE ON DECISION AND CONTROL (CDC)}},
Series = {{IEEE Conference on Decision and Control}},
Year = {{2010}},
Pages = {{7135-7140}},
Note = {{49th IEEE Conference on Decision and Control (CDC), Atlanta, GA, DEC
   15-17, 2010}},
Organization = {{IEEE}},
Abstract = {{Consensus in bistable and multistable multi-agent systems (MAS) is
   investigated. The considered MAS consists of agents with nonlinear,
   bistable or multistable dynamics and local coupling. For this MAS,
   consensus conditions are presented for arbitrary large networks
   depending on the algebraic connectivity of the underlying graph. In
   contrast to most publications in the consensus literature, the
   considered MAS has only a finite number of discrete consensus points
   instead of a consensus subspace. This introduces a new kind of consensus
   problem to the literature on MAS. The differences to classical consensus
   problems are discussed and the main result is illustrated in a
   simulation example.}},
Publisher = {{IEEE}},
Address = {{345 E 47TH ST, NEW YORK, NY 10017 USA}},
Type = {{Proceedings Paper}},
