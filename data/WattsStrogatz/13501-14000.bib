@article{ ISI:000470233700021,
Author = {Zong, Guangdeng and Yang, Dong},
Title = {{H-infinity synchronization of switched complex networks: A switching
   impulsive control method}},
Journal = {{COMMUNICATIONS IN NONLINEAR SCIENCE AND NUMERICAL SIMULATION}},
Year = {{2019}},
Volume = {{77}},
