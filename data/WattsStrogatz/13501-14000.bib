@inproceedings{ ISI:000246369100056,
Author = {Wu, Shiquan and Li, Jing},
Editor = {{Mandoiu, I and Zelikovsky, A}},
Title = {{Comparative analysis of gene-coexpression networks across species}},
Booktitle = {{BIOINFORMATICS RESEARCH AND APPLICATIONS, PROCEEDINGS}},
