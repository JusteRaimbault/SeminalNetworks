@article{ ISI:000383433100001,
Author = {Faulk, Lewis and Willems, Jurgen and Johnson, Jasmine McGinnis and
   Stewart, Amanda J.},
Title = {{Network Connections and Competitively Awarded Funding: The impacts of
   board network structures and status interlocks on nonprofit
   organizations' foundation grant acquisition}},
Journal = {{PUBLIC MANAGEMENT REVIEW}},
Year = {{2016}},
Volume = {{18}},
Number = {{10}},
Pages = {{1425-1455}},
Abstract = {{This analysis investigates whether nonprofit board connections with
   other nonprofit organizations and foundations explain organizational
   performance in earning foundation grants. Using a sample of 402
   nonprofits and sixty-eight foundations in a single metropolitan area, we
   find that greater connectedness and status interlocks significantly
   influence organizations' ability to acquire resources. Network effects
   are partially mediated by the number of past grants received and a
   nonprofit's financial characteristics, including organizational size,
   fundraising expenses, and financial health. These findings, while
   supporting the role of networks in resource attainment, point to the
   complex and mutual relationships between organizational characteristics,
   network characteristics, and organizational performance.}},
