@article{ ISI:000361009700008,
Author = {Vecchio, Fabrizio and Miraglia, Francesca and Valeriani, Lavinia and
   Scarpellini, Maria Gabriella and Bramanti, Placido and Mecarelli, Oriano
   and Rossini, Paolo M.},
Title = {{Cortical Brain Connectivity and B-Type Natriuretic Peptide in Patients
   With Congestive Heart Failure}},
