@article{ ISI:000376582900024,
Author = {Iyer, Swami and Killingback, Timothy},
Title = {{Evolution of Cooperation in Social Dilemmas on Complex Networks}},
Journal = {{PLOS COMPUTATIONAL BIOLOGY}},
Year = {{2016}},
Volume = {{12}},
Number = {{2}},
Month = {{FEB}},
Abstract = {{Cooperation in social dilemmas is essential for the functioning of
   systems at multiple levels of complexity, from the simplest biological
   organisms to the most sophisticated human societies. Cooperation,
   although widespread, is fundamentally challenging to explain
   evolutionarily, since natural selection typically favors selfish
   behavior which is not socially optimal. Here we study the evolution of
   cooperation in three exemplars of key social dilemmas, representing the
   prisoner's dilemma, hawk-dove and coordination classes of games, in
   structured populations defined by complex networks. Using
   individual-based simulations of the games on model and empirical
   networks, we give a detailed comparative study of the effects of the
   structural properties of a network, such as its average degree, variance
   in degree distribution, clustering coefficient, and assortativity
   coefficient, on the promotion of cooperative behavior in all three
   classes of games.}},
