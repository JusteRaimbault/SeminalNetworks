@article{ ISI:000381440400016,
Author = {Wang, Pei and Chen, Yao and Lu, Jinhu and Wang, Qingyun and Yu, Xinghuo},
Title = {{Graphical Features of Functional Genes in Human Protein Interaction
   Network}},
