@article{ ISI:000390228600007,
Author = {Shinoda, Kenji and Kaneko, Kunihiko},
Title = {{Chaotic Griffiths Phase with Anomalous Lyapunov Spectra in Coupled Map
   Networks}},
Journal = {{PHYSICAL REVIEW LETTERS}},
Year = {{2016}},
Volume = {{117}},
Number = {{25}},
Month = {{DEC 16}},
Abstract = {{Dynamics of coupled chaotic oscillators on a network are studied using
   coupled maps. Within a broad range of parameter values representing the
   coupling strength or the degree of elements, the system repeats
   formation and split of coherent clusters. The distribution of the
   cluster size follows a power law with the exponent a, which changes with
   the parameter values. The number of positive Lyapunov exponents and
   their spectra are scaled anomalously with the power of the system size
   with the exponent beta, which also changes with the parameters. The
   scaling relation alpha similar to 2(beta+1) is uncovered, which is
   universal independent of parameters and among random networks.}},
