@article{ ISI:000343003100019,
Author = {Feng, Minyu and Qu, Hong and Yi, Zhang},
Title = {{Highest Degree Likelihood Search Algorithm Using a State Transition
   Matrix for Complex Networks}},
Journal = {{IEEE TRANSACTIONS ON CIRCUITS AND SYSTEMS I-REGULAR PAPERS}},
Year = {{2014}},
Volume = {{61}},
