@article{ ISI:000345725300051,
Author = {Huang, He and Yan, Zhijun and Pan, Yaohui},
Title = {{Measuring edge importance to improve immunization performance}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2014}},
Volume = {{416}},
Pages = {{532-540}},
Month = {{DEC 15}},
Abstract = {{The edge heterogeneity has a remarkable influence on disease spreading,
   but it has seldom been considered in the disease-controlling policies.
   Based on the gravity model, we propose the edge importance index to
   describe the influence of edge heterogeneity on immunization strategies.
   Then the edge importance and contact weight are combined to calculate
   the infection rates on the I-S (Infected-Susceptible) edges in the
   complex network, and the difference of the infection rates on strong and
   weak ties is analyzed. Simulation results show that edge heterogeneity
   has a significant influence on the performance of immunization
   strategies, and better immunization efficiency is derived when the
   vaccination rate of the nodes in the weak I-S edges is increased. (C)
   2014 Elsevier B.V. All rights reserved.}},
