@article{ ISI:000275133900033,
Author = {Fang Jin-Qing and Li Yong},
Title = {{Transition Features from Simplicity-Universality to
   Complexity-Diversification Under UHNTF}},
Journal = {{COMMUNICATIONS IN THEORETICAL PHYSICS}},
Year = {{2010}},
Volume = {{53}},
Number = {{2}},
Pages = {{389-398}},
Month = {{FEB}},
Abstract = {{A large unified hybrid network model with a variable speed growth
   (LUHNM-VSG) is proposed as third model of the unified hybrid network
   theoretical framework (UHNTF). A hybrid growth ratio vg of deterministic
   linking number to random linking number and variable speed growth index
   alpha are introduced in it. The main effects of vg and alpha on
   topological transition features of the LUHNM-BSG are revealed. For
   comparison with the other models, we construct a type of the network
   complexity pyramid with seven levels, in which from the bottom level-1
   to the top level-7 of the pyramid simplicity-unversality is increasing
   but complexity-diversity is decreasing. The transition relations between
   them depend on matching of four hybrid ratios (dr, fd, gr, vg). The
   LUHNM-VSG as the level-1 of the pyramid is much better and closer to
   description of real-world networks as well as has potential application.}},
