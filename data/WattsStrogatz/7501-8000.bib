@article{ ISI:000256252600107,
Author = {Zhong Li-Xin and Qiu Tian and Xu Jiang-Rong},
Title = {{Heterogeneity improves cooperation in continuous snowdrift game}},
Journal = {{CHINESE PHYSICS LETTERS}},
Year = {{2008}},
Volume = {{25}},
Number = {{6}},
Pages = {{2315-2318}},
Month = {{JUN}},
Abstract = {{We study the networking effects on the population divergence and the
   increased level of cooperation in the continuous snowdrift game (CSG).
   In the regular world, limited interaction inhibits the occurrence of
   evolutionary branching. The formation of clusters defends the
   intermediate-investors from intruding by high- or low-investors. In a
   rewired network, the collective behaviour is related to the rewiring
   rules. A linear relationship I = alpha sigma + b between the average
   investment and the standard deviation of the degree distribution is
   found.}},
