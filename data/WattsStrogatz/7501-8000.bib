@article{ ISI:000279238400026,
Author = {Schmidt, G. and Zamora-Lopez, G. and Kurths, J.},
Title = {{SIMULATION OF LARGE SCALE CORTICAL NETWORKS BY INDIVIDUAL NEURON
   DYNAMICS}},
