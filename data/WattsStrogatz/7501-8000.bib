@inproceedings{ ISI:000272071500013,
Author = {Akoglu, Leman and Faloutsos, Christos},
Editor = {{Buntine, W and Grobelnik, M and Mladenic, D and ShaweTaylor, J}},
Title = {{RTG: A Recursive Realistic Graph Generator Using Random Typing}},
Booktitle = {{MACHINE LEARNING AND KNOWLEDGE DISCOVERY IN DATABASES, PT I}},
