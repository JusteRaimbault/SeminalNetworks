@article{ ISI:000375799600009,
Author = {Park, Bo-yong and Kim, Mansu and Seo, Jongbum and Lee, Jong-min and
   Park, Hyunjin},
Title = {{Connectivity Analysis and Feature Classification in Attention Deficit
   Hyperactivity Disorder Sub-Types: A Task Functional Magnetic Resonance
   Imaging Study}},
Journal = {{BRAIN TOPOGRAPHY}},
