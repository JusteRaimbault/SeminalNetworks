@article{ ISI:000367819200016,
Author = {Wang, Bohui and Wang, Jingcheng and Zhang, Langwen and Ge, Yang},
Title = {{Joining consensus of networked multi-agent systems with nonlinear
   couplings and weighting constraints}},
Journal = {{INTERNATIONAL JOURNAL OF SYSTEMS SCIENCE}},
Year = {{2016}},
Volume = {{47}},
Number = {{5}},
Pages = {{1185-1201}},
Month = {{APR 3}},
Abstract = {{This paper studies the joining consensus of networked multi-agent
   systems subject to nonlinear couplings and weighted directed graphs via
   pinning control. A weighted-average consensus protocol is proposed to
   achieve the collective decision by interacting with the local
   information of some pinned agents. By proposing a novel joining
   consensus protocol, average consensus and general consensus strategies
   are joined to achieve an agreement for the weighting networked system.
   Furthermore, by calculating a proper consensus gain and using finite
   control Lyapunov controllers, an efficient joining consensus protocol is
   presented to improve the consensus speed. Sufficient conditions for
   achieving the consensuses asymptotically are proved. Finally,
   theoretical results are validated via simulations.}},
