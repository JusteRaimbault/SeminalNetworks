@article{ ISI:000258427500005,
Author = {Carillo, Maria Rosaria and Papagni, Erasmo and Capitanio, Fabian},
Title = {{Effects of social interactions on scientists' productivity}},
Journal = {{INTERNATIONAL JOURNAL OF MANPOWER}},
Year = {{2008}},
