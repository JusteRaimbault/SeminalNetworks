@article{ ISI:000392318600004,
Author = {Cencetti, Giulia and Bagnoli, Franco and Battistelli, Giorgio and
   Chisci, Luigi and Di Patti, Francesca and Fanelli, Duccio},
Title = {{Topological stabilization for synchronized dynamics on networks}},
Journal = {{EUROPEAN PHYSICAL JOURNAL B}},
Year = {{2017}},
Volume = {{90}},
Number = {{1}},
Month = {{JAN 16}},
Abstract = {{A general scheme is proposed and tested to control the symmetry breaking
   instability of a homogeneous solution of a spatially extended
   multispecies model, defined on a network. The inherent discreteness of
   the space makes it possible to act on the topology of the inter-nodes
   contacts to achieve the desired degree of stabilization, without
   altering the dynamical parameters of the model. Both symmetric and
   asymmetric couplings are considered. In this latter setting the web of
   contacts is assumed to be balanced, for the homogeneous equilibrium to
   exist. The performance of the proposed method are assessed, assuming the
   Complex Ginzburg-Landau equation as a reference model. In this case, the
   implemented control allows one to stabilize the synchronous limit cycle,
   hence time-dependent, uniform solution. A system of coupled real
   Ginzburg-Landau equations is also investigated to obtain the topological
   stabilization of a homogeneous and constant fixed point.}},
Publisher = {{SPRINGER}},
Address = {{233 SPRING ST, NEW YORK, NY 10013 USA}},
