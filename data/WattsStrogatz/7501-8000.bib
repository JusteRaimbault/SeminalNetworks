@article{ ISI:000378872100006,
Author = {Franzosi, Roberto and Felice, Domenico and Mancini, Stefano and Pettini,
   Marco},
Title = {{Riemannian-geometric entropy for measuring network complexity}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2016}},
Volume = {{93}},
Number = {{6}},
Month = {{JUN 27}},
Abstract = {{A central issue in the science of complex systems is the quantitative
   characterization of complexity. In the present work we address this
   issue by resorting to information geometry. Actually we propose a
   constructive way to associate with a-in principle, any-network a
   differentiable object (a Riemannian manifold) whose volume is used to
   define the entropy. The effectiveness of the latter in measuring network
   complexity is successfully proved through its capability of detecting a
   classical phase transition occurring in both random graphs and
   scale-free networks, as well as of characterizing small exponential
   random graphs, configuration models, and real networks.}},
