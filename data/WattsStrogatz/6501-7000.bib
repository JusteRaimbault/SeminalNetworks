@article{ ISI:000213860200022,
Author = {O'Neill, Joseph and Feusner, Jamie D.},
Title = {{Cognitive-behavioral therapy for obsessive-compulsive disorder: access
   to treatment, prediction of long-term outcome with neuroimaging}},
Journal = {{PSYCHOLOGY RESEARCH AND BEHAVIOR MANAGEMENT}},
Year = {{2015}},
Volume = {{8}},
Pages = {{211-223}},
Abstract = {{This article reviews issues related to a major challenge to the field
   for obsessivecompulsive disorder (OCD): improving access to
   cognitive-behavioral therapy (CBT). Patientrelated barriers to access
   include the stigma of OCD and reluctance to take on the demands of CBT.
   Patient-external factors include the shortage of trained CBT therapists
   and the high costs of CBT. The second half of the review focuses on one
   partial, yet plausible aid to improve access - prediction of long-term
   response to CBT, particularly using neuroimaging methods. Recent pilot
   data are presented revealing a potential for pretreatment resting-state
   functional magnetic resonance imaging and magnetic resonance
   spectroscopy of the brain to forecast OCD symptom severity up to 1 year
   after completing CBT.}},
