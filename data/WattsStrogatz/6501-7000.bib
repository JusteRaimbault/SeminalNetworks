@article{ ISI:000350525000011,
Author = {Shi, Xia and Sun, Xiaojuan and Lv, Yongbing and Lu, Qishao and Wang,
   Haixia},
Title = {{Cluster synchronization and rhythm dynamics in a complex neuronal
   network with chemical synapses}},
