@inproceedings{ ISI:000289494000035,
Author = {Lopes, Fabricio M. and Martins-, Jr., David C. and Barrera, Junior and
   Cesar-, Jr., Roberto M.},
Editor = {{Dijkstra, TMH and Tsivtsivadze, E and Marchiori, E and Heskes, T}},
Title = {{SFFS-MR: A Floating Search Strategy for GRNs Inference}},
Booktitle = {{PATTERN RECOGNITION IN BIOINFORMATICS}},
