@article{ ISI:000395399200002,
Author = {DellaPosta, Daniel and Nee, Victor and Opper, Sonja},
Title = {{Endogenous dynamics of institutional change}},
Journal = {{RATIONALITY AND SOCIETY}},
Year = {{2017}},
Volume = {{29}},
