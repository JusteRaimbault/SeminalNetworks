@article{ ISI:000360437700001,
Author = {Schmittmann, Verena D. and Jahfari, Sara and Borsboom, Denny and Savi,
   Alexander O. and Waldorp, Lourens J.},
Title = {{Making Large-Scale Networks from fMRI Data}},
