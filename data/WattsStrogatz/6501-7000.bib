@inproceedings{ ISI:000330643700036,
Author = {Pasta, Muhammad Qasim and Jan, Zohaib and Sallaberry, Arnaud and Zaidi,
   Faraz},
Book-Group-Author = {{IEEE}},
Title = {{Tunable and Growing Network Generation Model with Community Structures}},
Booktitle = {{2013 IEEE THIRD INTERNATIONAL CONFERENCE ON CLOUD AND GREEN COMPUTING
   (CGC 2013)}},
Year = {{2013}},
Pages = {{233-240}},
