@article{ ISI:000229948900041,
Author = {Wang, SJ and Zhang, CS},
Title = {{Microscopic model of financial markets based on belief propagation}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2005}},
Volume = {{354}},
Pages = {{496-504}},
Month = {{AUG 15}},
Abstract = {{We present a simple microscopic model of financial markets based on
   belief propagation in order to simulate the dynamics of the stock
   markets. A two-dimensional small-world communication structure is
   introduced in our model and the beliefs of market leaders spread on the
   network which results in the herd behaviors of traders. Most of the
   stylized aspects of the financial market time series, including
   multifractal property, are reproduced by the model. A direct comparison
   is made with the daily closures of the Shenzhen composite index. (c)
   2005 Elsevier B.V. All rights reserved.}},
