@article{ ISI:000188086200041,
Author = {Silva, DDE and Soares, MM and Henriques, MVC and Alves, MTS and de
   Aguiar, SG and de Carvalho, TP and Corso, G and Lucena, LS},
Title = {{The complex network of the Brazilian Popular Music}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2004}},
Volume = {{332}},
Pages = {{559-565}},
Month = {{FEB 1}},
Abstract = {{We study the Brazilian Popular Music in a network perspective. We call
   the Brazilian Popular Music Network, BPMN, the graph where the vertices.
   are the song writers and the links are determined by the existence of at
   least a common singer. The linking degree distribution of such graph
   shows power law and exponential regions. The exponent of the power law
   is compatible with the values obtained by the evolving network
   algorithms seen in the literature. The average path length of the BPMN
   is similar to the correspondent random graph, its clustering
   coefficient, however, is significantly larger. These results indicate
   that the BPMN forms a small-world network. (C) 2003 Elsevier B.V. All
   rights reserved.}},
