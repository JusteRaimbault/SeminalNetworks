@article{ ISI:000261582100015,
Author = {Noh, J. D.},
Title = {{Loop statistics in complex networks}},
Journal = {{EUROPEAN PHYSICAL JOURNAL B}},
Year = {{2008}},
Volume = {{66}},
Number = {{2}},
Pages = {{251-257}},
Month = {{NOV}},
Abstract = {{We study a scaling property of the number M-h(N) of loops of size h in
   complex networks with respect to a network size N. For networks with a
   bounded second moment of degree, we find two distinct scaling behaviors:
   M-h(N) similar to (constant) and M-h(N) similar to ln N as N increases.
   Uncorrelated random networks specified only with a degree distribution
   and Markovian networks specified only with a nearest neighbor
   degree-degree correlation display the former scaling behavior, while
   growing network models display the latter. The difference is attributed
   to structural correlation that cannot be captured by a short-range
   degree-degree correlation.}},
