@article{ ISI:000300210600012,
Author = {Baek, Woon Hak and Lim, Gyuchang and Kim, Kyungsik and Choi, Young-Jean
   and Chang, Ki-Ho and Jung, Jae-Won and Yi, Myunggi and Lee, Dong-In and
   Ha, Deok-Ho},
Title = {{Analysis of topological properties in a seismic network}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2012}},
Volume = {{391}},
Number = {{6}},
Pages = {{2279-2285}},
Month = {{MAR 15}},
Abstract = {{In this study, we examine a seismic network by considering the cell
   resolution and the temporal causality. Seismic activity data from the
   Korean peninsula are mapped onto a growing network with links between
   consecutive events. After we especially show a seismic network with
   spatial heterogeneity and scales in an area, topological properties of
   this seismic activity network are measured and found. Our result for the
   robustness of these properties can be compared with other calculated
   findings. (C) 2011 Elsevier B.V. All rights reserved.}},
