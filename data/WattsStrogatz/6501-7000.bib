@article{ ISI:000263912300016,
Author = {Xu Qi-Xin and Xu Xin-Jian},
Title = {{Generating weighted community networks based on local events}},
Journal = {{CHINESE PHYSICS B}},
Year = {{2009}},
Volume = {{18}},
Number = {{3}},
Pages = {{933-938}},
Month = {{MAR}},
Abstract = {{Many realistic networks have community structures, namely, a network
   consists of groups of nodes within which links are dense but among which
   links are sparse. This paper proposes a growing network model based on
   local processes, the addition of new nodes intra-community and new links
   intra- or inter-community. Also, it utilizes the preferential attachment
   for building connections determined by nodes' strengths, which evolves
   dynamically during the growth of the system. The resulting network
   reflects the intrinsic community structure with generalized power-law
   distributions of nodes' degrees and strengths.}},
