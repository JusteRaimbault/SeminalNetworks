@article{ ISI:000361301000014,
Author = {Alter, Adrian and Craig, Ben R. and Raupach, Peter},
Title = {{Centrality-Based Capital Allocations}},
Journal = {{INTERNATIONAL JOURNAL OF CENTRAL BANKING}},
