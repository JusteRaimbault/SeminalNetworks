@article{ ISI:000242706400026,
Author = {Gade, Prashant M. and Sinha, Sudeshna},
Title = {{How crucial is small world connectivity for dynamics?}},
Journal = {{INTERNATIONAL JOURNAL OF BIFURCATION AND CHAOS}},
Year = {{2006}},
Volume = {{16}},
Number = {{9}},
Pages = {{2767-2775}},
Month = {{SEP}},
Abstract = {{We study the dynamical behavior of the collective field of chaotic
   systems on small world lattices. Coupled neuronal systems as well as
   coupled logistic maps are investigated. We observe that significant
   changes in dynamical properties occur only at a reasonably high strength
   of nonlocal coupling. Further, spectral features, such as
   signal-to-noise ratio (SNR), change monotonically with respect to the
   fraction of random rewiring, i.e. there is no optimal value of the
   rewiring fraction for which spectral properties are most pronounced. We
   also observe that for small rewiring, results are similar to those
