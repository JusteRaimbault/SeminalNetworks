@article{ ISI:000428490500020,
Author = {Zhang, Baogang and Zhang, Jun and Liu, Yao and Shi, Peng and Wei, Gehong},
Title = {{Co-occurrence patterns of soybean rhizosphere microbiome at a
   continental scale}},
