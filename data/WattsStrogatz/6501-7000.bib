@article{ ISI:000345922100006,
Author = {Yang, Yang and Huang, Ailing and Guan, Wei},
Title = {{Statistic properties and cascading failures in a coupled transit network
   consisting of bus and subway systems}},
Journal = {{INTERNATIONAL JOURNAL OF MODERN PHYSICS B}},
Year = {{2014}},
Volume = {{28}},
Number = {{30}},
Month = {{DEC 10}},
Abstract = {{A coupled network model consisting of bus and subway systems is
   proposed, and the statistic properties of the three networks: bus,
   subway and coupled networks of Beijing are studied with the theory of
   complex network. The result shows that the three networks have typical
   properties of small-world. We propose three parameters to depict the
   coupled network, they are: the coupled parameter beta the influence
   parameter S and the node tolerance parameter gamma. We use the binary
   influence model to simulate a feedback process and cascading failure in
   the coupled network and we obtain the conclusions: (1) The cascading
   size grows with beta; (2) The cascading size grows with S, but it has a
   critical point; (3) The cascading size grows with the decrease of gamma,
   when gamma <= 0.3, the cascading failure will extent to the whole
   network.}},
