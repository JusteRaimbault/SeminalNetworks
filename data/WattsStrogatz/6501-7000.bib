@article{ ISI:000311837300002,
Author = {Crook, Sharon M. and Bednar, James A. and Berger, Sandra and Cannon,
   Robert and Davison, Andrew P. and Djurfeldt, Mikael and Eppler, Jochen
   and Kriener, Birgit and Furber, Steve and Graham, Bruce and Plesser,
   Hans E. and Schwabe, Lars and Smith, Leslie and Steuber, Volker and van
   Albada, Sacha},
Title = {{Creating, documenting and sharing network models}},
Journal = {{NETWORK-COMPUTATION IN NEURAL SYSTEMS}},
Year = {{2012}},
Volume = {{23}},
Number = {{4}},
Pages = {{131-149}},
Abstract = {{As computational neuroscience matures, many simulation environments are
   available that are useful for neuronal network modeling. However,
   methods for successfully documenting models for publication and for
   exchanging models and model components among these projects are still
   under development. Here we briefly review existing software and
   applications for network model creation, documentation and exchange.
   Then we discuss a few of the larger issues facing the field of
   computational neuroscience regarding network modeling and suggest
   solutions to some of these problems, concentrating in particular on
   standardized network model terminology, notation, and descriptions and
   explicit documentation of model scaling. We hope this will enable and
   encourage computational neuroscientists to share their models more
