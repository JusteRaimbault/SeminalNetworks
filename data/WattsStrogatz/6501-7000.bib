@article{ ISI:000252998000010,
Author = {Zhang, Hao Lan and Leung, Clement H. C. and Raikundalia, Gitesh K.},
Title = {{Topological analysis of AOCD-based agent networks and experimental
   results}},
Journal = {{JOURNAL OF COMPUTER AND SYSTEM SCIENCES}},
