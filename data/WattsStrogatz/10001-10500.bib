@article{ ISI:000363206900001,
Author = {Ren, Fuchun and Zhao, Tingdi and Wang, Hongli},
Title = {{Risk and Resilience Analysis of Complex Network Systems Considering
   Cascading Failure and Recovery Strategy Based on Coupled Map Lattices}},
Journal = {{MATHEMATICAL PROBLEMS IN ENGINEERING}},
Year = {{2015}},
Abstract = {{Risk and resilience are important and challenging issues in complex
   network systems since a single failure may trigger a whole collapse of
   the systems due to cascading effect. New theories, models, and methods
   are urgently demanded to deal with this challenge. In this paper, a
   coupled map lattices (CML) based approach is adopted to analyze the risk
   of cascading process in Watts-Strogatz (WS) small-world network and
   Barabasi and Albert (BA) scale-free network, respectively. Then, to
   achieve an effective and robust system and provide guidance in
   countering the cascading failure, a modified CML model with recovery
   strategy factor is proposed. Numerical simulations are put forward based
   on small-world CML and scale-free CML. The simulation results reveal
   that appropriate recovery strategies would significantly improve the
   resilience of networks.}},
