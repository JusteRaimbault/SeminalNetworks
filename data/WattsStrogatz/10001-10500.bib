@article{ ISI:000329298700004,
Author = {Kim, In-Jae and Barthel, Brian P. and Park, Yuyoung and Tait, Jordan R.
   and Dobmeier, Joseph L. and Kim, Sung and Shin, Dooyoung},
Title = {{Network Analysis for Active and Passive Propagation Models}},
