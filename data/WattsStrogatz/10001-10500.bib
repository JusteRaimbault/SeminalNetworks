@article{ ISI:000377676600005,
Author = {Yuan, Jing and Li, Lixiang and Peng, Haipeng and Kurths, Juergen and
   Xiao, Jinghua and Yang, Yixian},
Title = {{The effect of randomness for dependency map on the robustness of
   interdependent lattices}},
Journal = {{CHAOS}},
Year = {{2016}},
Volume = {{26}},
Number = {{1}},
Month = {{JAN}},
Abstract = {{The percolation for interdependent networks with identical dependency
   map follows a second-order phase transition which is exactly the same
   with percolation on a single network, while percolation for random
   dependency follows a first-order phase transition. In real networks, the
   dependency relations between networks are neither identical nor
   completely random. Thus in this paper, we study the influence of
   randomness for dependency maps on the robustness of interdependent
   lattice networks. We introduce approximate entropy(ApEn) as the measure
   of randomness of the dependency maps. We find that there is critical
   ApEnc below which the percolation is continuous, but for larger ApEn, it
   is a first-order transition. With the increment of ApEn, the pc
   increases until ApEn reaching ApEn(c)' and then remains almost constant.
   The time scale of the system shows rich properties as ApEn increases.
