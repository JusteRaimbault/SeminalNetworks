@article{ ISI:000350339900005,
Author = {Ye, Bin and Jia, Jia-Jia and Zuo, Kang-Wei and Ma, Xiao-Ping},
Title = {{Can the structural robustness of complex networks be enhanced by
   interconnection?}},
Journal = {{INTERNATIONAL JOURNAL OF MODERN PHYSICS C}},
Year = {{2015}},
Volume = {{26}},
Number = {{4}},
Month = {{APR}},
Abstract = {{To improve the structural robustness of a large complex network, we
   propose to interconnect it with a smaller but more robust network. We
   demonstrate the validity of our method by employing the metric of
   natural connectivity. By this approach, one can enhance the robustness
   of a network without modifying its inner structure or reshuffling the
   original links. Our method will lead to build more robust distributed
   power grids.}},
