@inproceedings{ ISI:000354775400008,
Author = {Hirahara, Yuki and Toriumi, Fujio and Sugawara, Toshiharu},
Editor = {{Aiello, LM and McFarland, D}},
Title = {{Evolution of Cooperation in SNS-norms Game on Complex Networks and Real
   Social Networks}},
