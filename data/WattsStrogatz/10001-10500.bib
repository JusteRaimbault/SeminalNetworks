Author = {Guler, Basak and Varan, Burak and Tutuncuoglu, Kaya and Nafea, Mohamed
   and Zewail, Ahmed A. and Yener, Aylin and Octeau, Damien},
Title = {{Using Social Sensors for Influence Propagation in Networks With Positive
   and Negative Relationships}},
