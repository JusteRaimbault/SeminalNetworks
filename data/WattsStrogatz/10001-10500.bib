@article{ ISI:000347493000006,
Author = {Noguera, Jose A. and Miguel Quesada, Francisco J. and Tapia, Eduardo and
   Llacer, Toni},
Title = {{Tax Compliance, Rational Choice, and Social Influence: An Agent-Based
   Model}},
