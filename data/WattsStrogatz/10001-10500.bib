@article{ ISI:000433866600005,
Author = {Mansoor, Naseef and Iruthayaraj, Pratheep Joe Sullivai and Ganguly,
   Amlan},
Title = {{Design Methodology for a Robust and Energy-Efficient Millimeter-Wave
   Wireless Network-on-Chip}},
