@inproceedings{ ISI:000400048101146,
Author = {Cao, Hong-Tai and Gibson, Travis E. and Mou, Shaoshuai and Liu, Yang-Yu},
Book-Group-Author = {{IEEE}},
Title = {{Impacts of Network Topology on the Performance of a Distributed
   Algorithm Solving Linear Equations}},
Booktitle = {{2016 IEEE 55TH CONFERENCE ON DECISION AND CONTROL (CDC)}},
