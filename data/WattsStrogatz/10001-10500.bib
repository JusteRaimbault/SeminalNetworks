@article{ ISI:000301019800004,
Author = {Wang, Li and Gong, Yubing and Lin, Xiu},
Title = {{Ordered chaotic bursting and multiple coherence resonance by
   time-periodic coupling strength in Newman-Watts neuronal networks}},
Journal = {{CHAOS SOLITONS \& FRACTALS}},
Year = {{2012}},
Volume = {{45}},
Number = {{2}},
Pages = {{131-136}},
Month = {{FEB}},
Abstract = {{In this paper, we study the effect of time-periodic coupling strength
   (TPCS) on the temporal coherence of the chaotic bursting of Newman-Watts
   thermosensitive neuron networks. It is found that the chaotic bursting
   can exhibit coherence resonance and multiple coherence resonance
   behavior when TPCS amplitude and frequency is varied, respectively. It
   is also found that TPCS can also enhance the temporal coherence and
   spatial synchronization of the optimal spatio-temporal bursting in the
   case of fixed coupling strength. These results show that TPCS can tame
   the chaotic bursting and can repeatedly enhance the temporal coherence
