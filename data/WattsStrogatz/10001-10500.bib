@article{ ISI:000385787400008,
Author = {Oliveira Neto, Geraldo Cardoso and Shibao, Fabio Ytoshi and Godinho
   Filho, Moacir},
Title = {{THE STATE OF RESEARCH ON CLEANER PRODUCTION IN BRAZIL}},
