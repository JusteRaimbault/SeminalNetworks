@inproceedings{ ISI:000399925504021,
Author = {Wang, Haishuai and Wu, Jia and Zhou, Chuan and Ji, Zhenyan and Wu, Jun},
Book-Group-Author = {{IEEE}},
Title = {{Mining Subcascade Features for Cascade Outbreak Prediction in Big
   Networks}},
Booktitle = {{2016 INTERNATIONAL JOINT CONFERENCE ON NEURAL NETWORKS (IJCNN)}},
