@article{ ISI:000367275000009,
Author = {Mishra, Subhankar and Dinh, Thang N. and Thai, My T. and Seo, Jungtaek
   and Shin, Incheol},
Title = {{Optimal packet scan against malicious attacks in smart grids}},
Journal = {{THEORETICAL COMPUTER SCIENCE}},
Year = {{2016}},
Volume = {{609}},
Number = {{3, SI}},
Pages = {{606-619}},
