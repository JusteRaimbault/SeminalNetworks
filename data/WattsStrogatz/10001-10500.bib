Editor = {{Parashar, M and Ramesh, T and Zola, J and Nanjangud, N and Kothapalli, K and Amudha, J and Bangalore, P and Gupta, D and Pathak, A and Chaudhary, S and Dinesha, KV and Prasad, SK}},
Title = {{Significance of Clustering Coefficient over Jaccard Index}},
Booktitle = {{2015 EIGHTH INTERNATIONAL CONFERENCE ON CONTEMPORARY COMPUTING (IC3)}},
