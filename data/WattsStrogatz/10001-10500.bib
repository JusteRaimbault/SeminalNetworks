@inproceedings{ ISI:000354861400054,
Author = {Ranjan, Gyan and Zhang, Zhi-Li and Boley, Daniel},
Editor = {{Zhang, Z and Wu, L and Xu, W and Du, DZ}},
Title = {{Incremental Computation of Pseudo-Inverse of Laplacian}},
