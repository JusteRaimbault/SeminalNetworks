@inproceedings{ ISI:000380541000011,
Author = {Dinh, Thang N. and Li, Xiang and Thai, My T.},
Editor = {{Aggarwal, C and Zhou, ZH and Tuzhilin, A and Xiong, H and Wu, X}},
Title = {{Network Clustering via Maximizing Modularity: Approximation Algorithms
   and Theoretical Limits}},
