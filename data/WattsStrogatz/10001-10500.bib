@article{ ISI:000324794200014,
Author = {Zhao, Mo and Zhang, Huaguang and Wang, Zhiliang},
Title = {{Synchronization in complex dynamical networks based on the feedback of
   scalar signals}},
Journal = {{NEURAL COMPUTING \& APPLICATIONS}},
