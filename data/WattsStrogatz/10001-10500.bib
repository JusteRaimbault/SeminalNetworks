@inproceedings{ ISI:000454711900125,
Author = {Wang, Zhitao and Chen, Chengyao and Li, Wenjie},
Book-Group-Author = {{ACM/SIGIR}},
Title = {{Predictive Network Representation Learning for Link Prediction}},
Booktitle = {{SIGIR'17: PROCEEDINGS OF THE 40TH INTERNATIONAL ACM SIGIR CONFERENCE ON
   RESEARCH AND DEVELOPMENT IN INFORMATION RETRIEVAL}},
Year = {{2017}},
Pages = {{969-972}},
Note = {{40th International ACM SIGIR conference on research and development in
   Information Retrieval, Shinjuku, JAPAN, AUG 07-11, 2017}},
Organization = {{ACM SIGIR}},
Abstract = {{In this paper, we propose a predictive network representation learning
   (PNRL) model to solve the structural link prediction problem. The
   proposed model defines two learning objectives, i.e., observed structure
   preservation and hidden link prediction. To integrate the two objectives
   in a unified model, we develop an effective sampling strategy to select
   certain edges in a given network as assumed hidden links and regard the
   rest network structure as observed when training the model. By jointly
   optimizing the two objectives, the model can not only enhance the
   predictive ability of node representations but also learn additional
   link prediction knowledge in the representation space. Experiments on
   four real-world datasets demonstrate the superiority of the proposed
   model over the other popular and state-of-the-art approaches.}},
