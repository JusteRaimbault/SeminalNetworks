@inproceedings{ ISI:000349547600090,
Author = {Murray, Jacob and Wettin, Paul and Kim, Ryan and Yu, Xinmin and Pande,
   Partha Pratim and Shirazi, Behrooz and Heo, Deukhyoun},
Book-Group-Author = {{IEEE}},
Title = {{Thermal Hotspot Reduction in mm-Wave Wireless NoC Architectures}},
Booktitle = {{PROCEEDINGS OF THE FIFTEENTH INTERNATIONAL SYMPOSIUM ON QUALITY
   ELECTRONIC DESIGN (ISQED 2014)}},
