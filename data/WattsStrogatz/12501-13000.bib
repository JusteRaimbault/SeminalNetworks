@inproceedings{ ISI:000432014402046,
Author = {Wang, Caiyun and Han, Huawei and Han, Jing},
Editor = {{Liu, T and Zhao, Q}},
Title = {{Network Feature and the Intervention Performance}},
Booktitle = {{PROCEEDINGS OF THE 36TH CHINESE CONTROL CONFERENCE (CCC 2017)}},
Series = {{Chinese Control Conference}},
