@article{ ISI:000376788100008,
Author = {Fink, Christian G.},
Title = {{Simulating synchronization in neuronal networks}},
Journal = {{AMERICAN JOURNAL OF PHYSICS}},
Year = {{2016}},
Volume = {{84}},
Number = {{6}},
Pages = {{467-473}},
Month = {{JUN}},
Abstract = {{We discuss several techniques used in simulating neuronal networks by
   exploring how a network's connectivity structure affects its propensity
   for synchronous spiking. Network connectivity is generated using the
   Watts-Strogatz small-world algorithm, and two key measures of network
   structure are described. These measures quantify structural
   characteristics that influence collective neuronal spiking, which is
   simulated using the leaky integrate-and-fire model. Simulations show
   that adding a small number of random connections to an otherwise
   lattice-like connectivity structure leads to a dramatic increase in
   neuronal synchronization. (C) 2016 American Association of Physics
   Teachers.}},
