@article{ ISI:000411531900008,
Author = {Plakidas, Konstantinos and Schall, Daniel and Zdun, Uwe},
Title = {{Evolution of the R software ecosystem: Metrics, relationships, and their
   impact on qualities}},
Journal = {{JOURNAL OF SYSTEMS AND SOFTWARE}},
Year = {{2017}},
Volume = {{132}},
Pages = {{119-146}},
Month = {{OCT}},
Abstract = {{Software ecosystems are an important new concept for collaborative
   software development, and empirical studies on their development are
   important towards understanding the underlying dynamics and modelling
   their behaviour. We conducted an explorative analysis of the R ecosystem
   as an exemplar on high-level, ecosystem-wide assessment. Based
   principally on the documentation metadata of the R packages, we
   generated a variety of metrics that allow the quantification of the R
   ecosystem. We also categorized the ecosystem participants, both in the
   software marketplace and in the developer community, by characteristics
   that measure their activity and impact. By viewing our metrics across
   the ecosystem's lifecycle for the various participant categories, we
   discovered interrelationships between them and determined the
   contribution of each category to the ecosystem as a whole. (C) 2017
   Elsevier Inc. All rights reserved.}},
