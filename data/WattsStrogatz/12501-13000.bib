Title = {{A Fast and Complete Enumeration of Pseudo-Cliques for Large Graphs}},
Booktitle = {{ADVANCES IN KNOWLEDGE DISCOVERY AND DATA MINING, PAKDD 2016, PT I}},
Series = {{Lecture Notes in Artificial Intelligence}},
