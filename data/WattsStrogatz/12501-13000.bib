@article{ ISI:000426377000005,
Author = {Paarporn, Keith and Eksin, Ceyhun and Weitz, Joshua S. and Shamma, Jeff
   S.},
Title = {{Networked SIS Epidemics With Awareness}},
Journal = {{IEEE TRANSACTIONS ON COMPUTATIONAL SOCIAL SYSTEMS}},
