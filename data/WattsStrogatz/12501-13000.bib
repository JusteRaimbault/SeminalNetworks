@article{ ISI:000447238300006,
Author = {Knudstrup, Scott and Zochowski, Michal and Booth, Victoria},
Title = {{Synaptic loss and synaptic plasticity in heterogeneous neural networks}},
Journal = {{JOURNAL OF COMPLEX NETWORKS}},
Year = {{2016}},
Volume = {{4}},
Number = {{1}},
Pages = {{115-126}},
Month = {{MAR}},
Abstract = {{Heterogeneity in brain networks extends to both intrinsic properties of
   neurons and the synaptic connections between neurons. Intrinsic neural
   firing and response properties vary with changing levels of potent
   neuromodulators that are released throughout the brain by subcortical
   areas. Synaptic structure can vary by two distinct mechanisms: (1) the
   number of physical synaptic elements can change as a result of activity
   or pathology, and (2) the efficacy of synaptic signals can change by
   multiple mechanisms of long-term and short-term plasticity. In this
   study, we use biophysical models of neuronal networks to investigate the
   effects of heterogeneity in cellular and synaptic structural properties
   on network synchronization. We consider excitatory networks consisting
   of neurons with heterogeneous intrinsic properties, due to non-uniform
   neuromodulation by acetylcholine. By simulating synaptic loss, our
   results show that characteristics of synchronous network bursts depend
   on the connectivity density between different cell types in the network.
   We further show that synaptic loss affects plastic synaptic remodelling
   of the remaining synapses in the network. These results provide
   hypotheses for predicting how the profile of network bursts evolve due
   to dynamic changes in network heterogeneity.}},
