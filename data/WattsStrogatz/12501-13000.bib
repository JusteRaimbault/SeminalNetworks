@article{ ISI:000366633600023,
Author = {Sheshbolouki, Aida and Zarei, Mina and Sarbazi-Azad, Hamid},
Title = {{The role of leadership in the synchronization of directed complex
   networks}},
Journal = {{JOURNAL OF STATISTICAL MECHANICS-THEORY AND EXPERIMENT}},
Year = {{2015}},
Month = {{OCT}},
Abstract = {{Synchronization is a phenomenon that occurs in systems of interacting
   units, and is widespread in nature, society and technology. Recent
   studies have enlightened us regarding the interplay between
   synchronization dynamics and interaction structure. However, most of
   these studies neglect that real-world networks may actually be directed
   and disconnected. Here, we study the synchronization of directed
   networks with multiple leaders using the Kuramoto model. We found that
   in networks with high driving strength, the steady-state frequency of
   each node is determined by the linear combination of leaders' natural
   frequencies, with structural coeffcients that can be calculated using
   the eigenvectors of a network Laplacian matrix corresponding to zero
   eigenvalues. The steady-state frequencies of the nodes following
   multiple leaders are not fixed and have sharp peaks between consecutive
   time instances where leaders meet each other in the phase circle. The
   results suggest a new way of understanding how leadership style
   influences the synchronization dynamics of directed networks.}},
