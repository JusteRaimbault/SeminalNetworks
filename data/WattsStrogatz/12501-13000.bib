@inproceedings{ ISI:000389706200021,
Author = {Salas, Julian},
Editor = {{Torra, V and Narukawa, Y and NavarroArribas, G and Yanez, C}},
Title = {{Sampling and Merging for Graph Anonymization}},
Booktitle = {{MODELING DECISIONS FOR ARTIFICIAL INTELLIGENCE, (MDAI 2016)}},
