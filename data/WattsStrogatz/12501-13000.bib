@article{ ISI:000362266400025,
Author = {Lavi, Ayal and Perez, Omri and Ashery, Uri},
Title = {{Shaping Neuronal Network Activity by Presynaptic Mechanisms}},
Journal = {{PLOS COMPUTATIONAL BIOLOGY}},
