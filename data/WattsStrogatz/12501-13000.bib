@article{ ISI:000365111000005,
Author = {Wang, Yuying and Xiao, Yanni},
Title = {{An epidemic model on the dispersal networks at population and individual
   levels}},
Journal = {{JAPAN JOURNAL OF INDUSTRIAL AND APPLIED MATHEMATICS}},
Year = {{2015}},
Volume = {{32}},
