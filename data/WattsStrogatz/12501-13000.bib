@inproceedings{ ISI:000389498800026,
Author = {Liu, Feng and Xiang, Wei and Wang, Shouyi and Lega, Bradley},
Editor = {{Ascoli, GA and Hawrylycz, M and Ali, H and Khazanchi, D and Shi, Y}},
Title = {{Prediction of Seizure Spread Network via Sparse Representations of
