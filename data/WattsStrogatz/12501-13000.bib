@inproceedings{ ISI:000388259100010,
Author = {Costa, Gianni and Ortale, Riccardo},
Editor = {{Bostrom, H and Knobbe, A and Soares, C and Papapetrou, P}},
Title = {{A Mean-Field Variational Bayesian Approach to Detecting Overlapping
   Communities with Inner Roles Using Poisson Link Generation}},
