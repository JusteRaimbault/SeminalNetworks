Editor = {{RoblesKelly, A and Loog, M and Biggio, B and Escolano, F and Wilson, R}},
Title = {{Thermodynamic Network Analysis with Quantum Spin Statistics}},
Booktitle = {{STRUCTURAL, SYNTACTIC, AND STATISTICAL PATTERN RECOGNITION, S+SSPR 2016}},
Series = {{Lecture Notes in Computer Science}},
Year = {{2016}},
