@inproceedings{ ISI:000390749105047,
Author = {Francisco Robles, Juan and Chica, Manuel and Cordon, Oscar},
Book-Group-Author = {{IEEE}},
Title = {{Incorporating Awareness and Genetic-based Viral Marketing Strategies to
   a Consumer Behavior Model}},
