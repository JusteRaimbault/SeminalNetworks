@article{ ISI:000385641900013,
Author = {Molkenthin, Nora and Timme, Marc},
Title = {{Scaling Laws in Spatial Network Formation}},
Journal = {{PHYSICAL REVIEW LETTERS}},
Year = {{2016}},
Volume = {{117}},
Number = {{16}},
Month = {{OCT 14}},
Abstract = {{Geometric constraints impact the formation of a broad range of spatial
   networks, from amino acid chains folding to proteins structures to
   rearranging particle aggregates. How the network of interactions
   dynamically self-organizes in such systems is far from fully understood.
   Here, we analyze a class of spatial network formation processes by
   introducing a mapping from geometric to graph-theoretic constraints.
   Combining stochastic and mean field analyses yields an algebraic scaling
   law for the extent (graph diameter) of the resulting networks with
   system size, in contrast to logarithmic scaling known for networks
   without constraints. Intriguingly, the exponent falls between that of
   self-avoiding random walks and that of space filling arrangements,
   consistent with experimentally observed scaling of the radius of
   gyration of protein tertiary structures with their chain length.}},
