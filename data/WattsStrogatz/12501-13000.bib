@article{ ISI:000388965000003,
Author = {Cakir, Yuksel},
Title = {{Modeling of synchronization behavior of bursting neurons at nonlinearly
   coupled dynamical networks}},
Journal = {{NETWORK-COMPUTATION IN NEURAL SYSTEMS}},
