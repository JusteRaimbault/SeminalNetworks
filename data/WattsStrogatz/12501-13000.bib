@article{ ISI:000381220500020,
Author = {Laan, S. and Marx, M. and Mokken, R. J.},
Title = {{Close communities in social networks: boroughs and 2-clubs}},
Journal = {{SOCIAL NETWORK ANALYSIS AND MINING}},
Year = {{2016}},
Volume = {{6}},
Number = {{1}},
Month = {{DEC}},
Abstract = {{The structure of close communication, contacts and association in social
   networks is studied in the form of maximal subgraphs of diameter 2
   (2-clubs), corresponding to three types of close communities: hamlets,
   social circles and coteries. The concept of borough of a graph is
   defined and introduced. Each borough is a chained union of 2-clubs of
   the network and any 2-club of the network belongs to one borough. Thus
   the set of boroughs of a network, together with the 2-clubs held by
   them, are shown to contain the structure of close communication in a
   network. Applications are given with examples from real world network
   data.}},
