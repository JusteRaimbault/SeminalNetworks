@article{ ISI:000369186100001,
Author = {Taylor, Charles E. and Cody, Martin L.},
Title = {{Bird song: a model complex adaptive system}},
Journal = {{ARTIFICIAL LIFE AND ROBOTICS}},
Year = {{2015}},
Volume = {{20}},
Number = {{4}},
Pages = {{285-290}},
Month = {{DEC}},
Abstract = {{Bird songs make an attractive model for studying complex systems. They
   may range from simple repeated sequences, to complex sequences of
   different phrase types, much like human language. There is probably no
   single way to best characterize their complexity. We should avoid saying
   that that ``bird songs are in complexity class X{''}. The diversity of
   examples suggests that the song of one bird species or another can
   probably be found to exemplify and model many kinds of complex systems.
   We suggest that the complexity classes for cellular automata
   distinguished by Wolfram might give some insight into the capacity of
   bird songs to transmit information and of the complexity needed to
   generate them.}},
