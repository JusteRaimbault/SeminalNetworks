@inproceedings{ ISI:000380559900012,
Author = {Firmani, Donatella and Italiano, Giuseppe F. and Laura, Luigi},
Editor = {{Aiello, LM and McFarland, D}},
Title = {{The (not so) Critical Nodes of Criminal Networks}},
Booktitle = {{SOCIAL INFORMATICS}},
