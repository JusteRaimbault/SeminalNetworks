@inproceedings{ ISI:000427402902002,
Author = {Baingana, Brian and Giannakis, Georgios B.},
Book-Group-Author = {{IEEE}},
Title = {{KERNEL-BASED EMBEDDINGS FOR LARGE GRAPHS WITH CENTRALITY CONSTRAINTS}},
Booktitle = {{2015 IEEE INTERNATIONAL CONFERENCE ON ACOUSTICS, SPEECH, AND SIGNAL
   PROCESSING (ICASSP)}},
