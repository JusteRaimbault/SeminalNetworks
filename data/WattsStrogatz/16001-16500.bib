@inproceedings{ ISI:000377498500013,
Author = {Leitao, A. C. and Francisco, A. P. and Abreu, R. and Nunes, S. and
   Rodrigues, J. and Figueiredo, P. and Wald, L. L. and Bianciardi, M. and
   Silveira, L. M.},
