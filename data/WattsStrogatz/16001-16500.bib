@inproceedings{ ISI:000381010400041,
Author = {Alqurashi, Tahani and Wang, Wenjia},
Editor = {{Akerkar, R and Bassiliades, N and Davies, J and Ermolayev, V}},
Title = {{A Graph based Methodology for Web Structure Mining - with a Case Study
   on the Webs of UK Universities}},
Booktitle = {{4TH INTERNATIONAL CONFERENCE ON WEB INTELLIGENCE, MINING AND SEMANTICS}},
Year = {{2014}},
