Editor = {{HadjadjAoul, Y}},
Title = {{Topology Reduction and Probabilistic Information Extraction for Large
   Data-Sets: A Disaster Management Case Study}},
Booktitle = {{2015 2ND INTERNATIONAL CONFERENCE ON INFORMATION AND COMMUNICATION
   TECHNOLOGIES FOR DISASTER MANAGEMENT (ICT-DM)}},
Series = {{International Conference on Information and Communication Technologies
   for Disaster Management}},
