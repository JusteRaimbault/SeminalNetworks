@inproceedings{ ISI:000380509300021,
Author = {Dai Caiyan and Chen Ling and Chen Bolun},
Book-Group-Author = {{IEEE}},
Title = {{Using Suffix Tree to Detect Communities from Bipartite Network}},
Booktitle = {{2015 THIRD INTERNATIONAL CONFERENCE ON ADVANCED CLOUD AND BIG DATA}},
