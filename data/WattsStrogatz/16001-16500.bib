@inproceedings{ ISI:000380387700123,
Author = {de Macedo, Heverton Barros and Barbosa de Oliveira, Gina Maira and Costa
   Ribeiro, Carlos Henrique},
Book-Group-Author = {{IEEE}},
Title = {{Dynamic behaviour of network cellular automata with non-chaotic standard
   rules}},
Booktitle = {{2014 SECOND WORLD CONFERENCE ON COMPLEX SYSTEMS (WCCS)}},
Year = {{2014}},
Pages = {{451-456}},
Note = {{2014 Second World Conference on Complex Systems (WCCS), Agadir, MOROCCO,
   NOV 10-12, 2014}},
Organization = {{Ibn Zohr Univ; Moroccan Soc of Complex Syst; IEEE Morocco; Int Acad for
   Syst and Cybernet Sci IASCYS}},
Abstract = {{Rules classified as non-chaotic are usually discarded from the set of
   keys in cellular automata used as criptographic methods, as they exhibit
   predictable behavior regarding the dynamic evolution of the cellular
   automata configurations. The number of possible keys, in fact, is an
   important factor for the security of a cryptographic method. This paper
   investigates the dynamic behavior of a type of cellular automata network
   whose structure is derived from small-word concepts, when evolving rules
   not classified as chaotic. The constructed connection structure of the
   cellular automata network modifies the dynamic behavior of transition
   rules associated to the standard model. The experiments show that rules
