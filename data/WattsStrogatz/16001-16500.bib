@inproceedings{ ISI:000341949700063,
Author = {Stephan, Lehner and Gollnick, Volker and Nishant, Swaroop},
Book-Group-Author = {{IEEE}},
Title = {{Analyzing Collaboration in Self-Organizing Complex Systems of Flow}},
