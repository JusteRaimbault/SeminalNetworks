@article{ ISI:000355784000018,
Author = {Xu, Guang-Hui and Gong, Shi-Cai},
Title = {{The effect on synchronizability of networks under edge perturbation}},
Journal = {{UTILITAS MATHEMATICA}},
Year = {{2015}},
Volume = {{97}},
Pages = {{241-255}},
Month = {{JUL}},
Abstract = {{By using the algebraic graph-theoretical method, we investigate how the
   synchronizability of a coupled network is affected under edge
   perturbation. First, according the size of contribution to
   synchronizability, we classify all edges of a given network into three
   classes, that is synchronous edges, irrelevant edges and non-synchronous
   edges. Then we study the synchronizability of edges for some special
   graphs, including complete bipartite graph, complete graph, the
   complement graph of a path, and the graphs containing bridges; Giving a
   necessary condition for irrelevant edges of a given network. Next, some
   sharp upper bounds of eigenratio are established. Finally, we show by
   examples that those bounds can sometimes be considered as the measure
   for network synchronizability.}},
