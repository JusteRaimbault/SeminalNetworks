@inproceedings{ ISI:000370963703004,
Author = {Wu, Lihua and Jiang, Hao and Zheng, Hong and Leung, Henry},
Book-Group-Author = {{IEEE}},
Title = {{Long Tail and Small World Characteristic of Mobile Internet Traffic
   Dynamics}},
Booktitle = {{2014 IEEE INTERNATIONAL CONFERENCE ON SYSTEMS, MAN AND CYBERNETICS (SMC)}},
