@inproceedings{ ISI:000361021200013,
Author = {Prosser, Bryan and Dawes, Neal and Fulp, Errin W. and McKinnon, A. David
   and Fink, Glenn A.},
Book-Group-Author = {{IEEE}},
Title = {{Using Set-Based Heading to Improve Mobile Agent Movement}},
Booktitle = {{2014 IEEE EIGHTH INTERNATIONAL CONFERENCE ON SELF-ADAPTIVE AND
   SELF-ORGANIZING SYSTEMS (SASO)}},
Series = {{International Conference on Self-Adaptive and Self-Organizing Systems}},
