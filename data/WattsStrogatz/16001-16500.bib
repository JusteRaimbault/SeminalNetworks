@inproceedings{ ISI:000356417900095,
Author = {Kvasnicka, Michal},
Editor = {{Talasova, J and Stoklasa, J and Talasek, T}},
Title = {{Search for the optimal strategy to spread a viral video: An agent-based
   model optimized with genetic algorithms}},
