Title = {{A Core-Attach Based Method for Identifying Protein Complexes in Dynamic
   PPI Networks}},
Booktitle = {{ADVANCES IN KNOWLEDGE DISCOVERY AND DATA MINING, PART II}},
Series = {{Lecture Notes in Artificial Intelligence}},
