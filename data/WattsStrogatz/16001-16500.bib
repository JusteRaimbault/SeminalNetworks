@article{ ISI:000367496600051,
Author = {Yoshida, Masami and Duangchinda, Vorasuang},
Title = {{Study on Potential Online Communication of Regional In-Service Training}},
Journal = {{ADVANCED SCIENCE LETTERS}},
Year = {{2015}},
Volume = {{21}},
Number = {{7, SI}},
Pages = {{2390-2395}},
Month = {{JUL}},
Note = {{International Conference on Education (ICOED), Bali, INDONESIA, JUN
   02-04, 2015}},
Abstract = {{A Twitter hashtag was announced to participants in a local workshop for
   in-service teachers. The online communication data that emerged were
   collected through the application programming interface and analyzed
   using a social graph method. The online communication was used as a
   backchannel of the workshop and built relationship was mapped with their
   existing individual communication into the social graph. Then, an
   extensional survey of teachers who tweeted with the hashtag was
   conducted to infer their future communication with educators in
   different areas. As a result, six vertices suitable for bridge
   influencers and two vertices suitable for future hub influencers were
   extracted. The extensional study confirmed that two participants who
   tweeted the hashtags were already involved in a social graph of a
   centralized educational conference this year.}},
