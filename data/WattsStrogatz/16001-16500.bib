@article{ ISI:000441919200006,
Author = {Beuren, Ilse Maria and Machado, Debora Gomes and Dal Vesco, Delci
   Grapegia},
Title = {{Sociometric and Bibliometric Analysis of Research Published in
   Management Accounting Research}},
