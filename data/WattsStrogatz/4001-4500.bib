@article{ ISI:000241614600012,
Author = {Wang, L. and Du, F. and Dai, H. P. and Sun, Y. X.},
Title = {{Random pseudofractal scale-free networks with small-world effect}},
Journal = {{EUROPEAN PHYSICAL JOURNAL B}},
Year = {{2006}},
Volume = {{53}},
Number = {{3}},
Pages = {{361-366}},
Month = {{OCT}},
Abstract = {{A random pseudofractal network (RPN) is generated by a recursive growing
   rule. The RPN is of the scale-free feature and small-world effect. We
   obtain the theoretical results of power-law exponent gamma=3, clustering
   coefficient C=3 pi(2)-19 approximate to 0.74, and a proof that the mean
   distance increases no faster than ln N, where N is the network size.
   These results agree with the numerical simulation very well. In
   particular, we explain the property of growth and preferential
   attachment in RPNs. And the properties of a class of general RPNs are
   discussed in the end.}},
