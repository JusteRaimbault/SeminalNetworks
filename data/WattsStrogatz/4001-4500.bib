@article{ ISI:000326180700022,
Author = {Yu, Qingbao and Sui, Jing and Kiehl, Kent A. and Pearlson, Godfrey and
   Calhoun, Vince D.},
Title = {{State-related functional integration and functional segregation brain
   networks in schizophrenia}},
Journal = {{SCHIZOPHRENIA RESEARCH}},
