@inproceedings{ ISI:000245601900029,
Author = {Huang, Chong and Tian, Yonghong and Zhou, Zhi and Ling, Charles X. and
   Huang, Tiejun},
Editor = {{Clifton, CW and Zhong, N and Liu, JM and Wah, BW and Wu, XD}},
Title = {{Keyphrase extraction using semantic networks structure analysis}},
Booktitle = {{ICDM 2006: Sixth International Conference on Data Mining, Proceedings}},
Series = {{IEEE International Conference on Data Mining}},
Year = {{2006}},
Pages = {{275-284}},
Note = {{6th IEEE International Conference on Data Mining, Hong Kong, PEOPLES R
   CHINA, DEC 18-22, 2006}},
Organization = {{IEEE}},
Abstract = {{Keyphrases play a key role in text indexing, summarization and
   categorization. However, most of the existing keyphrase extraction
   approaches require human-labeled training sets. In this paper, we
   propose an automatic keyphrase extraction algorithm, which can be used
   in both supervised and unsupervised tasks. This algorithm treats each
   document as a semantic network. Structural dynamics of the network, are
