@article{ ISI:000292916800030,
Author = {Rothkegel, A. and Lehnertz, K.},
Title = {{Recurrent events of synchrony in complex networks of pulse-coupled
   oscillators}},
Journal = {{EPL}},
Year = {{2011}},
Volume = {{95}},
Number = {{3}},
Month = {{AUG}},
Abstract = {{We present and analyze deterministic complex networks of pulse-coupled
   oscillators that exhibit recurrent events comprised of an increase and a
   decline in synchrony. Events emerging from the networks may form an
   oscillatory behavior or may be separated by periods of asynchrony with
   varying duration. The phenomenon is specific to spatial networks with
   both short- and long-ranged connections and requires delayed
   interactions and refractoriness of oscillators. Copyright (C) EPLA, 2011}},
