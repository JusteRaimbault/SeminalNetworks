@article{ ISI:000277443300018,
Author = {Zuo, Zhiqiang and Yang, Cuili and Wang, Yijing},
Title = {{A unified framework of exponential synchronization for complex networks
   with time-varying delays}},
Journal = {{PHYSICS LETTERS A}},
Year = {{2010}},
Volume = {{374}},
Number = {{19-20}},
Pages = {{1989-1999}},
Month = {{APR 19}},
Abstract = {{This Letter presents a unified framework of exponential synchronization
   analysis for general delayed complex dynamical networks. Two kinds of
   time-varying delays, delays in the network couplings and delays in the
   dynamical nodes, are taken into consideration. A new Lyapunov-Krasovskii
   functional is constructed by dividing the delay interval into two
   segments, and different weighting matrices are employed to express the
   relationship between the time-varying delay and its lower and upper
   bounds corresponding to each segment. Numerical examples illustrate the
   effectiveness of the method and the improvement over some existing
   results in the literature. (C) 2010 Elsevier B.V. All rights reserved.}},
