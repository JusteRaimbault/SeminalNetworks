@article{ ISI:000259580400016,
Author = {Zhang, Zhongzhi and Zhou, Shuigeng and Su, Zhan and Zou, Tao and Guan,
   Jihong},
Title = {{Random Sierpinski network with scale-free small-world and modular
   structure}},
Journal = {{EUROPEAN PHYSICAL JOURNAL B}},
Year = {{2008}},
Volume = {{65}},
Number = {{1}},
Pages = {{141-147}},
Month = {{SEP}},
Abstract = {{In this paper, we define a stochastic Sierpinski gasket, on the basis of
   which we construct a network called random Sierpinski network (RSN). We
   investigate analytically or numerically the statistical characteristics
   of RSN. The obtained results reveal that the properties of RSN is
   particularly rich, it is simultaneously scale-free, small-world,
   uncorrelated, modular, and maximal planar. All obtained analytical
   predictions are successfully contrasted with extensive numerical
   simulations. Our network representation method could be applied to study
   the complexity of some real systems in biological and information
   fields.}},
