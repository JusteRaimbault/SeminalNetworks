Language = {{English}},
Affiliation = {{Bourjaily, MA (Reprint Author), Brandeis Univ, Dept Biol, Waltham, MA 02254 USA.
   Bourjaily, Mark A., Brandeis Univ, Dept Biol, Waltham, MA 02254 USA.
   Brandeis Univ, Neurosci Program, Volen Ctr Complex Syst, Waltham, MA USA.}},
DOI = {{10.1371/journal.pcbi.1001091}},
