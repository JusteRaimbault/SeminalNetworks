@article{ ISI:000348533000003,
Author = {Cardillo, Alessio and Petri, Giovanni and Nicosia, Vincenzo and Sinatra,
   Roberta and Gomez-Gardenes, Jesus and Latora, Vito},
Title = {{Evolutionary dynamics of time-resolved social interactions}},
