@article{ ISI:000252884600030,
Author = {Yu, D. and Parlitz, U.},
Title = {{Driving a network to steady states reveals its cooperative architecture}},
Journal = {{EPL}},
Year = {{2008}},
Volume = {{81}},
Number = {{4}},
Abstract = {{For a sparsely connected and balanced network with unknown interaction
   delays and 1D elements, we show that driving the network to steady
   states can reveal its cooperative architecture, i.e., the dynamics and
   output functions of individual nodes and the connection topology. Our
   approach is robust and can be extended to (sub) networks with a small
   topology uncertainty. Some examples are presented to illustrate the
   proposed identification method. Copyright (C) EPLA, 2008.}},
