@article{ ISI:000266074700069,
Author = {Du Wen-Bo and Cao Xian-Bin and Zhao Lin and Zhou Hong},
Title = {{Evolutionary Games on Weighted Newman-Watts Small-World Networks}},
Journal = {{CHINESE PHYSICS LETTERS}},
Year = {{2009}},
Volume = {{26}},
Number = {{5}},
Month = {{MAY}},
Abstract = {{We investigate the evolutionary prisoner's dilemma game (PDG) on
   weighted Newman-Watts (NW) networks. In weighted NW networks, the link
   weight w(ij) is assigned to the link between the nodes i and j as: w(ij)
   = (k(i) . k(j))(beta), where k(i)(k(j)) is the degree of node i (j) and
   beta represents the strength of the correlations. Obviously, the link
   weight can be tuned by only one parameter beta. We focus on the
   cooperative behavior and wealth distribution in the system. Simulation
   results show that the cooperator frequency is promoted by a large range
   of.. and there is a minimal cooperation frequency around beta = -1.
   Moreover, we also employ the Gini coefficient to study the wealth
   distribution in the population. Numerical results show that the Gini
   coefficient reaches its minimum when beta approximate to -1. Our work
   may be helpful in understanding the emergence of cooperation and unequal
   wealth distribution in society.}},
