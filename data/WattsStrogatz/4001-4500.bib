@article{ ISI:000355287900007,
Author = {LaRocca, Sarah and Johansson, Jonas and Hassel, Henrik and Guikema, Seth},
Title = {{Topological Performance Measures as Surrogates for Physical Flow Models
   for Risk and Vulnerability Analysis for Electric Power Systems}},
Journal = {{RISK ANALYSIS}},
