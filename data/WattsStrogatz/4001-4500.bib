@article{ ISI:000224475600006,
Author = {Chi, LP and Cai, X},
Title = {{Structural changes caused by error and attack tolerance in us airport
   network}},
Journal = {{INTERNATIONAL JOURNAL OF MODERN PHYSICS B}},
Year = {{2004}},
Volume = {{18}},
Number = {{17-19}},
Pages = {{2394-2400}},
Month = {{JUL 30}},
Note = {{1st Cross Taiwan-strait Conference on Statistical Physics, Yangzhou,
   PEOPLES R CHINA, AUG 26-31, 2003}},
Organization = {{Yangzhou Univ; Univ Sci \& Technol China; Natl Sci Fdn China; Peoples
   Govt Jiangsu Prov; Dept Educ Jiangsu Prov; Sci \& Technol Assoc Anhui
   Prov}},
Abstract = {{Through the study of US airport network, we find that the network
   displays a high degree of error tolerance and an extreme vulnerability
   to attacks. The topological properties, including average degree,
   clustering coefficient, diameter and efficiency, axe slightly affected
   when a few least connected airports axe removed. Such properties change
   drastically with the removal of a few most connected ones. The degree
   distribution and the weight distribution under errors behave similarly
   to those of the original network. Under attacks, the degree distribution
   changes from a two-segment power-law to a monotonic one. While the
   under-attacked weight distribution still displays a power-law tail, with
   the exponent changing from 1.50 to 1.24.}},
