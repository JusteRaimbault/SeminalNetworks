@article{ ISI:000448653700289,
Author = {Li, Taoying and Bai, Jie and Yang, Xue and Liu, Qianyu and Chen, Yan},
Title = {{Co-Occurrence Network of High-Frequency Words in the Bioinformatics
   Literature: Structural Characteristics and Evolution}},
Journal = {{APPLIED SCIENCES-BASEL}},
