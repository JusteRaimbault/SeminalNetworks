@article{ ISI:000461132600046,
Author = {Zhang, Xiaoping and Chai, Lihe},
Title = {{Structural features and evolutionary mechanisms of industrial symbiosis
   networks: Comparable analyses of two different cases}},
Journal = {{JOURNAL OF CLEANER PRODUCTION}},
Year = {{2019}},
Volume = {{213}},
Pages = {{528-539}},
Month = {{MAR 10}},
Abstract = {{Recently, industrial symbiosis networks (ISNs) have become popular
   development patterns for regional industrial systems in many countries.
   As such, it is important to analyze the underlying structural
   characteristics and evolutionary mechanisms of ISNs in diverse
   situations. This paper's contributions to the literature are threefold.
   (1) Based on all types of centrality and centralization measures in
   social networks (the direction of links included), the structural
   similarities, differences, and associated mechanisms of two typical and
   comparable ISNs-the spontaneously formed Kalundborg ISN in Denmark and
   the planned Lubei ISN in China are the first to be revealed. (2) Based
   on average clustering coefficient, average path length, and power law
   distribution of degree from complex network theory, the small-world and
   scale-free effects of the two ISNs are compared. (3) Network
   fragmentation is then defined to compare the two ISNs under the
   selective attack of removing critical enterprises from them at different
   development stages. As such, this paper not only extends the methodology
   of analyzing the structural characteristics of different ISNs, but also
   provides guidance for managers on the construction of actual
   eco-industrial parks. (C) 2018 Elsevier Ltd. All rights reserved.}},
