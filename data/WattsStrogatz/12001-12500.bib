@article{ ISI:000432644700001,
Author = {Zhang, Juping and Yang, Chan and Jin, Zhen and Li, Jia},
Title = {{Dynamics analysis of SIR epidemic model with correlation coefficients
   and clustering coefficient in networks}},
Journal = {{JOURNAL OF THEORETICAL BIOLOGY}},
Year = {{2018}},
Volume = {{449}},
Pages = {{1-13}},
Month = {{JUL 14}},
Abstract = {{In this paper, the correlation coefficients between nodes in states are
   used as dynamic variables, and we construct SIR epidemic dynamic models
   with correlation coefficients by using the pair approximation method in
   static networks and dynamic networks, respectively. Considering the
   clustering coefficient of the network, we analytically investigate the
   existence and the local asymptotic stability of each equilibrium of
   these models and derive threshold values for the prevalence of diseases.
   Additionally, we obtain two equivalent epidemic thresholds in dynamic
   networks, which are compared with the results of the mean field
   equations. (C) 2018 Elsevier Ltd. All rights reserved.}},
