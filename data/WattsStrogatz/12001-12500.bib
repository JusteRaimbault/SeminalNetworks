@inproceedings{ ISI:000450117400031,
Author = {Hong, Yunfeng and Lin, Yu-Cheng and Lai, Chun-Ming and Wu, S. Felix and
   Barnett, George A.},
Book-Group-Author = {{IEEE}},
Title = {{Profiling Facebook Public Page Graph}},
Booktitle = {{2018 INTERNATIONAL CONFERENCE ON COMPUTING, NETWORKING AND
   COMMUNICATIONS (ICNC)}},
Series = {{International Conference on Computer Networking and Communications}},
Year = {{2018}},
Pages = {{161-165}},
Note = {{International Conference on Computing, Networking and Communications
   (ICNC), Maui, HI, MAR 05-08, 2018}},
Abstract = {{This paper focuses on the Facebook public page graph, which represents
   the ``Like{''} connections among public pages on Facebook. In the first
   part, an efficient algorithm is proposed to predict the geographic
   location of the public page with 90.25\% accuracy. In the second part,
   we compute various properties of the graph including page distribution
   by country and category, graph connectivity, and country international
   impact. There are four main findings. First, the usage of the public
   page on Facebook is imbalanced. Second, nearly all countries are closely
   connected on Facebook. Third, the country's PageRank is highly
   correlated with the number of nations to which the country's pages
   connect. Finally, by clustering the countries, we find that the Islamic
   countries are clustered into a separate group.}},
