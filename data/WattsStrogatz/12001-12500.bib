@article{ ISI:000443760700012,
Author = {Lima, Thales S. and de Arruda, Henrique F. and Silva, Filipi N. and
   Comin, Cesar H. and Amancio, Diego R. and Costa, Luciano da F.},
Title = {{The dynamics of knowledge acquisition via self-learning in complex
   networks}},
