@article{ ISI:000435630100035,
Author = {de Regt, R. and Apunevych, S. and von Ferber, C. and Holovatch, Yu. and
   Novosyadlyj, B.},
Title = {{Network analysis of the COSMOS galaxy field}},
Journal = {{MONTHLY NOTICES OF THE ROYAL ASTRONOMICAL SOCIETY}},
