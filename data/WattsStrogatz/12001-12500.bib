@article{ ISI:000435113900010,
Author = {Liu, Dan and Liu, Yurong and Alsaadi, Fuad E.},
Title = {{Recursive state estimation based-on the outputs of partial nodes for
   discrete-time stochastic complex networks with switched topology}},
Journal = {{JOURNAL OF THE FRANKLIN INSTITUTE-ENGINEERING AND APPLIED MATHEMATICS}},
Year = {{2018}},
Volume = {{355}},
Number = {{11}},
Pages = {{4686-4707}},
Month = {{JUL}},
Abstract = {{In this paper, the state estimation problem is studied for a class of
   discrete-time stochastic complex networks with switched topology. In the
   network under consideration, we assume that measurement outputs can be
   got from only partial nodes, besides, the switching rule of this network
   is characterized by a sequence of Bernoulli random variables. The aim of
   the presented estimation problem is to develop a recursive estimator
   based on the framework of extended Kalman filter (EKF), such that the
   upper bound for the filtering error convariance is optimized In order to
   address the nonlinear functions, the Taylor series expansion is utilized
   and the high-order terms of linearization errors are expressed in an
   exact way. Furthermore, by solving two Ricatti-like difference
   equations, the gain matrix can be acquired at each time instant. It is
   shown that the filtering error is bounded in mean square under some
   conditions with the aid of stochastic analysis techniques. A numerical
   example is given to demonstrate the validity of the proposed estimator.
   (C) 2018 The Franklin Institute. Published by Elsevier Ltd. All rights
   reserved.}},
