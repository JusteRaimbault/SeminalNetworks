@article{ ISI:000449820000009,
Author = {Tian, Jianqiu and Liu, Enlong},
Title = {{Effect of particle shape on micro- and mesostructure evolution of
   granular assemblies under biaxial loading conditions}},
Journal = {{COMPTES RENDUS MECANIQUE}},
