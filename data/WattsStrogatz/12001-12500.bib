@article{ ISI:000426793900010,
Author = {Vazquez-Araujo, Francisco and Dapena, Adriana and Jose Souto-Salorio,
   Maria and Castro, Paula M.},
Title = {{Calculation of the Connected Dominating Set Considering Vertex
   Importance Metrics}},
Journal = {{ENTROPY}},
Year = {{2018}},
Volume = {{20}},
Number = {{2}},
Month = {{FEB}},
Abstract = {{The computation of a set constituted by few vertices to define a virtual
   backbone supporting information interchange is a problem that arises in
   many areas when analysing networks of different natures, like wireless,
   brain, or social networks. Recent papers propose obtaining such a set of
   vertices by computing the connected dominating set (CDS) of a graph. In
   recent works, the CDS has been obtained by considering that all vertices
   exhibit similar characteristics. However, that assumption is not valid
   for complex networks in which their vertices can play different roles.
   Therefore, we propose finding the CDS by taking into account several
   metrics which measure the importance of each network vertex e.g., error
   probability, entropy, or entropy variation (EV).}},
