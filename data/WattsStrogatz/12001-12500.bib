@article{ ISI:000432883500007,
Author = {Shi, Q. J. and Zhang, H. C. and Xu, D. L. and Qi, E. R. and Tian, C. and
   Ding, J. and Wu, Y. S. and Lu, Y. and Li, Z. W.},
Title = {{Experimental validation of network modeling method on a three-modular
   floating platform model}},
Journal = {{COASTAL ENGINEERING}},
Year = {{2018}},
Volume = {{137}},
Pages = {{92-102}},
Month = {{JUL}},
Abstract = {{This paper reports an experimental verification of a network modeling
   method that is recently proposed for forecasting responses of
   multi-modular floating platforms in regular waves. A three-dimensional
   network modeling method is presented, which includes the geometric
   effect of connectors and permits nonlinear dynamic analysis of the
   floating system. In the validation study, a three-modular floating
   platform model is fabricated for experimental test in wave basin. Both
   the linear model and nonlinear model of the network modeling method are
   examined in the collation of the hydro-elastic method and experiment
   test. The responses of modules and loads of connectors are employed for
   the comparison study. The results show that the linear model of the
   network modeling method is the closest to the test results and
   outperforms over the nonlinear model and the hydro-elastic method. The
   possible reasons of the discrepancy in the results of the different
   methods are discussed. This work validates that the network modeling
   method is reliable.}},
