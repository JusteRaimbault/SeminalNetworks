@article{ ISI:000458409000002,
Author = {Shi, Yongbin and Li, Le and Wang, Yougui and Chen, Jiawei and Yuan, Yida
   and Stanley, H. E.},
Title = {{Regional surname affinity: A spatial network approach}},
