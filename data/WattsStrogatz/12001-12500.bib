@article{ ISI:000455498900013,
Author = {Chen, Xingguang and Zhu, Zhentao},
Title = {{Interactional Effects Between Individual Heterogeneity and Collective
   Behavior in Complex Organizational Systems}},
Journal = {{COMPUTATIONAL ECONOMICS}},
