@article{ ISI:000416826200003,
Author = {Ramasamy, Thangarajan and Arjunasamy, Archudha},
Title = {{Advanced Heuristics for Selecting Friends in Social Internet of Things}},
Journal = {{WIRELESS PERSONAL COMMUNICATIONS}},
Year = {{2017}},
Volume = {{97}},
Number = {{4}},
Pages = {{4951-4965}},
Month = {{DEC}},
Abstract = {{The future of communication involves `things' in addition to people thus
   bringing about the term Internet of Things. When social networking
   concepts are incorporated into Internet of Things, it is called Social
   Internet of Things. Searching for a desired services may lead to high
   computational and memory demands as the number of friends of any node
   starts to grow over time. Hence, it is required to judiciously prune the
   friends of a node by applying some strategy in order to reduce
   computational load and at the same time preserving network navigability.
   In this paper, four new heuristics for efficient selection of friends
   are proposed. An analysis is performed based on various metrics and the
   results are presented.}},
