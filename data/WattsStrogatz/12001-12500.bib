@article{ ISI:000425324000029,
Author = {Rager, Scott T. and Ciftcioglu, Ertugrul N. and Ramanathan, Ram and La
   Porta, Thomas F. and Govindan, Ramesh},
Title = {{Scalability and Satisfiability of Quality-of-Information in Wireless
   Networks}},
Journal = {{IEEE-ACM TRANSACTIONS ON NETWORKING}},
