Author = {Giudicianni, Carlo and Di Nardo, Armando and Di Natale, Michele and
   Greco, Roberto and Santonastaso, Giovanni Francesco and Scala, Antonio},
Title = {{Topological Taxonomy of Water Distribution Networks}},
