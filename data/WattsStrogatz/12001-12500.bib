@article{ ISI:000452441000009,
Author = {Lu, Zhuo and Sagduyu, Yalin E. and Shi, Yi},
Title = {{Integrating Social Links into Wireless Networks: Modeling, Routing,
   Analysis, and Evaluation}},
Journal = {{IEEE TRANSACTIONS ON MOBILE COMPUTING}},
