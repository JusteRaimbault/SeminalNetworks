@article{ ISI:000445647600044,
Author = {Soriano-Sanchez, A. G. and Posadas-Castillo, C.},
Title = {{Smart pattern to generate small-world networks}},
Journal = {{CHAOS SOLITONS \& FRACTALS}},
Year = {{2018}},
Volume = {{114}},
Pages = {{415-422}},
Month = {{SEP}},
Abstract = {{This article proposes a new method to introduce the small-world property
   into regular networks. A smart connection pattern, achieved by rewiring
   and adding connections, is suggested in order to reduce the loss of
   connectivity produced by the introduction of randomness in the topology.
   The resulting complex network exhibits the small-world property, i.e.,
   small average distance node to node and high connectivity. This model
   could be used as an alternative to improve the robustness of some
   networks created artificially. (C) 2018 Elsevier Ltd. All rights
   reserved.}},
Publisher = {{PERGAMON-ELSEVIER SCIENCE LTD}},
