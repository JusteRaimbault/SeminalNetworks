@article{ ISI:000394219100013,
Author = {Rossetti, Giulio and Pappalardo, Luca and Kikas, Riivo and Pedreschi,
   Dino and Giannotti, Fosca and Dumas, Marlon},
Title = {{Homophilic network decomposition: a community-centric analysis of online
   social services}},
Journal = {{SOCIAL NETWORK ANALYSIS AND MINING}},
Year = {{2016}},
