@inproceedings{ ISI:000391433600103,
Author = {Basu, Prithwish},
Book-Group-Author = {{IEEE}},
Title = {{K-Anycasting Strategies in Networks: To Grow or To Prune?}},
Booktitle = {{MILCOM 2016 - 2016 IEEE MILITARY COMMUNICATIONS CONFERENCE}},
Series = {{IEEE Military Communications Conference}},
Year = {{2016}},
Pages = {{606-611}},
Note = {{35th IEEE Military Communications Conference (MILCOM), Baltimore, MD,
   NOV 01-03, 2016}},
Organization = {{IEEE; AFCEA; IEEE Commun Soc}},
Abstract = {{Disseminating content to a given subset of nodes in a network in a
   cost-effective manner is a fundamental problem in networks, and is
   referred to as multicast. If reaching all the nodes in the set is not
   necessary and reaching a quorum of size k within the set is sufficient,
   then the problem is referred to as k-anycast or any-k-cast. This has
   applications in a wide range of networks, e.g., function computation in
   spatial wireless networks such as sensor networks and influence flow in
   social networks. Since the k-anycast optimization problem is
   NP-complete, we propose two heuristic algorithms to address the problem:
   the first is based on greedy growth from the root node and the second is
   based on pruning from a larger multicast tree. We perform a comparative
   numerical study of their performance tradeoffs for a social
   collaboration network and a spatial network by evaluating the effect of
   terminal node distributions on the cost and exploration overhead of the
   k-anycast tree for different regimes of k for each network.}},
