@inproceedings{ ISI:000387430400069,
Author = {Li, Feng and Lin, Ning},
Editor = {{Huang, DS and Jo, KH}},
Title = {{A Multi-agent Approach for the Newsvendor Problem with Word-of-Mouth
   Marketing Strategies}},
