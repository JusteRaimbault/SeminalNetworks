@article{ ISI:000360649600010,
Author = {Ye, Bin and Zuo, Kang-Wei and Jia, Jia-Jia and Ma, Xiao-Ping},
Title = {{The epidemic cost of interconnected networks}},
Journal = {{INTERNATIONAL JOURNAL OF MODERN PHYSICS C}},
Year = {{2015}},
Volume = {{26}},
Number = {{11}},
Month = {{NOV}},
Abstract = {{We study the effect of networks interconnection on the epidemic cost of
   them. When an epidemic will eventually die out, we find that the total
   cost of the epidemic over the interconnected network is approximate to
   the sum of the total epidemic cost of the two sub-networks. We also
   prove that the interconnection behavior can reduce epidemic threshold.
   Thus network interconnecting makes an epidemic more easily to occur,
   while keeping the total cost of the epidemic almost unchanged.}},
