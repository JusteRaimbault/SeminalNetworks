@article{ ISI:000407837800011,
Author = {Uribe Gomez, Julian Alberto and Quintero Ramirez, Santiago and Giraldo
   Ramirez, Diana Patricia},
Title = {{Simulation Modeling and Network Analysis in Regional Innovation Systems}},
