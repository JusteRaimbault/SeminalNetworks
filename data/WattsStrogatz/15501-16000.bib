@inproceedings{ ISI:000389514100043,
Author = {Kang, Moon Jung and Hwang, Jongwoon},
Editor = {{Aaltio, I and Eskelinen, MT}},
Title = {{Systemic Energy Innovation Networks Funded by the European Union}},
