@inproceedings{ ISI:000434218600011,
Author = {Piernik, Maciej and Brzezinski, Dariusz and Morzy, Tadeusz and Morzy,
   Mikolaj},
Editor = {{Kryszkiewicz, M and Appice, A and Slezak, D and Rybinski, H and Skowron, A and Ras, ZW}},
Title = {{Using Network Analysis to Improve Nearest Neighbor Classification of
   Non-network Data}},
Booktitle = {{FOUNDATIONS OF INTELLIGENT SYSTEMS, ISMIS 2017}},
Series = {{Lecture Notes in Artificial Intelligence}},
Year = {{2017}},
Volume = {{10352}},
Pages = {{105-115}},
Note = {{23rd International Symposium on Methodologies for Intelligent Systems
   (ISMIS), Warsaw Univ Technol, Warsaw, POLAND, JUN 26-29, 2017}},
Organization = {{MBank S A; Warsaw Univ Technol, Inst Comp Sci; Univ Warsaw; Univ Bari
   Aldo Moro}},
Abstract = {{The nearest neighbor classifier is a powerful, straightforward, and very
   popular approach to solving many classification problems. It also
   enables users to easily incorporate weights of training instances into
   its model, allowing users to highlight more promising examples. Instance
   weighting schemes proposed to date were based either on attribute values
   or external knowledge. In this paper, we propose a new way of weighting
   instances based on network analysis and centrality measures. Our method
   relies on transforming the training dataset into a weighted signed
   network and evaluating the importance of each node using a selected
   centrality measure. This information is then transferred back to the
   training dataset in the form of instance weights, which are later used
   during nearest neighbor classification. We consider four centrality
   measures appropriate for our problem and empirically evaluate our
   proposal on 30 popular, publicly available datasets. The results show
   that the proposed instance weighting enhances the predictive performance
   of the nearest neighbor algorithm.}},
Publisher = {{SPRINGER INTERNATIONAL PUBLISHING AG}},
Address = {{GEWERBESTRASSE 11, CHAM, CH-6330, SWITZERLAND}},
Type = {{Proceedings Paper}},
