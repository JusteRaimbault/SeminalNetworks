@inproceedings{ ISI:000450053100131,
Author = {Xie, Xuan and Feng, Hui and Jia, Junlian and Hu, Bo},
Book-Group-Author = {{IEEE}},
Title = {{DESIGN OF SAMPLING SET FOR BANDLIMITED GRAPH SIGNAL ESTIMATION}},
