@inproceedings{ ISI:000413483400039,
Author = {Vermeer, W. and Head, B. and Wilensky, U.},
Editor = {{Cherifi, H and Gaito, S and Quattrociocchi, W and Sala, A}},
Title = {{The effects of local network structure on disease spread in coupled
   networks}},
Booktitle = {{COMPLEX NETWORKS \& THEIR APPLICATIONS V}},
