@inproceedings{ ISI:000405512400065,
Author = {Yong, Xi and Sun, Hong-liang and Li, Juan},
Editor = {{Xu, B}},
Title = {{Entanglement plays an important role in evolutionary generalized
   prisoner's dilemma game on small-world networks}},
Booktitle = {{PROCEEDINGS OF 2016 IEEE ADVANCED INFORMATION MANAGEMENT, COMMUNICATES,
   ELECTRONIC AND AUTOMATION CONTROL CONFERENCE (IMCEC 2016)}},
