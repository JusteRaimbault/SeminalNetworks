@article{ ISI:000392735300005,
Author = {Khan, Abhimanyu and Peeters, Ronald and Thuijsman, Frank and
   Uyttendaele, Philippe},
Title = {{Network Characteristics Enabling Efficient Coordination: A Simulation
   Study}},
Journal = {{Dynamic Games and Applications}},
Year = {{2016}},
Volume = {{6}},
Number = {{4, SI}},
Pages = {{495-519}},
Month = {{DEC}},
Abstract = {{Using data generated by extensive simulations of a process where
   individuals interact globally in a coordination game and iteratively
   imitate the action of the most successful individual in their local
   neighbourhood, we study the characteristics of the observation network
   and distribution of initial choices that facilitate (a speedy)
   convergence to efficient coordination. Knowing these characteristics is
   crucial when intervening in social network structures with the intention
   to nudge society to a socially preferred outcome. The most important
   factor appears to be the share of individuals that are initially seeded
   with the socially desirable action. Only for a small window of this
   share, other factors, including the degree distribution in the network
   and the segregation of individuals using similar actions, have an
   influence on the dynamic process of achieving efficient coordination.
   Moreover, networks possessing the properties of a scale-free network
   aremore likely to yield an efficient outcome compared to small-world
   networks.}},
