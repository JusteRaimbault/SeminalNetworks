@article{ ISI:000384579700014,
Author = {Yang, Lintao and Yu, Zetai and Qian, Jing and Liu, Shouyin},
Title = {{Overlapping community detection using weighted consensus clustering}},
Journal = {{PRAMANA-JOURNAL OF PHYSICS}},
Year = {{2016}},
Volume = {{87}},
Number = {{4}},
Month = {{OCT}},
Abstract = {{Many overlapping community detection algorithms have been proposed. Most
   of them are unstable and behave non-deterministically. In this paper, we
   use weighted consensus clustering for combining multiple base covers
   obtained by classic non-deterministic algorithms to improve the quality
   of the results. We first evaluate a reliability measure for each
   community in all base covers and assign a proportional weight to each
   one. Then we redefine the consensus matrix that takes into account not
   only the common membership of nodes, but also the reliability of the
   communities. Experimental results on both artificial and real-world
   networks show that our algorithm can find overlapping communities
   accurately.}},
