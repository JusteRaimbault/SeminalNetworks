@inproceedings{ ISI:000385789600006,
Author = {de Nigris, Sarah and Leoncini, Xavier},
Editor = {{Afraimovich, V and Machado, JAT and Zhang, J}},
Title = {{Hidden Dimensions in an Hamiltonian System on Networks}},
Booktitle = {{COMPLEX MOTIONS AND CHAOS IN NONLINEAR SYSTEMS}},
Series = {{Nonlinear Systems and Complexity}},
Year = {{2016}},
Volume = {{15}},
Pages = {{173-202}},
