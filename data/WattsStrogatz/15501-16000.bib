Title = {{Measuring Trust in Online Social Networks}},
Booktitle = {{ADVANCES IN DIGITAL TECHNOLOGIES}},
Series = {{Frontiers in Artificial Intelligence and Applications}},
Year = {{2016}},
Volume = {{282}},
