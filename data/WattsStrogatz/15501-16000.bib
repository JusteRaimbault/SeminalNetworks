@article{ ISI:000392858100003,
Author = {Koc, Yakup and Raman, Abhishek and Warnier, Martijn and Kumar, Tarun},
Title = {{Structural vulnerability analysis of electric power distribution grids}},
Journal = {{International Journal of Critical Infrastructures}},
Year = {{2016}},
Volume = {{12}},
Number = {{4}},
Pages = {{311-330}},
Abstract = {{Power grid outages cause huge economical and societal costs. Disruptions
   in the power distribution grid are responsible for a significant
   fraction of electric power unavailability to customers. The impact of
   extreme weather conditions, continuously increasing demand, and the
   over-ageing of assets in the grid, deteriorates the safety of electric
   power delivery. Measures to analyse the robustness characteristics and
   to identify vulnerabilities of power grids are of utmost importance.
   This paper proposes a metric to quantitatively assess the robustness of
   power distribution grids from a topological point of view. Real-world
   data is used to demonstrate the applicability of the proposed metric as
   a tool to assess the criticality of assets in a distribution grid.}},
