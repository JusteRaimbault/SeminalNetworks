@article{ ISI:000385471800014,
Author = {Yang, Lei and Wang, Shiyuan and Zhou, Meng and Chen, Xiaowen and Zuo,
   Yongchun and Lv, Yingli},
Title = {{Characterization of BioPlex network by topological properties}},
Journal = {{JOURNAL OF THEORETICAL BIOLOGY}},
