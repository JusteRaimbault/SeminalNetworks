@inproceedings{ ISI:000429718100090,
Author = {Nian, Fuzhong and Wang, Longjing and Liu, Tong and Wang, Ke},
Editor = {{Jing, W and Guiran, C and Huiyu, Z}},
Title = {{High-Contact-First Immunization Strategy on Complex Networks}},
Booktitle = {{PROCEEDINGS OF THE 2016 7TH INTERNATIONAL CONFERENCE ON EDUCATION,
   MANAGEMENT, COMPUTER AND MEDICINE (EMCM 2016)}},
Series = {{ACSR-Advances in Comptuer Science Research}},
Year = {{2017}},
Volume = {{59}},
Pages = {{476-487}},
Note = {{7th International Conference on Education, Management, Computer and
   Medicine (EMCM), Shenyang, PEOPLES R CHINA, DEC 29-31, 2016}},
Abstract = {{Different from traditional immunization strategies, which suppose that
   the contact frequency of all nodes is same, in this paper, a new
   immunization strategy was established based on the principle of
   `High-Contact-First'. The different link with different weight describes
   the relationship between different two nodes. The bigger weight means
   the higher contact. The higher contact means the higher risk which the
   two nodes be infected each other. Thus, to the purpose of immunization,
   the link with higher contact should be cut down first. The theory
   analysis and the simulations indicate that the proposed immunization
