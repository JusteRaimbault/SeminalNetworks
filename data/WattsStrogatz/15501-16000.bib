@article{ ISI:000369803700003,
Author = {Geiger, Andreas and Kroll, Andreas},
Title = {{Extention of Complex Networks for dynamical modelling and analysis of
   large, interconnected systems}},
Journal = {{AT-AUTOMATISIERUNGSTECHNIK}},
Year = {{2016}},
Volume = {{64}},
Number = {{2}},
Pages = {{110-120}},
Month = {{FEB}},
Abstract = {{Complex Networks are one possibility to model and analyze large
   interconnected systems. Therefore, statistical measures are used to
   analyze the system that is modeled as a graph, consisting of nodes and
   edges. However, Complex Networks ignore the process dynamics which are
   key in the case of developing control systems. This paper presents an
   approach of augmented Complex Networks, which makes it possible to use
   them for analyzing control relevant dynamic behavior. Therefore, the
   model is extended by edge weights which describe the dynamic behavior in
   a simple way. To illustrate the approach, an example for qualitative
   detection of the inverseresponse behavior will be given.}},
