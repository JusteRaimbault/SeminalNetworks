@inproceedings{ ISI:000382226300051,
Author = {Gao, Jie and Ghasemiesfeh, Golnaz and Schoenebeck, Grant and Yu, Fang-Yi},
Book-Group-Author = {{ACM}},
Title = {{General Threshold Model for Social Cascades: Analysis and Simulations}},
Booktitle = {{EC'16: PROCEEDINGS OF THE 2016 ACM CONFERENCE ON ECONOMICS AND
