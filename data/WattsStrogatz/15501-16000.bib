@inproceedings{ ISI:000424872103112,
Author = {Takano, Chisa and Aida, Masaki},
Book-Group-Author = {{IEEE}},
Title = {{Fundamental Framework for Describing Various Node Centralities Using an
   Oscillation Model on Social Media Networks}},
Booktitle = {{2017 IEEE INTERNATIONAL CONFERENCE ON COMMUNICATIONS (ICC)}},
Series = {{IEEE International Conference on Communications}},
Year = {{2017}},
Note = {{IEEE International Conference on Communications (ICC) / Wireless
   Communications Symposium, Paris, FRANCE, MAY 21-25, 2017}},
Organization = {{IEEE; IEEE France Sect; IEEE Commun Soc}},
Abstract = {{In recent years, with the rapid development of the Internet and cloud
   computing, an enormous amount of information is exchanged on various
   social networking services. In order to handle and maintain such a
   mountain of information properly by limited resources in the network, it
   is very important to comprehend the dynamics for propagation of
   information or activity on the social network. One of many indices used
   by social network analysis which investigates the network structure is
   ``Node centrality{''}. A common characteristic of conventional node
   centralities is that it depends on the topological structure of network
   and the value of node centrality does not change unless the topology
   changes. In our recent research, we have proposed an oscillation model
   for describing dynamics on networks generated from a certain kind of
   asymmetric interaction between nodes by using a symmetric matrix.
   Moreover, we proposed a new extended index of well-known two node
   centralities based on the oscillation model. In this paper, we discuss
   generally extension of node centralities based on the oscillation model.
   In addition, we show that the proposed index can describe various node
   centralities by properly assigning the weight of each link, and the
   proposed model is regarded as the fundamental framework for different
   node centralities.}},
