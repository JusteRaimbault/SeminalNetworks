@inproceedings{ ISI:000426448100033,
Author = {Yang, Po and Ma, Xuebin},
Book-Group-Author = {{IEEE}},
Title = {{Selection Strategy of Nodes with the Greatest Influence on Community
   Structure}},
Booktitle = {{2017 INTERNATIONAL CONFERENCE ON CYBER-ENABLED DISTRIBUTED COMPUTING AND
   KNOWLEDGE DISCOVERY (CYBERC)}},
Year = {{2017}},
Pages = {{199-205}},
