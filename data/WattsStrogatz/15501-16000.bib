@inproceedings{ ISI:000389821200026,
Author = {Kallas, Kassem and Tondi, Benedetta and Lazzeretti, Riccardo and Barni,
   Mauro},
Editor = {{Zhu, Q and Alpcan, T and Panaousis, E and Tambe, M and Casey, W}},
Title = {{Consensus Algorithm with Censored Data for Distributed Detection with
   Corrupted Measurements: A Game-Theoretic Approach}},
Booktitle = {{DECISION AND GAME THEORY FOR SECURITY, (GAMESEC 2016)}},
Series = {{Lecture Notes in Computer Science}},
Year = {{2016}},
Volume = {{9996}},
Pages = {{455-466}},
Note = {{7th International Conference on Decision and Game Theory for Security
   (GameSec), New York, NY, NOV 02-04, 2016}},
Abstract = {{In distributed detection based on consensus algorithm, all nodes reach
   the same decision by locally exchanging information with their
   neighbors. Due to the distributed nature of the consensus algorithm, an
   attacker can induce a wrong decision by corrupting just a few
   measurements. As a countermeasure, we propose a modified algorithm
   wherein the nodes discard the corrupted measurements by comparing them
   to the expected statistics under the two hypothesis. Although the nodes
   with corrupted measurements are not considered in the protocol, under
   proper assumptions on network topology, the convergence of the
   distributed algorithm can be preserved. On his hand, the attacker may
   try to corrupt the measurements up to a level which is not detectable to
