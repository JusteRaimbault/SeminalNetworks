@article{ ISI:000447240000002,
Author = {Khaleque, Abdul and Sen, Parongama},
Title = {{Frozen states and active-absorbing phase transitions of the Ising model
   on networks}},
Journal = {{JOURNAL OF COMPLEX NETWORKS}},
Year = {{2016}},
Volume = {{4}},
Number = {{3}},
Pages = {{330-341}},
Month = {{SEP}},
Abstract = {{A zero temperature quench of the Ising model is known to lead to a
   frozen steady state on random and small world networks. We study such
   quenches on random scale-free (RSF) networks and compare the scenario
   with that in the Barabasi-Albert (BA) network and the Watts Strogatz
   (WS) addition-type network. While frozen states are present in all the
   cases, the RSF shows an order-disorder phase transition of mean field
   nature as in the WS model as well as the existence of two absorbing
   phases separated by an active phase. The WS network also shows an
   active-absorbing (A-A) phase transition occurring at the known
   order-disorder transition point. The comparison of the RSF and the BA
   network results show interesting difference in finite size dependence.}},
