@inproceedings{ ISI:000390749102068,
Author = {Medland, Michael Richard and Harrison, Kyle Robert and Ombuki-Berman,
   Beatrice M.},
Book-Group-Author = {{IEEE}},
Title = {{Automatic Inference of Graph Models for Directed Complex Networks using
   Genetic Programming}},
