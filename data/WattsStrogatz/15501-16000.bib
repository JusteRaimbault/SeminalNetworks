@inproceedings{ ISI:000389505000124,
Author = {Lu, Yong and Shen, Jun},
Editor = {{Xu, B}},
Title = {{Small-World Model for Command Hierarchy Networks}},
Booktitle = {{2016 IEEE INFORMATION TECHNOLOGY, NETWORKING, ELECTRONIC AND AUTOMATION
   CONTROL CONFERENCE (ITNEC)}},
Year = {{2016}},
Pages = {{599-602}},
Note = {{IEEE Information Technology, Networking, Electronic and Automation
   Control Conference (ITNEC), Chongqing, PEOPLES R CHINA, MAY 20-22, 2016}},
Organization = {{IEEE}},
Abstract = {{The conventional command hierarchy networks are organized by the tree
   structure, which cannot meet the requirement of SoS (System-of-Systems)
   operation for the limitations on invulnerability, robustness and
   communication efficiency. Based on complex network theory, this paper
   proposes a small-world model for the conventional command hierarchy
   networks, which not only keeps the layered structure for the command
   hierarchy, but also improves its invulnerability and communication
   efficiency. The simulation results show that the proposed model has
   better invulnerability than the conventional command hierarchy networks
