@inproceedings{ ISI:000387956200110,
Author = {Sun, Xiaojuan},
Editor = {{Wang, R and Pan, X}},
Title = {{Partial Connection Delay Controls the Mean Interspike Intervals of
   Neuronal Networks}},
Booktitle = {{ADVANCES IN COGNITIVE NEURODYNAMICS (V)}},
Series = {{Advances in Cognitive Neurodynamics}},
Year = {{2016}},
Pages = {{819-824}},
Note = {{5th International Conference on Cognitive Neurodynamics (ICCN), Sanya,
   PEOPLES R CHINA, JUN 03-07, 2015}},
Organization = {{East China Univ Sci \& Technol, Inst Cognit Neurodynam; Chinese Soc
   Theoret \& Appl Mech; Brain Engn Soc Korea; Tamagawa Univ, Brain Sci
   Ctr; KAIST, Brain Sci Res Ctr; Int Neural Network Soc; Japan Soc Neural
   Network; Shanghai Soc Nonlinear Sci; Natl Nat Sci Fdn China}},
Abstract = {{In this paper, we discuss how partial time delays influence the mean
   interspike intervals of the neuronal systems. With the aids of
   simulation results, we reveal that the mean interspike intervals of
   neuronal networks has close relationship with connection delays. In some
   intervals, it approximately equals to the connection delays, and in some
   other intervals, it approximately equals to half of the connection
   delays. More surprisingly for us is that only a few delayed connections
   could have great influences on the mean interspike intervals.
   Considering the reverse of the mean interspike intervals is the mean
   firing rate of neuronal networks, thus, our results obtained in this
   paper may give us some important implications on how to control the mean
   firing rate of neuronal networks through partial connection delays.}},
