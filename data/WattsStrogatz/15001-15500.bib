@inproceedings{ ISI:000455346400211,
Author = {Park, Himchan and Kim, Min-Soo},
Book-Group-Author = {{ACM}},
Title = {{EvoGraph: An Effective and Efficient Graph Upscaling Method for
   Preserving Graph Properties}},
Booktitle = {{KDD'18: PROCEEDINGS OF THE 24TH ACM SIGKDD INTERNATIONAL CONFERENCE ON
   KNOWLEDGE DISCOVERY \& DATA MINING}},
