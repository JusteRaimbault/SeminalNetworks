@inproceedings{ ISI:000468622101038,
Author = {Zhu, Ye and Song, Yu-Rong and Li, Yin-wei},
Editor = {{Chen, X and Zhao, QC}},
Title = {{A Tabu Search Optimization Algorithm with 2.5K Null Model}},
Booktitle = {{2018 37TH CHINESE CONTROL CONFERENCE (CCC)}},
Series = {{Chinese Control Conference}},
Year = {{2018}},
Pages = {{1082-1086}},
Note = {{37th Chinese Control Conference (CCC), Wuhan, PEOPLES R CHINA, JUL
   25-27, 2018}},
Organization = {{Chinese Assoc Automat, Tech Comm Control Theory; Syst Engn Soc China;
   China Univ Geosciences; Chinese Acad Sci, Acad Math \& Syst Sci; China
   Soc Ind \& Appl Math; Huazhong Univ Sci \& Technol; Wuhan Univ Sci \&
   Technol; Hubei Assoc Automat; Asian Control Assoc; IEEE Control Syst
   Soc; Inst Control Robot \& Syst; Soc Instrument \& Control Engineers}},
Abstract = {{The 2.5K null model has the same joint degree distribution and
   clustering spectrum with the original network. As a frame of reference,
   the design of 2.5K null model is extremely significant tool to study the
   clustering characteristics of the original network. The existing 2.5K
   null model algorithms are mainly generated based on random scrambling
   methods, which lead to redundant exchange problems during the exchange
   of edges and usually can not effectively reach the 2.5K targets for
   high-clustering networks. To solve these problems, we propose a tabu
   search optimization algorithm with 2.5K null model, which is constructed
   by setting weight of edges in initial network- 2K null model, selecting
