@article{ ISI:000415593600039,
Author = {Hong, Chen and Yin, Nai-Yu and He, Ning and Lordan, Oriol and Maria
   Sallan, Jose},
Title = {{Cascades Tolerance of Scale-Free Networks with Attack Cost}},
