Book-Group-Author = {{IEEE}},
Title = {{Social Simulation of Promoting Electric Vehicles for Evaluation of
   Policies in Local City}},
Booktitle = {{2018 JOINT 10TH INTERNATIONAL CONFERENCE ON SOFT COMPUTING AND
   INTELLIGENT SYSTEMS (SCIS) AND 19TH INTERNATIONAL SYMPOSIUM ON ADVANCED
   INTELLIGENT SYSTEMS (ISIS)}},
