@inproceedings{ ISI:000440623602048,
Author = {Li, Hui},
Book-Group-Author = {{IEEE}},
Title = {{A New Algorithm for Enumerating All Maximal Cliques}},
Booktitle = {{PROCEEDINGS OF 2017 3RD IEEE INTERNATIONAL CONFERENCE ON COMPUTER AND
   COMMUNICATIONS (ICCC)}},
