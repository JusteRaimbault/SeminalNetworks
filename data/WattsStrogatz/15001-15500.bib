Title = {{Dynamic Evolution Research and System Implementation of International
   Soybean Trade Network Based on Complex Network}},
Booktitle = {{PROCEEDINGS OF THE 2ND INTERNATIONAL CONFERENCE ON COMPUTER SCIENCE AND
   APPLICATION ENGINEERING (CSAE2018)}},
Year = {{2018}},
