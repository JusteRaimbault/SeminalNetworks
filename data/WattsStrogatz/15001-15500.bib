@inproceedings{ ISI:000456287900066,
Author = {Green, Oded and Fox, James and Watkins, Alex and Tripathy, Alok and
   Gabert, Kasimir and Kim, Euna and An, Xiaojing and Aatish, Kumar and
   Bader, David A.},
Book-Group-Author = {{IEEE}},
Title = {{Logarithmic Radix Binning and Vectorized Triangle Counting}},
Booktitle = {{2018 IEEE HIGH PERFORMANCE EXTREME COMPUTING CONFERENCE (HPEC)}},
Series = {{IEEE High Performance Extreme Computing Conference}},
Year = {{2018}},
Note = {{IEEE High Performance Extreme Computing Conference (HPEC), Waltham, MA,
   SEP 25-27, 2018}},
Organization = {{IEEE}},
Abstract = {{Triangle counting is a building block for numerous graph applications
   and given the fact that graphs continue to grow in size, its scalability
   is important. As such, numerous algorithms have been designed for
   triangle counting - some of which are compute-bound rather than memory
   bound. Even for compute-bound algorithms, one of the key challenges is
   the limited control flow available on the processor. This is in-part due
   to the high dependency between the control flow, input data, and limited
   utilization of vector instructions. Not surprising, compilers are not
   always able to detect these data dependencies and vectorize the
   algorithms. Using the branch-avoiding model we show to remove control
   flow restrictions by replacing branches with an equivalent set of
   arithmetic operations. More so, we show how these can be vectorized
   using Intel's AVX-512 instruction set and that our new vectorized
   algorithms are 2 x -5x faster than scalar counterparts. We also present
   a new load balancing method, Logarithmic Radix Binning (LRB) that
   ensures that threads and the vector data lanes execute a near equal
   amount of work at any given time. Altogether, our algorithm outperforms
   several 2017 HPEC Graph Challenge Champions such as the KOKKOS framework
   and a GPU based algorithm by anywhere from 1. 5x and up to 14x.}},
