@article{ ISI:000456195000009,
Author = {dos Santos, Jorge Filipe and Maia, Maria Bernadete and Maneschy, Maria
   Cristina and Matlaba, Valente and Mota, Jose Aroudo},
Title = {{SOCIAL NETWORKS OF COMMUNITIES ALONG CARAJAS RAILROAD IN THE EASTERN
   AMAZON}},
Journal = {{FINISTERRA-REVISTA PORTUGUESA DE GEOGRAFIA}},
Year = {{2018}},
Volume = {{53}},
Number = {{109}},
Pages = {{149-166}},
Abstract = {{The populations along the 900km of the Carajas Railroad (EFC) in the
   Brazilian Amazon region present weak socioeconomic indicators. In part,
   this condition is due to its geographic and institutional isolation.
   This study aims to analyze a network of institutions working in the
   territory in order to understand how they articulate each other and what
   is its potential to break the isolation. A methodology widely used in
   this context is based on the theory of social networks. In this work,
   the network was constructed from data obtained by interviewing 17
   leaders of community associations and other institutions operating in
   the EFC corridor. The network was sparse, with few nodes of high
   centrality and reduced flows from external sources to local communities,
   limiting their capacity for collective mobilization and development. The
   strengthening of a dense and diverse network by local stakeholders is
   susceptible to transform the character of ``resistance networks{''}
   against adverse impacts, to ``collaborative networks{''} for sustainable
   development.}},
