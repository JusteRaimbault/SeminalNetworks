@inproceedings{ ISI:000427044800104,
Author = {Di Girolamo, Salvatore and Vella, Flavio and Hoefler, Torster},
Book-Group-Author = {{IEEE}},
Title = {{Transparent Caching for RMA Systems}},
Booktitle = {{2017 31ST IEEE INTERNATIONAL PARALLEL AND DISTRIBUTED PROCESSING
   SYMPOSIUM (IPDPS)}},
Series = {{International Parallel and Distributed Processing Symposium IPDPS}},
Year = {{2017}},
