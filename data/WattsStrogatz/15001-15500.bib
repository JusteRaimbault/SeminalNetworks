@inproceedings{ ISI:000419843900188,
Author = {Guenther, Markus and Luepke, Lars and Stummer, Christian},
Editor = {{Kocaoglu, DF and Anderson, TR and Daim, TU and Kozanoglu, DC and Niwa, K and Perman, G and Steenhuis, HJ}},
Title = {{Cross-Over Between Scenario Analysis and Agent-Based Market Simulation
   for Technology Planning}},
Booktitle = {{2017 PORTLAND INTERNATIONAL CONFERENCE ON MANAGEMENT OF ENGINEERING AND
   TECHNOLOGY (PICMET)}},
