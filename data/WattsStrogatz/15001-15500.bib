@inproceedings{ ISI:000426125100008,
Author = {Kown, Su-kyung and Jang, Bongsoo and Lee, Byoung-Dai and Do, Younghae
   and Baek, Hunki and Choi, Yoon-Ho},
Editor = {{Choi, D and Guilley, S}},
Title = {{Influence Evaluation of Centrality-Based Random Scanning Strategy on
   Early Worm Propagation Rate}},
