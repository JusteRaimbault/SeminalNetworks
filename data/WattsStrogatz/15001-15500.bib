Title = {{Influencing Information Spreading Processes in Complex Networks with
   Probability Spraying}},
Booktitle = {{2018 IEEE/ACM INTERNATIONAL CONFERENCE ON ADVANCES IN SOCIAL NETWORKS
   ANALYSIS AND MINING (ASONAM)}},
Year = {{2018}},
Pages = {{1038-1046}},
