@article{ ISI:000403702000008,
Author = {Iwata, Tomoharu and Ishiguro, Katsuhiko},
Title = {{Robust unsupervised cluster matching for network data}},
Journal = {{DATA MINING AND KNOWLEDGE DISCOVERY}},
