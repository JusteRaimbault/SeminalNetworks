@inproceedings{ ISI:000418418000005,
Author = {Garcia-Robledo, Alberto and Diaz-Perez, Arturo and Morales-Luna,
   Guillermo},
Editor = {{Hernandez, CJB and Gitler, I and Klapp, J}},
Title = {{Accelerating All-Sources BFS Metrics on Multi-core Clusters for
   Large-Scale Complex Network Analysis}},
Booktitle = {{HIGH PERFORMANCE COMPUTING CARLA 2016}},
Series = {{Communications in Computer and Information Science}},
Year = {{2017}},
Volume = {{697}},
Pages = {{61-75}},
Note = {{3rd Latin American High-Performance Computing Conference (CARLA), Mexico
   City, MEXICO, AUG 29-SEP 02, 2016}},
Abstract = {{All-Sources BFS (AS-BFS) is the main building block in a variety of
   complex network metric algorithms, such as the average path length and
   the betweenness centrality. However, AS-BFS calculations involve as many
   full BFS traversals as the total number of vertices, rendering AS-BFS
   impractical on commodity systems for real-world graphs with millions of
   vertices and links. In this paper we present our experience with the
   acceleration of AS-BFS graph metrics on multi-core HPC clusters by
   outlining hybrid coarse-grain parallel algorithms for computing the
   average path-length, the diameter and the betweenness centrality of
   complex networks in a lock-free fashion. We report speedups of up to
   171x on a heterogeneous cluster of 12-core Intel Xeon and 32-core AMD
   Opteron multi-core nodes; as well as resource utilizations of up to
   75\%.}},
