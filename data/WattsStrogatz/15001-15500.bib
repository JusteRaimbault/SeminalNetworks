@inproceedings{ ISI:000461361000193,
Author = {Han, Jingjing and Zeng, Ming and Zhang, Xiaonei and Lu, Jing and Zhao,
   Chunyu and Meng, Qinghao},
Book-Group-Author = {{IEEE}},
Title = {{Multivariate Multiscale Recurrence Network Analysis for Characterizing
   Near-surface 3D Wind Speed Time Series in Different Environments}},
Booktitle = {{2018 13TH WORLD CONGRESS ON INTELLIGENT CONTROL AND AUTOMATION (WCICA)}},
Year = {{2018}},
Pages = {{1126-1130}},
Note = {{13th World Congress on Intelligent Control and Automation (WCICA),
   Changsha, PEOPLES R CHINA, JUL 04-08, 2018}},
Organization = {{IEEE; RA; Hunan Univ; Natl Univ Defense Technol; Harbin Inst Technol,
   State Key Lab Robot \& Syst; China State Construct Int Investment China
   Ltd; Chinese Univ Hong Kong; Hunan Xiangjiang New Area, Econ Dev Bur;
   Hunan Assoc Automat; Yuelu Dist Govt}},
Abstract = {{Uncovering the dynamic characteristics of time series measured from
   different 3D wind field is of great significance to better understand
   the mechanisms of gas diffusion and further provide critical clues for
   the optimization of robotic gas source localization algorithms. In this
   paper, we elaborately record three orthogonal wind speed components of
   indoor and outdoor environments with a high precision 3D ultrasonic
   anemometer. We first employ the Adaptive Optimal Kernel Time-Frequency
   Representation (AOK TFR) method to characterize the wind speed time
   series from the perspective of energy and frequency. Then, we infer
   multiscale recurrence networks from the experimental multivariate time
   series and calculate the average degree and the average clustering
   coefficient to quantitatively probe the network topological properties.
   Results show that these two network measures enable to quantitatively
   distinguish different fluctuation patterns associated with different
   airflow environments, a task that the AOK TFR method fails to work These
   interesting findings suggest that bridging multivariate multiscale
   analysis and complex networks provides a powerful tool to deeply uncover
   the multiscale complex dynamic behaviors underlying the natural
   near-surface wind field.}},
