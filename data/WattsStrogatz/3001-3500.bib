@article{ ISI:000278150100071,
Author = {Bradde, Serena and Caccioli, Fabio and Dall'Asta, Luca and Bianconi,
   Ginestra},
Title = {{Critical Fluctuations in Spatial Complex Networks}},
Journal = {{PHYSICAL REVIEW LETTERS}},
Year = {{2010}},
Volume = {{104}},
Number = {{21}},
Month = {{MAY 28}},
Abstract = {{An anomalous mean-field solution is known to capture the nontrivial
   phase diagram of the Ising model in annealed complex networks.
   Nevertheless, the critical fluctuations in random complex networks
   remain mean field. Here we show that a breakdown of this scenario can be
   obtained when complex networks are embedded in geometrical spaces.
   Through the analysis of the Ising model on annealed spatial networks, we
   reveal, in particular, the spectral properties of networks responsible
   for critical fluctuations and we generalize the Ginsburg criterion to
   complex topologies.}},
