@article{ ISI:000346123200023,
Author = {Kolda, Tamara G. and Pinar, Ali and Plantenga, Todd and Seshadhri, C.},
Title = {{A SCALABLE GENERATIVE GRAPH MODEL WITH COMMUNITY STRUCTURE}},
