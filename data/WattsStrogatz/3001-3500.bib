@article{ ISI:000329172700052,
Author = {Cabral, Joana and Fernandes, Henrique M. and Van Hartevelt, Tim J. and
   James, Anthony C. and Kringelbach, Morten L. and Deco, Gustavo},
Title = {{Structural connectivity in schizophrenia and its impact on the dynamics
   of spontaneous functional networks}},
