@article{ ISI:000248552600006,
Author = {Ahnert, S. E. and Garlaschelli, D. and Fink, T. M. A. and Caldarelli, G.},
Title = {{Ensemble approach to the analysis of weighted networks}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2007}},
Volume = {{76}},
Number = {{1, 2}},
Month = {{JUL}},
Abstract = {{We present an approach to the analysis of weighted networks, by
   providing a straightforward generalization of any network measure
   defined on unweighted networks, such as the average degree of the
   nearest neighbors, the clustering coefficient, the ``betweenness,{''}
   the distance between two nodes, and the diameter of a network. All these
   measures are well established for unweighted networks but have hitherto
   proven difficult to define for weighted networks. Our approach is based
   on the translation of a weighted network into an ensemble of edges.
   Further introducing this approach we demonstrate its advantages by
   applying the clustering coefficient constructed in this way to two
   real-world weighted networks.}},
