@article{ ISI:000264095500039,
Author = {Wang, Li-Na and Guo, Jin-Li and Yang, Han-Xin and Zhou, Tao},
Title = {{Local preferential attachment model for hierarchical networks}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2009}},
Volume = {{388}},
Number = {{8}},
Pages = {{1713-1720}},
Month = {{APR 15}},
Abstract = {{In real-life networks, incomers may only connect to a few others in a
   local area for their limited information, and individuals in a local
   area are likely to have close relations. Accordingly, we propose a local
   preferential attachment model. Here, a local-a rea-network stands for a
   node and all its neighbors, and the new nodes perform nonlinear
   preferential attachment, pi (k(i)) alpha k(i)(alpha), in local areas.
   The stable degree distribution and clustering-degree correlations are
   analytically obtained. With the increasing of alpha, the clustering
   coefficient increases, while assortativity decreases from positive to
   negative. In addition, by adjusting the parameter or, the model can
   generate different kinds of degree distribution, from exponential to
   power-law. The hierarchical organization, independent of alpha, is the
   most significant character of this model. (c) 2008 Elsevier B.V. All
   rights reserved.}},
