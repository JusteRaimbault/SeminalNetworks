@article{ ISI:000231564100022,
Author = {Holyst, JA and Sienkiewicz, J and Fronczak, A and Fronczak, P and
   Suchecki, K},
Title = {{Universal scaling of distances in complex networks}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2005}},
Volume = {{72}},
Number = {{2, 2}},
Month = {{AUG}},
Abstract = {{Universal scaling of distances between vertices of Erdos-Renyi random
   graphs, scale-free Barabasi-Albert models, science collaboration
   networks, biological networks, Internet Autonomous Systems and public
   transport networks are observed. A mean distance between two nodes of
   degrees k(i) and k(j) equals to < l(ij)>=A-B log(k(i)k(j)). The scaling
   is valid over several decades. A simple theory for the appearance of
   this scaling is presented. Parameters A and B depend on the mean value
   of a node degree < k >(nn) calculated for the nearest neighbors and on
   network clustering coefficients.}},
