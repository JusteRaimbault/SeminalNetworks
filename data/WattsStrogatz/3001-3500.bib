@article{ ISI:000323727000013,
Author = {Sadovsky, Alexander J. and MacLean, Jason N.},
Title = {{Scaling of Topologically Similar Functional Modules Defines Mouse
   Primary Auditory and Somatosensory Microcircuitry}},
Journal = {{JOURNAL OF NEUROSCIENCE}},
