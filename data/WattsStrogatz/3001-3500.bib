@article{ ISI:000236782300046,
Author = {Caruso, F and Latora, V and Pluchino, A and Rapisarda, A and Tadic, B},
Title = {{Olami-Feder-Christensen model on different networks}},
Journal = {{EUROPEAN PHYSICAL JOURNAL B}},
Year = {{2006}},
Volume = {{50}},
Number = {{1-2}},
Pages = {{243-247}},
Month = {{MAR}},
Note = {{3rd International Conference on News, Expectations and Trends in
   Statistical Physics, NEXT-SigmaPhi, Orthodox Acad Crete, Kolymbari,
   GREECE, AUG 13-18, 2005}},
Abstract = {{We investigate numerically the Self Organized Criticality (SOC)
   properties of the dissipative Olami-Feder-Christensen model on
   small-world and scale-free networks. We find that the small-world OFC
   model exhibits self-organized criticality. Indeed, in this case we
   observe power law behavior of earthquakes size distribution with finite
   size scaling for the cut-off region. In the scale-free OFC model,
   instead, the strength of disorder hinders synchronization and does not
   allow to reach a critical state.}},
