@article{ ISI:000366646700051,
Author = {Kim, Dae-Jin and Davis, Elysia Poggi and Sandman, Curt A. and Sporns,
   Olaf and O'Donnell, Brian F. and Buss, Claudia and Hetrick, William P.},
Title = {{Children's intellectual ability is associated with structural network
   integrity}},
