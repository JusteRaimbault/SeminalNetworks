@article{ ISI:000298675400005,
Author = {Foster, David V. and Foster, Jacob G. and Grassberger, Peter and
   Paczuski, Maya},
Title = {{Clustering drives assortativity and community structure in ensembles of
   networks}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2011}},
Volume = {{84}},
Number = {{6, 2}},
Month = {{DEC 20}},
Abstract = {{Clustering, assortativity, and communities are key features of complex
   networks. We probe dependencies between these features and find that
   ensembles of networks with high clustering display both high
   assortativity by degree and prominent community structure, while
   ensembles with high assortativity show much less enhancement of the
   clustering or community structure. Further, clustering can amplify a
   small homophilic bias for trait assortativity in network ensembles. This
   marked asymmetry suggests that transitivity could play a larger role
   than homophily in determining the structure of many complex networks.}},
