@inproceedings{ ISI:000332851000034,
Author = {Whitbeck, John and de Amorim, Marcelo Dias and Conan, Vania and
   Guillaume, Jean-Loup},
Book-Group-Author = {{ACM}},
Title = {{Temporal Reachability Graphs}},
Booktitle = {{MOBICOM 12: PROCEEDINGS OF THE 18TH ANNUAL INTERNATIONAL CONFERENCE ON
   MOBILE COMPUTING AND NETWORKING}},
