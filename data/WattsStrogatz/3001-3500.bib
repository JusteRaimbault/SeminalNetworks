Title = {{JA-BE-JA: A Distributed Algorithm for Balanced Graph Partitioning}},
Booktitle = {{2013 IEEE 7TH INTERNATIONAL CONFERENCE ON SELF-ADAPTIVE AND
   SELF-ORGANIZING SYSTEMS (SASO)}},
Series = {{International Conference on Self-Adaptive and Self-Organizing Systems}},
