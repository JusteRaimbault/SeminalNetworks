@article{ ISI:000393513400005,
Author = {Li, Wenlin and Li, Chong and Song, Heshan},
Title = {{Quantum synchronization and quantum state sharing in an irregular
   complex network}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2017}},
Volume = {{95}},
Number = {{2}},
Month = {{FEB 6}},
Abstract = {{We investigate the quantum synchronization phenomenon of the complex
   network constituted by coupled optomechanical systems and prove that the
   unknown identical quantum states can be shared or distributed in the
   quantum network even though the topology is varying. Considering a
   channel constructed by quantum correlation, we show that quantum
   synchronization can sustain and maintain high levels in Markovian
   dissipation for a long time. We also analyze the state-sharing process
   between two typical complex networks, and the results predict that
   linked nodes can be directly synchronized, but the whole network will be
   synchronized only if some specific synchronization conditions are
   satisfied. Furthermore, we give the synchronization conditions
   analytically through analyzing network dynamics. This proposal paves the
   way for studying multi-interaction synchronization and achieving
   effective quantum information processing in a complex network.}},
