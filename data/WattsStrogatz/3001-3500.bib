@article{ ISI:000263911600006,
Author = {Wong, Pak Chung and Schneider, Kevin and Mackey, Patrick and Foote,
   Harlan and Chin, Jr., George and Guttromson, Ross and Thomas, Jim},
Title = {{A Novel Visualization Technique for Electric Power Grid Analytics}},
