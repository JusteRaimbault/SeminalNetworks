@article{ ISI:000351075000031,
Author = {Gianetto, David A. and Heydari, Babak},
Title = {{Catalysts of Cooperation in System of Systems: The Role of Diversity and
   Network Structure}},
Journal = {{IEEE SYSTEMS JOURNAL}},
Year = {{2015}},
Volume = {{9}},
Number = {{1}},
Pages = {{303-311}},
