@article{ ISI:000225682200020,
Author = {Sinha, S},
Title = {{Complexity vs. stability in small-world networks}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2005}},
Volume = {{346}},
