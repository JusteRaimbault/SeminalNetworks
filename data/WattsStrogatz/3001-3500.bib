@article{ ISI:000303599200025,
Author = {Pajevic, Sinisa and Plenz, Dietmar},
Title = {{The organization of strong links in complex networks}},
Journal = {{NATURE PHYSICS}},
Year = {{2012}},
Volume = {{8}},
Number = {{5}},
Pages = {{429-436}},
Month = {{MAY}},
Abstract = {{Many complex systems reveal a small-world topology, which allows
   simultaneously local and global efficiency in the interaction between
   system constituents. Here, we report the results of a comprehensive
   study that investigates the relation between the clustering properties
   in such small-world systems and the strength of interactions between its
   constituents, quantified by the link weight. For brain, gene, social and
   language networks, we find a local integrative weight organization in
   which strong links preferentially occur between nodes with overlapping
   neighbourhoods; we relate this to global robustness of the clustering to
   removal of the weakest links. Furthermore, we identify local learning
   rules that establish integrative networks and improve network traffic in
   response to past traffic failures. Our findings identify a general
   organization for complex systems that strikes a balance between
   efficient local and global communication in their strong interactions,
   while allowing for robust, exploratory development of weak interactions.}},
