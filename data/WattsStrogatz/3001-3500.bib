@article{ ISI:000273694300024,
Author = {Yang, Jianmei and Yao, Canzhong and Ma, Weicheng and Chen, Guanrong},
Title = {{A study of the spreading scheme for viral marketing based on a complex
   network model}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
