@inproceedings{ ISI:000248557700029,
Author = {Sayama, Hiroki},
Book-Group-Author = {{IEEE}},
Title = {{Generative Network Automata: A generalized framework for modeling
   complex dynamical systems with autonomously varying topologies}},
Booktitle = {{2007 IEEE SYMPOSIUM ON ARTIFICIAL LIFE}},
