@article{ ISI:000328584500007,
Author = {Stockton, Jr., Steven D. and Devi, Lakshmi A.},
Title = {{An Integrated Quantitative Proteomics and Systems Biology Approach to
   Explore Synaptic Protein Profile Changes During Morphine Exposure}},
Journal = {{NEUROPSYCHOPHARMACOLOGY}},
