@article{ ISI:000353553100002,
Author = {Xu, Bo and Shi, Huibin and Wang, Jianwei and Huang, Yun},
Title = {{Effective seeding strategy in evolutionary prisoner's dilemma games on
   online social networks}},
Journal = {{MODERN PHYSICS LETTERS B}},
Year = {{2015}},
Volume = {{29}},
Number = {{9}},
Month = {{APR 10}},
Abstract = {{This paper explores effective seeding strategies in prisoner's dilemma
   game (PDG) on online social networks, i.e. the optimal strategy to
   obtain global cooperation with minimum cost. Three distinct seeding
   strategies are compared by performing computer simulations on real
   online social network datasets. Our finding suggests that degree
   centrality seeding outperforms other strategies regardless of the
   initial payoff setting or network size. Celebrities of online social
   networks play key roles in preserving cooperation.}},
