@article{ ISI:000345516000010,
Author = {Ligo, Jonathan G. and Atia, George K. and Veeravalli, Venugopal V.},
Title = {{A Controlled Sensing Approach to Graph Classification}},
Journal = {{IEEE TRANSACTIONS ON SIGNAL PROCESSING}},
Year = {{2014}},
Volume = {{62}},
Number = {{24}},
Pages = {{6468-6480}},
Month = {{DEC 15}},
Abstract = {{The problem of classifying graphs with respect to connectivity via
   partial observations of nodes is posed as a composite hypothesis testing
   problem with controlled sensing. An observation at a node is a subset of
   edges incident to the node on the complete graph drawn according to a
   probability model, which is a function of a fixed underlying graph.
   Connectivity is measured through average node degree and is classified
   with respect to a threshold. A simple approximation of the controlled
   sensing test is derived and simulated on Erdos-Renyi graphs to
   characterize the error probabilities as a function of the expected
   stopping times. The test is also experimentally validated on a
   real-world example of the social structure of Long-Tailed Manakins. It
   is shown that the proposed test achieves favorable tradeoffs between the
   classification error and the number of measurements. Furthermore, the
   test outperforms existing approaches, especially at low target error
   rates. In addition, the proposed test achieves the optimal error
   exponent.}},
