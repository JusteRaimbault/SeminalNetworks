@inproceedings{ ISI:000241478200042,
Author = {Kumar, Ravi and Liben-Nowell, David and Tomkins, Andrew},
Editor = {{Azar, Y and Erlebach, T}},
Title = {{Navigating low-dimensional and hierarchical population networks}},
Booktitle = {{ALGORITHMS - ESA 2006, PROCEEDINGS}},
Series = {{Lecture Notes in Computer Science}},
