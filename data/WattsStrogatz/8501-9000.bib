@article{ ISI:000300594300008,
Author = {Li, Chong and Liu, Sifeng and Fang, Zhigeng and Chen, Ye},
Title = {{Emergence of structural properties in economic networks: A
   multi-local-world evolving model}},
Journal = {{SIMULATION MODELLING PRACTICE AND THEORY}},
Year = {{2012}},
Volume = {{21}},
Number = {{1}},
Pages = {{91-102}},
Month = {{FEB}},
Abstract = {{Economic networks represent prototypes of complex evolving networks. A
   novel evolving multi-local-world model is proposed, and the dynamic
   mechanisms that govern networks' evolution is described and analyzed.
   Based on the analytical model, we uncover the topological structure that
   characterizes both the scale-free feature and the small-world effect and
   further analyze the surprising emergence of characteristics mutation.
   Evolution mechanism of these structural properties and their
