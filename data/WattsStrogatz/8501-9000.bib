@inproceedings{ ISI:000297265302112,
Author = {Li, Bing and Pan, Weifeng and Lu, Jinhu},
Book-Group-Author = {{IEEE}},
Title = {{Multi-Granularity Dynamic Analysis of Complex Software Networks}},
Booktitle = {{2011 IEEE INTERNATIONAL SYMPOSIUM ON CIRCUITS AND SYSTEMS (ISCAS)}},
