@article{ ISI:000423495100088,
Author = {Wang, Xingyuan and Zhou, Wenjie and Li, Rui and Cao, Jianye and Lin,
   Xiaohui},
Title = {{Improving robustness of interdependent networks by a new coupling
   strategy}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2018}},
Volume = {{492}},
Pages = {{1075-1080}},
Month = {{FEB 15}},
Abstract = {{There are two kinds of methods to improve the robustness of
   interdependent networks by changing the edge connection. One is to
   change the connectivity link, and the other is to change the
   inter-links. In this paper, we focus on the factors that affect the
   propagation of cascading failures. And based on the study of the
   propagation process of failures, the two factors affecting the
   propagation of cascading failures are found. Finally, a coupling
   strategy of local optimization NPC (Neighbor node priority connection)
   is proposed. By applying this strategy to two kinds of interdependent
   networks ER-ER and SF-SF, we find that the robustness of the
   interdependent networks coupled by NPC is better than that of the three
   classical strategies. (C) 2017 Elsevier B.V. All rights reserved.}},
