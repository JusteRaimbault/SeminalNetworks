@inproceedings{ ISI:000291965200018,
Author = {Riviere, Etienne and Voulgaris, Spyros},
Editor = {{Babin, G and StanoevskaSlabeva, K and Kropf, P}},
Title = {{Gossip-Based Networking for Internet-Scale Distributed Systems}},
Booktitle = {{E-TECHNOLOGIES: TRANSFORMATION IN A CONNECTED WORLD}},
