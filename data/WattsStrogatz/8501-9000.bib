@inproceedings{ ISI:000260488700055,
Author = {Yoshida, Takashi and Tomizawa, Nobuyuki and Gotoh, Tomohisa and Iguchi,
   Hiroto and Sugioka, Kei and Ikeda, Ken'ichi},
