@inproceedings{ ISI:000289650200020,
Author = {Nguyen, Van and Martel, Chip},
Editor = {{Munro, JI and Sedgewick, R and Szpankowski, W and Wagner, D}},
Title = {{Augmented Graph Models for Small-World Analysis with Geographical
   Factors}},
Booktitle = {{PROCEEDINGS OF THE TENTH WORKSHOP ON ALGORITHM ENGINEERING AND
   EXPERIMENTS AND THE FIFTH WORKSHOP ON ANALYTIC ALGORITHMICS AND
   COMBINATORICS}},
