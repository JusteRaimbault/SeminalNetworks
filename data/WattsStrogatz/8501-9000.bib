@article{ ISI:000269357900036,
Author = {Wang, Wen-Xu and Huang, Liang and Lai, Ying-Cheng},
Title = {{Universal dynamics on complex networks}},
Journal = {{EPL}},
Year = {{2009}},
Volume = {{87}},
Number = {{1}},
Month = {{JUL}},
Abstract = {{We uncover a class of universal dynamics on weighted complex networks.
   In particular, we find that by incorporating a universal weighting
   scheme into real-world networks, the topological details of various
   real-world networks, whether biological, physical, technological, or
   social, have little influence on typical dynamical processes such as
   synchronization, epidemic spreading, and percolation. This striking
   finding is demonstrated using a large number of real-world networks and
   substantiated by analytic considerations. These findings make possible
   generic and robust control strategies for a variety of dynamical
   processes on complex networks. Copyright (C) EPLA, 2009}},
