@article{ ISI:000253297900009,
Author = {Manka, Anna and Malarz, Krzysztof and Kulakowski, Krzysztof},
Title = {{Clusterization, frustration and collectivity in random networks}},
Journal = {{INTERNATIONAL JOURNAL OF MODERN PHYSICS C}},
Year = {{2007}},
Volume = {{18}},
Number = {{11}},
Pages = {{1765-1773}},
Month = {{NOV}},
Abstract = {{We consider the random Erdos-Renyi network with enhanced clusterization
   and Ising spins s = +/- 1 at the network nodes. Mutually linked spins
   interact with energy J. Magnetic properties of the system that are
   dependent on the clustering coefficient C are investigated with the
   Monte Carlo heat bath algorithm. For J > 0 the Curie temperature T-c
   increases from 3.9 to 5.5 when C increases from almost zero to 0.18.
   These results deviate only slightly from the mean field theory. For J <
   0 the spin-glass phase appears below T-SG; this temperature decreases
   with C, on the contrary to the mean field calculations. The results are
   interpreted in terms of social systems.}},
