@article{ ISI:000298188700002,
Author = {Chen, Jing and Ma, Zhengming},
Title = {{LOCALLY LINEAR EMBEDDING: A REVIEW}},
Journal = {{INTERNATIONAL JOURNAL OF PATTERN RECOGNITION AND ARTIFICIAL INTELLIGENCE}},
