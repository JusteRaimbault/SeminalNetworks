@article{ ISI:000334071400002,
Author = {Sivakumar, Bellie},
Title = {{Planning and management of shared waters: hydropolitics and
   hydropsychology - two sides of the same coin}},
Journal = {{INTERNATIONAL JOURNAL OF WATER RESOURCES DEVELOPMENT}},
Year = {{2014}},
Volume = {{30}},
