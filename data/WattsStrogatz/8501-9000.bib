@inproceedings{ ISI:000259505003288,
Author = {Small, Tara and Li, Baochun and Liang, Ben},
Book-Group-Author = {{IEEE}},
Title = {{Topology affects the efficiency of network coding in peer-to-peer
   networks}},
Booktitle = {{2008 IEEE INTERNATIONAL CONFERENCE ON COMMUNICATIONS, PROCEEDINGS, VOLS
   1-13}},
Series = {{IEEE International Conference on Communications}},
