@article{ ISI:000340877800002,
Author = {Gyurko, David M. and Soti, Csaba and Stetak, Attila and Csermely, Peter},
Title = {{System Level Mechanisms of Adaptation, Learning, Memory Formation and
   Evolvability: The Role of Chaperone and Other Networks}},
Journal = {{CURRENT PROTEIN \& PEPTIDE SCIENCE}},
