@inproceedings{ ISI:000469098900012,
Author = {Wang, Gang and Wang, Bolun and Wang, Tianyi and Nika, Ana and Zheng,
   Haitao and Zhao, Ben Y.},
Book-Group-Author = {{ACM}},
Title = {{Whispers in the Dark: Analysis of an Anonymous Social Network}},
