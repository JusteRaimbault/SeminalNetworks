@article{ ISI:000225970700014,
Author = {Lo, TS and Chan, HY and Hui, PM and Johnson, NF},
Title = {{Theory of networked minority games based on strategy pattern dynamics}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2004}},
Volume = {{70}},
Number = {{5, 2}},
Month = {{NOV}},
Abstract = {{We formulate a theory of agent-based models in which agents compete to
   be in a winning group. The agents may be part of a network or not, and
   the winning group may be a minority group or not. An important feature
   of the present formalism is its focus on the dynamical pattern of
   strategy rankings, and its careful treatment of the strategy ties which
   arise during the system's temporal evolution. We apply it to the
   minority game with connected populations. Expressions for the mean
   success rate among the agents and for the mean success rate for agents
