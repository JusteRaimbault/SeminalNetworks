Affiliation = {{Criado, R (Reprint Author), Rey Juan Carlos Univ, Dept Appl Math, Madrid 28933, Spain.
   Criado, Regino; Flores, Julio; Garcia del Amo, Alejandro; Gomez-Gardenes, Jesus; Romance, Miguel, Rey Juan Carlos Univ, Dept Appl Math, Madrid 28933, Spain.}},
DOI = {{10.1080/00207160.2011.577212}},
