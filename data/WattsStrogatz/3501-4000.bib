@article{ ISI:000365094400004,
Author = {Cram, Jacob A. and Xia, Li C. and Needham, David M. and Sachdeva, Rohan
   and Sun, Fengzhu and Fuhrman, Jed A.},
Title = {{Cross-depth analysis of marine bacterial networks suggests downward
   propagation of temporal changes}},
Journal = {{ISME JOURNAL}},
