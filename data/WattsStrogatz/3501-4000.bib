@article{ ISI:000233282200014,
Author = {Baldwin, NE and Chesler, EJ and Kirov, S and Langston, MA and Snoddy, JR
   and Williams, RW and Zhang, B},
Title = {{Computational, integrative, and comparative methods for the elucidation
   of genetic coexpression networks}},
Journal = {{JOURNAL OF BIOMEDICINE AND BIOTECHNOLOGY}},
Year = {{2005}},
Number = {{2}},
Pages = {{172-180}},
Month = {{JUN 30}},
Abstract = {{Gene expression microarray data can be used for the assembly of genetic
   coexpression network graphs. Using mRNA samples obtained from
   recombinant inbred Mus musculus strains, it is possible to integrate
   allelic variation with molecular and higher-order phenotypes. The depth
   of quantitative genetic analysis of microarray data can be vastly
   enhanced utilizing this mouse resource in combination with powerful
   computational algorithms, platforms, and data repositories. The
   resulting network graphs transect many levels of biological scale. This
   approach is illustrated with the extraction of cliques of putatively
   coregulated genes and their annotation using gene ontology analysis and
   cis-regulatory element discovery. The causal basis for coregulation is
   detected through the use of quantitative trait locus mapping.}},
