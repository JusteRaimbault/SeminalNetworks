@article{ ISI:000324975700013,
Author = {Dickinson, Janis L. and Crain, Rhiannon L. and Reeve, H. Kern and
   Schuldt, Jonathon P.},
Title = {{Can evolutionary design of social networks make it easier to be `green'?}},
Journal = {{TRENDS IN ECOLOGY \& EVOLUTION}},
Year = {{2013}},
Volume = {{28}},
