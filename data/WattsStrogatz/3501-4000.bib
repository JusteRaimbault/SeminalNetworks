@article{ ISI:000329226500017,
Author = {Shafi, Mouhsin M. and Westover, M. Brandon and Oberman, Lindsay and
   Cash, Sydney S. and Pascual-Leone, Alvaro},
Title = {{Modulation of EEG Functional Connectivity Networks in Subjects
   Undergoing Repetitive Transcranial Magnetic Stimulation}},
Journal = {{BRAIN TOPOGRAPHY}},
