@inproceedings{ ISI:000287046500018,
Author = {Shah, Devavrat and Zaman, Tauhid},
Book-Group-Author = {{ACM}},
Title = {{Detecting Sources of Computer Viruses in Networks: Theory and Experiment}},
Booktitle = {{SIGMETRICS 2010: PROCEEDINGS OF THE 2010 ACM SIGMETRICS INTERNATIONAL
   CONFERENCE ON MEASUREMENT AND MODELING OF COMPUTER SYSTEMS}},
Series = {{ACM SIGMETRICS Performance Evaluation Review Special Issue}},
Year = {{2010}},
