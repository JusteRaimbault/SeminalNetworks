@article{ ISI:000364889500018,
Author = {Lu, Jianquan and Ding, Chengdan and Lou, Jungang and Cao, Jinde},
Title = {{Outer synchronization of partially coupled dynamical networks via
   pinning impulsive controllers}},
Journal = {{JOURNAL OF THE FRANKLIN INSTITUTE-ENGINEERING AND APPLIED MATHEMATICS}},
Year = {{2015}},
Volume = {{352}},
Number = {{11}},
Pages = {{5024-5041}},
Month = {{NOV}},
Abstract = {{This paper presents an analytical study of outer synchronization of
   partially coupled dynamical networks via pinning impulsive controller.
   At first, more realistic drive-response partially coupled networks are
   established. Then, based on the regrouping method, some efficient and
   less conservative synchronization criteria are derived and developed in
   terms of average impulsive interval. Our results show that, by
   impulsively controlling a crucial fraction of nodes in the response
   network, the outer synchronization can be achieved. Finally, illustrated
   examples are given to verify the effectiveness of the proposed strategy.
   (C) 2015 The Franklin Institute. Published by Elsevier Ltd. All rights
   reserved.}},
