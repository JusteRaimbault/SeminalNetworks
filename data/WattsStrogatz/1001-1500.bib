@article{ ISI:000301652300001,
Author = {Shafi, Mouhsin M. and Westover, M. Brandon and Fox, Michael D. and
   Pascual-Leone, Alvaro},
Title = {{Exploration and modulation of brain network interactions with
   noninvasive brain stimulation in combination with neuroimaging}},
