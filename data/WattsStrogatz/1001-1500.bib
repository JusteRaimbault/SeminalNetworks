@article{ ISI:000259199300016,
Author = {Gilligan, Christopher A. and van den Bosch, Frank},
Title = {{Epidemiological models for invasion and persistence of pathogens}},
Journal = {{ANNUAL REVIEW OF PHYTOPATHOLOGY}},
