@article{ ISI:000345629000004,
Author = {Liu, Chenlong and Liu, Jing and Jiang, Zhongzhou},
Title = {{A Multiobjective Evolutionary Algorithm Based on Similarity for
   Community Detection from Signed Social Networks}},
Journal = {{IEEE TRANSACTIONS ON CYBERNETICS}},
Year = {{2014}},
Volume = {{44}},
