@article{ ISI:000258959800016,
Author = {Sueur, Cedric and Petit, Odile},
Title = {{Organization of group members at departure is driven by social structure
   in Macaca}},
Journal = {{INTERNATIONAL JOURNAL OF PRIMATOLOGY}},
Year = {{2008}},
Volume = {{29}},
Number = {{4}},
Pages = {{1085-1098}},
Month = {{AUG}},
Note = {{21st Congress of the International-Primatological-Society, Entebbe,
   UGANDA, JUN 25-30, 2006}},
Organization = {{Int Primatol Soc}},
Abstract = {{Researchers have often explained order of progression of group members
   during joint movement in terms of the influence of ecological pressures
   but rarely that of social constraints. We studied the order of joining
   by group members to a movement in semifree-ranging macaques with
   contrasting social systems: 1 group of Tonkean macaques (Macaca
   tonkeana) and 1 group of rhesus macaques (M. mulatta). We used network
   metrics to understand roles and associations among individuals. The way
   the macaques joined a movement reflected the social differences between
   the species in terms of dominance and kinship. Old and dominant male
   rhesus macaques were more often at the front of the movement, contrary
   to the Tonkean macaques, which exhibited no specific order. Moreover,
   rhesus macaques preferred to join high-ranking or related individuals,
   whereas Tonkean macaques based associations during joining mostly on
   sexual relationships with a subgroup of peripheral males.}},
