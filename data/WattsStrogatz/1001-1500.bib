@article{ ISI:000225055700005,
Author = {Wu, JJ and Gao, ZY and Sun, HJ and Huang, HJ},
Title = {{Urban transit system as a scale-free network}},
Journal = {{MODERN PHYSICS LETTERS B}},
Year = {{2004}},
Volume = {{18}},
Number = {{19-20}},
Pages = {{1043-1049}},
Month = {{AUG 30}},
Abstract = {{Many systems can be represented by networks as a set of nodes joined
   together by links indicating interaction. Recently studies have
   suggested that a lot of real networks are scale-free, such as the WWW,
   social networks, etc. In this paper, discoveries of scale-free
   characteristics are reported on the network constructed from the real
   urban transit system data in Beijing. It is shown that the connectivity
   distribution of the transit network decays as a power-law, and the
   exponent lambda is about equal to 2.24 from the simulation graph. Based
   on the scale-free network topology structure of the transit network, if
   only transit ``hub nodes{''} are controlled well, the transit network
   can resist random failures (such as traffic congestion, traffic
   accidents, etc.) successfully.}},
