@article{ ISI:000181520300016,
Author = {Campos, PRA and de Oliveira, VM and Moreira, FGB},
Title = {{Small-world effects in the majority-vote model}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2003}},
Volume = {{67}},
Number = {{2, 2}},
Month = {{FEB}},
Abstract = {{We investigate the majority-vote model on small-world networks by
   rewiring the two-dimensional square lattice. We observe that the
   introduction of long-range interactions does not remove the critical
   character of the model, that is, the system still exhibits a
   well-defined phase transition. However, we find that now the critical
   point is a monotonically increasing function of the rewiring
   probability. Moreover, we find that small-world effects change the class
   of universality of the model.}},
