@article{ ISI:000298099800015,
Author = {Donner, R. V. and Heitzig, J. and Donges, J. F. and Zou, Y. and Marwan,
   N. and Kurths, J.},
Title = {{The geometry of chaotic dynamics - a complex network perspective}},
Journal = {{EUROPEAN PHYSICAL JOURNAL B}},
