@article{ ISI:000243893700017,
Author = {Kim, J. S. and Goh, K. -I. and Salvi, G. and Oh, E. and Kahng, B. and
   Kim, D.},
Title = {{Fractality in complex networks: Critical and supercritical skeletons}},
