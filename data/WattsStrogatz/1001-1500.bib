@article{ ISI:000255868300035,
Author = {Bromberg, Kenneth D. and Ma'ayan, Avi and Neves, Susana R. and Iyengar,
   Ravi},
Title = {{Design logic of a cannabinoid receptor signaling network that triggers
   neurite outgrowth}},
Journal = {{SCIENCE}},
