@article{ ISI:000303455200024,
Author = {Boettcher, Stefan and Singh, Vijay and Ziff, Robert M.},
Title = {{Ordinary percolation with discontinuous transitions}},
Journal = {{NATURE COMMUNICATIONS}},
Year = {{2012}},
Volume = {{3}},
Month = {{APR}},
Abstract = {{Percolation on a one-dimensional lattice and fractals, such as the
   Sierpinski gasket, is typically considered to be trivial, because they
   percolate only at full bond density. By dressing up such lattices with
   small-world bonds, a novel percolation transition with explosive cluster
   growth can emerge at a non-trivial critical point. There, the usual
   order parameter, describing the probability of any node to be part of
   the largest cluster, jumps instantly to a finite value. Here we provide
   a simple example in the form of a small-world network consisting of a
   one-dimensional lattice which, when combined with a hierarchy of
   long-range bonds, reveals many features of this transition in a
   mathematically rigorous manner.}},
