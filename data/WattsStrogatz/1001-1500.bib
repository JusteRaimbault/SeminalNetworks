@article{ ISI:000242785000005,
Author = {Castello, Xavier and Eguiluz, Victor M. and San Miguel, Maxi},
Title = {{Ordering dynamics with two non-excluding options: bilingualism in
   language competition}},
Journal = {{NEW JOURNAL OF PHYSICS}},
Year = {{2006}},
Volume = {{8}},
Month = {{DEC 6}},
Abstract = {{We consider an extension of the voter model in which a set of
   interacting elements ( agents) can be in either of two equivalent states
   (A or B) or in a third additional mixed ( AB) state. The model is
   motivated by studies of language competition dynamics, where the AB
   state is associated with bilingualism. We study the ordering process and
   associated interface and coarsening dynamics in regular lattices and
   small world networks. Agents in the AB state define the interfaces,
   changing the interfacial noise driven coarsening of the voter model to
   curvature driven coarsening. This change in the coarsening mechanism is
   also shown to originate for a class of perturbations of the voter model
   dynamics. When interaction is through a small world network the AB
   agents restore coarsening, eliminating the metastable states of the
   voter model. The characteristic time to reach the absorbing state scales
   with system size as tau similar to lnN to be compared with the result
   tau similar to N for the voter model in a small world network.}},
