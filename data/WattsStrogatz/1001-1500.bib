@article{ ISI:000297880200002,
Author = {Schilling, Melissa A. and Green, Elad},
Title = {{Recombinant search and breakthrough idea generation: An analysis of high
   impact papers in the social sciences}},
Journal = {{RESEARCH POLICY}},
Year = {{2011}},
Volume = {{40}},
Number = {{10}},
Pages = {{1321-1331}},
Month = {{DEC}},
Abstract = {{Some ideas have dramatically more impact than others - they may overturn
   existing paradigms or launch new areas of scientific inquiry. Where do
   such high impact ideas come from? Are some search processes
   significantly more likely to lead to breakthrough idea generation than
   others? In this research, we compare ``high impact{''} papers from the
   social sciences with random-but-matched articles published in the same
   journals in the same years. We find that search scope, search depth, and
   atypical connections between different research domains significantly
   increase a paper's impact, even when controlling for the experience and
   prior publishing success of the author(s). (C) 2011 Elsevier B.V. All
   rights reserved.}},
Publisher = {{ELSEVIER SCIENCE BV}},
Address = {{PO BOX 211, 1000 AE AMSTERDAM, NETHERLANDS}},
Type = {{Article}},
