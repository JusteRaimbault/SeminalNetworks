@article{ ISI:000308221300091,
Author = {Moussa, Malaak N. and Steen, Matthew R. and Laurienti, Paul J. and
   Hayasaka, Satoru},
Title = {{Consistency of Network Modules in Resting-State fMRI Connectome Data}},
Journal = {{PLOS ONE}},
Year = {{2012}},
Volume = {{7}},
Number = {{8}},
Month = {{AUG 31}},
Abstract = {{At rest, spontaneous brain activity measured by fMRI is summarized by a
   number of distinct resting state networks (RSNs) following similar
   temporal time courses. Such networks have been consistently identified
   across subjects using spatial ICA (independent component analysis).
   Moreover, graph theory-based network analyses have also been applied to
   resting-state fMRI data, identifying similar RSNs, although typically at
   a coarser spatial resolution. In this work, we examined resting-state
   fMRI networks from 194 subjects at a voxel-level resolution, and
   examined the consistency of RSNs across subjects using a metric called
   scaled inclusivity (SI), which summarizes consistency of modular
   partitions across networks. Our SI analyses indicated that some RSNs are
   robust across subjects, comparable to the corresponding RSNs identified
   by ICA. We also found that some commonly reported RSNs are less
   consistent across subjects. This is the first direct comparison of RSNs
   between ICAs and graph-based network analyses at a comparable
   resolution.}},
