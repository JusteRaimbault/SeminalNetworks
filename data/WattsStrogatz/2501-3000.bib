@article{ ISI:000236464500041,
Author = {Chavez, M and Hwang, DU and Amann, A and Boccaletti, S},
Title = {{Synchronizing weighted complex networks}},
Journal = {{CHAOS}},
Year = {{2006}},
Volume = {{16}},
Number = {{1}},
Month = {{MAR}},
Abstract = {{Real networks often consist of local units, which interact with each
   other via asymmetric and heterogeneous connections. In this work, we
   explore the constructive role played by such a directed and weighted
   wiring for the synchronization of networks of coupled dynamical systems.
   The stability condition for the synchronous state is obtained from the
   spectrum of the respective coupling matrices. In particular, we consider
   a coupling scheme in which the relative importance of a link depends on
   the number of shortest paths through it. We illustrate our findings for
   networks with different topologies: scale free, small world, and random
   wirings. (C) 2006 American Institute of Physics.}},
