@article{ ISI:000290986200004,
Author = {Xiang, Linying and Zhu, Jonathan J. H.},
Title = {{On pinning synchronization of general coupled networks}},
Journal = {{NONLINEAR DYNAMICS}},
Year = {{2011}},
Volume = {{64}},
Number = {{4}},
Pages = {{339-348}},
Month = {{JUN}},
Abstract = {{The synchronization in general coupled networks subjected to pinning
   control is investigated. Some generic stability criteria based on the
   Lyapunov approach are derived for such general controlled networks,
   which guarantee that the whole network can be pinned to a
   synchronization state by placing feedback control on only a small
   fraction of nodes. A real network of television audience flows across 28
   satellite channels in China and a representative BA scale-free network
   composed of chaotic systems are shown, respectively, for illustration
   and verification. It is found that pinning stability can be improved via
   increasing pinning density and/or pinning strength for complete diagonal
   inner coupling.}},
