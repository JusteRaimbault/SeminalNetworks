Author = {Guenther, M. and Stummer, C. and Wakolbinger, L. M. and Wildpaner, M.},
Title = {{An agent-based simulation approach for the new product diffusion of a
   novel biomass fuel}},
Journal = {{JOURNAL OF THE OPERATIONAL RESEARCH SOCIETY}},
