@article{ ISI:000224855300014,
Author = {Poyatos, JF and Hurst, LD},
Title = {{How biologically relevant are interaction-based modules in protein
   networks?}},
Journal = {{GENOME BIOLOGY}},
Year = {{2004}},
Volume = {{5}},
Number = {{11}},
Abstract = {{By applying a graph-based algorithm to yeast protein-interaction
   networks we have extracted modular structures and show that they can be
   validated using information from the phylogenetic conservation of the
   network components. We show that the module cores, the parts with the
   highest intramodular connectivity, are biologically relevant components
   of the networks. These constituents correlate only weakly with other
   levels of organization. We also discuss how such structures could be
   used for finding targets for antimicrobial drugs.}},
