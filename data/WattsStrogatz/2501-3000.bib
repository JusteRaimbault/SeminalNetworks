@article{ ISI:000391267800048,
Author = {del Genio, Charo I. and Gomez-Gardenes, Jesus and Bonamassa, Ivan and
   Boccaletti, Stefano},
Title = {{Synchronization in networks with multiple interaction layers}},
Journal = {{SCIENCE ADVANCES}},
Year = {{2016}},
Volume = {{2}},
Number = {{11}},
Month = {{NOV}},
Abstract = {{The structure of many real-world systems is best captured by networks
   consisting of several interaction layers. Understanding how a
   multilayered structure of connections affects the synchronization
   properties of dynamical systems evolving on top of it is a highly
   relevant endeavor inmathematics and physics and has potential
   applications in several socially relevant topics, such as power grid
   engineering and neural dynamics. We propose a general framework to
   assess the stability of the synchronized state in networks with multiple
   interaction layers, deriving a necessary condition that generalizes the
   master stability function approach. We validate our method by applying
   it to a network of Rossler oscillators with a double layer of
   interactions and show that highly rich phenomenology emerges from this.
   This includes cases where the stability of synchronization can be
   induced even if both layers would have individually induced unstable
   synchrony, an effect genuinely arising from the true multilayer
   structure of the interactions among the units in the network.}},
