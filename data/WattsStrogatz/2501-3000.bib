@article{ ISI:000225137700002,
Author = {Tomochi, M},
Title = {{Defectors' niches: prisoner's dilemma game on disordered networks}},
Journal = {{SOCIAL NETWORKS}},
Year = {{2004}},
Volume = {{26}},
Number = {{4}},
Pages = {{309-321}},
Month = {{OCT}},
Abstract = {{Effects of disordered networks on evolution of cooperation are studied
   based on the prisoner's dilemma game on a random regular relational
   graph. As a parameter q (0 less than or equal to q less than or equal to
   1) that controls a degree of randomness of the network varies from zero
   to unity, the initial two-dimensional square lattice network becomes
   more relationally randomized while regularity of the network is kept. It
   is shown that random connections that make possible cooperator jump into
   defectors' clusters ironically trigger the formation of defectors'
   niches, in which defectors impose upon cooperators and do not have
   incentive to change their strategy. (C) 2004 Elsevier B.V. All rights
   reserved.}},
