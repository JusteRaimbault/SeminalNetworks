@inproceedings{ ISI:000226202500001,
Author = {Yu, B and Singh, MP and Sycara, K},
Book-Group-Author = {{ieee}},
Title = {{Developing trust in large-scale peer-to-peer systems}},
Booktitle = {{2004 IEEE 1ST SYMPOSIUM ON MULTI-AGENT SECURITY \& SURVIVABILITY}},
Year = {{2004}},
Pages = {{1-10}},
Note = {{1st IEEE Symposium on Multi-Agent Security and Survivability, Drexel
   Univ, Philadelphia, PA, AUG 30-31, 2004}},
Organization = {{IEEE}},
Abstract = {{In peer-to-peer (P2P) systems, peers often must inter- act with unknown
   or unfamiliar peers without the benefit of trusted third parties or
   authorities to mediate the interactions. A peer will need reputation
   mechanisms to incorporate the knowledge of others to decide whether to
   trust another party, in P2P systems. This paper discusses the design of
   reputation mechanisms and proposes a novel distributed reputation
   mechanisms to detect malicious or unreliable peers in P2P systems. It
   illustrates the process for rating gathering and aggregation and
   presents some experimental results to evaluate the proposed approach.
   Moreover, it considers how to effectively aggregate noisy (dishonest or
   inaccurate) ratings from independent or collusive peers using weighted
   majority techniques. Furthermore, it analyzes some possible attacks on
   reputation mechanisms and shows how to defend against such attacks.}},
