@article{ ISI:000323555100002,
Author = {Bauer, Frank and Jost, Juergen and Liu, Shiping},
Title = {{OLLIVIER-RICCI CURVATURE AND THE SPECTRUM OF THE NORMALIZED GRAPH
   LAPLACE OPERATOR}},
Journal = {{MATHEMATICAL RESEARCH LETTERS}},
Year = {{2012}},
Volume = {{19}},
Number = {{6}},
Pages = {{1185-1205}},
Abstract = {{We prove the following estimate for the spectrum of the normalized
   Laplace operator Delta on a finite graph G,
   1 - (1 - k{[}t])(1/t) <= lambda(1) <= ... <= lambda(N-1) <= 1 + (1 -
   k{[}t])(1/t), for all integers t >= 1.
   Here k{[}t] is a lower bound for the Ollivier-Ricci curvature on the
   neighborhood graph G{[}t], which was introduced by Bauer-Jost. In
   particular, when t = 1 this is Ollivier's estimates k <= lambda(1) <=
   ... <= lambda(N-1) <= 2 - k. For sufficiently large t, we show that,
   unless G is bipartite, our estimates for lambda(1) and lambda(N-1) are
   always nontrivial and improve Ollivier's estimates for all graphs with k
   <= 0. By definition neighborhood graphs are weighted graphs which may
   have loops. To understand the Ollivier-Ricci curvature on neighborhood
   graphs, we generalize a sharp estimate of the Ricci curvature given by
   Jost-Liu to weighted graphs with loops and relate it to the relative
   local frequency of triangles and loops.}},
