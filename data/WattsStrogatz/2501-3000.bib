@article{ ISI:000236467000024,
Author = {Paoletti, MS and Nugent, CR and Solomon, TH},
Title = {{Synchronization of oscillating reactions in an extended fluid system}},
Journal = {{PHYSICAL REVIEW LETTERS}},
Year = {{2006}},
Volume = {{96}},
Number = {{12}},
Month = {{MAR 31}},
Abstract = {{We present experiments on the synchronization of a dynamical, chemical
   process in an extended, flowing, fluid system. The oscillatory
   Belousov-Zhabotinsky chemical reaction is the process studied, and the
   flow is an annular chain of counterrotating vortices. Azimuthal motion
   of the vortices is controlled externally, enabling us to vary the type
   of transport. We find that oscillations of the Belousov-Zhabotinsky
   reaction synchronize throughout the extended fluid system only if
   transport in the flow is superdiffusive, with tracers in the flow
   undergoing rapid, distant jumps called Levy flights.}},
