@article{ ISI:000252286500006,
Author = {Jonckheere, Edmond and Lohsoonthorn, Poonsuk and Bonahon, Francis},
Title = {{Scaled Gromov hyperbolic graphs}},
Journal = {{JOURNAL OF GRAPH THEORY}},
Year = {{2008}},
Volume = {{57}},
Number = {{2}},
Pages = {{157-180}},
Month = {{FEB}},
Abstract = {{In this article, the delta-hyperbolic concept, originally developed for
   infinite graphs, is adapted to very large but finite graphs. Such graphs
   can indeed exhibit properties typical of negatively curved spaces, yet
   the traditional delta-hyperbolic concept, which requires existence of an
   upper bound on the fatness delta of the geodesic triangles, is unable to
   capture those properties, as any finite graph has finite delta. Here the
   idea is to scale delta relative to the diameter of the geodesic
   triangles and use the Cartan-Alexandrov-Toponogov (CAT) theory to derive
   the thresholding value of delta/diam below which the geometry has
   negative curvature properties. (C) 2007 Wiley Periodicals, Inc.}},
Publisher = {{JOHN WILEY \& SONS INC}},
