@article{ ISI:000364268700002,
Author = {Chen, Zhen and Du, Wen-Bo and Cao, Xian-Bin and Zhou, Xing-Lian},
Title = {{Cascading failure of interdependent networks with different coupling
   preference under targeted attack}},
Journal = {{CHAOS SOLITONS \& FRACTALS}},
Year = {{2015}},
Volume = {{80}},
Number = {{SI}},
Pages = {{7-12}},
Month = {{NOV}},
Abstract = {{Cascading failure is one of the most central topics in the field of
   complex networks. In this paper, the cascading failure model is extended
   to the case of interdependent networks, and the effect of coupling
   preference on systems robustness is intensively investigated. It is
   found that the performance of coupling preference on robustness is
   dependent on coupling probability. Disassortative coupling is more
   robust for sparse coupling while assortative coupling performs better
   for dense coupling. We provide an explanation for this constructive
   phenomenon via examining cascading process from the microscopic point of
   view. Our work can be useful to the design and optimization of
   interdependent networked systems. (C) 2015 Elsevier Ltd. All rights
   reserved.}},
