@article{ ISI:000305669300008,
Author = {Peraza, Luis R. and Asghar, Aziz U. R. and Green, Gary and Halliday,
   David M.},
Title = {{Volume conduction effects in brain network inference from
   electroencephalographic recordings using phase lag index}},
Journal = {{JOURNAL OF NEUROSCIENCE METHODS}},
