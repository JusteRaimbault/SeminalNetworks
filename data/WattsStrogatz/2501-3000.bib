@article{ ISI:000340341600005,
Author = {Smucny, Jason and Wylie, Korey P. and Tregellas, Jason R.},
Title = {{Functional magnetic resonance imaging of intrinsic brain networks for
   translational drug discovery}},
Journal = {{TRENDS IN PHARMACOLOGICAL SCIENCES}},
Year = {{2014}},
Volume = {{35}},
Number = {{8}},
Pages = {{397-403}},
Month = {{AUG}},
Abstract = {{Developing translational biomarkers is a priority for psychiatry
   research. Task-independent functional brain imaging is a relatively
   novel technique that allows examination of the brain's intrinsic
   networks, defined as functionally and (often) structurally connected
   populations of neurons whose properties reflect fundamental
   neurobiological organizational principles of the central nervous system.
   The ability to study the activity and organization of these networks has
   opened a promising new avenue for translational investigation, because
   they can be analogously examined across species and disease states.
   Interestingly, imaging studies have revealed shared spatial and
   functional characteristics of the intrinsic network architecture of the
   brain across species, including mice, rats, non-human primates, and
   humans. Using schizophrenia as an example, we show how intrinsic
   networks may show similar abnormalities in human diseases and animal
   models of these diseases, supporting their use as biomarkers in drug
   development.}},
