@article{ ISI:000300283100002,
Author = {de Weerdt, Mathijs M. and Zhang, Yingqian and Klos, Tomas},
Title = {{Multiagent task allocation in social networks}},
Journal = {{AUTONOMOUS AGENTS AND MULTI-AGENT SYSTEMS}},
