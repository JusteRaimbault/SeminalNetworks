@article{ ISI:000247859300014,
Author = {Lu, Jianquan and Cao, Jinde},
Title = {{Adaptive synchronization in tree-like dynamical networks}},
Journal = {{NONLINEAR ANALYSIS-REAL WORLD APPLICATIONS}},
Year = {{2007}},
Volume = {{8}},
Number = {{4}},
Pages = {{1252-1260}},
Month = {{SEP}},
Abstract = {{Synchronization in an array of coupled identical nonlinear dynamical
   systems have attracted increasing attention from various fields of
   science and engineering. In this paper, we investigate the
   synchronization phenomenon in tree-like dynamical networks. Based on the
   LaSalle invariant principle, a simple and systematic adaptive control
   scheme with variable coupling strength is proposed for the
   synchronization of tree-like dynamical networks without any knowledge of
   the concrete structure of isolate system. This result indicates that
   synchronization can be achieved for strong enough coupling if there
   exists a system (located at the root of the tree) which directly or
   indirectly influences all other systems. Furthermore, the main result is
   applied to several Lorenz chaotic systems coupled by a tree. And
   numerical simulations are also given to show the effectiveness of the
   proposed synchronization method. (c) 2006 Elsevier Ltd. All rights
   reserved.}},
