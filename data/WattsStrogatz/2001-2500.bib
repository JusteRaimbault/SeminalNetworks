@article{ ISI:000296473400012,
Author = {Abbasi, Alireza and Hossain, Liaquat and Uddin, Shahadat and Rasmussen,
   Kim J. R.},
Title = {{Evolutionary dynamics of scientific collaboration networks: multi-levels
   and cross-time analysis}},
Journal = {{SCIENTOMETRICS}},
