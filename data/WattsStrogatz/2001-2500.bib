@article{ ISI:000264840600004,
Author = {Xu, Shiyun and Yang, Ying},
Title = {{Synchronization for a class of complex dynamical networks with
   time-delay}},
Journal = {{COMMUNICATIONS IN NONLINEAR SCIENCE AND NUMERICAL SIMULATION}},
Year = {{2009}},
Volume = {{14}},
Number = {{8}},
Pages = {{3230-3238}},
Month = {{AUG}},
Abstract = {{In this study, the synchronization problem is addressed for a class of
   complex dynamical networks in which every identical node is a
   time-delayed Lur'e system. Delay-dependent and delay-independent
   synchronization criteria are established through a decoupling technique,
   which reduces a group of high-dimensional linear matrix inequalities
