@article{ ISI:000186571200098,
Author = {Baiesi, M and Manna, SS},
Title = {{Scale-free networks from a Hamiltonian dynamics}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2003}},
Volume = {{68}},
Number = {{4, 2}},
Month = {{OCT}},
Abstract = {{Contrary to many recent models of growing networks, we present a model
   with fixed number of nodes and links, where a dynamics favoring the
   formation of links between nodes with degree of connectivity as
   different as possible is introduced. By applying a local rewiring move,
   the network reaches equilibrium states assuming broad degree
   distributions, which have a power-law form in an intermediate range of
   the parameters used. Interestingly, in the same range we find nontrivial
   hierarchical clustering.}},
