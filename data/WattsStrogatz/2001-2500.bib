@article{ ISI:000221713300060,
Author = {Zhu, CP and Xiong, SJ and Tian, YJ and Li, N and Jiang, KS},
Title = {{Scaling of directed dynamical small-world networks with random responses}},
Journal = {{PHYSICAL REVIEW LETTERS}},
Year = {{2004}},
Volume = {{92}},
Number = {{21}},
Month = {{MAY 28}},
Abstract = {{A dynamical model of small-world networks, with directed links which
   describe various correlations in social and natural phenomena, is
   presented. Random responses of sites to the input message are introduced
   to simulate real systems. The interplay of these ingredients results in
   the collective dynamical evolution of a spinlike variable S(t) of the
   whole network. The global average spreading length <L>(s) and average
   spreading time <T>(s) are found to scale as p(-alpha)lnN with different
   exponents. Meanwhile, S(t) behaves in a duple scaling form for Nmuch
   greater thanN{*}: Ssimilar tof(p(-beta)q(gamma<)(t)over tilde>), where p
   and q are rewiring and external parameters, alpha, beta, and gamma are
   scaling exponents, and f((t) over tilde) is a universal function.
   Possible applications of the model are discussed.}},
