@article{ ISI:000232376400004,
Author = {Liu, JG and Wang, ZT and Dang, YZ},
Title = {{Optimization of robustness of scale-free network to random and targeted
   attacks}},
Journal = {{MODERN PHYSICS LETTERS B}},
Year = {{2005}},
Volume = {{19}},
Number = {{16}},
Pages = {{785-792}},
Month = {{JUL 20}},
Abstract = {{Scale-free networks, having connectivity distribution P(k) similar to
   k(-1) (where k is the site connectivity), are very resilient to random
   failures but are fragile to intentional attacks. The purpose of this
   paper is to find the network design guideline which can make the
   robustness of the network to both random failures and intentional
   attacks maximum while keeping the average connectivity {[}k] per node
   constant. We find that when {[}k] = 3 the robustness of the scale-free
   networks reach its maximum value if the minimal connectivity m = 1, but
   when {[}k] is larger than four, the networks will become more robust to
   random failures and targeted attacks as the minimal connectivity m gets
   larger.}},
