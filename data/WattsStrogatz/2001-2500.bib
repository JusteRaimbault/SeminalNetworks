@article{ ISI:000309229400002,
Author = {Cheng, Tao and Haworth, James and Wang, Jiaqiu},
Title = {{Spatio-temporal autocorrelation of road network data}},
Journal = {{JOURNAL OF GEOGRAPHICAL SYSTEMS}},
Year = {{2012}},
Volume = {{14}},
Number = {{4}},
Pages = {{389-413}},
Month = {{OCT}},
Abstract = {{Modelling autocorrelation structure among space-time observations is
   crucial in space-time modelling and forecasting. The aim of this
   research is to examine the spatio-temporal autocorrelation structure of
   road networks in order to determine likely requirements for building a
   suitable space-time forecasting model. Exploratory space-time
   autocorrelation analysis is carried out using journey time data
   collected on London's road network. Through the use of both global and
   local autocorrelation measures, the autocorrelation structure of the
   road network is found to be dynamic and heterogeneous in both space and
   time. It reveals that a global measure of autocorrelation is not
   sufficient to explain the network structure. Dynamic and local
   structures must be accounted for space-time modelling and forecasting.
   This has broad implications for space-time modelling and network
   complexity.}},
