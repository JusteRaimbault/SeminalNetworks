@inproceedings{ ISI:000255924000016,
Author = {Jamakovic, A. and Van Mieghem, P.},
Editor = {{Das, A and Pung, HK and Lee, FBS and Wong, LWC}},
Title = {{On the robustness of complex networks by using the algebraic
   connectivity}},
Booktitle = {{NETWORKING 2008: AD HOC AND SENSOR NETWORKS, WIRELESS NETWORKS, NEXT
   GENERATION INTERNET, PROCEEDINGS}},
