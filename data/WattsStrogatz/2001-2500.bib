@article{ ISI:000221447300007,
Author = {Caldarelli, G and Pastor-Satorras, R and Vespignani, A},
Title = {{Structure of cycles and local ordering in complex networks}},
Journal = {{EUROPEAN PHYSICAL JOURNAL B}},
Year = {{2004}},
Volume = {{38}},
Number = {{2}},
Pages = {{183-186}},
Month = {{MAR}},
Abstract = {{We study the properties of quantities aimed at the characterization of
   grid-like ordering in complex networks. These quantities are based on
   the global and local behavior of cycles of order four, which are the
   minimal structures able to identify rectangular clustering. The analysis
   of data from real networks reveals the ubiquitous presence of a
   statistically high level of grid-like ordering that is non-trivially
   correlated with the local degree properties. These observations provide
   new insights on the hierarchical structure of complex networks.}},
