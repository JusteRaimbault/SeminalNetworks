@article{ ISI:000297779500047,
Author = {Pu, Cun-Lai and Zhou, Si-Yuan and Wang, Kai and Zhang, Yi-Feng and Pei,
   Wen-Jiang},
Title = {{Efficient and robust routing on scale-free networks}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2012}},
Volume = {{391}},
Number = {{3}},
Pages = {{866-871}},
Month = {{FEB 1}},
Abstract = {{Information routing is one of the most important problems in large
   communication networks. In this paper we propose a novel routing
   strategy in which the optimal paths between all pairs of nodes are
   chosen according to a cost function that incorporates degrees of nodes
   in paths. Results on large scale-free networks demonstrate that our
   routing strategy is more efficient than the shortest path algorithm and
   the efficient routing strategy proposed by Van et al. {[}Phys. Rev. E
   73, 046108 (2006)]. Furthermore our routing strategy has strong
   robustness against cascading failure attacks on networks. (C) 2011
   Elsevier B.V. All rights reserved.}},
