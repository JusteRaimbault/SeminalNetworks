@article{ ISI:000289149100027,
Author = {Yu, Haitao and Wang, Jiang and Deng, Bin and Wei, Xile and Wong, Y. K.
   and Chan, W. L. and Tsang, K. M. and Yu, Ziqi},
Title = {{Chaotic phase synchronization in small-world networks of bursting
   neurons}},
Journal = {{CHAOS}},
Year = {{2011}},
Volume = {{21}},
Number = {{1}},
Month = {{MAR}},
Abstract = {{We investigate the chaotic phase synchronization in a system of coupled
   bursting neurons in small-world networks. A transition to mutual phase
   synchronization takes place on the bursting time scale of coupled
   oscillators, while on the spiking time scale, they behave
   asynchronously. It is shown that phase synchronization is largely
   facilitated by a large fraction of shortcuts, but saturates when it
   exceeds a critical value. We also study the external chaotic phase
   synchronization of bursting oscillators in the small-world network by a
   periodic driving signal applied to a single neuron. It is demonstrated
   that there exists an optimal small-world topology, resulting in the
   largest peak value of frequency locking interval in the parameter plane,
   where bursting synchronization is maintained, even with the external
   driving. The width of this interval increases with the driving
   amplitude, but decrease rapidly with the network size. We infer that the
   externally applied driving parameters outside the frequency locking
   region can effectively suppress pathologically synchronized rhythms of
   bursting neurons in the brain. (C) 2011 American Institute of Physics.
   {[}doi: 10.1063/1.3565027]}},
