@article{ ISI:000286851200011,
Author = {Yamauchi, Atsuo and Tanimoto, Jun and Hagishima, Aya},
Title = {{An analysis of network reciprocity in Prisoner's Dilemma games using
   Full Factorial Designs of Experiment}},
Journal = {{BIOSYSTEMS}},
