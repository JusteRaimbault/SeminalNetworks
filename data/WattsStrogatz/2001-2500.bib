@article{ ISI:000288163100009,
Author = {Hu, Yanqing and Wang, Yougui and Li, Daqing and Havlin, Shlomo and Di,
   Zengru},
Title = {{Possible Origin of Efficient Navigation in Small Worlds}},
Journal = {{PHYSICAL REVIEW LETTERS}},
Year = {{2011}},
Volume = {{106}},
Number = {{10}},
Month = {{MAR 9}},
Abstract = {{The small-world phenomenon is one of the most important properties found
   in social networks. It includes both short path lengths and efficient
   navigation between two individuals. It is found by Kleinberg that
   navigation is efficient only if the probability density distribution of
   an individual to have a friend at distance r scales as P(r) similar to
   r(-1). Although this spatial scaling is found in many empirical studies,
   the origin of how this scaling emerges is still missing. In this Letter,
   we propose the origin of this scaling law using the concept of entropy
   from statistical physics and show that this scaling is the result of
   optimization of collecting information in social networks.}},
