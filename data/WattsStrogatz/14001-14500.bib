@article{ ISI:000458813600004,
Author = {Papegnies, Etienne and Labatut, Vincent and Dufour, Richard and Linares,
   Georges},
Title = {{Conversational Networks for Automatic Online Moderation}},
Journal = {{IEEE TRANSACTIONS ON COMPUTATIONAL SOCIAL SYSTEMS}},
Year = {{2019}},
Volume = {{6}},
