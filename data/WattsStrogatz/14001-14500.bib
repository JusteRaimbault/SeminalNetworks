@article{ ISI:000469351500010,
Author = {Li, Dandan and Li, Ting and Niu, Yan and Xiang, Jie and Cao, Rui and
   Liu, Bo and Zhang, Hui and Wang, Bin},
Title = {{Reduced hemispheric asymmetry of brain anatomical networks in attention
   deficit hyperactivity disorder}},
Journal = {{BRAIN IMAGING AND BEHAVIOR}},
