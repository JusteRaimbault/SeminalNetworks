@article{ ISI:000457508900031,
Author = {Wu, Kai and Liu, Jing and Chen, Dan},
Title = {{Network reconstruction based on time series via memetic algorithm}},
Journal = {{KNOWLEDGE-BASED SYSTEMS}},
