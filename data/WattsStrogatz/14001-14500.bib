@article{ ISI:000472016600007,
Author = {Palagina, Ganna and Meyer, Jochen F. and Smirnakis, Stelios M.},
Title = {{Inhibitory Units: An Organizing Nidus for Feature-Selective SubNetworks
   in Area V1}},
Journal = {{JOURNAL OF NEUROSCIENCE}},
