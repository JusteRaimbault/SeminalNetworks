@article{ ISI:000474503900131,
Author = {Zhang, Gui-Qing and Baro, Jordi and Cheng, Fang-Yin and Huang, He and
   Wang, Lin},
Title = {{Avalanche dynamics of a generalized earthquake model}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2019}},
