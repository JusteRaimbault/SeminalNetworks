@article{ ISI:000471967500005,
Author = {Hackl, Jurgen and Adey, Bryan T.},
Title = {{Modelling multi-layer spatially embedded random networks}},
Journal = {{JOURNAL OF COMPLEX NETWORKS}},
Year = {{2019}},
Volume = {{7}},
Number = {{2}},
Pages = {{254-280}},
Month = {{APR}},
Abstract = {{Most real and engineered systems, including transportation
   infrastructure, are embedded in space and interact with one another in a
   variety of ways. To study such systems, a novel multi-layer spatially
   embedded random network model is proposed. In the development of this
   model, concepts from spatial statistics and graph theory are used to map
   complex systems with interdependent subsystems to a simplified and
   condensed mathematical representation. The developed model combines
   Markov marked point processes for vertex creation, which accounts for
   spatial distribution, layer assignment, and clustering effects of the
   vertices, and a hybrid connection model for the edge creation. To test
   the capabilities of, and gain insights with respect to, a real-world
   network, the model was used to model a complex infrastructure system,
