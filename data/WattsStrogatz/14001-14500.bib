@article{ ISI:000465247600001,
Author = {Chen, Yue and Niu, Xiaojian and Zhang, Yan},
Title = {{Exploring Contrarian Degree in the Trading Behavior of China's Stock
   Market}},
Journal = {{COMPLEXITY}},
Year = {{2019}},
Abstract = {{We study the contrarian and trend-following trading behavior of market
   timers in China's stock market. Using a network model to describe
   interpersonal relationships, we deploy the Ising model to capture
   trading strategies for both contrarians and followers. With empirical
   data of China's stock market, we find that contrarians account for
   12-14\% of trading volume. We further compare the performance of
   contrarians and followers and demonstrate the inefficiency of China's
   stock market where timing arbitrage exists. We highlight the fact that
   while the actual return sequence is driven by followers, the contrarians
   seize a lot of profitable arbitrage opportunities.}},
