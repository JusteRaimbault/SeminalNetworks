@article{ ISI:000475984700018,
Author = {Fengler, Enrico and Totz, Jan Frederik and Kaluza, Pablo and Engel,
   Harald},
Title = {{Directed adaptation of synchronization levels in oscillator communities}},
Journal = {{CHAOS}},
Year = {{2019}},
Volume = {{29}},
Number = {{6}},
Month = {{JUN}},
Abstract = {{We present an adaptive control scheme that realizes desired dynamics of
   an oscillator network with a given number of communities by adjusting
   the coupling weights between oscillators accordingly. The scheme allows,
   for example, to simultaneously establish different pregiven
   synchronization levels in the particular communities as well as phase
   relationships between them. We apply the method in numerical simulations
   with all-to-all and randomly coupled networks. Moreover, we provide an
   experimental proof of concept validating our numerical findings in a
   network of optically coupled photosensitive chemical micro-oscillators.
   Published under license by AIP Publishing.}},
