@article{ ISI:000170369300010,
Author = {Batista, AM and Viana, RL},
Title = {{Lyapunov exponents of a lattice of chaotic maps with a power-law
   coupling}},
Journal = {{PHYSICS LETTERS A}},
Year = {{2001}},
Volume = {{286}},
Number = {{2-3}},
Pages = {{134-140}},
Month = {{JUL 23}},
Abstract = {{We consider a lattice of coupled chaotic logistic maps in which the
   coupling strength between sites decreases with the lattice distance in a
   power-law fashion, making possible to pass continuously from a local to
   a global coupling. The corresponding Lyapunov spectra are described by
   means of the maximal exponent and the average of the positive exponents,
   which are analyzed in terms of the coupling properties. Our results are
   compared with spatio-temporal patterns known for global and local
   couplings. (C) 2001 Elsevier Science B.V. All rights reserved.}},
