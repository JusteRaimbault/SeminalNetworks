@article{ ISI:000253129700006,
Author = {Broer, Henk and Efstathiou, Konstantinos and Subramanian, Easwar},
Title = {{Robustness of unstable attractors in arbitrarily sized pulse-coupled
   networks with delay}},
Journal = {{NONLINEARITY}},
Year = {{2008}},
Volume = {{21}},
Number = {{1}},
Pages = {{13-49}},
Month = {{JAN}},
Abstract = {{We consider arbitrarily large networks of pulse-coupled oscillators with
   non-zero delay where the coupling is given by the Mirollo-Strogatz
   function. We prove that such systems have unstable attractors (saddle
   periodic orbits whose stable set has non-empty interior) in an open
   parameter region for three or more oscillators. The evolution operator
   of the system can be discontinuous and we propose an improved model with
   continuous evolution operator.}},
