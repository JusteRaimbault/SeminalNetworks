@article{ ISI:000223064800002,
Author = {Rosato, V and Tiriticco, F},
Title = {{Growth mechanisms of the AS-level Internet network}},
Journal = {{EUROPHYSICS LETTERS}},
Year = {{2004}},
Volume = {{66}},
Number = {{4}},
Pages = {{471-477}},
Month = {{MAY}},
Abstract = {{The growth of the AS (Autonomous System) level router network has been
   analyzed during a period of almost two years (1998-2000). The behavior
   of the clustering coefficient and that of the ``min-cut{''} analysis on
   the network have been used as key features to simulate the network
