@article{ ISI:000291118900002,
Author = {Eggemann, N. and Noble, S. D.},
Title = {{The clustering coefficient of a scale-free random graph}},
Journal = {{DISCRETE APPLIED MATHEMATICS}},
Year = {{2011}},
Volume = {{159}},
Number = {{10}},
Pages = {{953-965}},
Month = {{JUN 28}},
Abstract = {{We consider a random graph process in which, at each time step, a new
   vertex is added with m out-neighbours, chosen with probabilities
   proportional to their degree plus a strictly positive constant. We show
   that the expectation of the clustering coefficient of the graph process
   is asymptotically proportional to log n/n. Bollobas and Riordan have
   previously shown that when the constant is zero, the same expectation is
   asymptotically proportional to (log n)(2)/n. (C) 2011 Elsevier B.V. All
   rights reserved.}},
