@article{ ISI:000250822500005,
Author = {Darabos, Christian and Giacobini, Mario and Tomassini, Marco},
Title = {{Performance and robustness of cellular automata computation on irregular
   networks}},
Journal = {{ADVANCES IN COMPLEX SYSTEMS}},
Year = {{2007}},
Volume = {{10}},
Number = {{1, S}},
Pages = {{85-110}},
Month = {{AUG}},
Abstract = {{We investigate the performances of collective task-solving capabilities
   and the robustness of complex networks of automata using the density and
   synchronization problems as typical cases. We show by computer
   simulations that evolved Watts - Strogatz small-world networks have
   superior performance with respect to several kinds of scale- free
   graphs. In addition, we show that Watts - Strogatz networks are as
   robust in the face of random perturbations, both transient and
   permanent, as configuration scale- free networks, while being widely
   superior to Barab ` asi - Albert networks. This result differs from
   information diffusion on scale-free networks, where random faults are
   highly tolerated by similar topologies.}},
