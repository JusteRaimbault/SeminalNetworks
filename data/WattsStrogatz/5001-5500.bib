@article{ ISI:000247900800003,
Author = {Cai, Shi-Min and Yan, Gang and Zhou, Tao and Zhou, Pei-Ling and Fu,
   Zhong-Qian and Wang, Bing-Hong},
Title = {{Scaling behavior of an artificial traffic model on scale-free networks}},
Journal = {{PHYSICS LETTERS A}},
Year = {{2007}},
Volume = {{366}},
Number = {{1-2}},
Pages = {{14-19}},
Month = {{JUN 18}},
Abstract = {{In this Letter, we investigate an artificial traffic model on scale-free
   networks. Instead of using the routing strategy of the shortest path, a
   generalized routing algorithm is introduced to improve the
   transportation throughput, which is measured by the value of the
   critical point disjoining the free-flow phase and the congested phase.
   By using the detrended fluctuation analysis, we found that the traffic
   rate fluctuation near the critical point exhibits the 1/f-type scaling
   in the power spectrum. The simulation results agree very well with the
   empirical data, thus the present model may contribute to the
   understanding of the underlying mechanism of network traffics. (c) 2007
   Elsevier B.V. All rights reserved.}},
