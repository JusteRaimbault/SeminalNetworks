@article{ ISI:000357704500020,
Author = {Chen, Kun and Luo, Peng and Sun, Bianxia and Wang, Huaiqing},
Title = {{Which stocks are profitable? A network method to investigate the effects
   of network structure on stock returns}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2015}},
Volume = {{436}},
Pages = {{224-235}},
Month = {{OCT 15}},
Abstract = {{According to asset pricing theory, a stock's expected returns are
   determined by its exposure to systematic risk. In this paper, we propose
   a new method for analyzing the interaction effects among industries and
   stocks on stock returns. We construct a complex network based on
   correlations of abnormal stock returns and use centrality and
   modularity, two popular measures in social science, to determine the
   effect of interconnections on industry and stock returns. Supported by
   previous studies, our findings indicate that a relationship exists
   between inter-industry closeness and industry returns and between stock
   centrality and stock returns. The theoretical and practical
   contributions of these findings are discussed. (C) 2015 Elsevier B.V.
   All rights reserved.}},
