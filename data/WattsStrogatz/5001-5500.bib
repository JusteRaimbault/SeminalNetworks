@article{ ISI:000300144100001,
Author = {Gignoux, Jacques and Davies, Ian D. and Flint, Shayne R. and Zucker,
   Jean-Daniel},
Title = {{The Ecosystem in Practice: Interest and Problems of an Old Definition
   for Constructing Ecological Models}},
Journal = {{ECOSYSTEMS}},
