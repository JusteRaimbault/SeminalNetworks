@article{ ISI:000386193500005,
Author = {Xiao, Yu and Lo, Hong K.},
Title = {{Day-to-day departure time modeling under social network influence}},
Journal = {{TRANSPORTATION RESEARCH PART B-METHODOLOGICAL}},
Year = {{2016}},
Volume = {{92}},
Number = {{A, SI}},
Pages = {{54-72}},
Month = {{OCT}},
Note = {{Croucher-Foundation-Advanced-Study-Institute Conference on Toward a New
   Generation of Dynamic Network Models, Hong Kong, PEOPLES R CHINA, NOV
   18-20, 2014}},
Organization = {{Croucher Fdn Adv Study Inst}},
Abstract = {{With the prevalence of social media and location-aware mobile devices,
   travelers may make travel decisions not only by referring to their own
   experiences and conventional travel information, but also information
   shared on their social media. This study investigates the influence of
   this novel information on commuters' day-to-day departure time choices.
   We introduce a general framework for departure time choice with
   information sharing via social networks, which can be applied to any
   social network structure and is flexible for future extensions. The key
   in the framework, the learning process from friends' information in
   decision-making, is modeled based on the Bayesian learning theory. The
   properties of this learning model and the dynamics of the day-to-day
   departure time choice are analyzed. We further propose an agent-based
   approach to simulate travelers' choices. The parameters in the learning
   model are estimated based on an experimental data set. The agent-based
   approach is applied to validate the model and examine the effect of
   different social network structures, in terms of both travel choices and
   transportation system performance. (C) 2016 Elsevier Ltd. All rights
   reserved.}},
