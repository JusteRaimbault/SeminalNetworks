@inproceedings{ ISI:000465207100036,
Author = {Li, Zhaofeng and Jiang, Yichuan},
Book-Group-Author = {{Assoc Comp Machinery}},
Title = {{Cross-Layers Cascade in Multiplex Networks}},
Booktitle = {{AAMAS'14: PROCEEDINGS OF THE 2014 INTERNATIONAL CONFERENCE ON AUTONOMOUS
