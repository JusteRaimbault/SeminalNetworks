@article{ ISI:000312456300027,
Author = {Tang, Longkun and Lu, Jun-An and Lu, Jinhu and Yu, Xinghuo},
Title = {{BIFURCATION ANALYSIS OF SYNCHRONIZED REGIONS IN COMPLEX DYNAMICAL
   NETWORKS}},
