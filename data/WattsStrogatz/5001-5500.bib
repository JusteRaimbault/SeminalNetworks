@article{ ISI:000289301600013,
Author = {Fox, Andrew D. and Hescott, Benjamin J. and Blumer, Anselm C. and
   Slonim, Donna K.},
Title = {{Connectedness of PPI network neighborhoods identifies regulatory hub
   proteins}},
Journal = {{BIOINFORMATICS}},
Year = {{2011}},
Volume = {{27}},
Number = {{8}},
Pages = {{1135-1142}},
Month = {{APR 15}},
Abstract = {{Motivation: With the growing availability of high-throughput
   protein-protein interaction (PPI) data, it has become possible to
   consider how a protein's local or global network characteristics predict
   its function.
   Results: We introduce a graph-theoretic approach that identifies key
   regulatory proteins in an organism by analyzing proteins' local PPI
   network structure. We apply the method to the yeast genome and describe
   several properties of the resulting set of regulatory hubs. Finally, we
   demonstrate how the identified hubs and putative target gene sets can be
   used to identify causative, functional regulators of differential gene
   expression linked to human disease.}},
