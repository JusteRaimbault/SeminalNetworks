@inproceedings{ ISI:000282834900004,
Author = {Habiba and Yu, Yintao and Berger-Wolf, Tanya Y. and Saia, Jared},
Editor = {{Giles, L and Smith, M and Yen, J and Zhang, HZ}},
Title = {{Finding Spread Blockers in Dynamic Networks}},
