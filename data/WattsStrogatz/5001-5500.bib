@article{ ISI:000175222600008,
Author = {Nelson, DL and McEvoy, CL},
Title = {{How can the same type of prior knowledge both help and hinder recall?}},
Journal = {{JOURNAL OF MEMORY AND LANGUAGE}},
Year = {{2002}},
Volume = {{46}},
Number = {{3}},
Pages = {{652-663}},
Month = {{APR}},
Abstract = {{When two pieces of information share associative features, models assume
   that them will facilitate recall when one piece cues the recall of the
   other (e.g., PIER 2. REM). This article. however. presents results
   showing that shared features processed under the same encoding and
   retrieval conditions can hake opposing effects. Studied words had either
   exceptionally strong primary associates (SALT pepper) or relatively weak
   primaries (RAZOR sharp). Recall was cued with words that had preexisting
   links with their targets (SPICE or BLADE). and that were either linked
   to the primary associates or not. Such links produce shared associates
   that facilitated recall when weakly activated by their targets and that
   disrupted it when strongly activated. A second experiment showed that
   studying both the targets and their primary associates reduced the
   facilitation effect but had no influence on the magnitude of the
   interference effect. Shared information that is strongly attached to the
   studied target apparently becomes attached to the context regardless of
   whether it is directly studied or only implicitly activated, and this
   attachment produces competition at recall. In contrast, information that
   is weakly and only implicitly primed facilitated, recall. These results
   are consistent with findings on false memories and can he explained by
   some models, but not without additional assumptions. (C) 2002 Elsevier
   Science (USA).}},
