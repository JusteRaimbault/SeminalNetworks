@article{ ISI:000365190000023,
Author = {Sun, Ao and Lu, Ling and Li, Chengren},
Title = {{Synchronization of an uncertain small-world neuronal network based on
   modified sliding mode control technique}},
Journal = {{NONLINEAR DYNAMICS}},
Year = {{2015}},
Volume = {{82}},
Number = {{4}},
Pages = {{1905-1912}},
Month = {{DEC}},
Abstract = {{In this paper, we propose a novel sliding mode control technique for
   synchronization transmission of information in a small-world neuronal
   network with uncertainty. First, we introduce the structure of
   small-world neuronal network. Then, we modified the sliding mode control
   technique to make it suitable for network synchronization. The control
   input of the small-world neuronal network and the adaptive law of the
   uncertain configuration coefficient can be effectively determined by
   using this modified technique. Finally, we compare our analytic results
   with numerical simulations through a few examples.}},
