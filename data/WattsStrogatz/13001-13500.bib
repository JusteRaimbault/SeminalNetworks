Language = {{English}},
Affiliation = {{Salimi, S (Reprint Author), Univ Kurdistan, Dept Phys, Fac Sci, Pasdaran Ave, Sanandaj, Iran.
   Radgohar, R.; Salimi, S., Univ Kurdistan, Dept Phys, Fac Sci, Sanandaj, Iran.}},
DOI = {{10.1007/s11128-012-0377-8}},
ISSN = {{1570-0755}},
