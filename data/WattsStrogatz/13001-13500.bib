@inproceedings{ ISI:000306943900019,
Author = {Chojnacki, Szymon and Klopotek, Mieczyslaw A.},
Editor = {{Bouvry, P and Klopotek, MA and Leprevost, F and Marciniak, M and Mykowiecka, A and Rybinski, H}},
