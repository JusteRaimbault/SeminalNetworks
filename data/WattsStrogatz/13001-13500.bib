@article{ ISI:000309089100001,
Author = {Qiu Shen-Wei and Wang Kai and Liu Qian and Pei Wen-Jiang and Hu Hen-Kai
   and Yang Guang and Wei Cheng-Jian and Zhang Yi-Feng},
Title = {{Epidemic spreading on scale-free networks with traffic flow}},
Journal = {{ACTA PHYSICA SINICA}},
Year = {{2012}},
Volume = {{61}},
Number = {{15}},
Month = {{AUG}},
Abstract = {{The infectivity of a node is determined by its actual betweenness b(k)
   in the epidemic model based on traffic-flow other than degree k which is
   different from the classical epidemic models. Utilizing the mean-field
   theory, we propose a modified SIS epidemic model based on traffic-flow.
   With this model, taking MIP route as an example, we re-study the
   relationship among spreading probability beta, traffic generation rate
   lambda, epidemic threshold beta(c), the stationary density of infected
   nodes rho. Both theoretical analysis and experimental results show that
   beta(c) is the ratio of the mean of the actual betweenness < b(k)> to
   its mean square < b(k)(2)>, when network topology and route strategy are
   given. Moreover, the stationary density of infected notes rho behaves as
   power-law exponent with the reciprocal of the product of the spreading
   probability beta, the traffic generation rate lambda and the mean of the
   actual betweenness (b(lambda=1)).}},
