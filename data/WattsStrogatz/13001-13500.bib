@inproceedings{ ISI:000345856900092,
Author = {Alam, Maksudul and Khan, Maleq and Marathe, Madhav V.},
Book-Group-Author = {{IEEE}},
Title = {{Distributed-Memory Parallel Algorithms for Generating Massive Scale-free
   Networks Using Preferential Attachment Model}},
Booktitle = {{2013 INTERNATIONAL CONFERENCE FOR HIGH PERFORMANCE COMPUTING,
   NETWORKING, STORAGE AND ANALYSIS (SC)}},
