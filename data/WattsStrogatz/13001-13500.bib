@article{ ISI:000317246200060,
Author = {Wang, Lin and Xu, Hui},
Title = {{Study on Victims' Post-disaster Psychological Spaces Tearing in
   Different Stages and Anti-tearing Planning-From Wenchuan Earthquake Case}},
Journal = {{DISASTER ADVANCES}},
Year = {{2013}},
Volume = {{6}},
Number = {{1}},
Pages = {{469-486}},
Month = {{MAR}},
Abstract = {{Wenchuan Earthquake brought severe trauma to the victims' psychological
   spaces. In this paper, people's psychological spaces are constructed as
   relationship circles that people's small world relationship network maps
   on the mentality. Based on the current situation of post-disaster
   resettlement measures, this paper focuses on the analysis of repeated
   tearing and reconstruction of victims' post-disaster psychological
   spaces in different stages. It also points out the extreme threat of the
   ``repeated tearing{''} phenomenon imposing on the victims' healthy
   psychological spaces reconstruction. On that basis, corresponding
   anti-tearing plans on the victims' psychological spaces are further
   studied from four dimensionalities to reduce secondary psychological
   damage, namely, the ``escaping-disaster disorder{''} response,
   ``migration disarray{''} reaction, public activities implementation,
   post-disaster psychological intervention.}},
