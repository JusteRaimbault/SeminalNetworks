@inproceedings{ ISI:000335317000011,
Author = {Brahmbhatt, Jigar and Menezes, Ronaldo},
Book-Group-Author = {{IEEE}},
Title = {{On the Relation Between Tourism and Trade: A Network Experiment}},
Booktitle = {{PROCEEDINGS OF THE 2013 IEEE 2ND INTERNATIONAL NETWORK SCIENCE WORKSHOP
   (NSW)}},
