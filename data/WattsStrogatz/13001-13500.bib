@article{ ISI:000311844100002,
Author = {Wang, Daoguang and Kang, Xiaosha and Lu, Huaping},
Title = {{THE INTERPLAY OF UNCORRELATED NOISE AND SMALL WORLD EFFECT ON PATTERN
   FORMATION IN TWO-DIMENSIONAL COUPLED MAP LATTICES}},
Journal = {{INTERNATIONAL JOURNAL OF MODERN PHYSICS B}},
Year = {{2012}},
Volume = {{26}},
Number = {{31}},
Month = {{DEC 20}},
Abstract = {{By using a neuron-like map model to denote the generic dynamics of
   excitable systems, Gaussian-noise-induced pattern formation in the
   two-dimensional coupled map lattices with nearest-neighbor coupling and
   shortcut links has been studied. Given the appropriate initial values
   and parameter regions, with all nodes concerned, the functions of
   delta(n), chi and R are introduced to analyze the evolution of pattern
   formation. It is found that there exists a critical epsilon(c) beyond
   which the stable rotating spiral wave will appear. After introducing the
   Gaussian noise for the homogeneous epsilon region, different
   spatiotemporal stable patterns will be achieved. Additionally, the
   importance of the parameter I on the coupling strength C is discussed.}},
