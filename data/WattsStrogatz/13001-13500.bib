@article{ ISI:000365405600015,
Author = {Duardo-Sanchez, Aliuska and Gonzalez-Diaz, Humberto and Pazos, Alejandro},
Title = {{MI-NODES Multiscale Models of Metabolic Reactions, Brain Connectome,
   Ecological, Epidemic, World Trade, and Legal-Social Networks}},
Journal = {{CURRENT BIOINFORMATICS}},
