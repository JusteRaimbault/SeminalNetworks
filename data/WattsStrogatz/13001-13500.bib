@inproceedings{ ISI:000330563800046,
Author = {Green, Oded and Bader, David A.},
Book-Group-Author = {{IEEE Comp Soc
   Acad Sci \& Engn}},
Title = {{Faster Clustering Coefficient Using Vertex Covers}},
