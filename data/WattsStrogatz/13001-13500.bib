@inproceedings{ ISI:000395422500090,
Author = {Yu, Yang and Chen, Yuzhong},
Editor = {{Cheng, D}},
Title = {{A Novel Content Based and Social Network Aided Online Spam Short Message
   Filter}},
Booktitle = {{PROCEEDINGS OF THE 10TH WORLD CONGRESS ON INTELLIGENT CONTROL AND
   AUTOMATION (WCICA 2012)}},
Year = {{2012}},
Pages = {{444-449}},
Note = {{10th World Congress on Intelligent Control and Automation (WCICA),
   Beijing, PEOPLES R CHINA, JUL 06-08, 2012}},
Organization = {{Chinese Acad Sci, Acad Math \& Syst Sci; IEEE Robot \& Automat Soc; IEEE
   Control Syst Soc; Natl Nat Sci Fdn China; Chinese Assoc Automat; Chinese
   Assoc Artificial Intelligence; IEEE RACS Hong Kong Chapter; IEEE Control
   Syst Soc Beijing Chapter; IEEE Control Syst Soc Singapore Chapter}},
Abstract = {{With the rapid development of mobile SMS (short message service), spam
   messages have grown explosively which trouble our daily lives seriously
   and lead to the loss of telecom operators. In this paper, an online spam
   filter based on the analysis of two criteria of content representations
   and relationship between the senders and receivers in social network is
   proposed. A Naive Bayesian classifier is used to build up the filter
   including both the content features and social network features. We use
   the data provided by a partner telecom operator to do the experiments.
   The results show that our model is effective and satisfies all the
   requirements of our partner and will be deployed recently.}},
Publisher = {{IEEE}},
