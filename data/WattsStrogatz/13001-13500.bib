@inproceedings{ ISI:000370073801098,
Author = {Charalampidis, Alexandros C.},
Book-Group-Author = {{IEEE}},
Title = {{Graph Topology Optimization for Fast Consensus Based on the Influence of
   New Links to a Quadratic Criterion}},
Booktitle = {{2014 IEEE 53RD ANNUAL CONFERENCE ON DECISION AND CONTROL (CDC)}},
Series = {{IEEE Conference on Decision and Control}},
Year = {{2014}},
Pages = {{1452-1457}},
