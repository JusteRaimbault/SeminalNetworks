@article{ ISI:000319436700005,
Author = {Zhang Xin-Wang and Jin Ning-De and Gao Zhong-Ke and Zhai Lu-Sheng},
Title = {{Local Property of Recurrence Network for Investigating Gas-Liquid
   Two-Phase Flow Characteristics}},
Journal = {{CHINESE PHYSICS LETTERS}},
Year = {{2013}},
Volume = {{30}},
Number = {{5}},
Month = {{MAY}},
Abstract = {{Understanding the dynamics of gas-liquid two-phase flows is a challenge
   in the fields of nonlinear dynamics. We first construct and analyze a
   recurrence network from Chen's chaotic system and find that the network
   local statistic is feasible to characterize chaotic dynamics associated
   with unstable periodic orbits. Then we construct recurrence networks
   from gas-liquid two-phase flow experimental signals and associate the
   network topological statistic with the flow pattern dynamics. The
   results indicate that the recurrence network could be a powerful tool
   for the dynamic characterization of experimental gas-liquid two-phase
   flows.}},
