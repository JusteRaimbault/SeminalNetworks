@article{ ISI:000321552000027,
Author = {Wu Jian-She and Li Li-Guang and Wang Xiao-Hua and Yu Xin and Jiao
   Li-Cheng},
Title = {{Network evolution driven by dynamics applied to graph coloring}},
Journal = {{CHINESE PHYSICS B}},
Year = {{2013}},
Volume = {{22}},
Number = {{6}},
Month = {{JUN}},
Abstract = {{An evolutionary network driven by dynamics is studied and applied to the
   graph coloring problem. From an initial structure, both the topology and
   the coupling weights evolve according to the dynamics. On the other
   hand, the dynamics of the network are determined by the topology and the
   coupling weights, so an interesting structure-dynamics co-evolutionary
   scheme appears. By providing two evolutionary strategies, a network
   described by the complement of a graph will evolve into several clusters
   of nodes according to their dynamics. The nodes in each cluster can be
   assigned the same color and nodes in different clusters assigned
   different colors. In this way, a co-evolution phenomenon is applied to
   the graph coloring problem. The proposed scheme is tested on several
   benchmark graphs for graph coloring.}},
