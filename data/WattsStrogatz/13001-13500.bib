@inproceedings{ ISI:000345122100009,
Author = {D'Agostino, Gregorio},
Editor = {{Matrasulov, D and Stanley, HE}},
Title = {{A Spectral Approach to Synchronizability of Interdependent Networks}},
Booktitle = {{NONLINEAR PHENOMENA IN COMPLEX SYSTEMS: FROM NANO TO MACRO SCALE}},
