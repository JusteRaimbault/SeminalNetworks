@inproceedings{ ISI:000306303100003,
Author = {Demertzis, Foivos F. and Xenakis, Christos},
Editor = {{Camenisch, J and Lambrinoudakis, C}},
Title = {{SOMA: Self-Organised Mesh Authentication}},
