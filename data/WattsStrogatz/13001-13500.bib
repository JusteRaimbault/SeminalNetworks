@inproceedings{ ISI:000312969600056,
Author = {Moewes, Christian and Kruse, Rudolf and Sabel, Bernhard A.},
Editor = {{Kruse, R and Berthold, MR and Moewes, C and Gil, MA and Grzegorzewski, P and Hryniewicz, O}},
Title = {{Analysis of Dynamic Brain Networks Using VAR Models}},
Booktitle = {{SYNERGIES OF SOFT COMPUTING AND STATISTICS FOR INTELLIGENT DATA ANALYSIS}},
