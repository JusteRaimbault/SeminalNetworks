@inproceedings{ ISI:000312859301121,
Author = {Greenwood, Garrison W. and Ashlock, Daniel},
Book-Group-Author = {{IEEE}},
Title = {{Evolutionary Games and the Study of Cooperation: Why Has So Little
   Progress Been Made?}},
Booktitle = {{2012 IEEE CONGRESS ON EVOLUTIONARY COMPUTATION (CEC)}},
Series = {{IEEE Congress on Evolutionary Computation}},
Year = {{2012}},
Note = {{IEEE Congress on Evolutionary Computation (CEC), Brisbane, AUSTRALIA,
   JUN 10-15, 2012}},
Organization = {{IEEE}},
Abstract = {{Natural selection acts as a culling force that favors highly fit
   organisms. There are numerous examples ranging from bacteria through
   humans showing fitness can sometimes improve if individuals cooperate.
   Yet despite the widespread evidence of cooperation throughout nature, we
   still do not fully understand how it evolves. Over the past 10-15 years
   a large number of theoretical (computer) models have been created,
   analyzed and published to try and get some answers. Unfortunately,
   little progress has been made despite all of this time and effort. In
   this paper we review what has been done and explain why the current
   directions of research in modeling cooperation in populations is
   unlikely to provide any insight. We make recommendations on the proper
   path for future research efforts.}},
