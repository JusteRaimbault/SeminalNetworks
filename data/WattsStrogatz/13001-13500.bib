@inproceedings{ ISI:000320654000019,
Author = {Fotouhi, Babak and Rabbat, Michael G.},
Book-Group-Author = {{IEEE}},
Title = {{Migration in a Small World: A Network Approach to Modeling Immigration
   Processes}},
Booktitle = {{2012 50TH ANNUAL ALLERTON CONFERENCE ON COMMUNICATION, CONTROL, AND
   COMPUTING (ALLERTON)}},
Series = {{Annual Allerton Conference on Communication Control and Computing}},
Year = {{2012}},
