@inproceedings{ ISI:000382032100172,
Author = {Shahrivar, Ebrahim Moradi and Sundaram, Shreyas},
Book-Group-Author = {{IEEE}},
Title = {{Multi-Layer Network Formation via a Colonel Blotto Game}},
