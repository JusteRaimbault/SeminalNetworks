@article{ ISI:000302877000065,
Author = {Zhao Qing-Bai and Zhang Xiao-Fei and Sui Dan-Ni and Zhou Zhi-Jin and
   Chen Qi-Cai and Tang Yi-Yuan},
Title = {{The Efficiency of a Small-World Functional Brain Network}},
Journal = {{CHINESE PHYSICS LETTERS}},
Year = {{2012}},
Volume = {{29}},
Number = {{4}},
Month = {{APR}},
Abstract = {{We investigate whether the small-world topology of a functional brain
   network means high information processing efficiency by calculating the
   correlation between the small-world measures of a functional brain
   network and behavioral reaction during an imagery task. Functional brain
   networks are constructed by multichannel event-related potential data,
   in which the electrodes are the nodes and the functional connectivities
   between them are the edges. The results show that the correlation
   between small-world measures and reaction time is task-specific, such
   that in global imagery, there is a positive correlation between the
   clustering coefficient and reaction time, while in local imagery the
   average path length is positively correlated with the reaction time.
   This suggests that the efficiency of a functional brain network is
   task-dependent.}},
