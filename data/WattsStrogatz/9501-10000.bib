@article{ ISI:000185204500013,
Author = {Sen, P},
Title = {{Phase transitions in Euclidean networks: A mini-review}},
Journal = {{PHYSICA SCRIPTA}},
Year = {{2003}},
Volume = {{T106}},
Pages = {{55-58}},
Note = {{Conference on Physics of Random Networks, Econophysics and Models of
   Biophysics and Sociophysics, SAHA INST NUCL PHYS, CALCUTTA, INDIA, MAR
   20-22, 2003}},
Abstract = {{In networks defined in Euclidean spaces, the probability of connection
   between two nodes depends on the distance between them. Various kinds of
   phase transitions occur in these networks when the distance dependent
   factor is governed by suitable tunable parameters. In this short review,
   we discuss the present status of the studies of the phase transitions in
