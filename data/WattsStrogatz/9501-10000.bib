@article{ ISI:000412544500001,
Author = {van der Hofstad, Remco and Komjathy, Julia},
Title = {{When is a Scale-Free Graph Ultra-Small?}},
Journal = {{JOURNAL OF STATISTICAL PHYSICS}},
