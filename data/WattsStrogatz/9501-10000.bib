@inproceedings{ ISI:000246237400015,
Author = {Lubovac, Zelmina and Corne, David and Gamalielsson, Jonas and Olsson,
   Bjorn},
Editor = {{Hochreiter, S and Wagner, R}},
Title = {{Weighted cohesiveness for identification of functional modules and their
   interconnectivity}},
