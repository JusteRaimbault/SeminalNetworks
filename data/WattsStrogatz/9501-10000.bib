@inproceedings{ ISI:000258929300012,
Author = {Pandit, Vinayaka and Modani, Natwar and Mukherjea, Sougata and Nanavati,
   Amit A. and Roy, Sambuddha and Agarwal, Amit},
Book-Group-Author = {{IEEE}},
Title = {{Extracting dense communities from telecom call graphs}},
