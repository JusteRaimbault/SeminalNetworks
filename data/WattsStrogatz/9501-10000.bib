@article{ ISI:000260270100005,
Author = {Liu, Jian-Guo and Zhou, Tao and Guo, Qiang and Wang, Bing-Hong},
Title = {{STRUCTURAL EFFECTS ON SYNCHRONIZABILITY OF SCALE-FREE NETWORKS}},
Journal = {{INTERNATIONAL JOURNAL OF MODERN PHYSICS C}},
Year = {{2008}},
Volume = {{19}},
Number = {{9}},
Pages = {{1359-1366}},
Month = {{SEP}},
Abstract = {{In this paper, we numerically investigate the structural characteristics
   that affect the synchronizability of coupled identical oscillators on
   scale-free networks. By using the edge-exchange method, we can change
   the network structure with degree sequence fixed. An optimal algorithm,
   namely Tabu Search, is applied, respectively, to enhance and weaken the
   synchronizability. The numerical results indicate that the
   synchronizability is most sensitive to the maximal betweenness. By
   tracking the optimization process, most previous works suggest that the
   synchonizability can be enhanced by reducing assortative coefficient and
   clustering coefficient; however, interestingly, our numerical
   simulations show that the depression of synchronizability also
   corresponds to the reduction process of those two coefficients.}},
