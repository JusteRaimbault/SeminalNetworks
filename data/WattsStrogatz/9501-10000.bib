@inproceedings{ ISI:000274496200120,
Author = {Beichl, Isabel and Cloteaux, Brian},
Book-Group-Author = {{IEEE}},
Title = {{MEASURING THE EFFECTIVENESS OF THE S-METRIC TO PRODUCE BETTER NETWORK
   MODELS}},
Booktitle = {{2008 WINTER SIMULATION CONFERENCE, VOLS 1-5}},
