@article{ ISI:000445357100010,
Author = {Siami, Milad and Bolouki, Sadegh and Bamieh, Bassam and Motee, Nader},
Title = {{Centrality Measures in Linear Consensus Networks With Structured Network
   Uncertainties}},
Journal = {{IEEE TRANSACTIONS ON CONTROL OF NETWORK SYSTEMS}},
