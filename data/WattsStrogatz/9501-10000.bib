@article{ ISI:000256262700035,
Author = {Zhou Xiao-Rong and Luo Xiao-Shu},
Title = {{Coherence resonance in neural networks with small-world connections}},
Journal = {{ACTA PHYSICA SINICA}},
Year = {{2008}},
Volume = {{57}},
Number = {{5}},
Pages = {{2849-2853}},
Month = {{MAY}},
Abstract = {{The nonlinear response of small-world biological nerve network in the
   Hodgkin-Huxley model in the absence of external periodic signal is
   studied. Simulation results show the ordered degree of spike sequence
   may reach a maximum when the strength of noise is a certain limited
   value. Namely, coherence resonance phenomenon is produced in small-world
   biology nerve network. The number of minima of coherence resonance
