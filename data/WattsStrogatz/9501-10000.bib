@article{ ISI:000280414200018,
Author = {Wang, Li and Zhu, Chen-ping and Gu, Zhi-ming and Li, Xi-tao},
Title = {{Modeling mobile ad hoc communication networks on two-dimensional square
   lattice}},
Journal = {{FRONTIERS OF PHYSICS IN CHINA}},
Year = {{2009}},
Volume = {{4}},
Number = {{4}},
Pages = {{556-560}},
Month = {{DEC}},
Abstract = {{In this paper, we model the mobile ad hoc communication network on a
   two-dimensional square lattice. Both structure and function of it depend
   on transmission range and site-occupancy of nodes. Critical occupancies
   sigma(c) for different transmission ranges r to maintain global
   connection are found. Universal scaling function behaves as eta similar
   to f(R-beta sigma), where R = (r - r(0))/r(0), and the scaling exponent
   beta = -0.61, which distinguishes itself from percolation in previous
   lattice or network models. When the occupancy sigma is near the
   threshold sigma(c), individual nodes self-organize into a dynamic small
   world network relative to geometric distance. The network has a cut-off
   degree below which clustering coefficient keeps constant, which
   distinguish itself from other systems and has its potential application
   in technical designs.}},
