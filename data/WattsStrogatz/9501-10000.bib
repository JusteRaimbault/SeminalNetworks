@article{ ISI:000446615600025,
Author = {Sizemore, Ann E. and Karuza, Elisabeth A. and Giusti, Chad and Bassett,
   Danielle S.},
Title = {{Knowledge gaps in the early growth of semantic feature networks}},
Journal = {{NATURE HUMAN BEHAVIOUR}},
