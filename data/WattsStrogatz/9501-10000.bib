Book-Author = {{Yang, JY}},
Title = {{Optimizing flow-based modularization by Iterative Centroid Search in
   protein interaction networks}},
Booktitle = {{PROCEEDINGS OF THE 7TH IEEE INTERNATIONAL SYMPOSIUM ON BIOINFORMATICS
   AND BIOENGINEERING, VOLS I AND II}},
Year = {{2007}},
Pages = {{342-349}},
