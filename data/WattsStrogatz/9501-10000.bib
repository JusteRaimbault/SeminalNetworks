@inproceedings{ ISI:000248919600004,
Author = {Skorin-Kapov, N. and Tonguz, O. and Puech, N.},
Editor = {{Car, Z and Kusek, M}},
Title = {{Self-organization in transparent optical networks: A new approach to
   security}},
