@article{ ISI:000295994800002,
Author = {Pan, Zhenxing and Yamaguchi, Rei and Doi, Shinji},
Title = {{Bifurcation analysis and effects of changing ionic conductances on
   pacemaker rhythm in a sinoatrial node cell model}},
Journal = {{BIOSYSTEMS}},
Year = {{2011}},
Volume = {{106}},
Number = {{1}},
Pages = {{9-18}},
Month = {{OCT}},
Abstract = {{The electrical excitation (action potential generation) of sinoatrial
   node (cardiac pacemaker) cells is directly related to various ion
   channels (pore-forming proteins) in cell membranes. In order to analyze
   the relation between action potential generation and ion channels, we
   use the Yanagihara-Noma-lrisawa (YNI) model of sinoatrial node cells,
   which is described by the Hodgkin-Huxley-type equations with seven
   variables. In this paper, we analyze the global bifurcation structure of
   the YNI model by varying various conductances of ion channels, and
   examine the effects of these conductance changes on pacemaker rhythm
   (frequency of action potential generation). The coupling effect on
   pacemaker rhythm is also examined approximately by applying external
   current to the YNI model. (C) 2011 Elsevier Ireland Ltd. All rights
   reserved.}},
