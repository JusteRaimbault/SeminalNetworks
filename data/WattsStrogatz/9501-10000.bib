@article{ ISI:000291243400090,
Author = {Sun Wei-Gang and Cao Jian-Ting and Wang Ru-Bin},
Title = {{Approach of Complex Networks for the Determination of Brain Death}},
Journal = {{CHINESE PHYSICS LETTERS}},
Year = {{2011}},
Volume = {{28}},
Number = {{6}},
Month = {{JUN}},
Abstract = {{In clinical practice, brain death is the irreversible end of all brain
   activity. Compared to current statistical methods for the determination
   of brain death, we focus on the approach of complex networks for
   real-world electroencephalography in its determination. Brain functional
   networks constructed by correlation analysis are derived, and
   statistical network quantities used for distinguishing the patients in
   coma or brain death state, such as average strength, clustering
   coefficient and average path length, are calculated. Numerical results
   show that the values of network quantities of patients in coma state are
   larger than those of patients in brain death state. Our findings might
   provide valuable insights on the determination of brain death.}},
