@article{ ISI:000444495100008,
Author = {Lutz, Christoph and Hoffmann, Christian Pieter},
Title = {{Making Academic Social Capital Visible: Relating SNS-Based, Alternative
   and Traditional Metrics of Scientific Impact}},
Journal = {{SOCIAL SCIENCE COMPUTER REVIEW}},
Year = {{2018}},
Volume = {{36}},
Number = {{5}},
Pages = {{632-643}},
Month = {{OCT}},
Abstract = {{The reliable assessment of individual faculty members' contributions is
   a key challenge in the governance of research institutions.
   Traditionally, scientific impact is estimated based on bibliographic
   analyses. With online platforms, particularly social media, gaining
   popularity among academics, new opportunities for the analysis of
   scientific impact arise. Proponents of the altmetrics approach hold that
   both general purpose social media and services tailored to the
   scientific community allow for a range of usage metrics that may inform
   scientific impact assessment. We propose that relational analyses of
   social media platforms may shed new light on these understudied
   dimensions of scientific impact and may enrich assessment efforts. Based
   on a sample of Swiss management scholars' active on ResearchGate, we
   conduct a social network analysis, derive relational metrics, and
   correlate these metrics with bibliometrics, webometrics, and altmetrics
   to gauge their potential to inform scientific impact assessment,
   specifically in business and management research.}},
