@article{ ISI:000463069900035,
Author = {Bentsen, Gregory and Gu, Yingfei and Lucas, Andrew},
Title = {{Fast scrambling on sparse graphs}},
Journal = {{PROCEEDINGS OF THE NATIONAL ACADEMY OF SCIENCES OF THE UNITED STATES OF
   AMERICA}},
