@inproceedings{ ISI:000221714200042,
Author = {Kanda, T and Ishiguro, H},
Editor = {{Orchard, B and Yang, C and Ali, M}},
Title = {{Reading human relationships from their interaction with an interactive
   humanoid robot}},
Booktitle = {{INNOVATIONS IN APPLIED ARTIFICIAL INTELLIGENCE}},
