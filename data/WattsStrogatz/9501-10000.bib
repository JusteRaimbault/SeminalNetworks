@article{ ISI:000407847800023,
Author = {Reimann, Michael W. and Horlemann, Anna-Lena and Ramaswamy, Srikanth and
   Muller, Eilif B. and Markram, Henry},
Title = {{Morphological Diversity Strongly Constrains Synaptic Connectivity and
   Plasticity}},
Journal = {{CEREBRAL CORTEX}},
Year = {{2017}},
Volume = {{27}},
