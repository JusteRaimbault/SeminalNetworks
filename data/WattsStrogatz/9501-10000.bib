@inproceedings{ ISI:000263406500099,
Author = {Ashlock, Daniel A. and Jafargholi, Fatemeh},
Book-Group-Author = {{IEEE}},
Title = {{Behavioral Regimes in the Evolution of Extremal Epidemic Graphs}},
Booktitle = {{2008 IEEE CONGRESS ON EVOLUTIONARY COMPUTATION, VOLS 1-8}},
