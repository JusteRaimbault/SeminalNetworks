@article{ ISI:000246813900011,
Author = {Li, Menghui and Fan, Ying and Wang, Dahui and Li, Daqing and Wu, Jinshan
   and Di, Zengru},
Title = {{Small-world effect induced by weight randomization on regular networks}},
Journal = {{PHYSICS LETTERS A}},
Year = {{2007}},
Volume = {{364}},
Number = {{6}},
Pages = {{488-493}},
Month = {{MAY 14}},
Abstract = {{The concept of edge weight provides additional depth for describing and
   adjusting the properties of networks. Redistribution of edge weight can
   effectively change the properties of networks even though the
   corresponding binary topology remains unchanged. Based on regular
   networks with initially homogeneous dissimilarity weights, random
   redistribution of edge weight can be enough to induce small world
   phenomena. The effects of random weight redistribution on both static
   properties and dynamical models of networks are investigated. The
   results reveal that randomization of weight can enhance the ability of
   synchronization of chaotic systems dramatically. (c) 2006 Elsevier B.V.
   All rights reserved.}},
