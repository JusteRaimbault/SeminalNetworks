@article{ ISI:000462916000005,
Author = {Fleischer, Vinzenz and Radetz, Angela and Ciolac, Dumitru and
   Muthuraman, Muthuraman and Gonzalez-Escamilla, Gabriel and Zipp, Frauke
   and Groppa, Sergiu},
Title = {{Graph Theoretical Framework of Brain Networks in Multiple Sclerosis: A
   Review of Concepts}},
Journal = {{NEUROSCIENCE}},
Year = {{2019}},
Volume = {{403}},
Number = {{SI}},
Pages = {{35-53}},
Month = {{APR 1}},
Abstract = {{Network science provides powerful access to essential organizational
   principles of the human brain. It has been applied in combination with
   graph theory to characterize brain connectivity patterns. In multiple
   sclerosis (MS), analysis of the brain networks derived from either
   structural or functional imaging provides new insights into pathological
   processes within the gray and white matter. Beyond focal lesions and
   diffuse tissue damage, network connectivity patterns could be important
   for closely tracking and predicting the disease course. In this review,
   we describe concepts of graph theory, highlight novel issues of tissue
   reorganization in acute and chronic neuroinflammation and address
   pitfalls with regard to network analysis in MS patients. We further
   provide an outline of functional and structural connectivity patterns
   observed in MS, spanning from disconnection and disruption on one hand
   to adaptation and compensation on the other. Moreover, we link network
   changes and their relation to clinical disability based on the current
   literature. Finally, we discuss the perspective of network science in MS
   for future research and postulate its role in the clinical framework.
   (C) 2017 IBRO. Published by Elsevier Ltd. All rights reserved.}},
