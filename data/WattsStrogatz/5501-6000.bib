@article{ ISI:000318571600004,
Author = {Rubulotta, Elena and Ignaccolo, Matteo and Inturri, Giuseppe and Rofe,
   Yodan},
Title = {{Accessibility and Centrality for Sustainable Mobility: Regional Planning
   Case Study}},
