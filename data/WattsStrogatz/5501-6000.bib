@article{ ISI:000339548700008,
Author = {Bale, Catherine S. E. and Mccullen, Nicholas J. and Foxon, Timothy J.
   and Rucklidge, Alastair M. and Gale, William F.},
Title = {{Modeling Diffusion of Energy Innovations on a Heterogeneous Social
   Network and Approaches to Integration of Real-World Data}},
Journal = {{COMPLEXITY}},
Year = {{2014}},
Volume = {{19}},
Number = {{6, SI}},
Pages = {{83-94}},
Month = {{JUL-AUG}},
Abstract = {{A model has been developed to simulate the diffusion of energy
   innovations on a heterogeneous social network. Nodes on a network
   represent households, whose adoption of an energy innovation is based on
   a combination of personal and social benefit; social benefit includes
   the positive influence from an individual's personal social network and
   feedback from the wider population. This article describes the
   development of the model to incorporate heterogeneous parameters and,
   thus, become more like a real social system. The sensitivity of the
   model is investigated and it is shown that heterogeneity matters. This
   has important implications for the inclusion of real-world data into
   this type of model. (C) 2014 Wiley Periodicals, Inc.}},
