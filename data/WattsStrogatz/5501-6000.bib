@article{ ISI:000286668800016,
Author = {Ermann, L. and Chepelianskii, A. D. and Shepelyansky, D. L.},
Title = {{Fractal Weyl law for Linux Kernel architecture}},
Journal = {{EUROPEAN PHYSICAL JOURNAL B}},
Year = {{2011}},
Volume = {{79}},
Number = {{1}},
Pages = {{115-120}},
Month = {{JAN}},
Abstract = {{We study the properties of spectrum and eigenstates of the Google matrix
   of a directed network formed by the procedure calls in the Linux Kernel.
   Our results obtained for various versions of the Linux Kernel show that
   the spectrum is characterized by the fractal Weyl law established
   recently for systems of quantum chaotic scattering and the
   Perron-Frobenius operators of dynamical maps. The fractal Weyl exponent
   is found to be nu a parts per thousand 0.65 that corresponds to the
   fractal dimension of the network d a parts per thousand 1.3. An
   independent computation of the fractal dimension by the cluster growing
   method, generalized for directed networks, gives a close value d a parts
   per thousand 1.4. The eigenmodes of the Google matrix of Linux Kernel
   are localized on certain principal nodes. We argue that the fractal Weyl
   law should be generic for directed networks with the fractal dimension d
   < 2.}},
