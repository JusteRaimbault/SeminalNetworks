@inproceedings{ ISI:000259411102092,
Author = {Hsu, Wei-jen and Dutta, Debojyoti and Helmy, Ahmed},
Book-Group-Author = {{IEEE}},
Title = {{Profile cast: Behavior-aware mobile networking}},
Booktitle = {{WCNC 2008: IEEE WIRELESS COMMUNICATIONS \& NETWORKING CONFERENCE, VOLS
   1-7}},
Series = {{IEEE Wireless Communications and Networking Conference}},
Year = {{2008}},
Pages = {{3033+}},
Note = {{IEEE Wireless Communications and Networking Conference, Las Vegas, NV,
   MAR 31-APR 03, 2008}},
Organization = {{IEEE}},
Abstract = {{In this paper we advocate a service paradigm, profile cast, within the
   communication framework of delay tolerant networks (DTN){[}2]. This
   novel approach leverages the behavioral patterns of mobile network users
   for delivering messages to a sub group of users as defined by their
   profiles (e.g., interest, social affiliation, etc.). We study large data
   sets of user mobility profiles and present a case study of mobilily
   profile cast with a similarity based forwarding protocol. We show that
   behavior aware protocol design has a great potential - we reduce the
   total number of transmissions to 45\% of flooding under 92\% delivery
   success rate, or to only 3\% transmissions of flooding under 61\%
   delivery success rate. It also leads to shorter delay (at least 30\%
   less) as compared to a random transmission protocol.}},
