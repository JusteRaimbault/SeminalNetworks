@article{ ISI:000320346300018,
Author = {Pu, Cun-Lai and Yang, Jian and Pei, Wen-Jiang and Tao, Yu-Ting and Lan,
   Shao-Hua},
Title = {{Robustness analysis of static routing on networks}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2013}},
Volume = {{392}},
Number = {{15}},
Pages = {{3293-3300}},
Month = {{AUG 1}},
Abstract = {{Robustness is one of the crucial properties that needs to be considered
   in the design of routing strategies on networks. We study the robustness
   of three typical routing strategies, which are the SP (shortest path),
   EP (efficient path), and OP (optimal path) strategies, by simulating
   several different kinds of attacks including random attacks, target
   attacks and cascading failures on scale-free networks. Results of the
   average path length, betweenness centrality, network capacity, etc.,
   demonstrate that the EP strategy is more robust than the other two, and
   the OP strategy is more reliable than the SP strategy in general.
   However, on the power-grid network, the OP strategy is more resistant
   against cascading failures than the EP and SP strategies. (C) 2013
   Elsevier B.V. All rights reserved.}},
Publisher = {{ELSEVIER}},
Address = {{RADARWEG 29, 1043 NX AMSTERDAM, NETHERLANDS}},
