@article{ ISI:000332160600007,
Author = {Rubido, Nicolas and Grebogi, Celso and Baptista, Murilo S.},
Title = {{Resiliently evolving supply-demand networks}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2014}},
Volume = {{89}},
Number = {{1}},
Month = {{JAN 9}},
Abstract = {{The ability to design a transport network such that commodities are
   brought from suppliers to consumers in a steady, optimal, and stable way
   is of great importance for distribution systems nowadays. In this work,
   by using the circuit laws of Kirchhoff and Ohm, we provide the exact
   capacities of the edges that an optimal supply-demand network should
   have to operate stably under perturbations, i.e., without overloading.
   The perturbations we consider are the evolution of the connecting
   topology, the decentralization of hub sources or sinks, and the
   intermittence of supplier and consumer characteristics. We analyze these
   conditions and the impact of our results, both on the current United
   Kingdom power-grid structure and on numerically generated evolving
   archetypal network topologies.}},
