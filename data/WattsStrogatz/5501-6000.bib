@article{ ISI:000258464100015,
Author = {Carchiolo, Vincenza and Malgeri, Michele and Mangioni, Giuseppe and
   Nicosia, Vincenzo},
Title = {{Emerging structures of P2P networks induced by social relationships}},
Journal = {{COMPUTER COMMUNICATIONS}},
Year = {{2008}},
Volume = {{31}},
Number = {{3}},
Pages = {{620-628}},
Month = {{FEB 25}},
Abstract = {{Networks seem to be the natural way chosen by nature to organise
   individuals, resources and interactions in an effective and robust
   structure. Studies about natural networks focused on the central role of
   emerging structures in distributed environments, and pointed out some
   properties such as small-world effect and communities which are of the
   most importance to guarantee a fast and efficient communication among
   nodes. In this paper we propose a model for P2P networks which mimics
   behaviours of peers in social and biological networks and naturally
   evolves to a robust graph of peers with some interesting properties,
   including small-world effect and community decomposition. (c) 2007
   Elsevier B.V. All rights reserved.}},
