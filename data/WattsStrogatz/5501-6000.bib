@inproceedings{ ISI:000187254200016,
Author = {Serra, R and Villani, M},
Editor = {{Bandini, S and Chopard, B and Tomassini, M}},
Title = {{Perturbing the regular topology of cellular automata: Implications for
   the dynamics}},
