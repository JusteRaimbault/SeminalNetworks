@article{ ISI:000335010700009,
Author = {Zhang, Shuai and Liang, Man-Gui and Jiang, Zhong-Yuan and Wu, Jia-Jing},
Title = {{Effective strategy of adding links for improving network transport
   efficiency on complex networks}},
Journal = {{INTERNATIONAL JOURNAL OF MODERN PHYSICS C}},
Year = {{2014}},
Volume = {{25}},
Number = {{6}},
Month = {{JUN}},
Abstract = {{In this paper, we propose an efficient strategy to enhance the network
   transport efficiency by adding links to the existing networks. In our
   proposed strategy, we consider both the node betweenness centrality (BC)
   and the shortest path length (L) as two important factors. The overall
   traffic capacity of a network system can be evaluated by the critical
   packet generating rate R-c. Simulation results show that the proposed
   strategy can bring better traffic capacity and shorter average shortest
   path length than the low-degree-first (LDF) strategy and the
   low-betweenness-first (LBF) strategy. This work is helpful for designing
   and optimizing of realistic networks.}},
