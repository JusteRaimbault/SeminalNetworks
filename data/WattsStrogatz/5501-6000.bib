@article{ ISI:000312831600025,
Author = {Sun, Yongzheng and Li, Wang and Zhao, Donghua},
Title = {{Outer synchronization between two complex dynamical networks with
   discontinuous coupling}},
Journal = {{CHAOS}},
Year = {{2012}},
Volume = {{22}},
Number = {{4}},
Month = {{DEC}},
Abstract = {{In this paper, we study the outer synchronization between two complex
   networks with discontinuous coupling. Sufficient conditions for complete
   outer synchronization and generalized outer synchronization are obtained
   based on the stability theory of differential equations. The theoretical
   results show that two networks can achieve outer synchronization even if
   two networks are switched off sometimes and the speed of synchronization
   is proportional to the on-off rate. Finally, numerical examples are
   examined to illustrate the effectiveness of the analytical results. (C)
   2012 American Institute of Physics.
   {[}http://dx.doi.org/10.1063/1.4768661]}},
