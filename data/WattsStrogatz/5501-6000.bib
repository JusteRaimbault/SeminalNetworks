@article{ ISI:000262359500006,
Author = {Cassi, Lorenzo and Corrocher, Nicoletta and Malerba, Franco and
   Vonortas, Nicholas},
Title = {{The impact of EU-funded research networks on knowledge diffusion at the
   regional level}},
Journal = {{RESEARCH EVALUATION}},
Year = {{2008}},
Volume = {{17}},
Number = {{4}},
Pages = {{283-293}},
Month = {{DEC}},
Abstract = {{Research networks foster the dissemination of innovation-related
   knowledge. To address this conjecture, we evaluate the structure of
   collaborative networks and of knowledge transfer between research,
   innovation and deployment activities in the field of information and
   communication technology for the European Union as a whole and for
   several European regions. We find that research networks complement
   diffusion networks by increasing the number of links and organisations
   involved in exchanging knowledge. Two types of actors are key players in
   these networks: hubs maintain the bulk of ties in the networks also
   helping the smaller and more isolated members remain connected;
   gatekeepers bridge research and diffusion networks.}},
