@article{ ISI:000272309500008,
Author = {Han, Yilong},
Title = {{Phase-space networks of geometrically frustrated systems}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2009}},
Volume = {{80}},
Number = {{5, 1}},
Month = {{NOV}},
Abstract = {{We illustrate a network approach to the phase-space study by using two
   geometrical frustration models: antiferromagnet on triangular lattice
   and square ice. Their highly degenerated ground states are mapped as
   discrete networks such that the quantitative network analysis can be
   applied to phase-space studies. The resulting phase spaces share some
   comon features and establish a class of complex networks with unique
   Gaussian spectral densities. Although phase-space networks are
   heterogeneously connected, the systems are still ergodic due to the
   random Poisson processes. This network approach can be generalized to
   phase spaces of some other complex systems.}},
