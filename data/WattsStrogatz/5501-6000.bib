@article{ ISI:000333504200019,
Author = {Kai, Cai Hong and Long, Jiancheng and Ling, Xiang and Hu, Mao-Bin},
Title = {{Upper bound of network capacity and a static optimal packet routing
   strategy}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2014}},
Volume = {{401}},
Pages = {{174-181}},
Month = {{MAY 1}},
Abstract = {{In this paper, we propose a max-flow-min-cost (MFMC) problem to obtain
   the upper bound of network capacity and develop an efficient packet
   routing strategy. The MFMC problem is formulated as two linear programs
   (LP). The optimal solutions of the two LPs are the upper bound of
   network capacity (i.e., maximum network flow rate) and an optimal packet
   flow pattern with minimum total free flow travel time under the maximum
   network flow rate, respectively. We also develop a static optimal
   routing strategy (SORS), which proportionally deliver packets along the
   path according to the optimal packet flow pattern. Simulation results
   indicate that the proposed SORS outperforms existing routing strategies
   in terms of both network capacity and traffic efficiency. Furthermore,
   the network capacity achieved by the SORS is very close to its upper
   bound. (c) 2014 Elsevier B.V. All rights reserved.}},
