@article{ ISI:000354440600011,
Author = {Sonderegger-Wakolbinger, Lea M. and Stummer, Christian},
Title = {{An agent-based simulation of customer multi-channel choice behavior}},
Journal = {{CENTRAL EUROPEAN JOURNAL OF OPERATIONS RESEARCH}},
Year = {{2015}},
Volume = {{23}},
Number = {{2, SI}},
Pages = {{459-477}},
Month = {{JUN}},
Abstract = {{Experimenting with multi-channel operations in reality is both costly
   and risky, because it can severely affect a firm's revenues,
   profitability, customer churn, and other customer metrics. In this
   paper, we introduce an agent-based simulation approach as a means for
   exploring and analyzing the impact of (combinations of) multi-channel
   activities on customer channel choices before having implemented them in
   practice. The simulation takes into account the heterogeneity of
   customers, social dynamics in their behavior, and the various phases of
   the purchasing process. To this end, customers are represented by agents
   whose phase-specific channel perceptions determine their channel
   choices. These perceptions may be altered by communication with other
   agents, first-hand experience of the channel, and/or marketing
   activities. The approach is illustrated through a sample application for
   which data from an international multi-channel retailer as well as from
   an empirical study among Austrian customers have been used.}},
