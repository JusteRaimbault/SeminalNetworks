@article{ ISI:000295602300059,
Author = {Jalili, Mandi},
Title = {{Synchronizability of dynamical scale-free networks subject to random
   errors}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2011}},
Volume = {{390}},
Number = {{23-24}},
Pages = {{4588-4595}},
Month = {{NOV 1}},
Abstract = {{In this paper the robustness of network synchronizability against random
   deletion of nodes, i.e. errors, in dynamical scale-free networks was
   studied. To this end, two measures of network synchronizability, namely,
   the eigenratio of the Laplacian and the order parameter quantifying the
   degree of phase synchrony were adopted, and the synchronizability
   robustness on preferential attachment scale-free graphs was
   investigated. The findings revealed that as the network size decreases,
   the robustness of its synchronizability against random removal of nodes
   declines, i.e. the more the number of randomly removed nodes from the
   network, the worse its synchronizability. We also showed that this
   dependence of the synchronizability on the network size is different
   with that in the growing scale-free networks. The profile of a number of
   network properties such as clustering coefficient, efficiency,
   assortativity, and eccentricity, as a function of the network size was
   investigated in these two cases, growing scale-free networks and those
   with randomly removed nodes. The results showed that these processes are
   also different in terms of these metrics. (C) 2011 Elsevier B.V. All
   rights reserved.}},
Publisher = {{ELSEVIER SCIENCE BV}},
Address = {{PO BOX 211, 1000 AE AMSTERDAM, NETHERLANDS}},
Type = {{Article}},
Language = {{English}},
