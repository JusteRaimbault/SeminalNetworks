@article{ ISI:000350547100011,
Author = {Aliakbary, Sadegh and Motallebi, Sadegh and Rashidian, Sina and Habibi,
   Jafar and Movaghar, Ali},
Title = {{Distance metric learning for complex networks: Towards size-independent
   comparison of network structures}},
