Author = {Michalski, Radoslaw and Kazienko, Przemyslaw and Jankowski, Jaroslaw},
Editor = {{Yetongnon, K and Dipanda, A and Chbeir, R}},
Title = {{Convince a Dozen More and Succeed - The Influence in Multi-layered
