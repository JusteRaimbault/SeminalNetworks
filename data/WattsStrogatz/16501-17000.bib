@article{ ISI:000311525400009,
Author = {Hu Heng-Kai and Wang Kai and Yang Guang and Liu Qian and Pei Wen-Jiang
   and Qiu Shen-Wei and Wei Cheng-Jian and Zhang Yi-Feng},
Title = {{Dynamical patterns of epidemic outbreaks on scale-free networks with
   traffic flow}},
Journal = {{ACTA PHYSICA SINICA}},
Year = {{2012}},
Volume = {{61}},
Number = {{20}},
Month = {{OCT}},
Abstract = {{A modified susceptible-infected (SI) model is developed in this paper.
   The mean field approximation method is further adopted. This present
   research is based on traffic-driven epidemic spreading behavior. Take
   MIP routing for example, diffusion coefficient tau, traffic generation
   rate lambda, infection rate beta and their relationship with the actual
   betweenness b(k) are re-explored under the SI model. Both the
   theoretical and experimental results indicate that under the same
   interne topology and routing strategy, the diffusion coefficient tau is
   inversely proportional to average traffic generation rate lambda and
   infection rate beta.}},
