@inproceedings{ ISI:000352082700068,
Author = {Ekpenyong, Uduakobong E. and Zhang, Jiangfeng and Xia, Xiaohua},
Book-Group-Author = {{IEEE}},
Title = {{Social Influence and Energy Efficiency Savings}},
Booktitle = {{AFRICON, 2013}},
