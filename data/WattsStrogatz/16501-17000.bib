@inproceedings{ ISI:000335317000030,
Author = {Chang, Hsiu-Ming and Chiang, Ann-Shyn and Didimo, Walter and Lin,
   Ching-Yao and Liotta, Giuseppe and Montecchiani, Fabrizio},
Book-Group-Author = {{IEEE}},
Title = {{On the Robustness of the Drosophila Neural Network}},
