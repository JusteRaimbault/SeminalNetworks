@article{ ISI:000209629900132,
Author = {Schmidt, Deena and Best, Janet and Blumberg, Mark S.},
Title = {{RANDOM GRAPH AND STOCHASTIC PROCESS CONTRIBUTIONS TO NETWORK DYNAMICS}},
Journal = {{DISCRETE AND CONTINUOUS DYNAMICAL SYSTEMS}},
Year = {{2011}},
Volume = {{31}},
Number = {{S}},
Pages = {{1279-1288}},
Month = {{SEP}},
Abstract = {{A fundamental question regarding neural systems and other applications
   involving networks is the extent to which the network architecture may
   contribute to the observed dynamics. We explore this question by
   investigating two different processes on three different graph
   structures, asking to what extent the graph structure (as represented by
   the degree distribution) is reflected in the dynamics of the process.
   Our findings suggest that memoryless processes are more likely to
   reflect the degree distribution, whereas processes with memory can
   robustly give power law or other heavy-tailed distributions regardless
   of degree distribution.}},
