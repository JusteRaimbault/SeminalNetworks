@inproceedings{ ISI:000312421800068,
Author = {Chen, Xi and Jiang, Hao and Ching, Wai-Ki and Li, Limin},
Editor = {{Hu, JW and Su, J}},
Title = {{Inferring Functional Annotation for Human Genes from Gene-PDB Structure
   Mapping}},
