Title = {{Liquid-drop-like Multi-orbit Topology vs. Ring Topology in PSO for
   Lennard-Jones Problem}},
Booktitle = {{PROCEEDINGS OF SEVENTH INTERNATIONAL CONFERENCE ON BIO-INSPIRED
   COMPUTING: THEORIES AND APPLICATIONS (BIC-TA 2012), VOL 2}},
Series = {{Advances in Intelligent Systems and Computing}},
Year = {{2013}},
Volume = {{202}},
