Title = {{Multi-agent simulation based governance of complex systems: architecture
   and example implementation on free-riding}},
Booktitle = {{2013 MEXICAN INTERNATIONAL CONFERENCE ON COMPUTER SCIENCE (ENC 2013)}},
Series = {{Proceedings of the Mexican Conference on Computer Science}},
Year = {{2013}},
Pages = {{89-98}},
