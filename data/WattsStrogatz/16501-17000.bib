@inproceedings{ ISI:000343670500007,
Author = {Westphal, Matthias and Hue, Julien and Woelfl, Stefan},
Book-Group-Author = {{IEEE}},
Title = {{On the Propagation Strength of SAT Encodings for Qualitative Temporal
   Reasoning}},
