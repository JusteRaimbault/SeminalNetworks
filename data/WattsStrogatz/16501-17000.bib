@inproceedings{ ISI:000353639700053,
Author = {Spezzano, Francesca and Subrahmanian, V. S. and Mannes, Aaron},
Editor = {{Ozyer, T and Carrington, P}},
Title = {{STONE: Shaping Terrorist Organizational Network Efficiency}},
