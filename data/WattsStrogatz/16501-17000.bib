@inproceedings{ ISI:000349838403025,
Author = {Marin, Ericsson Santana and de Carvalho, Cedric Luiz},
Book-Group-Author = {{IEEE}},
Title = {{SMALL-SCALE: A NEW MODEL OF SOCIAL NETWORKS}},
Booktitle = {{2013 WINTER SIMULATION CONFERENCE (WSC)}},
