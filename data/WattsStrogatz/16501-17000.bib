@article{ ISI:000305659100005,
Author = {Xia Yang and Dong Kai and Lai Yong-Xiu and Luo Cheng and Lei Lei and Yao
   De-Zhong},
Title = {{Study on Resting-State Functional Connectivity of Rat Sensorimotor
   System}},
Journal = {{PROGRESS IN BIOCHEMISTRY AND BIOPHYSICS}},
Year = {{2012}},
Volume = {{39}},
Number = {{6}},
Pages = {{519-524}},
Month = {{JUN}},
Abstract = {{In order to better understand functional connectivity of rodent brain,
   we analyzed the functional magnetic resonance imaging (fMRI) data of the
   resting-state and forepaw electrical stimulation from lightly
   anesthetized rat at 9.4 T using cross-correlation analysis. The results
   showed that the primary somatosensory cortex (Si) and thalamus (Tha)
   were significantly positively activated, the caudate putamen nucleus
   (CPu) was significantly negatively activated during forepaw stimulation.
   The higher connectivity bilateral intra-sensory/motor cortices and
   bilateral intra- thalamus, but lower connectivity between sensory/motor
   cortex and thalamus were found during the resting state. Synchronized
   low frequency fluctuation (LFF) was observed between functionally
   related brain regions. In addition, the brain network of sensorimotor
   system showed small-world feature during resting-state. These results
   suggested that rodents have some similar properties with human in
   functional separation and integration during brain information
   processing, and strongly support the view that the underlying physiology
   of CNS is conserved across mammalian species.}},
