Title = {{Hard QBF Encodings Made Easy: Dream or Reality?}},
Booktitle = {{AI (ASTERISK) IA 2009: EMERGENT PERSPECTIVES IN ARTIFICIAL INTELLIGENCE}},
Series = {{Lecture Notes in Artificial Intelligence}},
Year = {{2009}},
Volume = {{5883}},
