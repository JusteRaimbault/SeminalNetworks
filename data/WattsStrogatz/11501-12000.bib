@inproceedings{ ISI:000322137100199,
Author = {De Felice, Matteo and Meloni, Sandro and Panzieri, Stefano},
Editor = {{Krasnogor, N}},
Title = {{Effect of Topology on Diversity of Spatially-Structured Evolutionary
   Algorithms}},
