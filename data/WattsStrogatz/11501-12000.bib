@inproceedings{ ISI:000255181700171,
Author = {Tahbaz-Salehi, Alireza and Jadbabaie, Ali},
Book-Group-Author = {{IEEE}},
Title = {{Small world phenomenon, rapidly mixing Markov chains, and average
   consensus algorithms}},
Booktitle = {{PROCEEDINGS OF THE 46TH IEEE CONFERENCE ON DECISION AND CONTROL, VOLS
   1-14}},
Series = {{IEEE Conference on Decision and Control}},
Year = {{2007}},
Pages = {{1021-1026}},
