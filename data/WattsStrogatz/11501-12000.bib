
@article{ ISI:000329267200007,
Author = {Kim, Dongsoo and Noel, Eric and Tang, K. Wendy},
Title = {{Expanded Borel Cayley Graphs (Ex-BCGs): A novel communication topology
   for multi-agent systems}},
