@article{ ISI:000309889700007,
Author = {Dong, J. -Q. and Huang, Z. -G. and Zhou, Z. and Huang, L. and Wu, Z. -X.
   and Do, Y. and Wang, Y. -H.},
Title = {{Enhancing transport efficiency by hybrid routing strategy}},
