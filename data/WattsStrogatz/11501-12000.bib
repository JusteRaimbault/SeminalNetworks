@inproceedings{ ISI:000346159700086,
Author = {Deka, Deepjyoti and Vishwanath, Sriram},
Editor = {{Yetongnon, K and Dipanda, A and Chbeir, R}},
Title = {{Generative Growth Model for Power Grids}},
Booktitle = {{2013 INTERNATIONAL CONFERENCE ON SIGNAL-IMAGE TECHNOLOGY \&
   INTERNET-BASED SYSTEMS (SITIS)}},
Year = {{2013}},
Pages = {{586-593}},
Note = {{9th International Conference on Signal-Image Technology and
   Internet-Based Systems (SITIS), Kyoto, JAPAN, DEC 02-05, 2013}},
Abstract = {{The inherent structure of the power grid deeply influences its secure
   functioning and stability. As power grids move towards becoming
   `smarter' with increased demand response and decentralized control, the
   topological aspects of the grid have become even more important.
   Understanding the topology can thus lead to better strategies to control
   the smart grid as well as enable rapid identification and prevention of
   risks such as blackouts. This paper focuses on modeling and analyzing
   basic similarities in network structure of large power grids across
   America and Europe. A simple but accurate model of the power grid can
   help generate real-sized test cases to evaluate new grid technologies as
   well as aid in tractable theoretical analysis of the grid structure.}},
