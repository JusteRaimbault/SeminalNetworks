@inproceedings{ ISI:000399141700158,
Author = {McLaughlin, Adam and Bader, David A.},
Book-Group-Author = {{IEEE}},
Title = {{Revisiting Edge and Node Parallelism for Dynamic GPU Graph Analytics}},
Booktitle = {{PROCEEDINGS OF 2014 IEEE INTERNATIONAL PARALLEL \& DISTRIBUTED
   PROCESSING SYMPOSIUM WORKSHOPS (IPDPSW)}},
