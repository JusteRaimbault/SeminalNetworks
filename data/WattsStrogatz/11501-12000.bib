@incollection{ ISI:000260513700001,
Author = {Walker, Gordon},
Editor = {{Baum, JAC and Rowley, TJ}},
Title = {{THE RISE OF ECOMMERCE AS AN EPIDEMIC IN THE SMALL WORLD OF VENTURE
   CAPITAL}},
