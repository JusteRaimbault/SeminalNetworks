@article{ ISI:000311487800033,
Author = {Su, Guifeng and Zhang, Xiaobing and Zhang, Yi},
Title = {{Condensation phase transition in nonlinear fitness networks}},
Journal = {{EPL}},
Year = {{2012}},
Volume = {{100}},
Number = {{3}},
Month = {{NOV}},
Abstract = {{We analyze the condensation phase transitions in out-of-equilibrium
   complex networks in a unifying framework which includes the nonlinear
   model and the fitness model as its appropriate limits. We show a novel
   phase structure which depends on both the fitness parameter and the
   nonlinear exponent. The occurrence of the condensation phase transitions
   in the dynamical evolution of the network is demonstrated by using
   Bianconi-Barabasi method. We find that the nonlinear and the fitness
   preferential-attachment mechanisms play important roles in the formation
   of an interesting phase structure. Copyright (C) EPLA, 2012}},
