@article{ ISI:000323285400002,
Author = {Peng, Qian-Jin and Zhao, Ming and Zhang, Hai-Feng},
Title = {{INFLUENCE OF INTER-COMMUNITY LINK STRENGTH ON COMMUNITY NETWORKS}},
