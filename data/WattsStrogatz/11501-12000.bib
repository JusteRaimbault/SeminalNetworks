@article{ ISI:000321552000026,
Author = {Zhao Jun-Chan},
Title = {{Explosive synchronization of complex networks with different chaotic
   oscillators}},
Journal = {{CHINESE PHYSICS B}},
Year = {{2013}},
Volume = {{22}},
Number = {{6}},
Month = {{JUN}},
Abstract = {{Recent studies have shown that explosive synchronization transitions can
   be observed in networks of phase oscillators {[}Gomez-Gardenes J, Gomez
   S, Arenas A and Moreno Y 2011 Phys. Rev. Lett. 106 128701] and chaotic
   oscillators {[}Leyva I, Sevilla-Escoboza R, Buldu J M, Sendina-Nadal I,
   Gomez-Gardenes J, Arenas A, Moreno Y, Gomez S, Jaimes-Reategui R and
   Boccaletti S 2012 Phys. Rev. Lett. 108 168702]. Here, we study the
   effect of different chaotic dynamics on the synchronization transitions
   in small world networks and scale free networks. The continuous
   transition is discovered for Rossler systems in both of the above
   complex networks. However, explosive transitions take place for the
   coupled Lorenz systems, and the main reason is the abrupt change of
   dynamics before achieving complete synchronization. Our results show
   that the explosive synchronization transitions are accompanied by the
   change of system dynamics.}},
