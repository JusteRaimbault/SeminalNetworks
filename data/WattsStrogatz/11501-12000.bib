Author = {Trentin, Edmondo and Di Lorio, Ernesto},
Editor = {{Apolloni, B and Howlett, RJ and Jain, L}},
Title = {{A simple and effective neural model for the classification of structured
   patterns}},
Booktitle = {{KNOWLEDGE-BASED INTELLIGENT INFORMATION AND ENGINEERING SYSTEMS: KES
   2007 - WIRN 2007, PT I, PROCEEDINGS}},
