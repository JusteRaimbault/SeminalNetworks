@inproceedings{ ISI:000249783400028,
Author = {Trentin, Edmondo and Di Iorio, Ernesto},
Editor = {{MarquesDeSa, J and Alexandre, LA and Duch, W and Mandic, D}},
Title = {{Unbiased SVM density estimation with application to graphical pattern
   recognition}},
Booktitle = {{ARTIFICIAL NEURAL NETWORKS - ICANN 2007, PT 2, PROCEEDINGS}},
Series = {{Lecture Notes in Computer Science}},
Year = {{2007}},
Volume = {{4669}},
Pages = {{271+}},
