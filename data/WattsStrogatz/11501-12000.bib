@article{ ISI:000291119600007,
Author = {da Rocha, E. L. and da Cunha, C. R.},
Title = {{The transition from fracton to phonon states in a Sierpinski triangle
   lattice}},
Journal = {{CHAOS SOLITONS \& FRACTALS}},
Year = {{2011}},
Volume = {{44}},
Number = {{4-5}},
Pages = {{241-247}},
Month = {{APR-MAY}},
Abstract = {{Lattice dynamics of a Sierpinski triangle submitted to different levels
   of disorder was studied via atomistic Green's functions. It was found
   that there is a critical level of disorder that separates two regions of
   thermal transport. The first is characterized by a fast destruction of
   fracton states and the formation of spatially extended phonon states.
   The second region is characterized by a transition from extended to
   localized phonon states as predicted by the Anderson model. (C) 2011
   Elsevier Ltd. All rights reserved.}},
