@inproceedings{ ISI:000301803200006,
Author = {Hinne, Max and Marchiori, Elena},
Editor = {{Merz, P and Hao, JK}},
Title = {{Cutting Graphs Using Competing Ant Colonies and an Edge Clustering
   Heuristic}},
Booktitle = {{EVOLUTIONARY COMPUTATION IN COMBINATORIAL OPTIMIZATION}},
