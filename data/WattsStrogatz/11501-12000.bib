@inproceedings{ ISI:000252215400113,
Author = {Zio, E. and Sansavini, G.},
Editor = {{Aven, T and Vinnem, JE}},
Title = {{Service reliability analysis of a tramway network}},
Booktitle = {{RISK, RELIABILITY AND SOCIETAL SAFETY, VOLS 1-3: VOL 1: SPECIALISATION
   TOPICS; VOL 2: THEMATIC TOPICS; VOL 3: APPLICATIONS TOPICS}},
