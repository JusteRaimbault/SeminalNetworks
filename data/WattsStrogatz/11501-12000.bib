@article{ ISI:000253085100002,
Author = {Gong, Baihua and Liu, Jun and Huang, Liang and Yang, Kongqing and Yang,
   Lei},
Title = {{Geographical constraints to range-based attacks on links in complex
   networks}},
Journal = {{NEW JOURNAL OF PHYSICS}},
Year = {{2008}},
Volume = {{10}},
Month = {{JAN 25}},
Abstract = {{In this paper, we studied range-based attacks on links in geographically
   constrained scale-free networks and found that there is a continuous
   switching of roles of short-and long-range attacks on links when tuning
   the geographical constraint strength. Our results demonstrate that the
   geography has a significant impact on the network efficiency and
   security; thus one can adjust the geographical structure to optimize the
   robustness and the efficiency of the networks. We introduce a
   measurement of the impact of links on the efficiency of the network, and
   an effective attacking strategy is suggested.}},
