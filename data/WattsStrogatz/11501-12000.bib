@inproceedings{ ISI:000312876800031,
Author = {Taheri, Amir Asiaee and Afshar, Mohammad and Asadpour, Masoud},
Editor = {{Martinoli, A and Mondada, F and Correll, N and Mermoud, G and Egerstedt, M and Hsieh, MA and Parker, LE and Stoy, K}},
Title = {{Influence Maximization for Informed Agents in Collective Behavior}},
Booktitle = {{DISTRIBUTED AUTONOMOUS ROBOTIC SYSTEMS}},
Series = {{Springer Tracts in Advanced Robotics}},
