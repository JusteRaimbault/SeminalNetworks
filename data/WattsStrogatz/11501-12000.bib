@article{ ISI:000276421100020,
Author = {Takagi, Kosuke},
Title = {{Scale free distribution in an analytical approach}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2010}},
Volume = {{389}},
Number = {{10}},
Pages = {{2143-2146}},
Month = {{MAY 15}},
Abstract = {{In order to explain the scale free feature of complex networks, we
   introduce an analytical approach for investigating the degree
   distribution. We represent the degree distribution by the probability
   density function, where the correspondence between them is given
   approximately by the transformation from discrete number, degree, to a
   continuous variable. We find that arbitrary representations of the
   degree distribution as the probability density function are reduced to a
   specific form which obeys scale free. Our result provides one
   explanation for the ubiquity of scale free networks. (C) 2010 Elsevier
   B.V. All rights reserved.}},
