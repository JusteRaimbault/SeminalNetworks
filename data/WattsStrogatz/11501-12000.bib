@inproceedings{ ISI:000293603200004,
Author = {Basalyga, Gleb and Gleiser, Pablo M. and Wennekers, Thomas},
Editor = {{Hernandez, C and Sanz, R and GomezRamirez, J and Smith, LS and Hussain, A and Chella, A and Aleksander, I}},
Title = {{Emergence of Small-World Structure in Networks of Spiking Neurons
   Through STDP Plasticity}},
Booktitle = {{FROM BRAINS TO SYSTEMS: BRAIN-INSPIRED COGNITIVE SYSTEMS 2010}},
Series = {{Advances in Experimental Medicine and Biology}},
Year = {{2011}},
Volume = {{718}},
Pages = {{33-39}},
Note = {{4th Brain-inspired Cognitive Systems Conference (BICS 2010), Madrid,
   SPAIN, JUL, 2010}},
Abstract = {{In this work, we use a complex network approach to investigate how a
   neural network structure changes under synaptic plasticity. In
   particular, we consider a network of conductance-based,
   single-compartment integrate-and-fire excitatory and inhibitory neurons.
   Initially the neurons are connected randomly with uniformly distributed
   synaptic weights. The weights of excitatory connections can be
   strengthened or weakened during spiking activity by the mechanism known
   as spike-timing-dependent plasticity (STDP). We extract a binary
   directed connection matrix by thresholding the weights of the excitatory
   connections at every simulation step and calculate its major topological
   characteristics such as the network clustering coefficient,
   characteristic path length and small-world index. We numerically
   demonstrate that, under certain conditions, a nontrivial small-world
   structure can emerge from a random initial network subject to STDP
   learning.}},
