@article{ ISI:000427875800004,
Author = {Li, Shu-Bin and Cao, Dan-Ni and Dang, Wen-Xiu and Zhang, Lin},
Title = {{Variable speed limit strategies analysis with mesoscopic traffic flow
   model based on complex networks}},
