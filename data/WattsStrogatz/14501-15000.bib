@article{ ISI:000450685600002,
Author = {Li, Longchuan and Bachevalier, Jocelyne and Hu, Xiaoping and Klin, Ami
   and Preuss, Todd M. and Shultz, Sarah and Jones, Warren},
Title = {{Topology of the Structural Social Brain Network in Typical Adults}},
Journal = {{BRAIN CONNECTIVITY}},
