@article{ ISI:000456498500028,
Author = {Tanaka-Ishii, Kumiko and Kobayashi, Tatsuru},
Title = {{Taylor's law for linguistic sequences and random walk models}},
Journal = {{JOURNAL OF PHYSICS COMMUNICATIONS}},
