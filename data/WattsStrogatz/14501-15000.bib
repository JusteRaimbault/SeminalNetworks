@article{ ISI:000453735800011,
Author = {Lu, Xiangyang and Ouyang, Ling and Sun, Lijuan and Hu, Jin and Jia,
   Lijuan},
Title = {{Designing a Communication Field with a Transformation Method}},
Journal = {{JOURNAL OF ROBOTICS AND MECHATRONICS}},
Year = {{2018}},
Volume = {{30}},
Number = {{6}},
Pages = {{943-949}},
Month = {{DEC}},
Abstract = {{The transformation method that was originally used to tailor the
   physical fields into desired spatial patterns by designing material
   parameters is used herein to obtain necessary local dynamic parameters
   when the state distribution of a network system is prescribed in space.
   This constitutes a typical inverse problem that controls the state
   distribution of a complex network by designing its local dynamic
   parameters. Thus, it is difficult to obtain a direct solution. This
   coordinate transformation provides a direct method. The feasibility of
   this method is demonstrated and verified by two examples (a
   communication field bender and a communication field cloak) in
   corresponding network systems.}},
