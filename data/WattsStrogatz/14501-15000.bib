@article{ ISI:000440251600014,
Author = {Miranda, Pedro Jeferson and Baptista, Murilo Silva and de Souza Pinto,
   Sandro Ely},
Title = {{The Odyssey's mythological network}},
Journal = {{PLOS ONE}},
Year = {{2018}},
Volume = {{13}},
Number = {{7}},
Month = {{JUL 30}},
Abstract = {{In this work, we study the mythological network of Odyssey of Homer. We
   use ordinary statistical quantifiers in order to classify the network as
   real or fictional. We also introduce an analysis of communities which
   allows us to see how network properties shall emerge. We found that
   Odyssey can be classified both as real and fictional network. This
   statement is supported as far as mythological characters are removed,
   which results in a network with real properties. The community analysis
   indicated to us that there is a power-law relationship based on the max
   degree of each community. These results allow us to conclude that
   Odyssey might be an amalgam of myth and of historical facts, with
   communities playing a central role.}},
