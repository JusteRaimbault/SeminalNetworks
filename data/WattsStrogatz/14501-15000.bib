@article{ ISI:000453479000001,
Author = {Mizrahi, Alice and Marsh, Thomas and Hoskins, Brian and Stiles, M. D.},
Title = {{Scalable Method to Find the Shortest Path in a Graph with Circuits of
   Memristors}},
Journal = {{PHYSICAL REVIEW APPLIED}},
Year = {{2018}},
Volume = {{10}},
Number = {{6}},
Month = {{DEC 14}},
Abstract = {{Finding the shortest path in a graph has applications in a wide range of
   optimization problems. However, algorithmic methods scale with the size
   of the graph in terms of time and energy. We propose a method to solve
   the shortest-path problem using circuits of nanodevices called
   memristors and validate it on graphs of different sizes and topologies.
   It is both valid for an experimentally derived memristor model and
   robust to device variability. The time and energy of the computation
   scale with the length of the shortest path rather than with the size of
   the graph, making this method particularly attractive for solving large
   graphs with small path lengths.}},
