@article{ ISI:000443596900013,
Author = {Brenda, R. S. C. and Lima, F. W. S.},
Title = {{Majority-vote model on directed Small-World-Voronoi-Delaunay random
   lattices}},
Journal = {{INTERNATIONAL JOURNAL OF MODERN PHYSICS C}},
Year = {{2018}},
Volume = {{29}},
Number = {{7}},
Month = {{JUL}},
Abstract = {{We investigate the critical properties of the nonequilibrium
   majority-vote model in two-dimensions on directed small-world lattice
   with quenched connectivity disorder. The disordered system is studied
   through Monte Carlo simulations: the critical noise (T), as well as the
   critical exponents gamma/nu, beta/nu, and 1/nu for several values of the
   rewiring probability p. We find that this disordered system does not
   belong to the same universality class as the regular two-dimensional
   ferromagnetic model. The majority-vote model on directed small-world
   lattices presents in fact a second-order phase transition with new
   critical exponents which do not depend on p (0 < p < 1), but agree with
   the exponents of the equilibrium Ising model on directed small-world
   Voronoi-Delaunay random lattices.}},
