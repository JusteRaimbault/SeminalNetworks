@article{ ISI:000436468200040,
Author = {Chen, Yongxin and Georgiou, Tryphon T. and Pavon, Michele and
   Tannenbaum, Allen},
Title = {{Efficient Robust Routing for Single Commodity Network Flows}},
Journal = {{IEEE TRANSACTIONS ON AUTOMATIC CONTROL}},
