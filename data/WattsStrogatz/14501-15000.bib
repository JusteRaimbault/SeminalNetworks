@article{ ISI:000430126400003,
Author = {Shmargad, Yotam},
Title = {{Structural diversity and tie strength in the purchase of a social
   networking app}},
Journal = {{JOURNAL OF THE ASSOCIATION FOR INFORMATION SCIENCE AND TECHNOLOGY}},
Year = {{2018}},
Volume = {{69}},
Number = {{5}},
Pages = {{660-674}},
Month = {{MAY}},
Abstract = {{Although people increasingly rely on online services to maintain their
   relationships, we know relatively little about what drives their use. To
   address this, I analyze data from a social networking site that started
   charging its users for an app that populates their e-mail address books
   with updated contact information. I find that purchase rates of the app
   were higher for users with large, structurally diverse networks - which
   contain several distinct social groups. Moreover, personal ties (i.e.,
   family members and friends) increased purchase rates more than
   professional ties. I attribute the first effect to the difficulty of
   obtaining information about a large, diverse social network, which the
   app reduces, and the second effect to the regularity with which people
   use information about their personal ties.}},
