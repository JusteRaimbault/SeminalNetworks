@article{ ISI:000434779000005,
Author = {Li, Shouwei and You, Zongjun},
Title = {{Research on the exponential growth effect on network topology:
   Theoretical and empirical analysis}},
Journal = {{INTERNATIONAL JOURNAL OF MODERN PHYSICS C}},
