@article{ ISI:000456823900003,
Author = {Erikson, Emily and Samila, Sampsa},
Title = {{Networks, Institutions, and Uncertainty: Information Exchange in
   Early-Modern Markets}},
Journal = {{JOURNAL OF ECONOMIC HISTORY}},
Year = {{2018}},
Volume = {{78}},
Number = {{4}},
Pages = {{1034-1067}},
Month = {{DEC}},
Abstract = {{We analyze patterns of informal information exchange by the English East
   India Company's captains to assess the role of relational and
   institutional governance systems and uncertainty in encouraging
   information exchange. Using archival data from the emerging early-modern
   global trade network, we show that uncertainty drove information
   exchange. Relational-based contract enforcement mechanisms such as small
   group exchange were largely absent, and increasing institutional
   strength was associated with decreasing rates of information exchange.
   The results suggest that opportunity and demand were more important
   determinants of information exchange than the emergence of formal and
   informal governance systems.}},
