@article{ ISI:000459311600010,
Author = {Wang Xiong and Gu HaiBo and Wang QianYao and Lu JinHu},
Title = {{Identifying topologies and system parameters of uncertain time-varying
   delayed complex networks}},
Journal = {{SCIENCE CHINA-TECHNOLOGICAL SCIENCES}},
Year = {{2019}},
Volume = {{62}},
Number = {{1}},
Pages = {{94-105}},
Month = {{JAN}},
Abstract = {{Node dynamics and network topologies play vital roles in determining the
   network features and network dynamical behaviors. Thus it is of great
   theoretical significance and practical value to recover the topology
   structures and system parameters of uncertain complex networks with
   available information. This paper presents an adaptive anticipatory
   synchronization-based approach to identify the unknown system parameters
   and network topological structures of uncertain time-varying delayed
   complex networks in the presence of noise. Moreover, during the
   identification process, our proposed scheme guarantees anticipatory
   synchronization between the uncertain drive and constructed auxiliary
   response network simultaneously. Particularly, our method can be
   extended to several special cases. Furthermore, numerical simulations
   are provided to verify the effectiveness and applicability of our method
   for reconstructing network topologies and node parameters. We hope our
   method can provide basic insight into future research on addressing
   reconstruction issues of uncertain realistic and large-scale complex
   networks.}},
