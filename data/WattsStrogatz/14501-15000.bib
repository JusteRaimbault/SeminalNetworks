@article{ ISI:000451479000007,
Author = {Chalupa, David and Hawick, Ken A. and Walker, James A.},
Title = {{Hybrid Bridge-Based Memetic Algorithms for Finding Bottlenecks in
   Complex Networks}},
Journal = {{BIG DATA RESEARCH}},
