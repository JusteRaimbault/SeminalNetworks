@article{ ISI:000243481700017,
Author = {Jia, L. C. and Han, C. R. and Chang, C. H. and Tsai, M. T. and Chou, Y.
   S. and Lai, Pik-Yin and Chan, C. K.},
Title = {{Synchronized bursting induced by network connectivity in cortical
   neuronal cultures}},
