@article{ ISI:000271844100016,
Author = {Zheng, Jian-Feng and Gao, Zi-You and Zhao, Xiao-Mei and Dai, Shuai and
   Fu, Bai-Bai},
Title = {{Self-organized diffusion of congestion in complex networks}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2010}},
Volume = {{389}},
Number = {{2}},
Pages = {{342-348}},
Month = {{JAN 15}},
Abstract = {{In this work, we propose and study a model for the diffusion of
   congestion in complex networks. According to the proposed model, the
   level of congestion on each node will be self-organized into the same
   value. The diffusion of congestion throughout various networks with
   different topologies is investigated analytically and by numerical
   tests. The flow fluctuations in complex networks are studied. We recover
   a power-law scaling relation between the standard deviation and mean
   flow, which is consistent with the previous studies. Finally, we extend
   our model by adding two constraints, which may be effective strategies
   for diffusing the local and the global congestion in complex networks,
   respectively. (C) 2009 Elsevier B.V. All rights reserved.}},
