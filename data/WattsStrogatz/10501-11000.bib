@article{ ISI:000446604100168,
Author = {Abdul, Rehman and Paul, Anand and Gul, Junaid M. and Hong, Won-Hwa and
   Seo, Hyuncheol},
Title = {{Exploiting Small World Problems in a SIoT Environment}},
