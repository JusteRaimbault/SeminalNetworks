@inproceedings{ ISI:000312932600245,
Author = {Tsujimoto, Takahiro and Shindo, Takuya and Jin'no, Kenya},
Book-Group-Author = {{IEEE}},
Title = {{The Neighborhood of Canonical Deterministic PSO}},
Booktitle = {{2011 IEEE CONGRESS ON EVOLUTIONARY COMPUTATION (CEC)}},
