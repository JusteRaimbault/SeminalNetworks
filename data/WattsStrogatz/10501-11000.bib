@article{ ISI:000444667800033,
Author = {Szolnoki, Attila and Danku, Zsuzsa},
Title = {{Dynamic-sensitive cooperation in the presence of multiple strategy
   updating rules}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2018}},
Volume = {{511}},
Pages = {{371-377}},
Month = {{DEC 1}},
Abstract = {{The importance of microscopic details on cooperation level is an
   intensively studied aspect of evolutionary game theory. Interestingly,
   these details become crucial on heterogeneous populations where
   individuals may possess diverse traits. By introducing a coevolutionary
   model in which not only strategies but also individual dynamical
   features may evolve we revealed that the formerly established conclusion
   is not necessarily true when different updating rules are on stage. In
   particular, we apply two strategy updating rules, imitation and
   Death-Birth rule, which allow local selection in a spatial system. Our
   observation highlights that the microscopic feature of dynamics, like
   the level of learning activity, could be a fundamental factor even if
   all players share the same trait uniformly. (C) 2018 Elsevier B.V. All
   rights reserved.}},
