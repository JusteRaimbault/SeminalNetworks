@article{ ISI:000283826100047,
Author = {Tejera, E. and Rodrigues, A. I. and Areias, M. J. and Rebelo, I. and
   Nieto-Villar, J. M.},
Title = {{Network centrality and multiscale transition asymmetry in the heart rate
   variability analysis of normal and preeclamptic pregnancies}},
Journal = {{COMMUNICATIONS IN NONLINEAR SCIENCE AND NUMERICAL SIMULATION}},
Year = {{2011}},
Volume = {{16}},
Number = {{3}},
Pages = {{1589-1596}},
Month = {{MAR}},
Abstract = {{In the present work we define and apply a new methodology for the
   analysis of time series that involves a network representation. In this
   methodology, time series are transformed into a network with the
   subsequent calculation of some centrality indexes as well as the
   asymmetry of the transition frequency matrix, across different scales.
