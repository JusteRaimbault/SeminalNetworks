@article{ ISI:000247243600057,
Author = {Tian, Liang and Shi, Da-Ning and Zhu, Chen-Ping},
Title = {{The effect of generalized deactivation mechanism in weighted networks}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2007}},
Volume = {{380}},
Pages = {{611-620}},
Month = {{JUL 1}},
Abstract = {{In this paper, we propose a generalized deactivation model to
   characterize weighted networks. By introducing the special aging
   mechanism, the model can produce power-law distributions of degree,
   strength, and weight, as confirmed in many real networks. We also
   characterize the clustering and correlation properties of this class of
   networks. A scaling behavior of clustering coefficient C similar to 1/M
   is observed, where M refers to the number of active nodes. The generated
   network simultaneously exhibits hierarchical organization and
   disassortative degree correlation. All of these structural properties
   are confirmed by present empirical evidence. (C) 2007 Elsevier B.V. All
   rights reserved.}},
