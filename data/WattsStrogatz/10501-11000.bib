@article{ ISI:000241723000029,
Author = {Yurkevich, I. V. and Lerner, I. V. and Stepanenko, A. S. and
   Constantinou, C. C.},
Title = {{Random walks in local dynamics of network losses}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2006}},
Volume = {{74}},
Number = {{4, 2}},
Month = {{OCT}},
Abstract = {{We suggest a model for data losses in a single node (memory buffer) of a
   packet-switched network (like the Internet) which reduces to
   one-dimensional discrete random walks with unusual boundary conditions.
   By construction, the model has critical behavior with a sharp transition
   from exponentially small to finite losses with increasing data arrival
   rate. We show that for a finite-capacity buffer at the critical point
   the loss rate exhibits strong fluctuations and non-Markovian power-law
   correlations in time, in spite of the Markovian character of the data
   arrival process.}},
