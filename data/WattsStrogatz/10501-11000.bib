@article{ ISI:000265885900005,
Author = {Chen, Hanshuang and Hou, Zhonghuai and Xin, Houwen},
Title = {{Threshold-diversity-induced resonance}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2009}},
Volume = {{388}},
Number = {{12}},
Pages = {{2299-2305}},
Month = {{JUN 15}},
Abstract = {{The effect of diversity on a system of coupled threshold elements is
   investigated, where each element is driven by a common periodic signal.
   Diversity is introduced to the system by assuming that the thresholds of
   all units are heterogeneous, e.g. the thresholds follow a Gaussian or
   other distribution. A combined numerical and analytical approach shows
   that the response of the system to the input signal is maximized at a
   moderate Value of the diversity amplitude, Which is Similar to the
   well-known stochastic resonance phenomenon induced by noise. Our
   findings exhibit that the diversity, a kind of spatial disorder, may
   play a similar role to noise as a kind of temporal disorder. (c) 2009
   Elsevier B.V. All rights reserved.}},
