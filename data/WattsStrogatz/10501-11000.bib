@article{ ISI:000257167100003,
Author = {Shanker, O.},
Title = {{Sharp dimension transition in a shortcut model}},
Journal = {{JOURNAL OF PHYSICS A-MATHEMATICAL AND THEORETICAL}},
Year = {{2008}},
Volume = {{41}},
Number = {{28}},
Month = {{JUL 18}},
Abstract = {{Earlier studies of a parametrized class of models, the shortcut models,
   whose fractal dimension transitions between integer values, indicated
   that the transition occurs infinitely sharply at the parameter value p =
   0, as the system size increases to infinity. In this work, we explain
   the cause as being due to the combinatorially large increase in the
   available number of paths connecting a pair of points as the path length
   is increased.}},
