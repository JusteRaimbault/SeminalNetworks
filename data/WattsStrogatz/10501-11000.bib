@inproceedings{ ISI:000309948000019,
Author = {Punithan, Dharani and Kim, Dong-Kyun and McKay, R. I. (Bob)},
Editor = {{Kim, TH and Adeli, H and Cuzzocrea, A and Arslan, T and Zhang, YC and Ma, JH and Chung, KI and Mariyam, S and Song, XF}},
Title = {{Daisyworld in Two Dimensional Small-World Networks}},
