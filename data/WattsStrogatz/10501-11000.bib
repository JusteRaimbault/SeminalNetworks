@article{ ISI:000463067000032,
Author = {Zhang, Yichao and Wen, Guanghui and Chen, Guanrong and Wang, Jiasheng
   and Xiong, Minmin and Guan, Jihong and Zhou, Shuigeng},
Title = {{Gaming Temporal Networks}},
