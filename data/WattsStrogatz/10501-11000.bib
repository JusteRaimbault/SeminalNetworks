@article{ ISI:000439892100005,
Author = {Ki, Bo-Min and Ryu, Hee Wook and Cho, Kyung-Suk},
Title = {{Extended local similarity analysis (eLSA) reveals unique associations
   between bacterial community structure and odor emission during pig
   carcasses decomposition}},
Journal = {{JOURNAL OF ENVIRONMENTAL SCIENCE AND HEALTH PART A-TOXIC/HAZARDOUS
   SUBSTANCES \& ENVIRONMENTAL ENGINEERING}},
Year = {{2018}},
Volume = {{53}},
Number = {{8}},
Pages = {{718-727}},
Abstract = {{Soil burial and composting methods have been widely used for the
   disposal of pig carcasses. The relationship between bacterial community
   structure and odor emission was examined using extended local similarity
   analysis (eLSA) during the degradation of pig carcasses in soil and
   compost. In soil, Hyphomicrobium, Niastella, Rhodanobacter, Polaromonas,
   Dokdonella and Mesorhizobium were associated with the emission of
   sulfur-containing odors such as hydrogen sulfide, methyl mercaptan and
   dimethyl disulfide. Sphingomonas, Rhodanobacter, Mesorhizobium,
   Dokdonella, Leucobacter and Truepera were associated with the emission
   of nitrogen-containing odors including ammonia and trimetylamine. In
   compost, however, Carnobacteriaceae, Lachnospiaceae and Clostridiales
   were highly correlated with the emission of sulfur-containing odors,
   while Rumincoccaceae was associated with the emission of
   nitrogen-containing odors. The emission of organic acids was closely
   related to Massilia, Sphaerobacter and Bradyrhizobiaceae in soil, but to
