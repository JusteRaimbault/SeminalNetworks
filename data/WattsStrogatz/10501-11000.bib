@article{ ISI:000438907600077,
Author = {Pandey, Om Jee and Mahajan, Akshay and Hegde, Rajesh Mahanand},
Title = {{Joint Localization and Data Gathering Over a Small-World WSN With
   Optimal Data MULE Allocation}},
Journal = {{IEEE TRANSACTIONS ON VEHICULAR TECHNOLOGY}},
