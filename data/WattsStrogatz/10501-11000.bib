@article{ ISI:000412512800018,
Author = {Thomas, Albert and Menassa, Carol C. and Kamat, Vineet R.},
Title = {{Lightweight and adaptive building simulation (LABS) framework for
   integrated building energy and thermal comfort analysis}},
