@article{ ISI:000414340200006,
Author = {Wu, Jiacheng and Crawford, Forrest W. and Raag, Mait and Heimer, Robert
   and Uuskula, Anneli},
Title = {{Using data from respondent-driven sampling studies to estimate the
   number of people who inject drugs: Application to the Kohtla-Jarve
   region of Estonia}},
Journal = {{PLOS ONE}},
Year = {{2017}},
Volume = {{12}},
Number = {{11}},
Month = {{NOV 2}},
Abstract = {{Estimating the size of key risk populations is essential for determining
   the resources needed to implement effective public health intervention
   programs. Several standard methods for population size estimation exist,
   but the statistical and practical assumptions required for their use may
   not be met when applied to HIV risk groups. We apply three approaches to
   estimate the number of people who inject drugs (PWID) in the
   Kohtla-Jarve region of Estonia using data from a respondent-driven
   sampling (RDS) study: the standard ``multiplier{''} estimate gives 654
   people (95\% CI 509-804), the ``successive sampling{''} method gives
   estimates between 600 and 2500 people, and a network-based estimate that
   uses the RDS recruitment chain gives between 700 and 2800 people. We
   critically assess the strengths and weaknesses of these statistical
   approaches for estimating the size of hidden or hard-to-reach HIV risk
   groups.}},
Publisher = {{PUBLIC LIBRARY SCIENCE}},
Address = {{1160 BATTERY STREET, STE 100, SAN FRANCISCO, CA 94111 USA}},
Type = {{Article}},
