@article{ ISI:000407840500015,
Author = {Jiang, Weixiong and Shi, Feng and Liao, Jian and Liu, Huasheng and Wang,
   Tao and Shen, Celina and Shen, Hui and Hu, Dewen and Wang, Wei and Shen,
   Dinggang},
Title = {{Disrupted functional connectome in antisocial personality disorder}},
Journal = {{BRAIN IMAGING AND BEHAVIOR}},
