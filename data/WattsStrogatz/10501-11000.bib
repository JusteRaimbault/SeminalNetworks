@article{ ISI:000412491800012,
Author = {Gong, Maoguo and Wang, Yixing and Wang, Shanfeng and Liu, Wenfeng},
Title = {{Enhancing robustness of interdependent network under recovery based on a
   two-layer-protection strategy}},
Journal = {{SCIENTIFIC REPORTS}},
Year = {{2017}},
Volume = {{7}},
Month = {{OCT 6}},
Abstract = {{The robustness of coupled networks has attracted great attention
   recently, because the spread of failures from one network to its coupled
   network makes the coupled networks more vulnerable. Most existing
   achievements mainly focused on the integrity properties of coupled
   networks. However, failures also exist when networks are being
   reconstructed. Moreover, existing node-protection methods which aim to
   enhance the robustness of coupled networks only protect the influential
   nodes in one layer. In this paper, firstly, a two-layer-protection
   strategy is proposed to enhance the robustness of coupled networks under
   their reconstruction. Secondly, we adopt five strategies based on
   different centralities to select influential nodes, and propose a
   two-layer vision for each of them. Lastly, experiments on three
   different coupled networks show that by applying the
   two-layer-protection strategy, the robustness of coupled networks can be
   enhanced more efficiently compared with other methods which only protect
   nodes in one layer.}},
