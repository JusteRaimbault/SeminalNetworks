Author = {Chen, Xiaodan and Liao, Xuhong and Dai, Zhengjia and Lin, Qixiang and
   Wang, Zhiqun and Li, Kuncheng and He, Yong},
Title = {{Topological analyses of functional connectomics: A crucial role of
   global signal removal, brain parcellation, and null models}},
