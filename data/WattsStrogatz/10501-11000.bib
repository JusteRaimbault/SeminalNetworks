@inproceedings{ ISI:000250900900022,
Author = {Sudeikat, Jan and Renz, Wolfgang},
Editor = {{Burkhard, HD and Lindemann, G and Verbrugge, R and Varga, LZ}},
Title = {{On complex networks in software: How agent-orientation effects software
   structures}},
