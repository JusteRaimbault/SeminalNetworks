@article{ ISI:000436350700005,
Author = {Xie, Yu and Gong, Maoguo and Wang, Shanfeng and Yu, Bin},
Title = {{Community discovery in networks with deep sparse filtering}},
Journal = {{PATTERN RECOGNITION}},
Year = {{2018}},
