@article{ ISI:000270354000005,
Author = {Antiqueira, L. and Rodrigues, F. A. and Costa, L. da F.},
Title = {{Modeling connectivity in terms of network activity}},
Journal = {{JOURNAL OF STATISTICAL MECHANICS-THEORY AND EXPERIMENT}},
Year = {{2009}},
Month = {{SEP}},
Abstract = {{A new complex network model is proposed which is founded on growth, with
   new connections being established proportionally to the current
   dynamical activity of each node, which can be understood as a
   generalization of the Barabasi-Albert static model. By using several
   topological measurements, as well as optimal multivariate methods
   (canonical analysis and maximum likelihood decision), we show that this
   new model provides, among several other theoretical kinds of networks
   including Watts-Strogatz small-world networks, the greatest
   compatibility with three real-world cortical networks.}},
