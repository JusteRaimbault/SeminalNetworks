@article{ ISI:000251997500027,
Author = {Hui, P. M. and Xu, Chen and Zheng, Da-Fang},
Title = {{Cooperation in evolutionary snowdrift game: Networking effects}},
Journal = {{INTERNATIONAL JOURNAL OF MODERN PHYSICS B}},
Year = {{2007}},
