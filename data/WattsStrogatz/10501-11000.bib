@article{ ISI:000280115100111,
Author = {Windt, K. and Huett, M. -T.},
Title = {{Graph coloring dynamics: A simple model scenario for distributed
   decisions in production logistics}},
Journal = {{CIRP ANNALS-MANUFACTURING TECHNOLOGY}},
Year = {{2010}},
Volume = {{59}},
Number = {{1}},
Pages = {{461-464}},
Note = {{60th General Assembly of CIRP, Pisa, ITALY, AUG 22-28, 2010}},
Abstract = {{Graph coloring is the process of placing colors onto and modifying the
   color of nodes of a network until no neighbors share the same color. It
   is a powerful tool for analyzing coloring strategies that could be
   mapped to job shop scheduling strategies. We analyze how the performance
   of solving temporal conflict graphs depends on strategy and problem
   complexity. We observe for example under strategic waiting of nodes,
   some network features that can be exploited to better solve conflicts on
   graphs. We show how these features can be used in the design of robust
   production networks. (C) 2010 CIRP.}},
