@article{ ISI:000417054600001,
Author = {Chami, Goylette F. and Kontoleon, Andreas A. and Bulte, Erwin and
   Fenwick, Alan and Kabatereine, Narcis B. and Tukahebwa, Edridah M. and
   Dunne, David W.},
Title = {{Diffusion of treatment in social networks and mass drug administration}},
Journal = {{NATURE COMMUNICATIONS}},
Year = {{2017}},
Volume = {{8}},
Month = {{DEC 5}},
Abstract = {{Information, behaviors, and technologies spread when people interact.
   Understanding these interactions is critical for achieving the greatest
   diffusion of public interventions. Yet, little is known about the
   performance of starting points (seed nodes) for diffusion. We track
   routine mass drug administration-the large-scale distribution of
   deworming drugs-in Uganda. We observe friendship networks, socioeconomic
   factors, and treatment delivery outcomes for 16,357 individuals in 3491
   households of 17 rural villages. Each village has two community medicine
   distributors (CMDs), who are the seed nodes and responsible for
   administering treatments. Here, we show that CMDs with tightly knit
   (clustered) friendship connections achieve the greatest reach and speed
   of treatment coverage. Importantly, we demonstrate that clustering
   predicts diffusion through social networks when spreading relies on
   contact with seed nodes while centrality is unrelated to diffusion.
   Clustering should be considered when selecting seed nodes for
   large-scale treatment campaigns.}},
