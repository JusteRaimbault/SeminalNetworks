@article{ ISI:000427415600004,
Author = {Rguibi, Mohamed Amine and Moussa, Najem},
Title = {{The impact of hesitation on passive worm spreading in P2P networks}},
Journal = {{PEER-TO-PEER NETWORKING AND APPLICATIONS}},
