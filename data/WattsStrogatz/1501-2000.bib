@article{ ISI:000176552500128,
Author = {Moukarzel, CF and de Menezes, MA},
Title = {{Shortest paths on systems with power-law distributed long-range
   connections}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2002}},
Volume = {{65}},
Number = {{5, 2}},
Month = {{MAY}},
Abstract = {{We discuss shortest-path lengths l(l)(r) on periodic rings of size L
   supplemented with an average of pL randomly located long-range links
   whose lengths are distributed according to P(l)similar tol(-mu). Using
   resealing arguments and numerical simulation on systems of up to 10(7)
   sites, we show that a characteristic length exists such that l(r)similar
   tor for r<xi but l(r)similar to r(theta s(mu)) for r>>xi. For small p we
   find that the shortest-path length satisfies the scaling relation
   l(r,mu,p)/xi = f(mu,r/xi). Three,regions with different asymptotic
   behaviors are found, respectively: (a) mu > 2 where theta(s) = 1, (b) 1
   < mu < 2, where 0 < theta(s)(mu) < 1/2, and (c) mu < 1 where l(r)
   behaves logarithmically, i.e., theta(s) = 0. The characteristic length
   xi is of the form xi similar to p(-nu) with nu = 1/(2 - mu) in region
   (b), but depends on L as well in region (c). A directed model of
   shortest paths is solved and compared with numerical results.}},
