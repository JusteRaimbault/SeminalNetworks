@article{ ISI:000374199900008,
Author = {Jacoby, David M. P. and Freeman, Robin},
Title = {{Emerging Network-Based Tools in Movement Ecology}},
Journal = {{TRENDS IN ECOLOGY \& EVOLUTION}},
