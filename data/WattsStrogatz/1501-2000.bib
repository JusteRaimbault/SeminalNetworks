@article{ ISI:000232931200018,
Author = {McDonald, M and Suleman, O and Williams, S and Howison, S and Johnson,
   NF},
Title = {{Detecting a currency's dominance or dependence using foreign exchange
   network trees}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2005}},
Volume = {{72}},
Number = {{4, 2}},
Month = {{OCT}},
Abstract = {{In a system containing a large number of interacting stochastic
   processes, there will typically be many nonzero correlation
   coefficients. This makes it difficult to either visualize the system's
   interdependencies, or identify its dominant elements. Such a situation
   arises in foreign exchange (FX), which is the world's biggest market.
   Here we develop a network analysis of these correlations using minimum
   spanning trees (MSTs). We show that not only do the MSTs provide a
   meaningful representation of the global FX dynamics, but they also
   enable one to determine momentarily dominant and dependent currencies.
   We find that information about a country's geographical ties emerges
   from the raw exchange-rate data. Most importantly from a trading
   perspective, we discuss how to infer which currencies are ``in play{''}
   during a particular period of time.}},
