@article{ ISI:000240870300026,
Author = {Frasca, Mattia and Buscarino, Arturo and Rizzo, Alessandro and Fortuna,
   Luigi and Boccaletti, Stefano},
Title = {{Dynamical network model of infective mobile agents}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2006}},
Volume = {{74}},
Number = {{3, 2}},
Month = {{SEP}},
Abstract = {{A dynamical network (consisting of a time-evolving wiring of
   interactions among a group of random walkers) is introduced to model the
   spread of an infectious disease in a population of mobile individuals.
   We investigate the main properties of this model, and show that peculiar
   features arise when individuals are allowed to perform long-distance
   jumps. Such peculiarities are captured and conveniently quantified by a
   series of appropriate parameters able to highlight the structural
   differences emerging in the networks when long-distance jumps are
   combined with local random walk processes.}},
