@article{ ISI:000300131600002,
Author = {Daraganova, Galina and Pattison, Pip and Koskinen, Johan and Mitchell,
   Bill and Bill, Anthea and Watts, Martin and Baum, Scott},
Title = {{Networks and geography: Modelling community network structures as the
   outcome of both spatial and network processes}},
Journal = {{SOCIAL NETWORKS}},
Year = {{2012}},
Volume = {{34}},
Number = {{1, SI}},
Pages = {{6-17}},
Month = {{JAN}},
Abstract = {{This paper focuses on how to extend the exponential random graph models
   to take into account the geographical embeddedness of individuals in
   modelling social networks. We develop a hierarchical set of nested
   models for spatially embedded social networks, in which, following Butts
   (2002), an interaction function between tie probability and Euclidean
   distance between nodes is introduced. The models are illustrated by an
   empirical example from a study of the role of social networks in
   understanding spatial clustering in unemployment in Australia. The
   analysis suggests that a spatial effect cannot solely explain the
   emergence of organised network structure and it is necessary to include
   both spatial and endogenous network effects in the model. (C) 2010
   Elsevier B.V. All rights reserved.}},
