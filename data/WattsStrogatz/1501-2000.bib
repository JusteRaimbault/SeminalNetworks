@article{ ISI:000256126600037,
Author = {Zheng, Yan Hong and Lu, Qi Shao},
Title = {{Spatiotemporal patterns and chaotic burst synchronization in a
   small-world neuronal network}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2008}},
Volume = {{387}},
Number = {{14}},
Pages = {{3719-3728}},
Month = {{JUN 1}},
Abstract = {{The spatiotemporal patterns and chaotic burst synchronization of a
   small-world neuronal network are studied in this paper. The
   synchronization parameter, similarity parameter and order parameter are
   introduced to investigate the dynamics behaviour of the neurons. Chaotic
   burst synchronization and nearly complete synchronization can be
   observed if the link probability and the coupling strength are large
   enough. It is found that with increasing link probability and the
   coupling strength chaotic bursts become appreciably synchronous in space
   and coherent in time, and the maximal spatiotemporal order appears at
   some particular values of the probability and the coupling strength,
   respectively. The larger the size of the network, the smaller the
   probability and the coupling strength are needed for the network to
   achieve burst synchronization. Moreover, the bursting activity and the
   spatiotemporal patterns are robust to small noise. (C) 2008 Elsevier
   B.V. All rights reserved.}},
