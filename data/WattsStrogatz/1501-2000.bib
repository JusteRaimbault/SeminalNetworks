@article{ ISI:000248552600001,
Author = {Sinha, Sitabhra and Saramaki, Jari and Kaski, Kimmo},
Title = {{Emergence of self-sustained patterns in small-world excitable media}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2007}},
Volume = {{76}},
Number = {{1, 2}},
Month = {{JUL}},
Abstract = {{Motivated by recent observations that long-range connections (LRCs) play
   a role in various brain phenomena, we have observed two distinct
   dynamical transitions in the activity of excitable media where waves
   propagate both between neighboring regions and through LRCs. When the
   LRC density p is low, single or multiple spiral waves are seen to emerge
   and cover the entire system. This state is self-sustaining and robust
   against perturbations. At p=p(c)(l), the spirals are suppressed, and
   there is a transition to a spatially homogeneous, temporally periodic
   state. Finally, above p=p(c)(u), activity ceases after a brief
   transient.}},
