@article{ ISI:000295297000023,
Author = {Namaki, A. and Shirazi, A. H. and Raei, R. and Jafari, G. R.},
Title = {{Network analysis of a financial market based on genuine correlation and
   threshold method}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2011}},
Volume = {{390}},
Number = {{21-22}},
Pages = {{3835-3841}},
Month = {{OCT 15}},
Abstract = {{A financial market is an example of an adaptive complex network
   consisting of many interacting units. This network reflects market's
   behavior. In this paper, we use Random Matrix Theory (RMT) notion for
   specifying the largest eigenvector of correlation matrix as the market
   mode of stock network. For a better risk management, we clean the
   correlation matrix by removing the market mode from data and then
   construct this matrix based on the residuals. We show that this
   technique has an important effect on correlation coefficient
   distribution by applying it for Dow Jones Industrial Average (DJIA). To
   study the topological structure of a network we apply the removing
   market mode technique and the threshold method to Tehran Stock Exchange
   (TSE) as an example. We show that this network follows a power-law model
   in certain intervals. We also show the behavior of clustering
   coefficients and component numbers of this network for different
   thresholds. These outputs are useful for both theoretical and practical
   purposes such as asset allocation and risk management. (C) 2011 Elsevier
   B.V. All rights reserved.}},
