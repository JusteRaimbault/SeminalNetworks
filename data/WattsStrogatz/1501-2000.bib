@article{ ISI:000304878400016,
Author = {Xia, Chengyi and Wang, Li and Sun, Shiwen and Wang, Juan},
Title = {{An SIR model with infection delay and propagation vector in complex
   networks}},
Journal = {{NONLINEAR DYNAMICS}},
Year = {{2012}},
Volume = {{69}},
Number = {{3}},
Pages = {{927-934}},
Month = {{AUG}},
Abstract = {{Based on the SIR (Susceptible-Infected-Removed) model, we propose a
   novel epidemic model to investigate the impact of infection delay and
   propagation vector on the spreading behaviors in complex networks.
   Mean-field approximations and extensive numerical simulations indicate
   that the infection delay and propagation vector can largely reduce the
   critical threshold and promote the outbreak of epidemics, and even lead
   to the case that the infectious diseases transform from the disease-free
   state to endemic one. The current results are greatly instructive for us
   to further understand the epidemic spreading and design some effective
   prevention and containment strategies to fight the epidemics.}},
