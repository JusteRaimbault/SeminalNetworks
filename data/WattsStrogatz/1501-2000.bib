@article{ ISI:000382626800045,
Author = {Guzman, Segundo Jose and Schloegl, Alois and Frotscher, Michael and
   Jonas, Peter},
Title = {{Synaptic mechanisms of pattern completion in the hippocampal CA3 network}},
Journal = {{SCIENCE}},
Year = {{2016}},
Volume = {{353}},
Number = {{6304}},
Pages = {{1117-1123}},
Month = {{SEP 9}},
Abstract = {{The hippocampal CA3 region plays a key role in learning and memory.
   Recurrent CA3-CA3 synapses are thought to be the subcellular substrate
   of pattern completion. However, the synaptic mechanisms of this network
   computation remain enigmatic. To investigate these mechanisms, we
   combined functional connectivity analysis with network modeling.
   Simultaneous recording from up to eight CA3 pyramidal neurons revealed
   that connectivity was sparse, spatially uniform, and highly enriched in
   disynaptic motifs (reciprocal, convergence, divergence, and chain
   motifs). Unitary connections were composed of one or two synaptic
   contacts, suggesting efficient use of postsynaptic space. Real-size
   modeling indicated that CA3 networks with sparse connectivity,
   disynaptic motifs, and single-contact connections robustly generated
   pattern completion. Thus, macro-and microconnectivity contribute to
   efficient memory storage and retrieval in hippocampal networks.}},
