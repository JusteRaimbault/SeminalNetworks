@article{ ISI:000209845900009,
Author = {Xu, Qichao and Su, Zhou and Zhang, Kuan and Ren, Pinyi and Shen, Xuemin
   (Sherman)},
Title = {{Epidemic Information Dissemination in Mobile Social Networks With
   Opportunistic Links}},
Journal = {{IEEE TRANSACTIONS ON EMERGING TOPICS IN COMPUTING}},
