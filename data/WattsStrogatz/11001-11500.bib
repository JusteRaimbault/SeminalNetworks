@article{ ISI:000365371200006,
Author = {Wu, Jie and Sun, Yong-zheng and Zhao, Dong-hua},
Title = {{FINITE-TIME ADAPTIVE OUTER SYNCHRONIZATION BETWEEN TWO COMPLEX DYNAMICAL
   NETWORKS WITH NONIDENTICAL TOPOLOGICAL STRUCTURES}},
Journal = {{KYBERNETIKA}},
Year = {{2015}},
Volume = {{51}},
Number = {{4}},
Pages = {{655-666}},
Abstract = {{In this paper, we investigate the finite-time adaptive outer
   synchronization between two complex dynamical networks with nonidentical
   topological structures. We propose new adaptive controllers, with which
   we can synchronize two complex dynamical networks within finite time.
   Sufficient conditions for the finite-time adaptive outer synchronization
   are derived based on the finite-time stability theory. Finally,
   numerical examples are examined to demonstrate the effectiveness and
   feasibility of the theoretical results.}},
