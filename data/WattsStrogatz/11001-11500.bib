@article{ ISI:000381265400010,
Author = {Wang, YingFei and Wu, XiaoQun and Feng, Hui and Lu, JunAn and Lu, JinHu},
Title = {{Topology inference of uncertain complex dynamical networks and its
   applications in hidden nodes detection}},
