@article{ ISI:000403817900035,
Author = {Luo, Wuqiong and Tay, Wee Peng and Leng, Mei},
Title = {{On the Universality of Jordan Centers for Estimating Infection Sources
   in Tree Networks}},
Journal = {{IEEE TRANSACTIONS ON INFORMATION THEORY}},
Year = {{2017}},
Volume = {{63}},
