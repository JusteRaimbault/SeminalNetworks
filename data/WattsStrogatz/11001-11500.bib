@article{ ISI:000438357700012,
Author = {Mestrovic, Ana and Grubisa, Zoran},
Title = {{PRELIMINARY ANALYSIS OF CO-AUTHORSHIP NETWORKS AT THE UNIVERSITY OF
   RIJEKA}},
