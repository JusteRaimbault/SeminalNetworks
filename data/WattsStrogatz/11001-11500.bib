@article{ ISI:000368357800015,
Author = {Schaefer, Hendrik and Schaefer, Tim and Ackermann, Joerg and Dichter,
   Norbert and Doering, Claudia and Hartmann, Sylvia and Hansmann,
   Martin-Leo and Koch, Ina},
Title = {{CD30 cell graphs of Hodgkin lymphoma are not scale-free-an image
   analysis approach}},
