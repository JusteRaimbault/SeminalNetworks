@article{ ISI:000349100300004,
Author = {Teyhen, Deydre S. and Aldag, Matt and Centola, Damon and Edinborough,
   Elton and Ghannadian, Jason D. and Haught, Andrea and Jackson, Theresa
   and Kinn, Julie and Kunkler, Kevin J. and Levine, Betty and Martindale,
