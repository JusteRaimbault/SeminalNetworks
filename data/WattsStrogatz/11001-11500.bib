@article{ ISI:000356795300001,
Author = {Li, Longjie and Qian, Lvjian and Wang, Xiaoping and Luo, Shishun and
   Chen, Xiaoyun},
Title = {{Accurate similarity index based on activity and connectivity of node for
   link prediction}},
Journal = {{INTERNATIONAL JOURNAL OF MODERN PHYSICS B}},
Year = {{2015}},
Volume = {{29}},
Number = {{17}},
Month = {{JUL 10}},
Abstract = {{Recent years have witnessed the increasing of available network data;
   however, much of those data is incomplete. Link prediction, which can
   find the missing links of a network, plays an important role in the
   research and analysis of complex networks. Based on the assumption that
   two unconnected nodes which are highly similar are very likely to have
   an interaction, most of the existing algorithms solve the link
   prediction problem by computing nodes' similarities. The fundamental
   requirement of those algorithms is accurate and effective similarity
   indices. In this paper, we propose a new similarity index, namely
   similarity based on activity and connectivity (SAC), which performs link
   prediction more accurately. To compute the similarity between two nodes,
   this index employs the average activity of these two nodes in their
   common neighborhood and the connectivities between them and their common
   neighbors. The higher the average activity is and the stronger the
   connectivities are, the more similar the two nodes are. The proposed
   index not only commendably distinguishes the contributions of paths but
   also incorporates the influence of endpoints. Therefore, it can achieve
   a better predicting result. To verify the performance of SAC, we conduct
   experiments on 10 real-world networks. Experimental results demonstrate
   that SAC outperforms the compared baselines.}},
