@inproceedings{ ISI:000345507300004,
Author = {Cheng, Yichao and An, Hong and Chen, Zhitao and Li, Feng and Wang,
   Zhaohui and Jiang, Xia and Peng, Yi},
Editor = {{Sun, XH and Qu, W and Stojmenovic, I and Zhou, W and Li, Z and Guo, H and Min, G and Yang, T and Wu, Y and Liu, L}},
Title = {{Understanding the SIMD Efficiency of Graph Traversal on GPU}},
Booktitle = {{ALGORITHMS AND ARCHITECTURES FOR PARALLEL PROCESSING, ICA3PP 2014, PT I}},
Series = {{Lecture Notes in Computer Science}},
Year = {{2014}},
Volume = {{8630}},
Pages = {{42-56}},
Note = {{14th International Conference on Algorithms and Architectures for
   Parallel Processing (ICA3PP), Dalian, PEOPLES R CHINA, AUG 24-27, 2014}},
Abstract = {{Graph is a widely used data structure and graph algorithms, such as
   breadth-first search (BFS), are regarded as key components in a great
   number of applications. Recent studies have attempted to accelerate
   graph algorithms on highly parallel graphics processing unit (GPU).
   Although many graph algorithms based on large graphs exhibit abundant
   parallelism, their performance on GPU still faces formidable challenges,
   one of which is to map the irregular computation onto GPU's vectorized
   execution model.
   In this paper, we investigate the link between graph topology and
   performance of BFS on GPU. We introduce a novel model to analyze the
   components of SIMD underutilization. We show that SIMD lanes are wasted
   either due to the workload imbalance between tasks, or to the
   heterogeneity of each task. We also develop corresponding metrics to
   quantify the SIMD efficiency for BFS on GPU. Finally, we demonstrate the
   applicability of the metrics by using them to profile the performance
   for different mapping strategies.}},
