@inproceedings{ ISI:000389517400001,
Author = {Fernandez, Maribel and Kirchner, Helene and Pinaud, Bruno and Vallet,
   Jason},
Editor = {{Lucanu, D}},
Title = {{Labelled Graph Rewriting Meets Social Networks}},
Booktitle = {{REWRITING LOGIC AND ITS APPLICATIONS, WRLA 2016}},
