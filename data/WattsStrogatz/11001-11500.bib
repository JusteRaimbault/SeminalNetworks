@inproceedings{ ISI:000377887000141,
Author = {Xu, He and Kroupi, Eleni and Ebrahimi, Touradj},
Book-Group-Author = {{IEEE}},
Title = {{Functional Connectivity from EEG Signals during Perceiving Pleasant and
   Unpleasant Odors}},
Booktitle = {{2015 INTERNATIONAL CONFERENCE ON AFFECTIVE COMPUTING AND INTELLIGENT
   INTERACTION (ACII)}},
Series = {{International Conference on Affective Computing and Intelligent
   Interaction}},
Year = {{2015}},
Pages = {{911-916}},
Note = {{International Conference on Affective Computing and Intelligent
   Interaction (ACII), Xian, PEOPLES R CHINA, SEP 21-24, 2015}},
Abstract = {{The olfactory sense is strongly related to memory and emotional
   processes. Studies on the effects of odor perception from brain activity
   have been conducted by using different neuroimaging techniques. In this
   paper, we analyse electroencephalography (EEG) of 23 subjects during
   perceiving pleasant and unpleasant odor stimuli. We describe the
   construction of brain functional connectivity networks measured by most
   commonly used models. We discuss the network-based features of
   functional connectivity, and design classifiers by applying different
   functional connectivity network features. Finally, we show that pleasant
   and unpleasant emotions from olfactory perceptions can be better
   classified if we see the brain as a nonlinear small-world network. By
   extracting appropriate features from functional connectivity networks,
   we manage to classify pleasant and unpleasant olfactory perceptions with
   an average Kappa value of 0.11 +/- 0.17, which is significantly
   non-random.}},
