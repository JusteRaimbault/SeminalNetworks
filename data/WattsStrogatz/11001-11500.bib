@article{ ISI:000382164900004,
Author = {Latouche, Pierre and Robin, Stephane},
Title = {{Variational Bayes model averaging for graphon functions and motif
   frequencies inference in W-graph models}},
Journal = {{STATISTICS AND COMPUTING}},
Year = {{2016}},
Volume = {{26}},
Number = {{6}},
Pages = {{1173-1185}},
Month = {{NOV}},
Abstract = {{W-graph refers to a general class of random graph models that can be
   seen as a random graph limit. It is characterized by both its graphon
   function and its motif frequencies. In this paper, relying on an
   existing variational Bayes algorithm for the stochastic block models
   (SBMs) along with the corresponding weights for model averaging, we
   derive an estimate of the graphon function as an average of SBMs with
   increasing number of blocks. In the same framework, we derive the
   variational posterior frequency of any motif. A simulation study and an
   illustration on a social network complete our work.}},
