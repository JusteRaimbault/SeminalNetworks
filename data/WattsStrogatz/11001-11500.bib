@article{ ISI:000378963700019,
Author = {Gilboa-Freedman, Gail and Hassin, Refael},
Title = {{When Markov chains meet: A continuous-time model of network evolution}},
Journal = {{STATISTICS \& PROBABILITY LETTERS}},
Year = {{2016}},
Volume = {{116}},
Pages = {{131-138}},
Month = {{SEP}},
Abstract = {{We suggest a novel approach to model continuous time processes of the
   interactions of independent elements. The model assumes a finite number
   of independent Markov chains, each representing an element. Chains move
   among a common space of states. Sometimes chains intersect, being in the
   same state at the same time. These intersections relate the chains with
   each other and imply many interesting processes.
   In this paper, we examine our new approach in the context of network
   evolution. Our analytic study achieves a closed solution for the
   expected time until a node has any specific degree. Our numerical study
   demonstrates properties which are in agreement with real world networks.
   Thus we show the potential of our approach. (C) 2016 Elsevier B.V. All
   rights reserved.}},
