@inproceedings{ ISI:000380541000003,
Author = {Chang, Biao and Zhu, Feida and Chen, Enhong and Liu, Qi},
Editor = {{Aggarwal, C and Zhou, ZH and Tuzhilin, A and Xiong, H and Wu, X}},
Title = {{Information Source Detection via Maximum A Posteriori Estimation}},
