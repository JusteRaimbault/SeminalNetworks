@article{ ISI:000381220500001,
Author = {Fink, Clay and Schmidt, Aurora and Barash, Vladimir and Cameron,
   Christopher and Macy, Michael},
Title = {{Complex contagions and the diffusion of popular Twitter hashtags in
   Nigeria}},
