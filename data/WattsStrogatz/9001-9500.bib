@inproceedings{ ISI:000416784200052,
Author = {Di Nardo, A. and Di Natale, M. and Giudicianni, C. and Musmarra, D. and
   Rodriguez Varela, J. M. and Santonastaso, G. F. and Simone, A. and
   Tzatchkov, V.},
