@article{ ISI:000328328000003,
Author = {Daskalaki, Eleni and Papadopoulos, Gerassimos A. and Spiliotis,
   Konstantinos and Siettos, Constantinos},
Title = {{Analysing the topology of seismicity in the Hellenic arc using complex
   networks}},
Journal = {{JOURNAL OF SEISMOLOGY}},
Year = {{2014}},
Volume = {{18}},
Number = {{1}},
Pages = {{37-46}},
Month = {{JAN}},
Abstract = {{Based on the theory of complex networks, we quantify for the first time
   correlations between seismic events occurring in the Hellenic arc and
   Trench system, which is the most seismogenic structure in the
   European-Mediterranean region. We examine how relatively strong events
   with their aftershock sequences trigger phase transitions of the
   underlying network topology of seismic activity from random to
   scale-free structures. In particular, we show that the network is
   characterized by a highly clustered spatial structure giving rise to
   heterogeneous networks exhibiting enhanced small-world attributes.}},
