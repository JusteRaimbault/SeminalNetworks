@article{ ISI:000392405300170,
Author = {Yao, Peng and Hou, Bao-Yu and Pan, Yu-Jian and Li, Xiang},
Title = {{Structural Controllability of Temporal Networks with a Single Switching
   Controller}},
Journal = {{PLOS ONE}},
Year = {{2017}},
Volume = {{12}},
Number = {{1}},
Month = {{JAN 20}},
Abstract = {{Temporal network, whose topology evolves with time, is an important
   class of complex networks. Temporal trees of a temporal network describe
   the necessary edges sustaining the network as well as their active time
   points. By a switching controller which properly selects its location
   with time, temporal trees are used to improve the controllability of the
   network. Therefore, more nodes are controlled within the limited time.
   Several switching strategies to efficiently select the location of the
   controller are designed, which are verified with synthetic and empirical
   temporal networks to achieve better control performance.}},
