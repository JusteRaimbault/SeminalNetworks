@article{ ISI:000382830200010,
Author = {Zhang, Cheng-Jun and Zeng, An},
Title = {{Prediction of missing links and reconstruction of complex networks}},
Journal = {{INTERNATIONAL JOURNAL OF MODERN PHYSICS C}},
Year = {{2016}},
Volume = {{27}},
Number = {{10}},
Month = {{OCT}},
Abstract = {{Predicting missing links in complex networks is of great significance
   from both theoretical and practical point of view, which not only helps
   us understand the evolution of real systems but also relates to many
   applications in social, biological and online systems. In this paper, we
   study the features of different simple link prediction methods,
   revealing that they may lead to the distortion of networks' structural
   and dynamical properties. Moreover, we find that high prediction
   accuracy is not definitely corresponding to a high performance in
   preserving the network properties when using link prediction methods to
   reconstruct networks. Our work highlights the importance of considering
   the feedback effect of the link prediction methods on network properties
   when designing the algorithms.}},
