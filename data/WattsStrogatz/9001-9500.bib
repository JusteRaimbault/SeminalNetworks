@article{ ISI:000332401200023,
Author = {Schimit, P. H. T.},
Title = {{On exploring the genetic algorithm for modeling the evolution of
   cooperation in a population}},
Journal = {{COMMUNICATIONS IN NONLINEAR SCIENCE AND NUMERICAL SIMULATION}},
Year = {{2014}},
Volume = {{19}},
Number = {{8}},
Pages = {{2801-2810}},
Month = {{AUG}},
Abstract = {{In this paper, we propose a genetic algorithm approximation for modeling
   a population which individuals compete with each other based on
   prisoner's dilemma game. Players act according to their genome, which
   gives them a strategy (phenotype); in our case, a probability for
   cooperation. The most successful players will produce more offspring and
   that depends directly of the strategy adopted. As individuals die, the
   newborns parents will be those fittest individuals in a certain spatial
   region. Four different fitness functions are tested to investigate the
   influence in the evolution of cooperation. Individuals live in a lattice
   modeled by probabilistic cellular automata and play the game with some
   of their neighborhoods. In spite of players homogeneously distributed
   over the space, a mean-field approximation is presented in terms of
   ordinary differential equations. (C) 2013 Elsevier B. V. All rights
   reserved.}},
