Title = {{Band-specific atypical functional connectivity pattern in childhood
   autism spectrum disorder}},
Journal = {{CLINICAL NEUROPHYSIOLOGY}},
Year = {{2017}},
Volume = {{128}},
Number = {{8}},
Pages = {{1457-1465}},
Month = {{AUG}},
