@inproceedings{ ISI:000388373802178,
Author = {Zeng, Ming and Zhao, Mingyuan and Meng, Qinghao and Wang, Jiaying},
Book-Group-Author = {{IEEE}},
Title = {{Community Structure Detection in Complex Networks for Characterizing
   Atmospheric Boundary-Layer Wind Speed Time Series}},
Booktitle = {{PROCEEDINGS OF THE 2016 12TH WORLD CONGRESS ON INTELLIGENT CONTROL AND
   AUTOMATION (WCICA)}},
Year = {{2016}},
Pages = {{2660-2665}},
Note = {{12th World Congress on Intelligent Control and Automation (WCICA),
   Guilin, PEOPLES R CHINA, JUN 12-15, 2016}},
Organization = {{IEEE}},
Abstract = {{Better understanding of boundary-layer wind field is fundamental for
   modeling of various wind-related phenomena, e.g., gas dispersal and
   energy generation. In this paper, we first record three groups of the
   near surface wind speed time series in different locations using a
   high-resolution 3D ultrasonic anemometer. Then, we map these
   experimental time series into complex networks and detect the
   corresponding communities based on the modularity analysis. Next, we
   extract two network characteristics, e.g., clustering coefficient and
   average shortest path. Finally, we find that although the wind speed
   time series are recorded at different locations, the fluctuation
   behaviors exhibit the similar community structures. This interesting
   finding suggests that network community structure analysis allows better
   understanding of the inherent structures of nonlinear and nonstationary
   near-surface wind speed signals.}},
