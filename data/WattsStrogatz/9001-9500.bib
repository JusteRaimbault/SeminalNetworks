@article{ ISI:000400372900006,
Author = {Lai, Longbin and Qin, Lu and Lin, Xuemin and Chang, Lijun},
Title = {{Scalable subgraph enumeration in MapReduce: a cost-oriented approach}},
Journal = {{VLDB JOURNAL}},
Year = {{2017}},
Volume = {{26}},
Number = {{3}},
