@article{ ISI:000418337400003,
Author = {Kundu, Srilena and Majhi, Soumen and Sasmal, Sourav Kumar and Ghosh,
   Dibakar and Rakshit, Biswambhar},
Title = {{Survivability of a metapopulation under local extinctions}},
Journal = {{PHYSICAL REVIEW E}},
