@article{ ISI:000375705300009,
Author = {Almeida, M. L. and Macedo-Filho, A. and Mendes, G. A. and da Silva, L.
   R. and Albuquerque, E. L. and Fulco, U. L.},
Title = {{Critical properties of the contact process on a scale-free homophilic
   network}},
Journal = {{JOURNAL OF STATISTICAL MECHANICS-THEORY AND EXPERIMENT}},
Year = {{2016}},
Month = {{APR}},
Abstract = {{Using the contact process model within a Monte Carlo numerical
   simulation approach, we mimic an epidemic spreading on a homophilic
   network, a scale-free network displaying a small world effect, to show a
   continuous phase transition to an absorbing-state at a critical
   threshold lambda(c). Since the connection number k of the vertices'
   homophilic network is cumulative, the degree distribution exhibits, for
   a large value of networks, a power-law behavior according to P(k)
   proportional to k-theta, with the distribution exponent theta = 2.84(1).
   A finite-size scaling analysis allows us to characterize the transition
   by a set of critical exponents 1/nu, beta/nu, beta and gamma/nu, whose
   universality class indicates a disagreement with those associated to the
   CP on scale-free networks in terms of the heterogeneous mean-field
   theory.}},
