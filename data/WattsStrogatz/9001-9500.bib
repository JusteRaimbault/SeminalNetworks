@article{ ISI:000408102700049,
Author = {Wright, Nathaniel C. and Hoseini, Mahmood S. and Wessel, Ralf},
Title = {{Adaptation modulates correlated subthreshold response variability in
   visual cortex}},
Journal = {{JOURNAL OF NEUROPHYSIOLOGY}},
