@article{ ISI:000331632100004,
Author = {Gunes, Ismail and Cataltepe, Zehra and Gunduz-Oguducu, Sule},
Title = {{GA-TVRC-Het: genetic algorithm enhanced time varying relational
   classifier for evolving heterogeneous networks}},
Journal = {{DATA MINING AND KNOWLEDGE DISCOVERY}},
