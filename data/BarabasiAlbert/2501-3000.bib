@article{ ISI:000179878200060,
Author = {Moreira, AA and Andrade, JS and Amaral, LAN},
Title = {{Extremum statistics in scale-free network models}},
Journal = {{PHYSICAL REVIEW LETTERS}},
Year = {{2002}},
Volume = {{89}},
Number = {{26}},
Month = {{DEC 23}},
Abstract = {{We investigate the statistics of the most connected node in scale-free
   networks. For a scale-free network model with homogeneous nodes, we show
   by means of extensive simulations that the exponential truncation, due
   to the finite size of the network, of the degree distribution governs
   the scaling of the extreme values and that the distribution of maxima
   follows the Gumbel statistics. For a scale-free network model with
   heterogeneous nodes, we show that scaling no longer holds and that the
   truncation of the degree distribution no longer controls the maxima
   distribution.}},
