@article{ ISI:000242215500016,
Author = {Wang Mao-Sheng and Hou Zhong-Huai and Xin Hou-Wen},
Title = {{Synchronization and coherence resonance in chaotic neural networks}},
Journal = {{CHINESE PHYSICS}},
