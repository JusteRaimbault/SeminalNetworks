@article{ ISI:000348949500019,
Author = {Zhang, J. and Tong, L. and Lamberson, P. J. and Durazo-Arvizu, R. A. and
   Luke, A. and Shoham, D. A.},
Title = {{Leveraging social influence to address overweight and obesity using
   agent-based models: The role of adolescent social networks}},
Journal = {{SOCIAL SCIENCE \& MEDICINE}},
