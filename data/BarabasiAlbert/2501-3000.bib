@article{ ISI:000379163100029,
Author = {Sevilla-Escoboza, R. and Sendina-Nadal, I. and Leyva, I. and Gutierrez,
   R. and Buldu, J. M. and Boccaletti, S.},
Title = {{Inter-layer synchronization in multiplex networks of identical layers}},
Journal = {{CHAOS}},
Year = {{2016}},
Volume = {{26}},
Number = {{6}},
Month = {{JUN}},
Abstract = {{Inter-layer synchronization is a distinctive process of multiplex
   networks whereby each node in a given layer evolves synchronously with
   all its replicas in other layers, irrespective of whether or not it is
   synchronized with the other units of the same layer. We analytically
   derive the necessary conditions for the existence and stability of such
   a state, and verify numerically the analytical predictions in several
   cases where such a state emerges. We further inspect its robustness
   against a progressive de-multiplexing of the network, and provide
   experimental evidence by means of multiplexes of nonlinear electronic
   circuits affected by intrinsic noise and parameter mismatch. Published
   by AIP Publishing.}},
