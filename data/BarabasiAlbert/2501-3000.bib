@article{ ISI:000265543600005,
Author = {Xia, C. Y. and Sun, S. W. and Liu, Z. X. and Chen, Z. Q. and Yuan, Z. Z.},
Title = {{EPIDEMICS OF SIRS MODEL WITH NONUNIFORM TRANSMISSION ON SCALE-FREE
