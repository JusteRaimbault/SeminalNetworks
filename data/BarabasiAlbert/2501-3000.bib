@article{ ISI:000303344600008,
Author = {Jiang, Zhong-Yuan and Liang, Man-Gui},
Title = {{IMPROVED EFFICIENT ROUTING STRATEGY ON SCALE-FREE NETWORKS}},
Journal = {{INTERNATIONAL JOURNAL OF MODERN PHYSICS C}},
Year = {{2012}},
Volume = {{23}},
Number = {{2}},
Month = {{FEB}},
Abstract = {{Since the betweenness of nodes in complex networks can theoretically
   represent the traffic load of nodes under the currently used routing
   strategy, we propose an improved efficient (IE) routing strategy to
   enhance to the network traffic capacity based on the betweenness
   centrality. Any node with the highest betweenness is susceptible to
   traffic congestion. An efficient way to improve the network traffic
   capacity is to redistribute the heavy traffic load from these central
   nodes to non-central nodes, so in this paper, we firstly give a path
   cost function by considering the sum of node betweenness with a tunable
   parameter beta along the actual path. Then, by minimizing the path cost,
   our IE routing strategy achieved obvious improvement on the network
   transport efficiency. Simulations on scale-free Barabasi-Albert (BA)
   networks confirmed the effectiveness of our strategy, when compared with
   the efficient routing (ER) and the shortest path (SP) routing.}},
