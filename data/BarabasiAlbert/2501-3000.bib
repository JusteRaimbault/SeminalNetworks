@article{ ISI:000328179400016,
Author = {Kim, Rakhyun E.},
Title = {{The emergent network structure of the multilateral environmental
   agreement system}},
Journal = {{GLOBAL ENVIRONMENTAL CHANGE-HUMAN AND POLICY DIMENSIONS}},
