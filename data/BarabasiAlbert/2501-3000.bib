@article{ ISI:000315353500005,
Author = {Manfredi, Sabato and Oliviero, Francesco and Romano, Simon Pietro},
Title = {{A Distributed Control Law for Load Balancing in Content Delivery
   Networks}},
Journal = {{IEEE-ACM TRANSACTIONS ON NETWORKING}},
Year = {{2013}},
Volume = {{21}},
Number = {{1}},
Pages = {{55-68}},
Month = {{FEB}},
Abstract = {{In this paper, we face the challenging issue of defining and
   implementing an effective law for load balancing in Content Delivery
   Networks (CDNs). We base our proposal on a formal study of a CDN system,
   carried out through the exploitation of a fluid flow model
   characterization of the network of servers. Starting from such
   characterization, we derive and prove a lemma about the network queues
   equilibrium. This result is then lever-aged in order to devise a novel
   distributed and time-continuous algorithm for load balancing, which is
   also reformulated in a time-discrete version. The discrete formulation
   of the proposed balancing law is eventually discussed in terms of its
   actual implementation in a real-world scenario. Finally, the overall
   approach is validated by means of simulations.}},
