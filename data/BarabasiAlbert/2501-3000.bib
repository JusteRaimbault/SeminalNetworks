@article{ ISI:000245863600010,
Author = {Fahmy, Sonia and Kwon, Minseok},
Title = {{Characterizing overlay multicast networks and their costs}},
Journal = {{IEEE-ACM TRANSACTIONS ON NETWORKING}},
