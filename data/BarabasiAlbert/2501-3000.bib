@article{ ISI:000235097900003,
Author = {Korniss, G and Hastings, MB and Bassler, KE and Berryman, MJ and Kozma,
   B and Abbott, D},
Title = {{Scaling in small-world resistor networks}},
Journal = {{PHYSICS LETTERS A}},
Year = {{2006}},
Volume = {{350}},
Number = {{5-6}},
Pages = {{324-330}},
Month = {{FEB 13}},
Abstract = {{We study the effective resistance of small-world resistor networks.
   Utilizing recent analytic results for the propagator of the
   Edwards-Wilkinson process on small-world networks, we obtain the
   asymptotic behavior of the disorder-averaged two-point resistance in the
   large system-size limit. We find that the small-world structure
   suppresses large network resistances: both the average resistance and
   its standard deviation approaches a finite value in the large
   system-size limit for any non-zero density of random links. We also
   consider a scenario where the link conductance decays as a power of the
   length of the random links, l(-alpha). In this case we find that the
   average effective system resistance diverges for any non-zero value of
   alpha. (c) 2005 Elsevier B.V. All rights reserved.}},
