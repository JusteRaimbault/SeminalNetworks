@article{ ISI:000339348400005,
Author = {Sun, Weigang and Li, Shixing},
Title = {{Generalized outer synchronization between two uncertain dynamical
   networks}},
Journal = {{NONLINEAR DYNAMICS}},
Year = {{2014}},
Volume = {{77}},
Number = {{3}},
Pages = {{481-489}},
Month = {{AUG}},
Abstract = {{This paper investigates generalized outer synchronization between two
   uncertain dynamical networks with a novel feature that the couplings of
   each network are unknown functions. With nonlinear control schemes, two
   sufficient criteria for generalized outer synchronization with or
   without time delay are obtained by Lyapunov stability theory and
   Barbalat's lemma. Our results are valid for many studies of the
   couplings inside each network being linear or nonlinear. Finally,
   numerical simulations are given to verify the effectiveness of the
   control schemes.}},
