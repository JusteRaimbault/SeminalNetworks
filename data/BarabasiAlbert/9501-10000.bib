@inproceedings{ ISI:000426941100150,
Author = {Zeng Linyi and Li Shugang},
Editor = {{Xu, B}},
Title = {{The node influence for link prediction based on triadic closure
   structure}},
Booktitle = {{PROCEEDINGS OF 2017 IEEE 2ND INFORMATION TECHNOLOGY, NETWORKING,
   ELECTRONIC AND AUTOMATION CONTROL CONFERENCE (ITNEC)}},
Year = {{2017}},
Pages = {{761-766}},
Note = {{IEEE 2nd Information Technology, Networking, Electronic and Automation
   Control Conference (ITNEC), Chengdu, PEOPLES R CHINA, DEC 15-17, 2017}},
Organization = {{IEEE PRESS}},
Abstract = {{Efficiently and accurately predicting missing links is becoming a
   concerned topic in the complex network field in recent years. It is
   promising to address this challenging problem according to specific
   structure of the network. Motivated by the Structural Balance Theory
   (Triadic Closure Theory), we propose a comprehensive index to fully
   quantify the common neighbor influence and inter-node influence on
   forming the link between the nodes. Unlike the classical common neighbor
   based prediction methods which just consider the number of common
   neighbors, our new algorithm is highly accuracy and stable robust when
   the inter-node influence on forming the triadic closure is characterized
   by the similarity of common neighbors, and the common neighbor impact on
   developing the triadic closure is comprehensively characterized by the
   common neighbor influence shared by the directly connected nodes and the
   dispersion of common neighbor influence. Consequently, we propose an
   ensemble link prediction algorithm which integrates above three
   indicators. The performance of the proposed model is tested on realistic
   complex networks, and the computational results show that the proposed
   algorithm is highly accurate and robust compared with the current common
   neighbor-based algorithm.}},
