@article{ ISI:000402555000008,
Author = {Jia, Junbo and Jin, Zhen and Chang, Lili and Fu, Xinchu},
Title = {{STRUCTURAL CALCULATIONS AND PROPAGATION MODELING OF GROWING NETWORKS
   BASED ON CONTINUOUS DEGREE}},
Journal = {{MATHEMATICAL BIOSCIENCES AND ENGINEERING}},
