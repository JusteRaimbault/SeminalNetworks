@article{ ISI:000452866000050,
Author = {Ma, Yifang and Uzzi, Brian},
Title = {{Scientific prize network predicts who pushes the boundaries of science}},
Journal = {{PROCEEDINGS OF THE NATIONAL ACADEMY OF SCIENCES OF THE UNITED STATES OF
   AMERICA}},
Year = {{2018}},
Volume = {{115}},
