@article{ ISI:000440693200029,
Author = {Wu, Jiajing and Zhong, Jian and Chen, Zhenhao and Chen, Bokui},
Title = {{Optimal Coupling Patterns in Interconnected Communication Networks}},
Journal = {{IEEE TRANSACTIONS ON CIRCUITS AND SYSTEMS II-EXPRESS BRIEFS}},
Year = {{2018}},
Volume = {{65}},
Number = {{8}},
Pages = {{1109-1113}},
Month = {{AUG}},
Abstract = {{Traffic dynamics on single or isolated complex networks has been
   extensively studied in the past decade. Recently, several coupled
   network models have been developed to describe the interactions between
   real-world networked systems. In interconnected communication networks,
   the coupling links refer to the physical links between networks and
   provide paths for traffic transmission. In this brief, we employ a
   simulated annealing (SA) algorithm to find a near-optimal configuration
   of the coupling links, which effectively improves the overall traffic
   capacity compared with random, assortative, and disassortative
   couplings. Furthermore, we investigate the optimal configuration of
   coupling links given by the SA algorithm and develop a faster method to
   select the coupling links.}},
