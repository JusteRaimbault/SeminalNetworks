@inproceedings{ ISI:000455787300095,
Author = {Eikmeier, Nicole and Gleich, David F.},
Book-Group-Author = {{ACM}},
Title = {{Revisiting Power-law Distributions in Spectra of Real World Networks}},
Booktitle = {{KDD'17: PROCEEDINGS OF THE 23RD ACM SIGKDD INTERNATIONAL CONFERENCE ON
   KNOWLEDGE DISCOVERY AND DATA MINING}},
Year = {{2017}},
Pages = {{817-826}},
