@article{ ISI:000433948000001,
Author = {Smaldino, Paul E. and D'Souza, Raissa M. and Maoz, Zeev},
Title = {{Resilience by structural entrenchment: Dynamics of single-layer and
   multiplex networks following sudden changes to tie costs}},
Journal = {{NETWORK SCIENCE}},
Year = {{2018}},
Volume = {{6}},
Number = {{2}},
Pages = {{157-175}},
Month = {{JUN}},
Abstract = {{We examine a model of network formation in single-layer and multiplex
   networks in which individuals have positive incentives for social ties,
   closed triangles, and spillover edges. In particular, we investigate the
   influence of shocks to the network in which the cost of social ties
   changes after an initial equilibrium. We highlight the emergence of
   structural entrenchment: the retention of structural features, such as
   closed triangles and spillover edges, which are formed under
   historically different conditions from those currently driving network
   evolution. This work has broad implications for understanding path
   dependence in the structure and dynamics of single-layer and multiplex
   networks.}},
