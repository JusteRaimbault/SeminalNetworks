@article{ ISI:000428507100058,
Author = {Alarcon Ramos, Luis A. and Bernal Jaquez, Roberto and Schaum, Alexander},
Title = {{Output-Feedback Control for Discrete-Time Spreading Models in Complex
   Networks}},
Journal = {{ENTROPY}},
Year = {{2018}},
Volume = {{20}},
Number = {{3}},
Month = {{MAR}},
Abstract = {{The problem of stabilizing the spreading process to a prescribed
   probability distribution over a complex network is considered, where the
   dynamics of the nodes in the network is given by discrete-time
   Markov-chain processes. Conditions for the positioning and
   identification of actuators and sensors are provided, and sufficient
   conditions for the exponential stability of the desired distribution are
   derived. Simulations results for a network of N = 10(6) corroborate our
   theoretical findings.}},
