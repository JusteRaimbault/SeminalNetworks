@inproceedings{ ISI:000229696900008,
Author = {Gao, L and Ding, YS},
Editor = {{Gervasi, O and Gavrilova, ML and Kumar, V and Lagana, A and Lee, HP and Mun, Y and Taniar, D and Tan, CJK}},
Title = {{A flexible communication scheme to support Grid service emergence}},
