@article{ ISI:000439585400014,
Author = {Krawiecki, A. and Gradowski, T. and Siudem, G.},
Title = {{Majority Vote Model on Multiplex Networks}},
Journal = {{ACTA PHYSICA POLONICA A}},
Year = {{2018}},
Volume = {{133}},
Number = {{6}},
Pages = {{1433-1440}},
Month = {{JUN}},
Note = {{13th Econophysics Colloquium (EC) / 9th Polish Symposium of Physics in
   Economy and Social Sciences (FENS), Univ Warsaw, Warsaw, POLAND, JUL
   05-07, 2017}},
Organization = {{Univ Warsaw, Fac Phys; Univ Warsaw, Biol \& Chem Res Ctr; Polish Phys
   Soc}},
Abstract = {{Majority vote model on multiplex networks with two independently
   generated layers in the form of scale-free networks is investigated by
   means of Monte Carlo simulations and heterogeneous mean-field
   approximation. In a version of the model under study each agent with
   probability 1 - q (0 <= q <= 1/2) follows the opinions of the majorities
   of her neighbors within both layers if these opinions are identical;
   otherwise, she makes decision randomly. The model exhibits second-order
   ferromagnetic transition as q, the parameter measuring the level of
   internal noise, is decreased, with critical exponents depending on the
   details of the degree distributions in the layers. The critical value qc
   of the parameter q evaluated in the heterogeneous mean-field
   approximation shows quantitative agreement with that obtained from
   numerical simulations for a broad range of parameters characterizing the
   degree distributions of the layers.}},
