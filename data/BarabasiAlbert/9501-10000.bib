@article{ ISI:000423285400008,
Author = {Matta, John and Ercal, Gunes and Borwey, Jeffrey},
Title = {{THE VERTEX ATTACK TOLERANCE OF COMPLEX NETWORKS}},
Journal = {{RAIRO-OPERATIONS RESEARCH}},
