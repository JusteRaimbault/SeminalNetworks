@inproceedings{ ISI:000185660100037,
Author = {Vuong, S and Li, J},
Editor = {{Shahmehri, N and Graham, RL and Carroni, G}},
Title = {{Efa: an efficient content routing algorithm in large peer-to-peer
   overlay networks}},
Booktitle = {{THIRD INTERNATIONAL CONFERENCE ON PEER-TO-PEER COMPUTING (P2P2003),
   PROCEEDINGS}},
Year = {{2003}},
Pages = {{216-217}},
Note = {{3rd International Conference on Peer-to-Peer Computing (P2P 2003),
   LINKOPINGS UNIV, LINKOPING, SWEDEN, SEP 01-03, 2003}},
Organization = {{IEEE Sweden; Intelligent Informat Syst Lab}},
Abstract = {{An important issue for peer-to-peer application is to locate content
   within the network. There are many existing solutions to this problem,
   however, each of them addresses different aspects and each has its
   deficiencies. In this paper, we focus on the unstructured peer-to-peer
   scenario and present a constrained flooding routing algorithm, Efa,
   which overcomes some of the deficiencies of those existing strategies.
   Efa performs application level broadcasting in a potentially very large
   peer-to-peer network overlaid on the Internet. Efa is completely
   decentralized and self-organized. It is a more scalable alternative to
   flooding, which is commonly used in unstructured peer-to-peer systems.
   Utilizing just a small amount of topology info, Efa is almost as simple
   as flooding, but it is much more efficient and scalable.}},
