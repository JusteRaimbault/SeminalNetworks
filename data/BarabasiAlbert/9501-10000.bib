@article{ ISI:000378554800011,
Author = {Rey, David and Gardner, Lauren and Waller, S. Travis},
Title = {{Finding Outbreak Trees in Networks with Limited Information}},
Journal = {{NETWORKS \& SPATIAL ECONOMICS}},
