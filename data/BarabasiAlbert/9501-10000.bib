@article{ ISI:000444824900002,
Author = {Sagduyu, Yalin E. and Grushin, Alexander and Shi, Yi},
Title = {{Synthetic Social Media Data Generation}},
Journal = {{IEEE TRANSACTIONS ON COMPUTATIONAL SOCIAL SYSTEMS}},
