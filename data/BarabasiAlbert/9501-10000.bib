@article{ ISI:000429597100001,
Author = {Xu, Qiang and Fu, Jilian and Liu, Feng and Qin, Wen and Liu, Bing and
   Jiang, Tianzi and Yu, Chunshui},
Title = {{Left Parietal Functional Connectivity Mediates the Association Between
   COMT rs4633 and Verbal Intelligence in Healthy Adults}},
Journal = {{FRONTIERS IN NEUROSCIENCE}},
Year = {{2018}},
Volume = {{12}},
Month = {{APR 10}},
Abstract = {{In Chinese Han population, Catechol-O-methyltransferase gene (COMT)
   rs4633 is found to be associated with impaired cognitive process. We
   aimed to investigate the association between COMT rs4633 and verbal
   intelligence and the underlying neural mechanisms in Chinese Han healthy
   young adults. In 256 Chinese Han healthy young adults, we explored the
   modulatory effects of COMT rs4633 on verbal intelligence quotient (VIQ)
   and functional connectivity density (FCD) of the brain and the mediation
   effect of FCD on the association between COMT and VIQ. We further
   investigated the association between the expression patterns of dopamine
   receptor genes and the effect of COMT on FCD in the human brain. COMT
   rs4633 TT homozygotes exhibited lower VIQ than CC homozygotes and TC
   heterozygotes, higher long-range FCD (IrFCD) than CC homozygotes and TO
   heterozygotes in the left superior frontal gyrus. TT homozygotes and TC
   heterozygotes showed higher IrFCD than CC homozygotes in the left
   inferior parietal lobule. The IrFCD differences across genotypic
   subgroups were negatively associated with the expression of DRD2 and
   DRD3 genes. The left parietal IrFCD mediated the association between
   COMT rs4633 and VIQ. These findings provide a biological pathway that
   COMT rs4633 affects verbal intelligence via modulating the IrFCD of the
   left inferior parietal lobule.}},
