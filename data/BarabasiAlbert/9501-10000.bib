@article{ ISI:000452941700053,
Author = {Encinas, J. M. and Chen, Hanshuang and de Oliveira, Marcelo M. and
   Fiore, Carlos E.},
Title = {{Majority vote model with ancillary noise in complex networks}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2019}},
