@article{ ISI:000371276100170,
Author = {Monteiro, Roberto L. S. and Carneiro, Tereza Kelly G. and Fontoura, Jose
   Roberto A. and da Silva, Valeria L. and Moret, Marcelo A. and de Barros
   Pereira, Hernane Borges},
Title = {{A Model for Improving the Learning Curves of Artificial Neural Networks}},
Journal = {{PLOS ONE}},
Year = {{2016}},
Volume = {{11}},
Number = {{2}},
Month = {{FEB 22}},
Abstract = {{In this article, the performance of a hybrid artificial neural network
   (i.e. scale-free and small-world) was analyzed and its learning curve
   compared to three other topologies: random, scale-free and small-world,
   as well as to the chemotaxis neural network of the nematode
   Caenorhabditis Elegans. One hundred equivalent networks (same number of
   vertices and average degree) for each topology were generated and each
   was trained for one thousand epochs. After comparing the mean learning
   curves of each network topology with the C. elegans neural network, we
   found that the networks that exhibited preferential attachment exhibited
   the best learning curves.}},
