@article{ ISI:000371745800003,
Author = {Xie, Zheng and Rogers, Tim},
Title = {{Scale-invariant geometric random graphs}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2016}},
Volume = {{93}},
Number = {{3}},
Month = {{MAR 9}},
Abstract = {{We introduce and analyze a class of growing geometric random graphs that
   are invariant under rescaling of space and time. Directed connections
   between nodes are drawn according to influence zones that depend on node
   position in space and time, mimicking the heterogeneity and increased
   specialization found in growing networks. Through calculations and
   numerical simulations we explore the consequences of scale invariance
   for geometric random graphs generated this way. Our analysis reveals a
   dichotomy between scale-free and Poisson distributions of in-and
   out-degree, the existence of a random number of hub nodes, high
   clustering, and unusual percolation behavior. These properties are
   similar to those of empirically observed web graphs.}},
