@article{ ISI:000316950900021,
Author = {Zou, Zhiyun and Lai, Junyi and Gao, Jianzhi},
Title = {{Reducing the vulnerability of network by inserting modular topologies}},
Journal = {{CHAOS}},
Year = {{2013}},
Volume = {{23}},
Number = {{1}},
Month = {{MAR}},
Abstract = {{In this paper, we present a strategy whose purpose is to reduce the
   vulnerability of a network via inserting modular topologies. The modular
   topologies are generated as WS small-world random network, which is
   relatively highly robust. Using betweenness and betweenness centrality
   as the vulnerability measurement, the strategy searches for remote nodes
   with low betweenness in the network and sets these nodes to be connected
   to the modular topologies. We test our strategy on some basis networks
   and the results show sufficient availability of our strategy. And by
   comparing with other methods of adding topologies into the network, we
   show that our strategy is especially efficient in reducing the
   vulnerability of the critical network components. (C) 2013 American
   Institute of Physics. {[}http://dx.doi.org/10.1063/1.4792267]}},
