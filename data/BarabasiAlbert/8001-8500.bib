@article{ ISI:000270618500011,
Author = {Tang, Xiao-Gai and Wong, Eric W. M.},
Title = {{Information traffic in scale-free networks with fluctuations in packet
   generation rate}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2009}},
Volume = {{388}},
Number = {{22}},
Pages = {{4797-4802}},
Month = {{NOV 15}},
Abstract = {{We study the information traffic in scale-free networks where the
   information generation rate varies with time as a periodic function. We
   observe that when the fluctuation in packet generation rate increases,
   the average transit time increases and network performance degrades. In
   order to improve the transportation efficiency in this situation, we
   propose a new routing method called mixed routing. It operates in two
   modes: (1) when the packet generation rate is small, the shortest paths
   are used to deliver the packets to the destination; (2) when the packet
   generation rate is large, the traffic loads in central nodes are
   redistributed to other non-central nodes, using the so-called efficient
   routing method. We find that the time shifting between the two modes is
   very critical for the routing performance. Consequently, we provide an
   efficient method to determine the critical times to shift the routing
   modes for achieving good network performance. (C) 2009 Elsevier B.V. All
   rights reserved.}},
