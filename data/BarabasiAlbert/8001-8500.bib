@article{ ISI:000268662800002,
Author = {Lei Chuang and Jia Jian-Yuan and Chen Xiao-Jie and Cong Rui and Wang
   Long},
Title = {{Prisoner's Dilemma Game on Clustered Scale-Free Networks under Different
   Initial Distributions}},
Journal = {{CHINESE PHYSICS LETTERS}},
Year = {{2009}},
Volume = {{26}},
Number = {{8}},
Month = {{AUG}},
Abstract = {{The evolutionary prisoner's dilemma game is investigated under different
   initial distributions for cooperators and defectors on scale-free
   networks with a tunable clustering coefficient. It is found that, on the
   one hand, cooperation can be enhanced with the increasing clustering
   coefficient when only the most connected nodes are occupied by
   cooperators initially. On the other hand, if cooperators just occupy the
   lowest-degree nodes at the beginning, then the higher the value of the
   clustering coefficient, the more unfavorable the environment for
   cooperators to survive for the increment of temptation to defect.
   Thereafter, we analytically argue these nontrivial phenomena by
   calculating the cooperation probability of the nodes with different
   degrees in the steady state, and obtain the critical values of initial
   frequency of cooperators below which cooperators would vanish finally
   for the two initial distributions.}},
