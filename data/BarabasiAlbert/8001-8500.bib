@article{ ISI:000308628600015,
Author = {Aleixandre-Benavent, Rafael and Aleixandre-Tudo, Jose L. and Gonzalez
   Alcaide, Gregorio and Ferrer-Sapena, Antonia and Aleixandre, Jose L. and
   du Toit, Wessel},
Title = {{Bibliometric analysis of publications by South African viticulture and
   oenology research centres}},
