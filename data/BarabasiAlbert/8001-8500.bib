@article{ ISI:000276081200004,
Author = {Scholtes, Ingo and Botev, Jean and Esch, Markus and Sturm, Peter},
Title = {{EPIDEMIC SELF-SYNCHRONIZATION IN COMPLEX NETWORKS OF KURAMOTO
   OSCILLATORS}},
Journal = {{ADVANCES IN COMPLEX SYSTEMS}},
Year = {{2010}},
Volume = {{13}},
Number = {{1}},
Pages = {{33-58}},
Month = {{FEB}},
Note = {{1st International Conference on Complex Sciences, Shanghai, PEOPLES R
   CHINA, FEB 23-25, 2009}},
Abstract = {{In this article, we present and evaluate an epidemic scheme for the
   synchronization of coupled Kuramoto oscillators in communication
   networks. It addresses the problem of efficiently providing globally
   synchronous time epochs in complex, dynamic Peer-to-Peer network
   topologies. Rather than the usual model of continuously coupled nodes, a
   discretized version with sporadic message-based couplings to nearest
   neighbors is considered. This article empirically studies the emergence
   of coherent oscillator states for different network topologies, coupling
   functions, and sporadic coupling intensities. It further investigates
   the protocol's minimum bandwidth requirements in small-world network
   topologies. Synchronization resilience under the effect of random
   perturbations is studied for two coupling variations. Finally, the
   potential utilization of the scheme for a local inference of global
   network topology characteristics is discussed.}},
