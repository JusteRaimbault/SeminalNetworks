@article{ ISI:000284446200006,
Author = {Shiga, Motoki and Takigawa, Ichigaku and Mamitsuka, Hiroshi},
Title = {{A spectral approach to clustering numerical vectors as nodes in a
   network}},
Journal = {{PATTERN RECOGNITION}},
Year = {{2011}},
Volume = {{44}},
Number = {{2}},
Pages = {{236-251}},
Month = {{FEB}},
Abstract = {{We address the issue of clustering examples by integrating multiple data
   sources, particularly numerical vectors and nodes in a network. We
   propose a new, efficient spectral approach, which integrates the two
   costs for clustering numerical vectors and clustering nodes in a network
   into a matrix trace, reducing the issue to a trace optimization problem
   which can be solved by an eigenvalue decomposition. We empirically
   demonstrate the performance of the proposed approach through a variety
   of experiments, including both synthetic and real biological datasets.
