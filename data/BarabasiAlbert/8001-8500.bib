@inproceedings{ ISI:000330563800055,
Author = {Liu, Haifeng and Hu, Zheng and Zhou, Dian and Tian, Hui},
Book-Group-Author = {{IEEE Comp Soc
   Acad Sci \& Engn}},
Title = {{Cumulative Probability Distribution Model for Evaluating User Behavior
   Prediction Algorithms}},
