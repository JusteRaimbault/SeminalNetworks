@article{ ISI:000330622600014,
Author = {Zhai, Li and Yan, Xiangbin and Zhu, Bin},
Title = {{The H-l -index: improvement of H-index based on quality of citing papers}},
Journal = {{SCIENTOMETRICS}},
Year = {{2014}},
Volume = {{98}},
Number = {{2}},
Pages = {{1021-1031}},
Month = {{FEB}},
Abstract = {{This paper proposes h(l) -index as an improvement of the h-index, a
   popular measurement for the research quality of academic researchers.
   Although the h-index integrates the number of publications and the
   academic impact of each publication to evaluate the productivity of a
   researcher, it assumes that all papers that cite an academic article
   contribute equally to the academic impact of this article. This
   assumption, of course, could not be true in most times. The citation
   from a well-cited paper certainly brings more attention to the article
   than the citation from a paper that people do not pay attention to. It
   therefore becomes important to integrate the impact of papers that cite
   a researcher's work into the evaluation of the productivity of the
   researcher. Constructing a citation network among academic papers, this
   paper therefore proposes h(l) -index that integrating the h-index with
   the concept of lobby index, a measures that has been used to evaluate
   the impact of a node in a complex network based on the impact of other
   nodes that the focal node has direct link with. This paper also explores
   the characteristics of the proposed h (l) -index by comparing it with
   citations, h-index and its variant g-index.}},
