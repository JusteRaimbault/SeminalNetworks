@inproceedings{ ISI:000411479100041,
Author = {Biasiolo, Marta and Forcato, Mattia and Possamai, Lino and Ferrari,
   Francesco and Agnelli, Luca and Lionetti, Marta and Todoerti, Katia and
   Neri, Antonino and Marchiori, Massimo and Bortoluzzi, Stefania and
   Bicciato, Silvio},
