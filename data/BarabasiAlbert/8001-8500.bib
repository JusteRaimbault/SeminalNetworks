@article{ ISI:000373608000127,
Author = {Zou, You and Xie, Jiarong and Wang, Binghong},
Title = {{Evacuation of Pedestrians with Two Motion Modes for Panic System}},
Journal = {{PLOS ONE}},
Year = {{2016}},
Volume = {{11}},
Number = {{4}},
Month = {{APR 7}},
Abstract = {{In this paper, we have captured an underlying mechanism of emergence of
   collective panic in pedestrian evacuations by using a modification of
   the lattice-gas model. We classify the motion of pedestrians into two
   modes according to their moods. One is gentle (mode I), the other is
   flustered (mode II). First, to research the cause for crowd, we fix the
   motion modes of pedestrians and increase the proportion of pedestrians
   with motion mode II (rho(II)). The simulation results show that the
   pedestrians with motion mode II are lack of evacuation efficiency and
   cause more casualties. Further, we use the SIS
   (susceptible-infective-susceptible) model to describe the spreading of
   the panic mood. The system can be in the high-mix state when the
   infection probability lambda is greater than a fuzzy threshold. In
   addition, the distances S from wounded people to the exit are
   researched, the number of wounded people gets maximum at the internal S
   = 5 similar to 10, which is independent of rho(II) and lambda. This
   research can help us to understand and prevent the emergence of
   collective panic and reduce wounds in the real evacuation.}},
