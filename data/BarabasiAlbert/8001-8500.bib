@article{ ISI:000278414000001,
Author = {Cibert, Christian and Toscano, Jeremy and Pensee, Vincent and Bonnet,
   Guy},
Title = {{Bending of the ``9+2{''} axoneme analyzed by the finite element method}},
Journal = {{JOURNAL OF THEORETICAL BIOLOGY}},
Year = {{2010}},
Volume = {{264}},
Number = {{4}},
Pages = {{1089-1101}},
Month = {{JUN 21}},
Abstract = {{Many data demonstrate that the regulation of the bending polarity of the
   ``9+2{''} axoneme is supported by the curvature itself, making the
   internal constraints central in this process, adjusting either the
   physical characteristics of the machinery or the activity of the enzymes
   involved in different pathways Among them, the very integrated Geometric
   Clutch model founds this regulation on the convenient adjustments of the
   probability of interaction between the dynein arms and the beta-tubulin
   monomers of the outer doublet pairs on which they walk. Taking into
   consideration (i) the deviated bending of the outer doublets pairs
