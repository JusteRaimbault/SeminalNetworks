@article{ ISI:000385640100005,
Author = {Kim, Yup and Park, Seokjong and Yook, Soon-Hyung},
Title = {{Network exploration using true self-avoiding walks}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2016}},
Volume = {{94}},
Number = {{4}},
Month = {{OCT 14}},
Abstract = {{We study the mean first passage time (MFPT) of true self-avoiding walks
   (TSAWs) on various networks as a measure of searching efficiency. From
   the numerical analysis, we find that the MFPT of TSAWs, tau(TSAW),
   approaches the theoretical minimum tau th/N = 1/2 on synthetic networks
   whose degree-degree correlations are positive. On the other hand, for
   biased random walks (BRWs) we find that the MFPT, tau (BRW), depends on
   the parameter alpha, which controls the degree-dependent bias. More
   importantly, we find that the minimum MFPT of BRWs, tau(BRW)(min),
   always satisfies the inequality tau(min) (BRW)> tau(TSAW) on any
   synthetic network. The inequality is also satisfied on various real
   networks. From these results, we show that the TSAWis one of the most
   efficient models, whose efficiency approaches the theoretical limit in
   network explorations.}},
