@article{ ISI:000209853900001,
Author = {Yu, Rongzhong and Li, Kezan and Chen, Baidi and Shi, Dingqin},
Title = {{Dynamical analysis of an SIRS network model with direct immunization and
   infective vector}},
Journal = {{ADVANCES IN DIFFERENCE EQUATIONS}},
Year = {{2015}},
Month = {{APR 9}},
Abstract = {{With the awareness of risk in infective disease spreading, healthy
   individuals (the susceptible ones) will take some measures to acquire
   temporary immunity. This paper addresses an SIRS model with direct
   immunization and an infective vector in complex networks and performs
   the dynamical analysis for this model. By theoretical analysis, we
   obtain the epidemic threshold lambda(c) and prove that if infection rate
   lambda < lambda(c), the disease-free equilibrium is globally
   asymptotically stable; if lambda > lambda(c), there exists a unique
   endemic equilibrium, and it is globally attractive. These theoretical
   results are confirmed by numerical simulations.}},
