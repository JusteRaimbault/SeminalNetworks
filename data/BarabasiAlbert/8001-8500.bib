@article{ ISI:000410134600002,
Author = {Cickovski, Trevor and Peake, Eli and Aguiar-Pulido, Vanessa and
   Narasimhan, Giri},
Title = {{ATria: a novel centrality algorithm applied to biological networks}},
Journal = {{BMC BIOINFORMATICS}},
