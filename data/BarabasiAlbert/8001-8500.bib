Author = {Scibetta, M. and Boano, F. and Revelli, R. and Ridolfi, L.},
Editor = {{Brunone, B and Giustolisi, O and Ferrante, M and Laucelli, D and Meniconi, S and Berardi, L and Campisano, A}},
Title = {{Community detection as a tool for district metered areas identification}},
Booktitle = {{12TH INTERNATIONAL CONFERENCE ON COMPUTING AND CONTROL FOR THE WATER
   INDUSTRY, CCWI2013}},
Series = {{Procedia Engineering}},
Year = {{2014}},
Volume = {{70}},
