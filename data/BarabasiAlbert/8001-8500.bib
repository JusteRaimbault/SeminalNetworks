@inproceedings{ ISI:000361480100014,
Author = {Topirceanu, Alexandru and Barina, Gabriel and Udrescu, Mihai},
Book-Group-Author = {{IEEE}},
Title = {{MuSeNet: Collaboration in the Music Artists Industry}},
