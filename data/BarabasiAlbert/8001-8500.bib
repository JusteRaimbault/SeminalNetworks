@article{ ISI:000401747900025,
Author = {Chariker, Julia H. and Zhang, Yihang and Pani, John R. and Rouchka, Eric
   C.},
Title = {{Identification of successful mentoring communities using network-based
   analysis of mentor-mentee relationships across Nobel laureates}},
Journal = {{SCIENTOMETRICS}},
