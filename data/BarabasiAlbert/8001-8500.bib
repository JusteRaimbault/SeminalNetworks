@inproceedings{ ISI:000393410400007,
Author = {Bhuiyan, Hasanuzzaman and Chen, Jiangzhuo and Khan, Maleq and Marathe,
   Madhav V.},
Book-Group-Author = {{IEEE}},
Title = {{Fast Parallel Algorithms for Edge-Switching to Achieve a Target Visit
   Rate in Heterogeneous Graphs}},
Booktitle = {{2014 43RD INTERNATIONAL CONFERENCE ON PARALLEL PROCESSING (ICPP)}},
