@article{ ISI:000270003300002,
Author = {Lu, Wenlian and Chen, Tianping},
Title = {{Synchronisation in complex networks of coupled systems with directed
   topologies}},
Journal = {{INTERNATIONAL JOURNAL OF SYSTEMS SCIENCE}},
