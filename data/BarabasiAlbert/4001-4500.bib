@article{ ISI:000186571200096,
Author = {Capocci, A and Caldarelli, G and De Los Rios, P},
Title = {{Quantitative description and modeling of real networks}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2003}},
Volume = {{68}},
Number = {{4, 2}},
Month = {{OCT}},
Abstract = {{We present data analysis and modeling of two particular cases of study
   in the field of growing networks. We analyze World Wide Web data set and
   authorship collaboration networks in order to check the presence of
   correlation in the data. The results are reproduced with good agreement
   through a suitable modification of the standard Albert-Barabasi model of
   network growth. In particular, intrinsic relevance of sites plays a role
   in determining the future degree of the vertex.}},
