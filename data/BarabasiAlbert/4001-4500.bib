@article{ ISI:000373644100005,
Author = {Kinugawa, Hikaru and Ueda, Kazuhiro and Gotoda, Hiroshi},
Title = {{Chaos of radiative heat-loss-induced flame front instability}},
Journal = {{CHAOS}},
Year = {{2016}},
Volume = {{26}},
Number = {{3}},
Month = {{MAR}},
Abstract = {{We are intensively studying the chaos via the period-doubling
   bifurcation cascade in radiative heat-loss-induced flame front
   instability by analytical methods based on dynamical systems theory and
   complex networks. Significant changes in flame front dynamics in the
   chaotic region, which cannot be seen in the bifurcation diagrams, were
   successfully extracted from recurrence quantification analysis and
   nonlinear forecasting and from the network entropy. The temporal
   dynamics of the fuel concentration in the well-developed chaotic region
   is much more complicated than that of the flame front temperature. It
   exhibits self-affinity as a result of the scale-free structure in the
   constructed visibility graph. (C) 2016 AIP Publishing LLC.}},
