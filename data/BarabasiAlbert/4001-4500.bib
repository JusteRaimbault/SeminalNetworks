@article{ ISI:000279238400025,
Author = {Poncela, Julia and Gomez-Gardenes, Jesus and Moreno, Yamir and Mario
   Floria, Luis},
Title = {{COOPERATION IN THE PRISONER'S DILEMMA GAME IN RANDOM SCALE-FREE GRAPHS}},
