@article{ ISI:000268653900028,
Author = {Qian, Jiang-Hai and Han, Ding-Ding},
Title = {{A spatial weighted network model based on optimal expected traffic}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2009}},
Volume = {{388}},
Number = {{19}},
Pages = {{4248-4258}},
Month = {{OCT 1}},
Abstract = {{We propose a spatial weighted network model based on the optimal
   expected traffic. The expected traffic represents the prediction of the
   now created by two vertices and is Calculated by the improved gravity
   equation. The model maximizes the total expected traffic of the network.
   By changing two parameters which control the fitness and the
   geographical constraints, the model can vary its topology and give rise
   to a variety of statistical properties observed in the real-world
   network. Notably, our study shows that a linear and a nonlinear
   strength-degree correlation can emerge when considering and neglecting
   the ``transport effect{''}, respectively. (C) 2009 Elsevier B.V. All
   rights reserved.}},
