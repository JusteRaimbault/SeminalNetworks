@article{ ISI:000282052400005,
Author = {Raschke, Mathias and Schlaepfer, Markus and Nibali, Roberto},
Title = {{Measuring degree-degree association in networks}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2010}},
Volume = {{82}},
Number = {{3, 2}},
Month = {{SEP 17}},
Abstract = {{The Pearson correlation coefficient is commonly used for quantifying the
   global level of degree-degree association in complex networks. Here, we
   use a probabilistic representation of the underlying network structure
   for assessing the applicability of different association measures to
   heavy-tailed degree distributions. Theoretical arguments together with
   our numerical study indicate that Pearson's coefficient often depends on
   the size of networks with equal association structure, impeding a
   systematic comparison of real-world networks. In contrast,
   Kendall-Gibbons' tau(b) is a considerably more robust measure of the
   degree-degree association.}},
