@article{ ISI:000356986700009,
Author = {Zhang, Wenping and Xia, Yongxiang and Ouyang, Bo and Jiang, Lurong},
Title = {{Effect of network size on robustness of interconnected networks under
   targeted attack}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2015}},
Volume = {{435}},
Pages = {{80-88}},
Month = {{OCT 1}},
Abstract = {{Cascading failures in coupled networks have been intensively studied
   recently. The underlying assumption is that all networks share the same
   size. However, this setting cannot capture real-world coupled networks
   very well. Hence, in this paper, we explore how different network sizes
   affect cascades from the perspective of the whole interconnected
   networks. We find that when two networks with similar sizes are coupled,
   the interconnected networks are the most fragile for sparse coupling
   while they are the most robust for dense coupling. Moreover, we also
   study the effect of network size on the robustness of individual
   network. We find that when two networks with different sizes are
   coupled, the larger one is more robust for sparse coupling, while more
   fragile for dense coupling. (C) 2015 Elsevier B.V. All rights reserved.}},
