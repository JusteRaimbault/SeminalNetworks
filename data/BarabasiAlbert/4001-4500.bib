@article{ ISI:000304907300008,
Author = {Shigaki, K. and Kokubo, S. and Tanimoto, J. and Hagishima, A. and
   Ikegaya, N.},
Title = {{Does copy-resistance enhance cooperation in spatial prisoner's dilemma?}},
Journal = {{EPL}},
Year = {{2012}},
Volume = {{98}},
Number = {{4}},
Month = {{MAY}},
Abstract = {{We propose a novel idea for the so-called pairwise-Fermi process by
   considering copy-resistance when an agent copies a neighbor's strategy,
   which implies that the focal agent with relatively affluent payoff
   vis-a-vis social average might be negative to copy her neighbor's
   strategy even if her payoff is less than the neighbor's payoff.
   Simulation results reveal that this idea with a revised strategy
   adaptation process significantly enhances cooperation for prisoner's
   dilemma games played on time-constant networks. Copyright (C) EPLA, 2012}},
