@article{ ISI:000259349000001,
Author = {Konagurthu, Arun S. and Lesk, Arthur M.},
Title = {{On the origin of distribution patterns of motifs in biological networks}},
Journal = {{BMC SYSTEMS BIOLOGY}},
Year = {{2008}},
Volume = {{2}},
Month = {{AUG 12}},
Abstract = {{Background: Inventories of small subgraphs in biological networks have
   identified commonly-recurring patterns, called motifs. The inference
   that these motifs have been selected for function rests on the idea that
   their occurrences are significantly more frequent than random.
   Results: Our analysis of several large biological networks suggests, in
   contrast, that the frequencies of appearance of common subgraphs are
   similar in natural and corresponding random networks.
   Conclusion: Indeed, certain topological features of biological networks
   give rise naturally to the common appearance of the motifs. We therefore
   question whether frequencies of occurrences are reasonable evidence that
   the structures of motifs have been selected for their functional
   contribution to the operation of networks.}},
