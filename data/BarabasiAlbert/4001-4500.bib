@article{ ISI:000299607300012,
Author = {Yang, Han-Xin and Wang, Wen-Xu and Lai, Ying-Cheng and Wang, Bing-Hong},
Title = {{Convergence to global consensus in opinion dynamics under a nonlinear
   voter model}},
Journal = {{PHYSICS LETTERS A}},
Year = {{2012}},
Volume = {{376}},
Number = {{4}},
Pages = {{282-285}},
Month = {{JAN 9}},
Abstract = {{We propose a nonlinear voter model to study the emergence of global
   consensus in opinion dynamics. In our model, agent i agrees with one of
   binary opinions with the probability that is a power function of the
   number of agents holding this opinion among agent i and its nearest
   neighbors, where an adjustable parameter alpha controls the effect of
   herd behavior on consensus. We find that there exists an optimal value
   of alpha leading to the fastest consensus for lattices, random graphs,
   small-world networks and scale-free networks. Qualitative insights are
   obtained by examining the spatiotemporal evolution of the opinion
   clusters. (C) 2011 Elsevier B.V. All rights reserved.}},
