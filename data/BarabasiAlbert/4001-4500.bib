@article{ ISI:000328523600002,
Author = {Zhao, Laijun and Wang, Jiajia and Huang, Rongbing and Cui, Hongxin and
   Qiu, Xiaoyan and Wang, Xiaoli},
Title = {{Sentiment contagion in complex networks}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2014}},
Volume = {{394}},
Pages = {{17-23}},
Month = {{JAN 15}},
Abstract = {{Sentiment contagion such as the spread of panic in emergencies is a
   common phenomenon in human society. Considering the difference between
   sentiment contagion and epidemic contagion, we define the transition
   probabilities of the binary emotional state (optimism, pessimism) and
   establish a sentiment contagion model. Transition equations are given in
   a homogeneous network and the stability of the zero solution is
   discussed. Also the Monte Carlo method is used for numerical simulation
   in the inhomogeneous networks. Simulation results show that the overall
   tendency of sentiment variation in the BA scale-free network is similar
   in the homogeneous network. Furthermore, the assimilation and weight
   combination exert influences on sentiment contagion. (C) 2013 Elsevier
   B.V. All rights reserved.}},
