@article{ ISI:000384095600001,
Author = {Ju, Ying and Zhang, Songming and Ding, Ningxiang and Zeng, Xiangxiang
   and Zhang, Xingyi},
Title = {{Complex Network Clustering by a Multi-objective Evolutionary Algorithm
   Based on Decomposition and Membrane Structure}},
Journal = {{SCIENTIFIC REPORTS}},
Year = {{2016}},
Volume = {{6}},
Month = {{SEP 27}},
Abstract = {{The field of complex network clustering is gaining considerable
   attention in recent years. In this study, a multi-objective evolutionary
   algorithm based on membranes is proposed to solve the network clustering
   problem. Population are divided into different membrane structures on
   average. The evolutionary algorithm is carried out in the membrane
   structures. The population are eliminated by the vector of membranes. In
   the proposed method, two evaluation objectives termed as Kernel J-means
   and Ratio Cut are to be minimized. Extensive experimental studies
   comparison with state-of-the-art algorithms proves that the proposed
   algorithm is effective and promising.}},
