@article{ ISI:000418953500006,
Author = {Said, Anwar and Abbasi, Rabeeh Ayaz and Maqbool, Onaiza and Daud, Ali
   and Aljohani, Naif Radi},
Title = {{CC-GA: A clustering coefficient based genetic algorithm for detecting
   communities in social networks}},
Journal = {{APPLIED SOFT COMPUTING}},
