@inproceedings{ ISI:000230988700008,
Author = {Pacheco, JM and Santos, FC},
Editor = {{Mendes, JFF and Dorogovtsev, SN and Povolotsky, A and Abreu, FV and Oliveira, JG}},
Title = {{Network dependence of the dilemmas of cooperation}},
Booktitle = {{Science of Complex Networks: From Biology to the Internet and WWW}},
Series = {{AIP CONFERENCE PROCEEDINGS}},
