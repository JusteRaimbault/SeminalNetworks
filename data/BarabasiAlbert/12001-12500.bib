@article{ ISI:000260617200011,
Author = {Cabana, Alvaro and Mizraji, Eduardo and Pomi, Andres and Valle-Lisboa,
   Juan Carlos},
Title = {{Looking for Robust Properties in the Growth of an Academic Network: The
