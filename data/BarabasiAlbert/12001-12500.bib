@article{ ISI:000473520100005,
Author = {Zhang, Ting and Zhang, Kun and Lv, Laishui and Bardou, Dalal},
Title = {{Graph Regularized Non-negative Matrix Factorization for Temporal Link
   Prediction Based on Communicability}},
Journal = {{JOURNAL OF THE PHYSICAL SOCIETY OF JAPAN}},
Year = {{2019}},
Volume = {{88}},
Number = {{7}},
Month = {{JUL 15}},
Abstract = {{Temporal link prediction is a fundamental research problem in network
   analysis and has gained increasing attention in recent years. In order
   to improve the accuracy of link prediction, the dynamic nature of
   networks should be extracted. However, many methods in the literature
   focus on the collapsing temporal networks and could not exploit the
   temporal inherence of networks effectively. To address the issue, we
   propose a novel temporal link prediction algorithm based on matrix
   factorization. We apply graph communicability to discover the temporal
   topology of a network and exploit the local information of vertices by
   graph regularization. The experiments on synthetic and real datasets
   show that the proposed method effectively improves the performance of
   temporal link prediction. Furthermore, our approach still shows high
   robustness when the noise ratio reaches as high as 40\%.}},
