@inproceedings{ ISI:000185994700023,
Author = {Flaxman, A and Frieze, A and Fenner, T},
Editor = {{Arora, S and Jansen, K and Rolim, JDP and Sahai, A}},
Title = {{High degree vertices and eigenvalues in the preferential attachment
   graph}},
Booktitle = {{APPROXIMATION, RANDOMIZATION, AND COMBINATORIAL OPTIMIZATION}},
Series = {{LECTURE NOTES IN COMPUTER SCIENCE}},
Year = {{2003}},
Volume = {{2764}},
Pages = {{264-274}},
Note = {{6th Int Workshop on Approximation Algorithms for Combinatorial
   Optimization Problems/7th Int Workshop on Randomization and
   Approximation Techniques in Computer Science, PRINCETON, NEW JERSEY, AUG
   24-26, 2003}},
Abstract = {{The preferential attachment graph is a random graph formed by adding a
   new vertex at each time step, with a single edge which points to a
   vertex selected at random with probability proportional to its degree.
   Every m steps the most recently added m vertices are contracted into a
   single vertex, so at time t there are roughly t/m vertices and exactly t
   edges. This process yields a graph which has been proposed as a simple
   model of the world wide web {[}BA99]. For any constant k, let Delta(1)
   greater than or equal to Delta(2) greater than or equal to (. . .)
   greater than or equal to Delta(k) be the degrees of the k highest degree
   vertices. We show that at time t, for any function f with f(t) -->
   infinity as t --> infinity, t(1/2)/f(t) less than or equal to Delta(1)
   less than or equal to t(1/2)f(t), and for i = 2, . . ., k, t(1/2)/f(t)
   less than or equal to Delta(i) less than or equal to Delta(i-1) -
   t(1/2)/f(t), with high probability (whp). We use this to show that at
   time t the largest k eigenvalues of the adjacency matrix of this graph
   have lambda(k) = (1 +/- o(1))Delta(k)(1/2) whp.}},
