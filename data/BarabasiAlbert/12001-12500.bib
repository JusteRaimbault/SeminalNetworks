@inproceedings{ ISI:000179966100009,
Author = {An, Y and Janssen, JCM and Milios, EE},
Editor = {{Unger, H and Bohme, T and Mikler, A}},
Title = {{Characterizing the citation graph as a self-organizing networked
   information space}},
Booktitle = {{INNOVATIVE INTERNET COMPUTING SYSTEMS}},
