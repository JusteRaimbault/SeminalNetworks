@article{ ISI:000180257500003,
Author = {Kolobov, MY and Burkovsky, IV},
Title = {{Microspatial organization of Priapulus caudatus and Halicryptus
   spinulosus (Priapulida) populations in a tidal gradient}},
Journal = {{ZOOLOGICHESKY ZHURNAL}},
Year = {{2002}},
Volume = {{81}},
Number = {{12}},
Pages = {{1439-1447}},
Month = {{DEC}},
Abstract = {{The structure of two macrobenthic priapulid worm species (Priapulus
   caudatus and Halicryptus spinulosus) was studied based on the data
   collected in the coastal zone of the Gryaznaya Gulf (the White Sea,
   Kandalaksha Bay). Priapulids of these species were shown to form dense
   microclumps including joint ones. The size and species composition of
   the clumps change gradually in a tidal gradient, and. their intrinsic
   structure reflects the hierarchy of mutual relationships between
   different-sized priapulids. The microstructure of priapulid populations
   is organized according to the ``scale-free network{''} principle
   (Barabasi, Albert, 1999). Taking into account a relatively low numbers
   of priapulids and the long-term period of their ontogenesis, this
   organization, along with ecological complementarity of Priapulus
   caudatus and Halicryptus spinulosus observed at the population level,
   supports the stability of these species populations and raises their
   competitiveness.}},
