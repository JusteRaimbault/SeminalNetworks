@inproceedings{ ISI:000251345401116,
Author = {Chen, Xiaogang and Sun, Ke and Cao, Yijia and Wang, Shaobu},
Book-Group-Author = {{IEEE}},
Title = {{Identification of vulnerable lines in power grid based on complex
   network theory}},
Booktitle = {{2007 IEEE POWER ENGINEERING SOCIETY GENERAL MEETING, VOLS 1-10}},
Series = {{IEEE Power Engineering Society General Meeting}},
Year = {{2007}},
Pages = {{1699-1704}},
Note = {{IEEE-Power-Engineering-Society General Meeting, Tampa, FL, JUN 24, 2007}},
Organization = {{IEEE Power Engn Soc}},
Abstract = {{Some critical lines can have important impact on the large-scale
   blackouts and cascading failures in power grid. Based on the newest
   progress in the field of complex network, a new vulnerability index
   called weighted line betweenness is proposed as vulnerability index in
   this paper. The weighted line betweenness of one fine is defined as the
   sum of the loads acted on this line, which are brought by the shortest
   electric paths between generator nodes and load nodes that passing
   through this fine. We revise the weighted line betweenness by increasing
   the betweenness to the highest betweenness in the neighboring lines
   before. revision. Vulnerability analysis has been carried out on the
   IEEE 39 bus system and Huazhong-Chuanyu power grid. The time domain
