@inproceedings{ ISI:000292494100006,
Author = {Antunes, Luis and Balsa, Joao and Urbano, Paulo and Coelho, Helder},
Editor = {{Takadama, K and CioffiRevilla, C and Deffuant, G}},
Title = {{Exploring Context Permeability in Multiple Social Networks}},
Booktitle = {{SIMULATING INTERACTING AGENTS AND SOCIAL PHENOMENA: THE SECOND WORLD
   CONGRESS}},
Series = {{Springer Series on Agent Based Social Systems}},
