@article{ ISI:000467224400001,
Author = {Jia, Junbo and Jin, Zhen and Fu, Xinchu},
Title = {{Epidemic spread in directed interconnected networks}},
Journal = {{COMMUNICATIONS IN NONLINEAR SCIENCE AND NUMERICAL SIMULATION}},
Year = {{2019}},
Volume = {{75}},
Pages = {{1-13}},
Month = {{AUG}},
Abstract = {{In the real world, many complex systems interact with other systems.
   Besides, the spread of disease in intra-systems or inter-systems is
   often not random, but with direction. Hence, in this paper, we build an
   epidemic model based on a directed interconnected network, which can be
   considered as the generalization of undirected networks and bipartite
   networks. By using the mean-field approach, we establish the
   Susceptible-InfectiousSusceptible model on this network, and then
   analyze the model theoretically, and obtain the basic reproduction
   number, which is also the generalization of the critical number
   corresponding to undirected or bipartite networks. We explain how
   coupling can increase disease infection, and this phenomenon may even
   lead to disease outbreaks. Furthermore, we prove the global stability of
   disease-free and endemic equilibria via the basic reproduction number as
   a forward bifurcation parameter. We also give a condition for epidemic
   prevalence only on a single subnetwork. Finally, we carry out numerical
   simulations and find that the independence between each node's in- and
   out-degrees significantly reduce the impact of the network's topological
   structure on disease spread. (C) 2019 Elsevier B.V. All rights reserved.}},
