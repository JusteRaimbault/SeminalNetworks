@article{ ISI:000474230900014,
Author = {Papp, Istvan and Varga, Levente and Afifi, Mounir and Gere, Istvan and
   Neda, Zoltan},
Title = {{Scaling in the space-time of the Internet}},
Journal = {{SCIENTIFIC REPORTS}},
Year = {{2019}},
Volume = {{9}},
Month = {{JUL 5}},
Abstract = {{The Internet on the router level, is a complex network embedded in a
   geographical space. We provide experimental evidences suggesting that
   the average travel time for a message, with fixed length, increases
   roughly as the square root of the geographical distance. To understand
   this scaling law and other measurable topological properties of the
   Internet as a graph, we introduce and study a simple network model. The
   model is based on a few realistic socio-economic facts/assumptions and
   qualitatively reproduces the experimentally observed stylized facts.}},
