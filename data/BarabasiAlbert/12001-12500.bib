@inproceedings{ ISI:000310759600013,
Author = {Sriram, Ilango and Cliff, Dave},
Editor = {{Costa, LD and Evsukoff, A and Mangioni, G and Menezes, R}},
Title = {{Hybrid Complex Network Topologies Are Preferred for
   Component-Subscription in Large-Scale Data-Centres}},
Booktitle = {{COMPLEX NETWORKS}},
Series = {{Communications in Computer and Information Science}},
Year = {{2011}},
