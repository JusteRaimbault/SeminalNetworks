@article{ ISI:000245061600049,
Author = {Liu, Yuhua and Cao, Jiwei and Xu, Kaihua and Huang, Hao},
Title = {{A self-organization Internet topology model based on message transfer}},
Journal = {{DYNAMICS OF CONTINUOUS DISCRETE AND IMPULSIVE SYSTEMS-SERIES
   B-APPLICATIONS \& ALGORITHMS}},
Year = {{2006}},
Volume = {{13}},
Number = {{2, S}},
Pages = {{679-683}},
Month = {{DEC}},
Note = {{International Conference on Complex Systems and Applications, Huhhot,
   PEOPLES R CHINA, JUN 16-18, 2006}},
Abstract = {{On reviewing the local-world evolving network model, this paper proposes
   a self-organization Internet topology model based on message transfer in
   terms of the statistical characteristic and forming mechanism of
   Internet. This topology model simulates the growth of the whole Internet
   dynamically: some isolated nodes which are randomly distributed on a
   plain building connections by sending messages to each other; each node
   decides how to build connection according to the message it received, in
