@article{ ISI:000274267700004,
Author = {Legara, Erika and Monterola, Christopher and David, Clarissa and Atun,
   Jenna Mae},
Title = {{NEWS FRAMING OF POPULATION AND FAMILY PLANNING ISSUES VIA SYNTACTIC
   NETWORK ANALYSIS}},
Journal = {{INTERNATIONAL JOURNAL OF MODERN PHYSICS C}},
