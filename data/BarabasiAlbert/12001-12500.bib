@article{ ISI:000254252700056,
Author = {Yin Yan-Ping and Zhang Duan-Ming and Tan Jin and Pan Gui-Jun and He
   Min-Hua},
Title = {{Continuous weight attack on complex network}},
Journal = {{COMMUNICATIONS IN THEORETICAL PHYSICS}},
Year = {{2008}},
Volume = {{49}},
Number = {{3}},
Pages = {{797-800}},
Month = {{MAR 15}},
Abstract = {{We introduce a continuous weight attack strategy and numerically
   investigate the effect of continuous weight attack strategy on the
   Barabasi-Albert (BA) scale-free network and the Erdos-Renyi (ER) random
   network. We use a weight coefficient omega to define the attack
   intensity. The weight coefficient omega increases continuously from 1 to
   infinity, where 1 represents no attack and infinity represents complete
   destructive attack. OUT results show that the continuous weight attack
   on two selected nodes with small omega (omega approximate to 3) could
   achieve the same damage of complete elimination of a single selected
   node on both BA and ER networks. It is found that the continuous weight
   attack on a single selected edge with small omega (omega approximate to
   2) can reach the same effect of complete elimination of a single edge on
   BA network, but on ER network the damage of the continuous weight attack
   on a single edge is close to but always smaller than that of complete
   elimination of edge even if omega is very large.}},
