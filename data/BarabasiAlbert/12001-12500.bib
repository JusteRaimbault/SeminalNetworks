@article{ ISI:000282982600028,
Author = {Almeida, Helvecio M. and de Queiroz, Eduardo M. G. and Cesar, Amilcar C.},
Title = {{Connection Admission Control for a WDM Optical Network Based on Traffic
   Monitoring and Bandwidth Release}},
Journal = {{IEEE COMMUNICATIONS LETTERS}},
Year = {{2010}},
Volume = {{14}},
Number = {{7}},
Pages = {{673-675}},
Month = {{JUL}},
Abstract = {{We proposed a connection admission control (CAC) to monitor the traffic
   in a multi-rate WDM optical network. The CAC searches for the shortest
   path connecting source and destination nodes, assigns wavelengths with
   enough bandwidth to serve the requests, supervises the traffic in the
   most required nodes, and if needed activates a reserved wavelength to
   release bandwidth according to traffic demand. We used a scale-free
   network topology, which includes highly connected nodes ( hubs), to
   enhance the monitoring procedure. Numerical results obtained from
   computational simulations show improved network performance evaluated in
   terms of blocking probability.}},
