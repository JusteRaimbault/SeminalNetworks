@inproceedings{ ISI:000297171900134,
Author = {Yu, Peng and Miao, Lei and Jia, Guo},
Book-Group-Author = {{IEEE}},
Title = {{Clustered Complex Echo State Networks for Traffic Forecasting with Prior
   Knowledge}},
