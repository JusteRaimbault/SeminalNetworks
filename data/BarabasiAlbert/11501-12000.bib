Title = {{Pathways toward understanding Macroscopic Quantum Phenomena}},
Booktitle = {{6TH INTERNATIONAL WORKSHOP DICE2012 SPACETIME - MATTER - QUANTUM
   MECHANICS: FROM THE PLANCK SCALE TO EMERGENT PHENOMENA}},
Series = {{Journal of Physics Conference Series}},
