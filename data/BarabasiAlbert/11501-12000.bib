@inproceedings{ ISI:000377498500003,
Author = {Tsugawa, Sho and Ohsaki, Hiroyuki},
Editor = {{Mangioni, G and Simini, F and Uzzo, SM and Wang, D}},
Title = {{Analysis of the Robustness of Degree Centrality against Random Errors in
   Graphs}},
Booktitle = {{COMPLEX NETWORKS VI}},
Series = {{Studies in Computational Intelligence}},
Year = {{2015}},
Volume = {{597}},
Pages = {{25-36}},
Note = {{6th International Workshop on Complex Networks (CompleNet), New York,
   NY, MAR 25-27, 2015}},
Abstract = {{Research on network analysis, which is used to analyze large-scale and
   complex networks such as social networks, protein networks, and brain
   function networks, has been actively pursued. Typically, the networks
   used for network analyses will contain multiple errors because it is not
   easy to accurately and completely identify the nodes to be analyzed and
   the appropriate relationships among them. In this paper, we analyze the
   robustness of centrality measure, which is widely used in network
   analyses, against missing nodes, missing links, and false links. We
   focus on the stability of node rankings based on degree centrality, and
   derive Top(m) and Overlap(m), which evaluate the robustness of node
   rankings. Through extensive simulations, we show the validity of our
   analysis, and suggest that our model can be used to analyze the
   robustness of not only degree centrality but also other types of
   centrality measures. Moreover, by using our analytical models, we
   examine the robustness of degree centrality against random errors in
   graphs.}},
