@inproceedings{ ISI:000310759600002,
Author = {Cupertino, Thiago Henrique and Zhao, Liang},
Editor = {{Costa, LD and Evsukoff, A and Mangioni, G and Menezes, R}},
Title = {{Traffic Congestion on Clustered Random Complex Networks}},
