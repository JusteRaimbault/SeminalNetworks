@article{ ISI:000344616700058,
Author = {Ouyang Bo and Jin Xin-Yu and Xia Yong-Xiang and Jiang Lu-Rong and Wu
   Duan-Po},
Title = {{Dynamic interplay between epidemics and cascades: Epidemic outbreaks in
   uncorrelated networks}},
Journal = {{ACTA PHYSICA SINICA}},
Year = {{2014}},
Volume = {{63}},
Number = {{21}},
Month = {{NOV 5}},
Abstract = {{Epidemics and cascades are independently studied topics in network
   science, but in practice, there are cases where they interact and
   neither of their effects can be ignored, e. g., when a digital virus
   spreads in a communication network that is transferring data. We have
   built a model to study their interplay in previous works. Here we
   present the epidemic threshold criteria of this model. When the
   infectivity is fixed, the tolerance parameter alpha, in capturing the
   capacity of nodes, must be larger than a critical value to fulfill the
   criteria, and at equilibrium the fraction of nodes both uninfected and
   un-failed is the largest at this critical point. So the the presentation
   of the epidemic threshold criteria is of significance.}},
