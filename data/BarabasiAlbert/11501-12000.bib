@article{ ISI:000334980100004,
Author = {Hajibagheri, A. R. and Hamzeh, A. and Alvari, H. and Hashemi, S.},
Title = {{SOCIAL NETWORKS COMMUNITY DETECTION USING THE SHAPLEY VALUE}},
Journal = {{IRANIAN JOURNAL OF SCIENCE AND TECHNOLOGY-TRANSACTIONS OF ELECTRICAL
   ENGINEERING}},
Year = {{2013}},
Volume = {{37}},
Number = {{E1}},
Pages = {{51-65}},
Month = {{JUN}},
Abstract = {{As a result of the increasing popularity of social networking websites
   like Facebook and Twitter, analysis of the structure of these networks
   has received significant attention. The most important part of these
   analyses is towards detecting communities. The aforementioned structures
   are usually known with extremely high inter-connections versus few
   intra-connections in the graphs. In this paper, in spite of most
   approaches being optimization based, we have addressed the community
   detection problem (CDP) by a novel framework based on Information
   Diffusion Model and Shapley Value Concept. Here, each node of the
   underlying graph is attributed to a rational agent trying to maximize
   its Shapley Value in the form of information it receives. Nash
   equilibrium of the game corresponds to the community structure of the
   graph. Compared with the other methods, our approach demonstrates
   promising results on the well-known real world and synthetic graphs.}},
