Title = {{RMDN: New Approach to Maximize Influence Spread}},
Booktitle = {{39TH ANNUAL IEEE COMPUTERS, SOFTWARE AND APPLICATIONS CONFERENCE
   (COMPSAC 2015), VOL 2}},
Series = {{Proceedings International Computer Software and Applications Conference}},
Year = {{2015}},
Pages = {{702-711}},
