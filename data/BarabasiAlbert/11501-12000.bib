@article{ ISI:000389653900005,
Author = {Wang, Yali and Song, Mei and Wei, Yifei},
Title = {{Complex Network Evolving Model with Preference and Anti-Preference for
   Cognitive Radio Ad Hoc Networks}},
Journal = {{AD HOC \& SENSOR WIRELESS NETWORKS}},
Year = {{2016}},
