@inproceedings{ ISI:000348502100017,
Author = {Weldehawaryat, Goitom and Wolthusen, Stephen},
Editor = {{Butts, J and Shenoi, S}},
Title = {{ASYNCHRONOUS BINARY BYZANTINE CONSENSUS OVER GRAPHS WITH POWER-LAW
   DEGREE SEQUENCE}},
Booktitle = {{CRITICAL INFRASTRUCTURE PROTECTION VIII}},
