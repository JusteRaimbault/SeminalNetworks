@article{ ISI:000335795700009,
Author = {Hooyberghs, Hans and Van Schaeybroeck, Bert and Indekeu, Joseph O.},
Title = {{Degree-dependent network growth: From preferential attachment to
   explosive percolation}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2014}},
Volume = {{89}},
Number = {{4}},
Month = {{APR 25}},
Abstract = {{We present a simple model of network growth and solve it by writing the
   dynamic equations for its macroscopic characteristics such as the degree
   distribution and degree correlations. This allows us to study carefully
   the percolation transition using a generating functions theory. The
   model considers a network with a fixed number of nodes wherein links are
   introduced using degree-dependent linking probabilities p(k). To
   illustrate the techniques and support our findings using Monte Carlo
   simulations, we introduce the exemplary linking rule p(k) alpha
   k(-alpha), with alpha between -1 and +infinity. This parameter may be
   used to interpolate between different regimes. For negative alpha, links
   are most likely attached to high-degree nodes. On the other hand, in
   case alpha > 0, nodes with low degrees are connected and the model
   asymptotically approaches a process undergoing explosive percolation.}},
