@article{ ISI:000291946800008,
Author = {Wang, Mingyang and Yu, Guang and Yu, Daren},
Title = {{THE PREFERENTIAL ATTACHMENT MECHANISM BASING ON WEIGHTED PAST CITATIONS}},
