@inproceedings{ ISI:000365955500016,
Author = {Kouroupas, Georgios and Markakis, Evangelos and Papadimitriou, Christos
   and Rigas, Vasileios and Sideri, Martha},
Editor = {{Hoefer, M}},
Title = {{The Web Graph as an Equilibrium}},
