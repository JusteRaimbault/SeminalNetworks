@inproceedings{ ISI:000310859800031,
Author = {Choobdar, Sarvenaz and Silva, Fernando and Ribeiro, Pedro},
Editor = {{Antunes, L and Pinto, HS}},
Title = {{Network Node Label Acquisition and Tracking}},
