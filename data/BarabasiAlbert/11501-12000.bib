@article{ ISI:000359493200005,
Author = {Eum, Suyong and Jibiki, Masahiro and Murata, Masayuki and Asaeda,
   Hitoshi and Nishinaga, Nozomu},
Title = {{Self-Organizing Name Resolution System in a Small World}},
Journal = {{IEICE TRANSACTIONS ON COMMUNICATIONS}},
Year = {{2014}},
Volume = {{E97B}},
Number = {{12}},
Pages = {{2606-2615}},
Month = {{DEC}},
Abstract = {{This article introduces a self-organizing model which builds the
   topology of a DHT mapping system for ICN. Due to its self-organizing
   operation and low average degree of maintenance, the management overhead
   of the system is reduced dramatically, which yields inherent
   scalability. The proposed model can improve latency by around 10\%
   compared to an existing approach which has a near optimal average
   distance when the number of nodes and degree are given. In particular,
   its operation is simple which eases maintenance concerns. Moreover, we
   analyze the model theoretically to provide a deeper understanding of the
   proposal.}},
