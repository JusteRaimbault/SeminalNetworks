Title = {{Recommending Items via Interest-Similar Cluster Identification in Online
   Social Networks}},
Booktitle = {{2013 THIRD INTERNATIONAL CONFERENCE ON INSTRUMENTATION \& MEASUREMENT,
   COMPUTER, COMMUNICATION AND CONTROL (IMCCC)}},
Series = {{International Conference on Instrumentation Measurement Computer
   Communication and Control}},
