@inproceedings{ ISI:000345626300058,
Author = {Rubina, Tatjana and Stalidzans, Egils},
Book-Group-Author = {{IEEE}},
Title = {{BINESA - a software tool for evolution modelling of biochemical
   networks' structure}},
Booktitle = {{14TH IEEE INTERNATIONAL SYMPOSIUM ON COMPUTATIONAL INTELLIGENCE AND
   INFORMATICS (CINTI)}},
Series = {{International Symposium on Computational Intelligence and Informatics}},
