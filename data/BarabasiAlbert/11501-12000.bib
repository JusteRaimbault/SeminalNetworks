@article{ ISI:000291185900010,
Author = {Cappelli, Amedeo and Coscia, Michele and Giannotti, Fosca and Pedreschi,
   Dino and Rinzivillo, Salvo},
Title = {{THE SOCIAL NETWORK OF DANTE'S INFERNO}},
Journal = {{LEONARDO}},
Year = {{2011}},
Volume = {{44}},
Number = {{3}},
Pages = {{246-247}},
Abstract = {{This work aims to approach the phenomenon of culture through the
   development of new methods and more powerful tools to capture the
   content of digitally stored literary material. The authors chose as a
   test bed Dante's characters of al di la, a domain consisting in a set of
   data and relations complex enough to sharpen existing tools. The methods
   of investigation under development should help scholars of a literary
   text to concretize part of their interpretative intentions. For this
   purpose, the authors adopt advanced methodologies suitable to obtain a
   rich representation, expressing the multi-relational network inherent in
   the text.}},
Publisher = {{MIT PRESS}},
Address = {{55 HAYWARD STREET, CAMBRIDGE, MA 02142 USA}},
