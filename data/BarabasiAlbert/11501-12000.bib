@article{ ISI:000366207400004,
Author = {Kazakevicius, Rytis and Ruseckas, Julius},
Title = {{Power-law statistics from nonlinear stochastic differential equations
   driven by Levy stable noise}},
Journal = {{CHAOS SOLITONS \& FRACTALS}},
Year = {{2015}},
Volume = {{81}},
Number = {{B}},
Pages = {{432-442}},
Month = {{DEC}},
Abstract = {{Anomalous diffusion occurring in complex dynamical systems can often be
   described by Langevin equations driven by Levy stable noise. Nonlinear
   stochastic differential equations yielding power-law steady state
   distribution and generating signals with 1/f power spectral density can
   be generalized by replacing the Gaussian noise with a more general Levy
   stable noise. These nonlinear equations can generate signals exhibiting
   anomalous diffusion: either sub-diffusion or super-diffusion. In a
   special case when stability index is alpha = 2, we retain the equations
   with the Gaussian noise. We investigate numerically the frequency range
   where the spectrum has 1/f form and demonstrate that this frequency
   range depends on power-law exponent in steady state distribution as well
   as on the index of stability alpha. We expect that this generalization
   may be useful for describing 1/f fluctuations in the complex systems
   exhibiting anomalous diffusion. (C) 2015 Elsevier Ltd. All rights
   reserved.}},
