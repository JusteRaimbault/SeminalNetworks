@article{ ISI:000297230700031,
Author = {Gao, Liang and Si, Bingfeng and Yang, Xiaobao and Sun, Huijun and Gao,
   Ziyou},
Title = {{A matrix method for finding last common nodes in an origin-based traffic
   assignment problem}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2012}},
Volume = {{391}},
Number = {{1-2}},
Pages = {{285-290}},
Month = {{JAN 1}},
Abstract = {{Many algorithms have been presented to solve the traffic assignment
   problem. Recently, Bar-Gera introduced the concept of ``last common
   node{''} into an origin-based algorithm to solve the traffic assignment
   problem. However, how to find the last common nodes has not been
   investigated in detail. In this paper, we present a matrix method for
   finding the last common nodes in an origin-based traffic assignment
   problem. In an acyclic network, the power of binary adjacency matrix
   (A(k)) will record the number of directed simple routes of length k.
   Taking this feature into consideration, S(p), the total number of the
   simple routes related to an origin node p in the subnetwork G(p), is
   counted by S(p) = Sigma(k) A(p)(k) = (1 - A(p))(-1). Then, every common
   node for OD pair pq is picked out by comparing (S(p))(pr) x (S(p))(rq)
   and (S(p))(pq), and the last common node for OD pair pq is filtered out
   according to the topological order I(r). Our method is implemented to
   find out all LCNs for all n {*} (n - 1) OD pairs, then tested on three
   kinds of model networks and four urban transportation networks. We find
   that the overall computing time T and the size of network n, has a
   relation like T similar to O(n(3)), which is better than the theoretical
   estimation O(n(4)). (C) 2011 Elsevier B.V. All rights reserved.}},
