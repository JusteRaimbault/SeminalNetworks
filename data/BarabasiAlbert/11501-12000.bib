@article{ ISI:000316732600079,
Author = {Li Yong and Dou Fei-Ling and Fan Ying and Di Zeng-Ru},
Title = {{Theoretical analysis on optimal navigation with total energy restriction
   in a two-dimensional lattice}},
Journal = {{ACTA PHYSICA SINICA}},
Year = {{2012}},
Volume = {{61}},
Number = {{22}},
Abstract = {{Recently, a certain total energy constraint A = cN was introduced into
   the Kleinberg's navigation model, where A is the total length of the
   long-range connections, c is a positive constant and N is the network
   size. The simulation results obtained in the one and two-dimensional
   cases indicate that with total cost restricted the optimal power-law
   exponent for adding extra long-range links between any two nodes seems
   to be alpha = d + 1, where d is the dimension of the underlying lattice.
   Based on the mean field theory, the navigation process on the
   two-dimensional energy constrained navigation model is described by
   dynamical equations in this paper. Based on our theoretical analysis and
   the numerical results of the dynamical equations, we prove that for
   large networks and comparatively small total energy, the optimal
   power-law exponent is alpha = 3 for the two-dimensional case. Our
   results can perfectly correspond to simulations reported previously.}},
