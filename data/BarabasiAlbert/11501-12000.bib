@article{ ISI:000305897600008,
Author = {Trossen, Dirk},
Title = {{Turing, the Internet and a Theory for Architecture: A (Fictional?) Tale
   in Three Parts}},
Journal = {{ACM SIGCOMM COMPUTER COMMUNICATION REVIEW}},
Year = {{2012}},
Volume = {{42}},
Number = {{3}},
Pages = {{47-53}},
Month = {{JUL}},
Abstract = {{The late noughties have seen an influx of work in different scientific
   disciplines, all addressing the question of `design' and `architecture'.
   It is a battle between those advocating the theory of `emergent
   properties' and others who strive for a `theory for architecture'. We
   provide a particular insight into this battle, represented in the form
   of a story that focuses on the role of a possibly unusual protagonist
   and his influence on computer science, the Internet, architecture and
   beyond. We show his relation to one of the great achievements of system
   engineering, the Internet, and the possible future as it might unfold.}},
