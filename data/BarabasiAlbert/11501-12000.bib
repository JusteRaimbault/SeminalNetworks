Author = {Matsui, Tomomi and Miwa, Hiroyoshi},
Editor = {{Xhafa, F and Barolli, L and Palmieri, F and Koeppen, M and Loia, V}},
Title = {{Method for Finding Protected Nodes for Robust Network against Node
