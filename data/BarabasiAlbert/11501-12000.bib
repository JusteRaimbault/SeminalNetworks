@inproceedings{ ISI:000360222300017,
Author = {Romero, Oscar J. and Lebiere, Christian},
Editor = {{Grimaldo, F and Norling, E}},
Title = {{Cognitive Modeling of Behavioral Experiments in Network Science Using
   ACT-R Architecture}},
Booktitle = {{MULTI-AGENT-BASED SIMULATION XV}},
