@inproceedings{ ISI:000380440400013,
Author = {Meghanathan, Natarajan},
Editor = {{Stoica, A and Kang, BH and Kim, TH}},
Title = {{A Random Network Model with High Clustering Coefficient and Variation in
   Node Degree}},
