@article{ ISI:000342160700006,
Author = {Zhang, Haifeng and Jin, Zhen and Wang, Binghong},
Title = {{Dynamics of opinion formation with strengthen selection probability}},
Journal = {{INTERNATIONAL JOURNAL OF MODERN PHYSICS C}},
Year = {{2014}},
Volume = {{25}},
Number = {{10}},
Month = {{OCT}},
Abstract = {{The local majority rule is extensively accepted as a paradigmatic model
   to reflect the formation of opinion. In this paper, we study a model of
   opinion formation where opinion update rule is not based on the majority
   rule or linear selection probability but on a strengthen selection
   probability controlled by an adjustable parameter beta. In particular,
   our proposed probability function can proximately fit the two extreme
   cases-linear probability function and majority rule or in between the
   two cases under different values of beta. By studying such model on
   different kinds of networks, including different regular networks and
   complex networks, we find that there exists an optimal value of beta
   giving the most efficient convergence to consensus regardless of the
   topology of networks. This work reveals that, compared with the majority
   rule and linear selection probability, the strengthen selection
   probability might be a more proper model in understanding the formation
   of opinions in society.}},
