@article{ ISI:000311018800013,
Author = {Li, Haihong and Dai, Qionglin and Cheng, Hongyan and Yang, Junzhong},
Title = {{Cooperation in an evolutionary prisoner's dilemma game with
   probabilistic strategies}},
Journal = {{CHAOS SOLITONS \& FRACTALS}},
Year = {{2012}},
Volume = {{45}},
Number = {{11}},
Pages = {{1397-1403}},
Month = {{NOV}},
Abstract = {{In this work, we investigate an evolutionary prisoner's dilemma game in
   structured populations with probabilistic strategies instead of the pure
   strategies of cooperation and defection. We explore the model in details
   by considering different strategy update rules and different population
   structures. We find that the distribution of probabilistic strategies
   patterns is dependent on both the interaction structures and the
   updating rules. We also find that, when an individual updates her
   strategy by increasing or decreasing her probabilistic strategy a
   certain amount towards that other opponent, there exists an optimal
   increment of the probabilistic strategy at which the cooperator
   frequency reaches its maximum. Crown Copyright (C) 2012 Published by
   Elsevier Ltd. All rights reserved.}},
