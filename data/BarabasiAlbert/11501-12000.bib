@article{ ISI:000208832400002,
Author = {Aldrich, Preston R. and Horsley, Robert K. and Turcic, Stefan M.},
Title = {{Symmetry in the Language of Gene Expression: A Survey of Gene Promoter
   Networks in Multiple Bacterial Species and Non-sigma Regulons}},
Journal = {{SYMMETRY-BASEL}},
Year = {{2011}},
Volume = {{3}},
Number = {{4}},
Pages = {{750-766}},
Month = {{DEC}},
Abstract = {{The language of gene expression displays topological symmetry. An
   important step during gene expression is the binding of transcriptional
   proteins to DNA promoters adjacent to a gene. Some proteins bind to many
   promoters in a genome, defining a regulon of genes wherein each promoter
   might vary in DNA sequence relative to the average consensus. Here we
   examine the linguistic organization of gene promoter networks, wherein
   each node in the network represents a promoter and links between nodes
   represent the extent of base pair-sharing. Prior work revealed a fractal
   nucleus in several sigma-factor regulons from Escherichia coli. We
   extend these findings to show fractal nuclei in gene promoter networks
   from three bacterial species, E. coli, Bacillus subtilis, and
   Pseudomonas aeruginosa. We surveyed several non-sigma transcription
   factors from these species and found that many contain a nucleus that is
   both visually and numerically fractal. Promoter footprint size scaled as
   a negative power-law with both information entropy and fractal
   dimension, while the latter two parameters scaled positively and
   linearly. The fractal dimension of the diffuse networks (d(B) = similar
   to 1.7) was close to that expected of a diffusion limited aggregation
   process, confirming prior predictions as to a possible mechanism for
   development of this structure.}},
