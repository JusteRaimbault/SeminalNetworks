@article{ ISI:000474503900046,
Author = {Karataieva, Tatiana and Koshmanenko, Volodymyr and Krawczyk, Malgorzata
   J. and Kulakowski, Krzysztof},
Title = {{Mean field model of a game for power}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2019}},
Volume = {{525}},
Pages = {{535-547}},
Month = {{JUL 1}},
Abstract = {{Our aim is to model a game for power (equivalent to total energy) as a
   dynamical process, where an excess of power possessed by a player allows
   him to gain even more power. Such a positive feedback is often termed as
   the Matthew effect. Analytical and numerical methods allow to identify a
   set of stationary states, i.e. fixed points of the model dynamics. The
   positions of the unstable fixed points give an insight on the basins of
   attraction of the stable fixed points. The results are interpreted in
   terms of modeling of coercive power. (C) 2019 Elsevier B.V. All rights
   reserved.}},
