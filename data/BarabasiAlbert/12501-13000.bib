@article{ ISI:000461500100017,
Author = {Cheng, Liyan and Qiu, Jianlong and Chen, Xiangyong and Zhang, Ancai and
   Yang, Chengdong and Chen, Xiao},
Title = {{Adaptive aperiodically intermittent control for pinning synchronization
   of directed dynamical networks}},
