@article{ ISI:000466061000018,
Author = {Tian, Meng and Dong, Zhengcheng and Cui, Mingjian and Wang, Jianhui and
   Wang, Xianpei and Zhao, Le},
Title = {{Energy-supported cascading failure model on interdependent networks
   considering control nodes}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2019}},
Volume = {{522}},
Pages = {{195-204}},
Month = {{MAY 15}},
Abstract = {{Cascading failures are emergent phenomena on interdependent networks and
   plenty of models have been proposed to explore the features of cascading
   failures. However, few of them focus on energy-supported dependence
   relations between interdependent networks including control nodes. In
   this paper, a load-based energy-supported cascading failure model is
   proposed on interdependent networks, where the energy control factor
   lambda and control nodes are considered. According to the numerical
   simulations, it is found that as the energy control factor lambda
   increases, both interdependent Erdos-Renyi (ER) networks and scale-free
   (SF) networks become more vulnerable. Meanwhile, with the increase of
   energy control factor lambda, interdependent SF networks become more
   robust than ER networks against random failures. In addition, increasing
   the power exponent gamma can improve the robustness of interdependent SF
   networks. Finally, although coupling preferences and control node
   setting strategies have few impacts on cascading failures on
   interdependent ER networks, the assortative coupling preference (AC) and
   betweenness control node setting strategy (BS) can improve the
   robustness of interdependent SF networks. (C) 2019 Elsevier B.V. All
   rights reserved.}},
