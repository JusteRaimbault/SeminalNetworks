@article{ ISI:000473268300010,
Author = {Zhang, Wenxin and Yang, Guanxue and Wang, Lin},
Title = {{Reconstruction of time-varying small-world networks incorporating
   structural priors}},
Journal = {{IFAC PAPERSONLINE}},
Year = {{2019}},
Volume = {{52}},
Number = {{3}},
Pages = {{49-54}},
Note = {{15th International-Federation-of-Automatic-Control (IFAC) Symposium on
   Large Scale Complex Systems (IFAC LSS), Delft Univ Technol, Delft,
   NETHERLANDS, MAY 26-28, 2019}},
Organization = {{Int Federat Automat Control; Int Federat Automat Control Tech Comm 5 4
   Large Scale Complex Syst; Int Federat Automat Control Tech Comm 1 5
   Networked Syst}},
Abstract = {{When inferring the undergoing network structure, which describes the
   dynamic mutual influence among large scale variables, it is a challenge
   to take full advantage of structural prior information when it is
   available. In this paper, we focus on reconstruction of
   piecewise-constant time-varying small-world networks. Specifically, we
   propose an identification method incorporating structural properties as
   prior information, including the average degree of the network. On the
   one hand, we adjust the network sparsity by re-weighting l(1) norm
   according to the deviation of the estimated average degree, based on the
   assumption that the average degree is almost constant over time. On the
   other hand, for each node in the network, we encourage the existence of
   potential associated edges while discouraging non-existing edges based
   on predictions from the previous iteration. Finally, an adaptive LASSO
   algorithm is utilized to uncover the time-varying structures which
   performs better on small-world networks when comparing with the method
   without prior information. (C) 2019, IFAC (International Federation of
   Automatic Control) Hosting by Elsevier Ltd. All rights reserved.}},
