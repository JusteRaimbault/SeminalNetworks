@article{ ISI:000471967800003,
Author = {Bunimovich, L. A. and Smith, D. C. and Webb, B. Z.},
Title = {{Specialization Models of Network Growth}},
Journal = {{JOURNAL OF COMPLEX NETWORKS}},
