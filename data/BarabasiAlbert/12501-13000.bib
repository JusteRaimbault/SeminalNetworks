@article{ ISI:000474377300010,
Author = {Campos, Guilherme Oliveira and Moreira, Edre and Meira, Jr., Wagner and
   Zimek, Arthur},
Title = {{Outlier Detection in Graphs: A Study on the Impact of Multiple Graph
   Models}},
