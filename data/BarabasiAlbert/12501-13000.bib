@article{ ISI:000467052900013,
Author = {Huang, Wanyu and Krejci, Caroline C. and Dorneich, Michael C. and Passe,
   Ulrike and Shenk, Linda and Stonewall, Jacklin},
Title = {{Analyzing residential weatherization decisions using hybrid simulation
   modeling}},
Journal = {{BUILDING SIMULATION}},
