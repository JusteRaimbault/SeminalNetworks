@article{ ISI:000476966100042,
Author = {Liang, Wei and Wang, Kunpeng},
Title = {{Relationships among the statistical parameters in evolving modern
   Chinese linguistic co-occurrence networks}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2019}},
Volume = {{524}},
Pages = {{532-539}},
Month = {{JUN 15}},
Abstract = {{A total of 206 co-occurrence networks of Chinese characters and words
   are constructed from modern Chinese articles. Spectra of the adjacency
   matrices especially the largest eigenvalues lambda(1), the second
   largest eigenvalues, the smallest eigenvalues, the number of different
   eigenvalues N-lambda, the spectral distribution, and the spectral
   density rho(lambda) are computed and studied. Some formulas about the
   statistical parameters are given by applying multiple linear regression
   analysis. It is found that not only the growth trends of lambda(1) and
   N-lambda but also the properties of spectral distributions and
   rho(lambda) do not change with the network size. Therefore, the spectral
   behavior of the modern Chinese linguistic topology is consistent over
   time unless something having a major impact on Chinese language happen.
   (C) 2019 Elsevier B.V. All rights reserved.}},
