@article{ ISI:000474628100027,
Author = {Kong, Peng-Yong},
Title = {{Optimal Configuration of Interdependence Between Communication Network
   and Power Grid}},
Journal = {{IEEE TRANSACTIONS ON INDUSTRIAL INFORMATICS}},
