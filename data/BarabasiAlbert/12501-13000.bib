@article{ ISI:000460781800010,
Author = {Luo, Jianxi and Whitney, Daniel E.},
Title = {{The Hierarchy-Niche Model for Supply Networks}},
Journal = {{PRODUCTION AND OPERATIONS MANAGEMENT}},
