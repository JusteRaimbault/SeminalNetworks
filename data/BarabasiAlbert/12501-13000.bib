@article{ ISI:000463454400001,
Author = {Sriram, Lalitha Madhavi Konila and Ulak, Mehmet Baran and Ozguven, Eren
   Erman and Arghandeh, Reza},
Title = {{Multi-Network Vulnerability Causal Model for Infrastructure
   Co-Resilience}},
