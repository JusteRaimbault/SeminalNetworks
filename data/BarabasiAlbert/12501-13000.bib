@article{ ISI:000452941100057,
Author = {de Arruda, Henrique F. and Silva, Filipi N. and Comin, Cesar H. and
   Amancio, Diego R. and Costa, Luciano da F.},
Title = {{Connecting network science and information theory}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
