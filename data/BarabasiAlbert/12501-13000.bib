@article{ ISI:000475305600008,
Author = {Riris, Philip and Oliver, Jose},
Title = {{Patterns of Style, Diversity, and Similarity in Middle Orinoco Rock Art
   Assemblages}},
Journal = {{ARTS}},
Year = {{2019}},
Volume = {{8}},
Number = {{2}},
Month = {{JUN}},
Abstract = {{The area encompassed by the Orinoco river basin is home to some of the
   largest and most diverse rock art sites in lowland South America. In
   this paper, we aim to formally describe the spatial distribution and
   stylistic attributes of rock engravings and paintings on both banks of
   the Orinoco, centred on the atures Rapids. Drawing on an exhaustive
   literature review and four years of field survey, we identify salient
   aspects of this corpus by investigating patterns of diversity and
   similarity. Based on a stylistic classification of Middle Orinoco rock
   art, this permits us to discuss potential links, as well as notable
   discontinuities, within the assemblage and possibly further afield. We
   consider the theoretical implications of our work for the study of
   pre-Columbian art and conclude with some suggestions for advances in
   methods for achieving the goal of deriving broader syntheses.}},
Publisher = {{MDPI}},
Address = {{ST ALBAN-ANLAGE 66, CH-4052 BASEL, SWITZERLAND}},
Type = {{Article}},
