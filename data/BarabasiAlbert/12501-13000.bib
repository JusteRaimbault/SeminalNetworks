@article{ ISI:000474316300012,
Author = {Carayol, Nicolas and Berge, Laurent and Cassi, Lorenzo and Roux, Pascale},
Title = {{Unintended triadic closure in social networks: The strategic formation
   of research collaborations between French inventors}},
Journal = {{JOURNAL OF ECONOMIC BEHAVIOR \& ORGANIZATION}},
Year = {{2019}},
Volume = {{163}},
Pages = {{218-238}},
Month = {{JUL}},
Abstract = {{Observing that most social networks are clustered, the literature often
   argues that agents are more willing to form links that close triangles.
   We challenge this idea by proposing a simple model of new collaboration
   formation that shows why network clustering may arise even though agents
   do not ``like{''} network closure. We address empirically this question
   on the longitudinal evolution of the French co-invention network, and
   find that two inventors are less likely to form a first research
   collaboration when they have common partners. Our findings further
   reveal the preferences of inventors towards forming non-redundant
   connections. (C) 2018 Published by Elsevier B.V.}},
