@article{ ISI:000457302800006,
Author = {Kobayashi, Koichi},
Title = {{Predictive Pinning Control with Communication Delays for Consensus of
   Multi-Agent Systems}},
Journal = {{IEICE TRANSACTIONS ON FUNDAMENTALS OF ELECTRONICS COMMUNICATIONS AND
   COMPUTER SCIENCES}},
Year = {{2019}},
Volume = {{E102A}},
Number = {{2}},
Pages = {{359-364}},
Month = {{FEB}},
Abstract = {{In this paper, based on the policy of model predictive control, a new
   method of predictive pinning control is proposed for the consensus
   problem of multi-agent systems. Pinning control is a method that the
   external control input is added to some agents (pinning nodes), e.g.,
   leaders. By the external control input, consensus to a certain target
   value (not the average of the initial states) and faster consensus are
   achieved. In the proposed method, the external control input is
   calculated by the controller node connected to only pinning nodes. Since
   the states of all agents are required in calculation of the external
   control input, communication delays must be considered. The proposed
   algorithm includes not only calculation of the external control input
   but also delay compensation. The effectiveness of the proposed method is
   presented by a numerical example.}},
