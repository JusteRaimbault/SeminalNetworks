@article{ ISI:000307804000002,
Author = {Devlin, Stephen and Treloar, Thomas},
Title = {{Network-based criterion for the success of cooperation in an
   evolutionary prisoner's dilemma}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2012}},
Volume = {{86}},
Number = {{2, 2}},
Month = {{AUG 22}},
Abstract = {{We consider an evolutionary prisoner's dilemma on a random network. We
   introduce a simple quantitative network-based parameter and show that it
   effectively predicts the success of cooperation in simulations on the
   network. The criterion is shown to be accurate on a variety of networks
   with degree distributions ranging from regular to Poisson to scale free.
   The parameter allows for comparisons of random networks regardless of
   their underlying topology. Finally, we draw analogies between the
   criterion for the success of cooperation introduced here and existing
   criteria in other contexts.}},
