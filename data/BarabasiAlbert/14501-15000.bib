@article{ ISI:000333352800106,
Author = {Liu, Longlong and Qu, Jieqiong and Zhou, Xilong and Liu, Xuefeng and
   Zhang, Zhaobao and Wang, Xumin and Liu, Tao and Liu, Guiming},
Title = {{Discovery of a Strongly-Interrelated Gene Network in Corals under
   Constant Darkness by Correlation Analysis after Wavelet Transform on
   Complex Network Model}},
Journal = {{PLOS ONE}},
Year = {{2014}},
Volume = {{9}},
Number = {{3}},
Month = {{MAR 20}},
Abstract = {{Coral reefs occupy a relatively small portion of sea area, yet serve as
   a crucial source of biodiversity by establishing harmonious ecosystems
   with marine plants and animals. Previous researches mainly focused on
   screening several key genes induced by stress. Here we proposed a novel
   method-correlation analysis after wavelet transform of complex network
   model, to explore the effect of light on gene expression in the coral
   Acropora millepora based on microarray data. In this method, wavelet
   transform and the conception of complex network were adopted, and 50 key
   genes with large differences were finally captured, including both
   annotated genes and novel genes without accurate annotation. These
   results shed light on our understanding of coral's response toward light
   changes and the genome-wide interaction among genes under the control of
   biorhythm, and hence help us to better protect the coral reef
   ecosystems. Further studies are needed to explore how functional
   connections are related to structural connections, and how connectivity
   arises from the interactions within and between different systems. The
