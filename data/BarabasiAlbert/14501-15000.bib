@article{ ISI:000301429400020,
Author = {Wang, H. J. and Qi, G. X. and Chen, L. and Qiu, C. and Huang, H. B.},
Title = {{Three types of synchronisation in complex modular networks}},
Journal = {{EUROPEAN PHYSICAL JOURNAL B}},
Year = {{2012}},
Volume = {{85}},
Number = {{1}},
Month = {{JAN}},
Abstract = {{We study the synchronisation dynamics in complex modular networks.
   Through theoretical analysis, three types of synchronisation, i.e.
