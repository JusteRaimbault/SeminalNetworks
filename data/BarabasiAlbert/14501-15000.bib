Title = {{Engineering Tourism System of Systems for Guided Emergence and Desired
   Capability From a perspective of intelligence approach}},
Booktitle = {{INTERNATIONAL CONFERENCE ON SIGNAL PROCESSING, IMAGE PROCESSING AND
   PATTERN RECOGNITION (ICSIPR 2013)}},
Year = {{2013}},
Pages = {{70-74}},
Note = {{International Conference on Signal Processing, Image Processing and
