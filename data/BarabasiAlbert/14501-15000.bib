@article{ ISI:000309867000032,
Author = {Du Kang, Deok and Lee, Dong-In and Chang, Ki-Ho and Park, Jong-Kil and
   Kim, Kyungsik},
Title = {{Characteristics of network metrics in seismic phenomena}},
Journal = {{JOURNAL OF THE KOREAN PHYSICAL SOCIETY}},
Year = {{2012}},
Volume = {{61}},
Number = {{7}},
Pages = {{1163-1166}},
Month = {{OCT}},
Abstract = {{In this paper, we investigate the seismic network by considering the
   cell resolution and the temporal causality of an earthquake from seismic
   data taken in southern California of the USA. For our case, the seismic
   activity data can be mapped onto a growing network with links between
   consecutive events by using the complex-network approach method. After
   we specifically show a seismic network against spatial heterogeneity and
   scales in an cell, we simulate and analyze the topological properties of
   our seismic activity network. We then compare our results with other
   findings that estimate the global network metrics.}},
