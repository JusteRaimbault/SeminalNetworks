@inproceedings{ ISI:000327260900159,
Author = {Niu, Jianwei and Peng, Jing and Tong, Chao},
Book-Group-Author = {{IEEE}},
Title = {{Patterns and A Generator of Social Networks: From the Perspective of
   Non-giant Connected Components}},
Booktitle = {{2013 15TH INTERNATIONAL CONFERENCE ON ADVANCED COMMUNICATION TECHNOLOGY
   (ICACT)}},
Series = {{International Conference on Advanced Communication Technology}},
Year = {{2013}},
