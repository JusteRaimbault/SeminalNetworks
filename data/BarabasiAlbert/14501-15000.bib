@inproceedings{ ISI:000343821400030,
Author = {Hoang, Dinh Tien and Hasegawa, Go and Murata, Masayuki},
Book-Group-Author = {{IEEE}},
Title = {{Monitoring available bandwidth in overlay networks using local
   information exchange}},
