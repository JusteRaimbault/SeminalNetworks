@inproceedings{ ISI:000350701800014,
Author = {Wolf, Ingo and de Haan, Gerhard},
Editor = {{Gausemeier, J}},
Title = {{The Simulated Customer - Computer-based Market Analysis using the
   Example of Electric Vehicles}},
