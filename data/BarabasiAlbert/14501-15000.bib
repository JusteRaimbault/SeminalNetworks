@inproceedings{ ISI:000343883800055,
Author = {Oliveira, Douglas and Carvalho, Marco},
Editor = {{Cai, Z and Zelikovsky, A and Bourgeois, A}},
Title = {{Empirical Models for Complex Network Dynamics: A Preliminary Study}},
Booktitle = {{COMPUTING AND COMBINATORICS, COCOON 2014}},
