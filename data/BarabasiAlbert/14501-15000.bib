@inproceedings{ ISI:000395422505027,
Author = {Li, Huijia and Liu, Zhi-Ping and Chen, Luonan and Zhang, Xiang-Sun},
Editor = {{Cheng, D}},
Title = {{Identification of overlapping communities in protein interaction
   networks using multi-scale local information expansion}},
Booktitle = {{PROCEEDINGS OF THE 10TH WORLD CONGRESS ON INTELLIGENT CONTROL AND
   AUTOMATION (WCICA 2012)}},
Year = {{2012}},
Pages = {{5071-5076}},
Note = {{10th World Congress on Intelligent Control and Automation (WCICA),
   Beijing, PEOPLES R CHINA, JUL 06-08, 2012}},
Organization = {{Chinese Acad Sci, Acad Math \& Syst Sci; IEEE Robot \& Automat Soc; IEEE
   Control Syst Soc; Natl Nat Sci Fdn China; Chinese Assoc Automat; Chinese
   Assoc Artificial Intelligence; IEEE RACS Hong Kong Chapter; IEEE Control
   Syst Soc Beijing Chapter; IEEE Control Syst Soc Singapore Chapter}},
Abstract = {{Most existing clustering approaches require the complete graph
   information, which is often impractical for large-scale protein-protein
   interaction networks. We proposed a novel algorithm which does not
   embrace the universal approach but instead tries to focus on local ties
   and model multi-scales of biological interactions in these networks. It
   identifies functional leaders and modules around these leaders using
   local information. It naturally supports overlapping information by
   associating each node with a membership vector that describes its
   involvement of each community. In addition to uncover overlapping
   communities, we can describe different multi-scale partitions allowing
   to tune the characteristic size of biologically meaningful modules. The
   high efficiency and accuracy of the proposed algorithm make it feasible
   to be used for accurately detecting community structure in real
   biomolecular networks.}},
