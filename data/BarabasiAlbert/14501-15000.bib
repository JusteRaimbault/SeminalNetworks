@inproceedings{ ISI:000349617700366,
Author = {Xie, Fuli and Cheng, Guangquan},
Editor = {{Liu, HW and Wang, G and Zhang, GW}},
Title = {{A Link Prediction Model Based on Similarity Between Links}},
Booktitle = {{MATERIAL SCIENCE, CIVIL ENGINEERING AND ARCHITECTURE SCIENCE, MECHANICAL
   ENGINEERING AND MANUFACTURING TECHNOLOGY II}},
Series = {{Applied Mechanics and Materials}},
Year = {{2014}},
Volume = {{651-653}},
Pages = {{1748-1752}},
Note = {{3rd International Conference on Advanced Engineering Materials and
   Architecture Science (ICAEMAS), Huhhot, PEOPLES R CHINA, JUL 26-27, 2014}},
Abstract = {{With the development of network science, the link prediction problem has
   attracted more and more attention. Among which, link prediction methods
   based on similarity has been most widely studied. Previous methods
   depicting similarity of nodes mainly consider their common neighbors.
   But in this paper, from the view of network environment of nodes, which
