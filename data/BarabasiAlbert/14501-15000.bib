@inproceedings{ ISI:000392912000006,
Author = {Derhami, Vali and Paksima, Javad and Khajeh, Homa},
Book-Group-Author = {{IEEE}},
Title = {{RLRAUC: Reinforcement Learning based Ranking Algorithm using User Clicks}},
Booktitle = {{2014 4TH INTERNATIONAL CONFERENCE ON COMPUTER AND KNOWLEDGE ENGINEERING
   (ICCKE)}},
