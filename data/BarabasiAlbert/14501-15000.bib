@article{ ISI:000335454400016,
Author = {Zhao, Ying-Si and Liu, Yan-Ping and Shen, Bo},
Title = {{AN EVALUATING MODEL OF INNOVATION PERFORMANCE OF ENTERPRISES IN
   INDUSTRIAL CLUSTER}},
Journal = {{PAKISTAN JOURNAL OF STATISTICS}},
Year = {{2013}},
Volume = {{29}},
Number = {{6, SI}},
Pages = {{1117-1128}},
Month = {{DEC}},
Abstract = {{Innovation performance of enterprises in industrial cluster is an
   important factor to judge of the efficiency of innovation network.
   However, it is difficult to measure it directly. In this paper, we
   propose a model for evaluating enterprise's innovation performance. The
   model uses native relationship characteristic of enterprises to express
   their closeness and then constructs a uniform estimating function.
   Analysis and numerical results demonstrate that the proposed evaluating
   model could reflect the innovation performance of enterprises in
   innovation network of industrial cluster and be used to scale their
   importance. Furthermore, it should be also useful to determine the path
   of information and knowledge propagation in such networks.}},
