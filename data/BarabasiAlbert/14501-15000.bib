@inproceedings{ ISI:000382032100148,
Author = {Al-Shyoukh, Ibrahim and Chasparis, Georgios and Shamma, Jeff S.},
Book-Group-Author = {{IEEE}},
Title = {{Coevolutionary modeling in network formation}},
