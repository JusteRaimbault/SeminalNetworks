@inproceedings{ ISI:000346492602154,
Author = {Moriano, Pablo and Finke, Jorge},
Book-Group-Author = {{IEEE}},
Title = {{Characterizing the Relationship Between Degree Distributions and
   Community Structures}},
Booktitle = {{2014 AMERICAN CONTROL CONFERENCE (ACC)}},
