@article{ ISI:000330275800012,
Author = {Yang, Jian and Wang, Jiabin and Hu, Yuling and Tang, Yiheng and Hou,
   Xugang and Li, Xiao},
Title = {{Global characterization of Arabidopsis protein interactome}},
