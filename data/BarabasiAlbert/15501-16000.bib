@inproceedings{ ISI:000250322700031,
Author = {Miguens, Joana I. L. and Mendes, Jose F. F. and Costa, Carlos M. M.},
Editor = {{Kertesz, J and Bornholdt, S and Mantegna, RN}},
Title = {{International tourism network}},
