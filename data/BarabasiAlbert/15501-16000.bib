@inproceedings{ ISI:000237179000035,
Author = {Yanmaz, Evsen and Tonguz, Ozan K. and Dixit, Sudhir},
Book-Group-Author = {{IEEE}},
Title = {{Power law property and self-organization in hybrid ad hoc wireless
   networks}},
