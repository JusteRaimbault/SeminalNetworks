@inproceedings{ ISI:000247069500024,
Author = {Smorodkina, Ekaterina and Thakur, Mayur and Tauritz, Daniel},
Editor = {{Demetrescu, C}},
Title = {{Algorithms for the balanced edge partitioning problem}},
