@inproceedings{ ISI:000268585800031,
Author = {Schumm, P. and Scoglio, C. and Gruenbacher, D. and Easton, T.},
Book-Group-Author = {{IEEE}},
Title = {{Epidemic Spreading on Weighted Contact Networks}},
