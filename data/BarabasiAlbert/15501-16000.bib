@inproceedings{ ISI:000239057000169,
Author = {Zhang, Wei and Jiang, Ping and Chen, Qijun},
Book-Group-Author = {{IEEE}},
Title = {{Stochastic routing in large scale sensor networks}},
Booktitle = {{PROCEEDINGS OF THE 2006 IEEE INTERNATIONAL CONFERENCE ON NETWORKING,
   SENSING AND CONTROL}},
Series = {{IEEE International Conference on Networking Sensing and Control}},
Year = {{2006}},
Pages = {{947-952}},
Note = {{IEEE International Conference on Networking, Sensing and Control, Ft
   Lauderdale, FL, APR 23-25, 2006}},
Organization = {{IEEE Syst, Man \& Cybernet Soc}},
Abstract = {{Effective routing in large scale sensor networks is important in the
   engineering practice. A simple and robust mechanism named stochastic
   routing (SR) is put forward in this paper. It utilizes the relationship
   between the network topology and the data distribution. An approximate
   scale-free topology is firstly established based on an extended
   preferential attachment algorithm (EPA). The sink nodes are assumed to
   be the starting nodes in EPA and they have higher initial degree than
   others. The sensor nodes can decide their next hop simply by
   stochastically selecting one node from their neighbors. Higher degree
   nodes have more chances to be selected. The stochastic feature can also
   simplify the routing update process. The simulations show that most of
   the packets can reach sink nodes successfully. Longer evolving time of
   the network can improve the reaching probability. The average length of
   packets' traversing path is in proportion to the square root of the WSN
   network size. This feature is helpful to keep the stable transportation
   of packets. Furthermore, the stochastic routing is quite simple and easy
   to be implemented in resource constraint sensor nodes. All these
   achievements are helpful to deploy large scale sensor networks.}},
