@article{ ISI:000245061700004,
Author = {Fu, Jianming and Huang, Zhiyi and Chen, Binglan and Cui, Jingsong},
Title = {{Containing worm based on immune-group in scale-free P2P}},
Journal = {{DYNAMICS OF CONTINUOUS DISCRETE AND IMPULSIVE SYSTEMS-SERIES
   B-APPLICATIONS \& ALGORITHMS}},
Year = {{2006}},
Volume = {{13}},
Number = {{3, S}},
Pages = {{945-949}},
Month = {{DEC}},
Note = {{International Conference on Complex Systems and Applications, Huhhot,
   PEOPLES R CHINA, JUN 16-18, 2006}},
Abstract = {{P2P applications and scale free networks are very popular, which makes
   the way of containing worm in scale free P2P important. This paper{*}
   presents four basic immune groups and two hybrid immune groups to
   mitigate the spread of worm, and then introduces a response mechanism
   using benign worms triggered by a worm. Furthermore two approaches are
   provided in order to suppress the propagation of worm: degree
   suppression and probe suppression. Finally, the experimental results
   demonstrate that degree immune group is the best among all
   immune-groups, and the effect of containing worm is observable with
   probe suppression, but degree suppression accelerates the spread of
   worm. Under our simulation condition, fitting function of the prevalence
   is given with respect to the number of nodes in scale-free P2P network,
   the average degree and immune rate for targeted immunization.}},
