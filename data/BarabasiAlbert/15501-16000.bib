@inproceedings{ ISI:000250322700016,
Author = {Duch, Jordi and Arenas, Alex},
Editor = {{Kertesz, J and Bornholdt, S and Mantegna, RN}},
Title = {{Effect of random failures on traffic in complex networks}},
Booktitle = {{NOISE AND STOCHASTICS IN COMPLEX SYSTEMS AND FINANCE}},
