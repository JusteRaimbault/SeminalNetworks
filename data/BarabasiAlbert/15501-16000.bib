@article{ ISI:000238214100018,
Author = {Abe, Sumiyoshi and Suzuki, Norikazu},
Title = {{Complex network of seismicity}},
Journal = {{PROGRESS OF THEORETICAL PHYSICS SUPPLEMENT}},
Year = {{2006}},
Number = {{162}},
Pages = {{138-146}},
Note = {{International Workshop on Complexity and Nonextensivity - New Trends in
   Statistical Mechanics, Kyoto Univ, Yukawa Inst Theoret Phys, Kyoto,
   JAPAN, MAR 14-18, 2005}},
Organization = {{Kyoto Univ Fdn; Yukawa Inst Theoret Phys}},
Abstract = {{The structural and dynamical properties of seismicity are discussed by
   constructing the complex growing networks from the seismic data taken in
   California and Japan. It is shown that the connectivity distributions
   decay as a power law. In the undirected network picture, the
   characteristic path length between two earthquakes chosen at random
   takes small value between 2 and 3. The clustering coefficient is found
   to be about 10 times larger than that in the case of the completely
   random network. In the directed network picture, two
   topologically-invariant statistical quantities are studied: one is the
   period distribution and the other is the path length distribution. Both
   of them are found to be scale free. These features highlight novel.
   aspects of seismicity as a complex phenomenon.}},
