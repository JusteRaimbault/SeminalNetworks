@inproceedings{ ISI:000245469500118,
Author = {Castillo, Carlos and Nelli, Alberto and Panconesi, Alessandro},
Book-Group-Author = {{IEEE COMPUTER SOC}},
Title = {{A memory-efficient strategy for exploring the web}},
Booktitle = {{2006 IEEE/WIC/ACM INTERNATIONAL CONFERENCE ON WEB INTELLIGENCE, (WI 2006
   MAIN CONFERENCE PROCEEDINGS)}},
Year = {{2006}},
Pages = {{680+}},
Note = {{IEEE/WIC/ACM International Conference on Web Intelligence and
   Intelligent Agent Technology (WI-IAT 2006), Hong Kong, PEOPLES R CHINA,
   DEC 18-22, 2006}},
Organization = {{IEEE; WIC; ACM}},
Abstract = {{Search engines rely on Web crawlers to create an index of the Web. Web
   crawlers explore the Web downloading pages and finding links to new
   pages to be explored. At any given moment, there are a number of pages
   waiting to be downloaded in the crawler queue. We study the growth of
   this queue of pending pages during a crawl of a large subset of the Web.
   In a normal breadth-first crawler the queue quickly grows very large.
   We present a strategy for managing the pending queue that reduces its
   maximum size by 50\% while preserving the coverage and quality of the
   pages visited. This can be applied to general purpose Web crawlers as
   well as topic-specific crawling, peer-to-peer search, on-demand Web
   crawling, and other environments in which memory usage has to be kept to
   a minimum.}},
