@inproceedings{ ISI:000259262400043,
Author = {Sato, Yasuhiro and Ata, Shingo and Oka, Ikuo},
Book-Group-Author = {{IEEE}},
Title = {{A strategic approach for re-organization of Internet topology for
   improving both efficiency and attack tolerance}},
Booktitle = {{2008 IEEE NETWORK OPERATIONS AND MANAGEMENT SYMPOSIUM, VOLS 1 AND 2}},
Series = {{IEEE IFIP Network Operations and Management Symposium}},
Year = {{2008}},
Pages = {{331-338}},
Note = {{IEEE Network Operations and Management Symposium, Salvador, BRAZIL, APR
   07-11, 2008}},
Organization = {{cgibr; CNPq; Chesf; BRSIL; CAPES; IBM; Google; Padtec; CISCO; EMANICS;
   IEEE Commun Soc; IFIP}},
Abstract = {{Recent studies have described the topologies of various networks are
   categorized as scale-free networks, focusing especially on the
   scale-free nature of the Internet. However, the suitability of the
   current topology of the Internet for communications has not been
   studied. In this paper, we investigate whether the current Internet is
   optimized on both aspects of communication efficiency and attack
   tolerance. For this purpose, we define four metrics to represent the
   capabilities of the network from various viewpoints. Our investigation
   showed that there is a network topology superior to that of the current
   Internet. We then considered how we should change the network topology
   from the current to more appropriate one. We introduce four typical
   strategies that the end-user can act and evaluate which strategy is most
   effective to re-organize the topology of the Internet to the new one.}},
