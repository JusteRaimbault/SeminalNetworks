Author = {Chen, Enhong and Chen, Xujia and Sheu, Phillip C-Y and Qian, Tieyun},
Book-Group-Author = {{IEEE Comp Soc}},
Title = {{An evolutionary computational method for N-connection subgraph discovery}},
Booktitle = {{ICTAI-2006: EIGHTEENTH INTERNATIONAL CONFERENCE ON TOOLS WITH ARTIFICIAL
   INTELLIGENCE, PROCEEDINGS}},
Series = {{Proceedings-International Conference on Tools With Artificial
   Intelligence}},
