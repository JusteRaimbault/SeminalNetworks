@article{ ISI:000248657500013,
Author = {Yin Yan-Ping and Zhang Duan-Ming and Pan Gui-Jun and He Min-Hua},
Title = {{Sandpile dynamics driven by degree on scale-free networks}},
Journal = {{CHINESE PHYSICS LETTERS}},
Year = {{2007}},
Volume = {{24}},
Number = {{8}},
Pages = {{2200-2203}},
Month = {{AUG}},
Abstract = {{We introduce a sandpile model driven by degree on scale-free networks,
   where the perturbation is trigerred at nodes with the same degree. We
   numerically investigate the avalanche area has the same behaviour with
   avalanche size. When the sandpile is driven at nodes with the minimal
   degree, the avalanches of our model behave similarly to those of the
   original Bak-Tang-Wiesenfeld (BTW) model on scale-free networks. As the
   degree of driven nodes increases from the minimal value to the maximal
   value, the avalanche distribution gradually changes from a clean power
   law, then a mixture of Poissonian and power laws, finally to a
   Poisson-like distribution. The average avalanche area is found to
   increase with the degree of driven nodes so that pertubation triggered
   on higher-degree nodes will result in broader spreading of the avalanche
   propagation.}},
