@article{ ISI:000251989600009,
Author = {Wang, Xutao},
Title = {{The modelling of weighted complex networks}},
Journal = {{INTERNATIONAL JOURNAL OF MODERN PHYSICS B}},
Year = {{2007}},
Volume = {{21}},
Number = {{16}},
Pages = {{2813-2820}},
Month = {{JUN 20}},
Abstract = {{In order to further explore the mechanism responsible for weighted
   complex networks, we introduce a new model that incorporates the network
   topology and the weights' dynamical evolutions. Our model can capture
   the details of weight dynamics caused not only by the addition of a new
   node with new links and new links between old nodes, but also the
   deletion of old links. We calculate analytically the distributions of
   both degree and strength and found that all these distributions show
   scale-free behavior, as confirmed in many real networks. Thus our model
   characterizes the real weighted complex networks more precisely.}},
