@inproceedings{ ISI:000225505100010,
Author = {Pujol, JM and Flache, A and Sanguesa, R and Delgado, J},
Editor = {{LopezdeMantaras, R and Saitta, L}},
Title = {{Emergence of complex networks through local optimization}},
Booktitle = {{ECAI 2004: 16TH EUROPEAN CONFERENCE ON ARTIFICIAL INTELLIGENCE,
   PROCEEDINGS}},
