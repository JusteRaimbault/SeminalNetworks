@inproceedings{ ISI:000260562700107,
Author = {Xiao Yong and Shao Shihuang and Li Doudou and Wu Jimei},
Book-Group-Author = {{IEEE}},
Title = {{Distributed Test and Control Network Reconfiguration Based on the
   Variation of Network Average Degree}},
Booktitle = {{2008 CHINESE CONTROL AND DECISION CONFERENCE, VOLS 1-11}},
Year = {{2008}},
Pages = {{531-535}},
Note = {{20th Chinese Control and Decision Conference, Yantai, PEOPLES R CHINA,
   JUL 02-04, 2008}},
Abstract = {{A study on the construction of distributed test and control network
   (ab.DTCN) oriented aerospace reveals the difficulty in reconfigurating
   its original structure when there are faults with the network. Under
   this circumstance, an analysis helps to bring out the characters of the
   variation of network average degree. Thus, it is pointed out that the
   minimum of the varying average degree can be taken as the target of
