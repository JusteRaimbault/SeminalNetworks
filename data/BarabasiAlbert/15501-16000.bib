@inproceedings{ ISI:000253441300069,
Author = {Przukj, Natasa},
Book-Group-Author = {{IEEE}},
Title = {{Geometric local structure in biological networks}},
Booktitle = {{2007 IEEE INFORMATION THEORY WORKSHOP, VOLS 1 AND 2}},
Year = {{2007}},
Pages = {{402-407}},
Note = {{IEEE Information Theory Workshop, Tahoe City, CA, SEP 02-06, 2007}},
Organization = {{IEEE}},
Abstract = {{The recent explosion in biological and other real-world network data has
   created the need for improved tools for large network analyses. Several
   new mathematical techniques for analyzing local structural properties of
   large networks have recently been developed. Our work introduces small
   induced subgraphs of large networks, called graphlets. We use graphlets
   to develop ``network signatures{''} that quantify local structural
   properties of a network. Based on these network signatures, we design
   two novel ``network agreement{''} measures. These measures lead us to
   new, well-fitting geometric graph models of biological networks. Models
   are in tarn used to design efficient heuristics.}},
