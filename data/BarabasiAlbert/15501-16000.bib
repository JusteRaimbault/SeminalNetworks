@article{ ISI:000257497600038,
Author = {Diosan, Laura and Dumitrescu, Dumitru},
Title = {{Evolutionary coalition formation in full connected and scale free
   networks}},
Journal = {{INTERNATIONAL JOURNAL OF COMPUTERS COMMUNICATIONS \& CONTROL}},
Year = {{2008}},
Volume = {{3}},
Number = {{S}},
Pages = {{259-264}},
Abstract = {{An optimal clusterization model is studied through an original approach
   that combines an evolutionary algorithm with the principles of the
   physical spin systems. The method is used in order to investigate the
   process of coalition formation that appears in complex systems, actually
   in full connected and scale free networks. The numerical experiments
   show that the proposed evolutionary model is able to detect the optimal
   coalition formation in small and large systems by a reasonable cost of
   complexity (considered in terms of time and physical computational
   resources).}},
