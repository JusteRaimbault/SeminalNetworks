@inproceedings{ ISI:000187206600096,
Author = {Krishnamurthy, V and Sun, JH and Faloutsos, M and Tauro, S},
Editor = {{Arabnia, HR and Mun, Y}},
Title = {{Sampling Internet topologies: How small can we go?}},
