@inproceedings{ ISI:000248441100013,
Author = {Zhang, Yuping and Qian, Minping},
Editor = {{Feng, J and Jost, J and Qian, MP}},
Title = {{The Stochastic Model and Metastability of the Gene Network}},
Booktitle = {{NETWORKS: FROM BIOLOGY TO THEORY}},
Year = {{2007}},
