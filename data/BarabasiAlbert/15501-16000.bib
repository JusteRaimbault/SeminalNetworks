@inproceedings{ ISI:000234851600095,
Author = {Huang, XL and Ma, FY and Zhang, WJ and Li, Y},
Editor = {{Deng, X and Ye, Y}},
Title = {{On protection of threatened unstructured overlays: An economic defense
   model and its applications}},
Booktitle = {{INTERNET AND NETWORK ECONOMICS, PROCEEDINGS}},
