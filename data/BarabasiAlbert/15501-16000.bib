@inproceedings{ ISI:000245414204058,
Author = {Miyashita, Shuhei and Hotz, Peter Eggenberger},
Book-Group-Author = {{IEEE}},
Title = {{Morphogenetic evolution of 3D sheets exploiting a spatial constraint}},
Booktitle = {{2006 IEEE CONGRESS ON EVOLUTIONARY COMPUTATION, VOLS 1-6}},
