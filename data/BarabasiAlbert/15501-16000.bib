@inproceedings{ ISI:000244501000038,
Author = {Acosta-Elias, J. and Luna-Rivera, J. M. and Recio-Lara, M. and
   Gutierrez-Navarro, O. and Pineda-Reyes, B.},
Editor = {{Guo, M and Yang, LT and DiMartino, B and Zima, HP and Dongarra, J and Tang, F}},
Title = {{Topology-sensitive epidemic algorithm for information spreading in
   large-scale systems}},
Booktitle = {{PARALLEL AND DISTRIBUTED PROCESSING AND APPLICATIONS}},
Series = {{Lecture Notes in Computer Science}},
Year = {{2006}},
Volume = {{4330}},
