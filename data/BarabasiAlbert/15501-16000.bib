Title = {{The role of bioinformatics in toxicogenomics and proteomics}},
Booktitle = {{TOXICOGENOMICS AND PROTEOMICS}},
Series = {{NATO SCIENCE SERIES, SUB-SERIES I: LIFE AND BEHAVIOURAL SCIENCES}},
