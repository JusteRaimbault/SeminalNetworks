@inproceedings{ ISI:000260563000164,
Author = {Ye, Qi and Wang, Chen and Wu, Bin and Wang, Bai},
Book-Group-Author = {{IEEE}},
Title = {{VisCRM: A Social Network Visual Analytic Tool to Enhance Customer
   Relationship Management}},
Booktitle = {{IEEE/SOLI'2008: PROCEEDINGS OF 2008 IEEE INTERNATIONAL CONFERENCE ON
   SERVICE OPERATIONS AND LOGISTICS, AND INFORMATICS, VOLS 1 AND 2}},
Year = {{2008}},
Pages = {{825+}},
