@inproceedings{ ISI:000264526900090,
Author = {Sun, Caihong and Wang, Shu},
Editor = {{Guo, MZ and Zhao, L and Wang, LP}},
Title = {{Modeling Adaptive Behaviors on Growing Social Networks}},
