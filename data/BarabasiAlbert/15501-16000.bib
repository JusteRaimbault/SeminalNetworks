@inproceedings{ ISI:000262930000049,
Author = {Stevens, Tim and De Turck, Filip and Dhoedt, Bart and Demeester, Piet},
Editor = {{Vazao, T and Freire, MM and Chong, I}},
Title = {{Achieving Network Efficient Stateful Anycast Communications}},
