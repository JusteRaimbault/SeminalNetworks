@inproceedings{ ISI:000263151801085,
Author = {Hennemann, Stefan and Liefner, Ingo},
Editor = {{Duysters, G and Kaminishi, K}},
Title = {{Proposing Bipartite Network Analysis for the Evaluation of Regional
   Innovation Systems-/-Regions, Actors, and Content}},
