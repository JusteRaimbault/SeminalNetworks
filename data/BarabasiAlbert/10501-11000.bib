@article{ ISI:000262340000002,
Author = {Hase, M. O. and Mendes, J. F. F.},
Title = {{Solvable metric growing networks}},
Journal = {{JOURNAL OF STATISTICAL MECHANICS-THEORY AND EXPERIMENT}},
Year = {{2008}},
Month = {{DEC}},
Abstract = {{In considering the structure and dynamics of complex networks, we
   usually deal with degree distributions, clustering, shortest path
   lengths and other graph properties. Although these concepts have been
   analysed for graphs on abstract spaces, many networks happen to be
   embedded in a metric arrangement, where the geographical distance
   between vertices plays a crucial role. The present work proposes a model
   for a growing network that takes into account the geographical distance
   between vertices: the probability that they are connected is higher if
   they are located nearer to each other than if they are further apart. In
   this framework, the mean degree of vertices, degree distribution and
   shortest path length between two randomly chosen vertices are analysed.}},
