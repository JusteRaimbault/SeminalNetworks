@article{ ISI:000175694900002,
Author = {Thiele, HG},
Title = {{Contemplations on the paradigm of self and nonself discrimination and on
   other concepts ruling contemporary immunology}},
Journal = {{CELLULAR AND MOLECULAR BIOLOGY}},
Year = {{2002}},
Volume = {{48}},
Number = {{3}},
Pages = {{221-236}},
