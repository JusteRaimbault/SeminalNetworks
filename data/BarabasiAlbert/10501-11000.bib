@article{ ISI:000476885000006,
Author = {Kumar, Krishan and Shrimankar, Deepti D.},
Title = {{ESUMM: Event SUMMarization on Scale-Free Networks}},
Journal = {{IETE TECHNICAL REVIEW}},
Year = {{2019}},
Volume = {{36}},
