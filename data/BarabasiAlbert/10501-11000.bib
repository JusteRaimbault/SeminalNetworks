@article{ ISI:000253852900015,
Author = {Mendez, Patricio F. and Furrer, Reinhard and Ford, Ryan and Ordonez,
   Fernando},
Title = {{Scaling laws as a tool of materials informatics}},
Journal = {{JOM}},
Year = {{2008}},
Volume = {{60}},
Number = {{3}},
Pages = {{60-66}},
Month = {{MAR}},
Abstract = {{This paper discusses the utility of scaling lows to materials
   informatics and presents the algorithm Scaling LAW (SLAW), useful to
   obtain scaling laws front statistical data. These laws call be used to
   extrapolate known materials property data to untested materials by using
   other more readily available information. This technique is independent
   of a characteristic length or time scale, so it is useful for a broad
   diversion of problems. In some cases, SLAW call reproduce the
   mathematical expression that would have been obtained through all
   analytical treatment of the problem. This technique was originally
   designed for mining statistical data in materials processing and
   materials behavior at a system level, and it shows promise for the study
   of the relationship between structure and properties in materials.}},
