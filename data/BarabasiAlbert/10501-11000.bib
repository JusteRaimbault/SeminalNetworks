@inproceedings{ ISI:000230664200100,
Author = {Lu, TB and Fang, BX and Sun, YZ and Cheng, XQ and Guo, L},
Editor = {{He, X and Hintz, T and Piccardi, M and Wu, Q and Huang, M and Tien, D}},
Title = {{Building scale-free overlay mix networks with small-world properties}},
