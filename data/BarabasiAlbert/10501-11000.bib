@article{ ISI:000244721300033,
Author = {Chen Wei and Fang Jin-Qing and Kang Ge-Wen},
Title = {{Effects of scale-free topological properties on dynamical
   synchronization and control in coupled map lattices}},
Journal = {{COMMUNICATIONS IN THEORETICAL PHYSICS}},
Year = {{2007}},
Volume = {{47}},
Number = {{2}},
Pages = {{361-368}},
Month = {{FEB 15}},
Abstract = {{In the paper, we study effects of scale-free (SF) topology on dynamical
   synchronization and control in coupled map lattices (CML) Our strategy
   is to apply three feedback control methods, including constant feedback
   and two types of time-delayed feedback, to a small fraction of network
   nodes to reach desired synchronous state. Two controlled bifurcation
   diagrams verses feedback strength are obtained respectively. It is found
   that the value of critical feedback strength gamma(c), for the first
   time-delayed feedback control is increased linearly as s is increased
   linearly. The CML with SF loses synchronization and intermittency occurs
   if gamma > gamma(c). Numerical examples are presented to demonstrate all
   results.}},
