@article{ ISI:000474503900021,
Author = {Xu, Hao and Li, Tao and Liu, Xiongding and Liu, Wenjin and Dong, Jing},
Title = {{Spreading dynamics of an online social rumor model with psychological
   factors on scale-free networks}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2019}},
Volume = {{525}},
Pages = {{234-246}},
Month = {{JUL 1}},
Abstract = {{Considering people's psychological factors such as hesitation and
   forgetting, and the heterogeneity of online social networks, a novel
   SHPRS (susceptible-hesitating-propagating-resisted-susceptible) rumor
   spreading model on scale-free networks is proposed in this paper. The
   spreading dynamics of rumor is analyzed in detail through the mean-field
   theory. First, the basic reproductive number R-0 is derived by the next
   generation matrix method and equilibriums are obtained. Both the
   existence of rumor equilibriums and the dynamic behaviors of model are
   determined by the basic reproductive number R-0. Then, the global
   stability of the rumor-free equilibrium E-0 and the permanence of rumor
   spreading are studied in detail. Furthermore, we studied the global
   attractivity of the rumor-prevailing equilibrium. Finally, numerical
   simulations are performed to confirm the analytical results. (C) 2019
   Elsevier B.V. All rights reserved.}},
