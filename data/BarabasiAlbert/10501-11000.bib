@inproceedings{ ISI:000235325603062,
Author = {Yang, B and Liu, DY},
Book-Group-Author = {{IEEE}},
Title = {{Incremental algorithm for detecting community structure in dynamic
   networks}},
Booktitle = {{PROCEEDINGS OF 2005 INTERNATIONAL CONFERENCE ON MACHINE LEARNING AND
   CYBERNETICS, VOLS 1-9}},
Year = {{2005}},
Pages = {{2284-2290}},
