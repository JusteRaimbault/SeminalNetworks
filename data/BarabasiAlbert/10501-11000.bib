@article{ ISI:000460742800072,
Author = {Zhao, Xiaohui and Liu, Zhi-Ping},
Title = {{Analysis of Topological Parameters of Complex Disease Genes Reveals the
   Importance of Location in a Biomolecular Network}},
Journal = {{GENES}},
