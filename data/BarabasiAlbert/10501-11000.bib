@article{ ISI:000221813400024,
Author = {de Moura, APS},
Title = {{Biased growth processes and the ``rich-get-richer{''} principle}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2004}},
Volume = {{69}},
Number = {{5, 2}},
Month = {{MAY}},
Abstract = {{We study a simple stochastic system with a ``rich-get-richer{''}
   behavior, in which there are 2 states, and N particles that are
   successively assigned to one of the states, with a probability p(i) that
   depends on the states' occupation n(i) as
   p(i)=n(i)(gamma)/(n(1)(gamma)+n(2)(gamma)). We show that there is a
   phase transition as gamma crosses the critical value gamma(c)=1. For
   gamma<1, in the thermodynamic limit the occupations are approximately
   the same, n(1)approximate ton(2). For gamma>1, however, a spontaneous
   symmetry breaking occurs, and the system goes to a highly clustered
   configuration, in which one of the states has almost all the particles.
   These results also hold for any finite number of states (not only two).
   We show that this ``rich-get-richer{''} principle governs the growth
   dynamics in a simple model of gravitational aggregation, and we argue
   that the same is true in all growth processes mediated by long-range
   forces like gravity.}},
