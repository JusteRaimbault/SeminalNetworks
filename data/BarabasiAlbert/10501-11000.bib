@article{ ISI:000224475600039,
Author = {Jiang, YM and Xu, T and He, DR},
Title = {{A self-adaptive model of actor-collaboration network}},
Journal = {{INTERNATIONAL JOURNAL OF MODERN PHYSICS B}},
Year = {{2004}},
Volume = {{18}},
Number = {{17-19}},
Pages = {{2604-2607}},
Month = {{JUL 30}},
Note = {{1st Cross Taiwan-strait Conference on Statistical Physics, Yangzhou,
   PEOPLES R CHINA, AUG 26-31, 2003}},
Organization = {{Yangzhou Univ; Univ Sci \& Technol China; Natl Sci Fdn China; Peoples
   Govt Jiangsu Prov; Dept Educ Jiangsu Prov; Sci \& Technol Assoc Anhui
   Prov}},
Abstract = {{We suggest a self-adaptive model for a description of
   actor-collaboration. Suppose that the numbers of actors and directors
   increase as time progresses (with different velocity). Each of the
   people is randomly given a style coefficient and a famousness
   coefficient, which vary on time according to his achievements in every
   year. A director can conduct more films and an actor can be invited to
   perform in more films in condition they have a higher famousness
   coefficient. Our numerical simulation results show that the
   actor-collaboration network described by this model is both small-world
   network and scaling-free network. All the important statistical
   parameters obtained by the simulation are in a good agreement with the
   statistical results. We also suggest a new parameter, which is addressed
   as ``degree of connexity{''}. Its distribution shows a new power law.}},
