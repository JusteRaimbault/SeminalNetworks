@inproceedings{ ISI:000246358300032,
Author = {Horiuchi, Hideto and Wakamiya, Naoki and Murata, Masayuki},
Editor = {{Roccetti, M}},
Title = {{A network construction method for a scalable P2P video conferencing
   system}},
