@article{ ISI:000452559700016,
Author = {Soriano-Panos, D. and Arias-Castro, H. and Naranjo-Mayorga, F. and
   Gomez-Gardenes, J.},
Title = {{Impact of human-human contagions in the spread of vector-borne diseases}},
Journal = {{EUROPEAN PHYSICAL JOURNAL-SPECIAL TOPICS}},
Year = {{2018}},
Volume = {{227}},
Number = {{5-6, SI}},
Pages = {{661-672}},
Month = {{SEP}},
Note = {{International Conference on Dynamics Days Latin America and the
   Caribbean, Puebla, MEXICO, OCT 24-NOV 01, 2016}},
Abstract = {{This article is aimed at proposing a generalization of the
   Ross-Macdonald model for the transmission of Vector-borne diseases in
   which human-to-human contagions are also considered. We first present
   this generalized model by formulating a mean field theory, checking its
   validity by comparing to numerical simulations. To make the premises of
   our model more realistic, we adapt the mean field equations to the case
   in which human contacts are described by a complex network. In this case
   we are also able to derive an analytical expression for the epidemic
   threshold. In both the mean-field and network-based models, we estimate
   the value of the epidemic threshold which corresponds to the boundary
   between the disease-free and epidemic regimes. The expression of this
   threshold allows us to discuss the impact that human-to-human contagions
   have on the spread of vector-borne diseases.}},
