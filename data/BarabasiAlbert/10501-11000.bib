@inproceedings{ ISI:000307413400066,
Author = {Ramasco, Jose J. and Colizza, Vittoria and Panzarasa, Pietro},
Editor = {{Zhou, J}},
Title = {{Using the Weighted Rich-Club Coefficient to Explore Traffic Organization
   in Mobility Networks}},
Booktitle = {{COMPLEX SCIENCES, PT 1}},
