@article{ ISI:000456967400015,
Author = {Dietert, Helge and Fernandez, Bastien},
Title = {{The mathematics of asymptotic stability in the Kuramoto model}},
Journal = {{PROCEEDINGS OF THE ROYAL SOCIETY A-MATHEMATICAL PHYSICAL AND ENGINEERING
   SCIENCES}},
Year = {{2018}},
Volume = {{474}},
Number = {{2220}},
Month = {{DEC}},
Abstract = {{Now a standard in Nonlinear Sciences, the Kuramoto model is the perfect
   example of the transition to synchrony in heterogeneous systems of
   coupled oscillators. While its basic phenomenology has been sketched in
   early works, the corresponding rigorous validation has long remained
