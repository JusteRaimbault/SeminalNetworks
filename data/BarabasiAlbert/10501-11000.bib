@article{ ISI:000239804600007,
Author = {Bhan, Ashish and Mjolsness, Eric},
Title = {{Static and dynamic models of biological networks}},
Journal = {{COMPLEXITY}},
Year = {{2006}},
Volume = {{11}},
Number = {{6}},
Pages = {{57-63}},
Month = {{JUL-AUG}},
Abstract = {{We consider static and dynamic approaches to the specification of
   probability distributions on graphs, consistent with desired statistical
   properties such as degree distributions, for use in modeling biological
   networks. In the static approach we develop analytical approximations to
   the Hamiltonian and partition functions. In the dynamic approach, we use
   a stochastic parameterized grammar to construct an evolutionary tree in
   which the nodes represent elements such as genes or cells and the links
   represent inheritance relations between the nodes. The grammar then
   constructs a network based on the feature vectors of the nodes in the
   tree. (c) 2006 Wiley Periodicals, Inc.}},
