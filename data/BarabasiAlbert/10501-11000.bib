@article{ ISI:000460804500002,
Author = {Ji, Jiaojiao and Barnett, George A. and Chu, Jianxun},
Title = {{Global networks of genetically modified crops technology: a patent
   citation network analysis}},
Journal = {{SCIENTOMETRICS}},
Year = {{2019}},
Volume = {{118}},
Number = {{3}},
Pages = {{737-762}},
Month = {{MAR}},
Abstract = {{This paper employs the patent data of four major genetically modified
   (GM) crops, soybeans, cotton, maize and rapeseed, to illustratee how the
   innovation of GM crop technology diffused and distributed globally over
   time. Data collected from the Derwent Innovation Index, were employed to
   construct country patent citation networks, from 1984 to 2015, and the
   results revealed that developed countries were early adopters, and the
   primary actors in the innovation of GM crop technology. Only seven
   developing countries appeared in the country citation network. Most
   developed countries were reluctant to apply GM crop technology for
   commercial cultivation. Private businesses stood out in the patent
   citation network. The early adoption and better performance of developed
   countries can be explained by the activities of large established
   private companies.}},
