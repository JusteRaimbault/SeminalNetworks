@article{ ISI:000239448100018,
Author = {Rejto, Lidia and Tusnady, Gabor},
Title = {{Reconstruction of Kauffman networks applying trees}},
Journal = {{LINEAR ALGEBRA AND ITS APPLICATIONS}},
Year = {{2006}},
Volume = {{417}},
Number = {{1, SI}},
