@article{ ISI:000262616300009,
Author = {Shin, J. K. and Shin, G. S. and Mansury, Y. S.},
Title = {{CLUSTERING IN A SPATIAL SYSTEM OF SELF-AGGREGATING AGENTS}},
Journal = {{FRACTALS-COMPLEX GEOMETRY PATTERNS AND SCALING IN NATURE AND SOCIETY}},
Year = {{2009}},
Volume = {{17}},
Number = {{1}},
Pages = {{99-107}},
Month = {{MAR}},
Abstract = {{We examine a dynamic spatial system of self-aggregating individuals that
   migrate under mutual gravitational attraction, forming clusters of
   varying sizes and distributions. To that end, we develop a computational
   model in which a frictional factor limits agent mobility, which in turn
   leads to the concentration of population in clusters. Our numerical
   results reveal that the clustering behavior of the system generates two
   distinct phases broken down by a phase transition. In the sub-critical
   phase, the spatial size-distribution of clusters follows the well-known
   exponential or power-law distribution. By contrast, in the
   super-critical phase the entire population converges into either a
   single giant cluster (a ``Black-hole{''}), or a few large clusters
   ({''}Oligarchs{''}). In between these two phases, there exists a
   threshold critical level corresponding to a phase transition in which
   clusters abruptly enter a period of rapid growth. The key contribution
   of this study is to solve for the phase transition regime analytically.
   We also discuss the application of our results to study the development
   of cities, the evolution of languages, the growth of asteroids, and the
   emergence of monopoly and oligopoly.}},
