@article{ ISI:000373693600019,
Author = {Jiao, Bo and Nie, Yuan-ping and Shi, Jian-mai and Lu, Gang and Zhou,
   Ying and Du, Jing},
Title = {{Accurately and quickly calculating the weighted spectral distribution}},
Journal = {{TELECOMMUNICATION SYSTEMS}},
Year = {{2016}},
Volume = {{62}},
Number = {{1}},
Pages = {{231-243}},
Month = {{MAY}},
Abstract = {{The weighted spectral distribution (WSD) is a measure defined on the
   normalized Laplacian spectrum. It can be used for comparing complex
   networks with different sizes (number of nodes) and provides a sensitive
   discrimination of the structural robustness of complex networks. In this
   paper, we design an algorithm for the accurate calculation of the WSD in
   large-scale complex networks by utilizing characteristics of the graph
   structure. As an extension to Sylvester's Law of Inertia for the
   calculation of the WSD based on spectral characteristics, our algorithm
   exhibits a higher time efficiency when applied to large-scale complex
   networks.}},
