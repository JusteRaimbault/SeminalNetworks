@article{ ISI:000188946700095,
Author = {Manna, SS and Mukherjee, G and Sen, P},
Title = {{Scale-free network on a vertical plane}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2004}},
Volume = {{69}},
Number = {{1, 2}},
Month = {{JAN}},
Abstract = {{A scale-free network is grown in the Euclidean space with a global
   directional bias. On a vertical plane, nodes are introduced at unit rate
   at randomly selected points and a node is allowed to be connected only
   to the subset of nodes which are below it using the attachment
   probability, pi(i)(t)similar tok(i)(t)l(alpha). Our numerical results
   indicate that the directed scale-free network for alpha=0 belongs to a
   different universality class compared to the isotropic scale-free
   network. For alpha<alpha(c), the degree distribution is stretched
   exponential in general which takes a pure exponential form in the limit
   of alpha-->-infinity. The link length distribution is calculated
   analytically for all values of alpha.}},
