
@article{ ISI:000267945100002,
Author = {Geard, Nicholas and Willadsen, Kai},
Title = {{Dynamical Approaches to Modeling Developmental Gene Regulatory Networks}},
Journal = {{BIRTH DEFECTS RESEARCH PART C-EMBRYO TODAY-REVIEWS}},
Year = {{2009}},
Volume = {{87}},
Number = {{2}},
Pages = {{131-142}},
Month = {{JUN}},
Abstract = {{The network of interacting regulatory signals within a cell comprises
   one of the most complex and powerful computational systems in biology.
   Gene regulatory networks (GRNs) play a key role in transforming the
   information encoded in a genome into morphological form. To achieve this
   feat, GRNs must respond to and integrate environmental signals with
   their internal dynamics in a robust and coordinated fashion. The highly
   dynamic nature of this process lends itself to interpretation and
   analysis in the language of dynamical models. Modeling provides a means
   of systematically untangling the complicated structure of GRNs, a
   framework within which to simulate the behavior of reconstructed systems
   and, in some cases, suites of analytic tools for exploring that behavior
   and its implications. This review provides a general background to the
   idea of treating a regulatory network as a dynamical system, and
   describes a variety of different approaches that have been taken to the
   dynamical modeling of GRNs. Birth Defects Research (Part C) 87:131-142,
   2009. (c) 2009 Wiley-Liss, Inc.}},
