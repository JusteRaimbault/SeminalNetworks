@article{ ISI:000247224100022,
Author = {Sun Wei-Gang and Xu Cong-Xiang and Li Chang-Pin and Fang Jin-Qing},
Title = {{Synchronization and bifurcation of general complex dynamical networks}},
Journal = {{COMMUNICATIONS IN THEORETICAL PHYSICS}},
Year = {{2007}},
Volume = {{47}},
Number = {{6}},
Pages = {{1073-1075}},
Month = {{JUN 15}},
Abstract = {{In the present paper, synchronization and bifurcation of general complex
   dynamical networks are investigated. We mainly focus on networks with a
   somewhat general coupling matrix, i.e., the sum of each row equals a
   nonzero constant u. We derive a result that the networks can reach a new
   synchronous state, which is not the asymptotic limit set determined by
   the node equation. At the synchronous state, the networks appear
   bifurcation if we regard the constant u as a bifurcation parameter.
   Numerical examples are given to illustrate our derived conclusions.}},
