@inproceedings{ ISI:000274190600040,
Author = {Chen, Wei and Sommer, Christian and Teng, Shang-Hua and Wang, Yajun},
Editor = {{Keidar, I}},
Title = {{Compact Routing in Power-Law Graphs}},
Booktitle = {{DISTRIBUTED COMPUTING, PROCEEDINGS}},
