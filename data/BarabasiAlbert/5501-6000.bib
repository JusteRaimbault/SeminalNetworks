@article{ ISI:000232236300018,
Author = {Katona, Z},
Title = {{Width of a scale-free tree}},
Journal = {{JOURNAL OF APPLIED PROBABILITY}},
Year = {{2005}},
Volume = {{42}},
Number = {{3}},
Pages = {{839-850}},
Month = {{SEP}},
Abstract = {{Consider the random graph model of Barabasi and Albert, where we add a
   new vertex in every step and connect it to some old vertices with
   probabilities proportional to their degrees. If we connect it to only
   one of the old vertices then this will be a tree. These graphs have been
   shown to have a power-law degree distribution, the same as that observed
   in some large real-world networks. We are interested in the width of the
   tree and we show that it is W(n) similar to n/ root pi log n at the nth
   step; this also holds for a slight generalization of the model with
   another constant. We then see how this theoretical result can be applied
   to directory trees.}},
