@article{ ISI:000400899300001,
Author = {Yang, Dan and Pan, Liming and Zhou, Tao},
Title = {{Lower bound of assortativity coefficient in scale-free networks}},
Journal = {{CHAOS}},
Year = {{2017}},
Volume = {{27}},
Number = {{3}},
Month = {{MAR}},
Abstract = {{The degree-degree correlation is important in understanding the
   structural organization of a network and dynamics upon a network. Such
   correlation is usually measured by the assortativity coefficient r, with
   natural bounds r is an element of {[}-1,1]. For scale-free networks with
   power-law degree distribution p(k) similar to k(-gamma), we analytically
   obtain the lower bound of assortativity coefficient in the limit of
   large network size, which is not -1 but dependent on the power-law
   exponent gamma. This work challenges the validation of the assortativity
   coefficient in heterogeneous networks, suggesting that one cannot judge
   whether a network is positively or negatively correlated just by looking
   at its assortativity coefficient alone. Published by AIP Publishing.}},
