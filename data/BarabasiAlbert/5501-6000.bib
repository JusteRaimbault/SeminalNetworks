@article{ ISI:000259504300004,
Author = {Siler, Kyle S. and McLaughlin, Neil},
Title = {{The Canada research chairs program and social science reward structures}},
Journal = {{CANADIAN REVIEW OF SOCIOLOGY-REVUE CANADIENNE DE SOCIOLOGIE}},
