@article{ ISI:000330908400022,
Author = {Mai, Yong and Chen, Huan and Meng, Lei},
Title = {{An analysis of the sectorial influence of CSI300 stocks within the
   directed network}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2014}},
Volume = {{396}},
Pages = {{235-241}},
Month = {{FEB 15}},
Abstract = {{This paper uses the Partial Correlation Planar maximally filtered Graph
   (PCPG) method to construct a directed network for the constituent stocks
   underlying the China Securities Index 300 (CSI300). We also analyse the
   impact of individual stocks. We find that the CSI300 market is a
   scale-free network with a relatively small power law exponent. The
   volatility of the stock prices has significant impact on other stocks.
   In the sectorial network, the industrial sector is the most influential
   one over other sectors, the financial sector only has a modest
   influence, while the telecommunication services sector's influence is
   marginal. In addition, such inter-sector influence displays quarterly
   stability. (C) 2013 Elsevier B.V. All rights reserved.}},
