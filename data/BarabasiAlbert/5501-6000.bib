@inproceedings{ ISI:000303506200041,
Author = {Bogdan, Paul and Marculescu, Radu},
Book-Group-Author = {{IEEE}},
Title = {{A Fractional Calculus Approach to Modeling Fractal Dynamic Games}},
Booktitle = {{2011 50TH IEEE CONFERENCE ON DECISION AND CONTROL AND EUROPEAN CONTROL
   CONFERENCE (CDC-ECC)}},
Series = {{IEEE Conference on Decision and Control}},
Year = {{2011}},
Pages = {{255-260}},
Note = {{50th IEEE Conference of Decision and Control (CDC)/European Control
   Conference (ECC), Orlando, FL, DEC 12-15, 2011}},
Organization = {{Honeywell; MathWorks; United Technol Res Ctr; HYCON2; IEEE; Contrl Syst
   Soc (CSS); EUCA}},
Abstract = {{Motivated by the complexity of spatio-temporal patterns of
   interconnected human processes (e.g., crowds, car traffic, social
   networks), this paper sets forth the fractal dynamic games as an
   analytical tool for modeling and predicting human dynamics. Starting
   from a statistical physics description of interactions between agents
   and from the observed statistical properties of economic measures, we
   construct a master equation characterizing the dynamics of cost
   functionals as stochastic variables affected by additive and
   multiplicative noise forces. Given the significance of human behavior,
   we allow the cost distribution to depend on the evolution of agents
   density. By coupling the description of agent dynamics through a fractal
   structure with a generic stochastic utility function, we formulate a new
   dynamic game. Employing optimal control theory concepts, we derive a
   continuum formulation of the car traffic dynamics optimization resulting
   in a nonlinear fractional partial differential equation.}},
