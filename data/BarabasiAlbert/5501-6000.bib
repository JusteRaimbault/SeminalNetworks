@article{ ISI:000269211000015,
Author = {Herrero, C. P.},
Title = {{Antiferromagnetic Ising model in scale-free networks}},
Journal = {{EUROPEAN PHYSICAL JOURNAL B}},
Year = {{2009}},
Volume = {{70}},
Number = {{3}},
Pages = {{435-441}},
Month = {{AUG}},
Abstract = {{The antiferromagnetic Ising model in uncorrelated scale-free networks
   has been studied by means of Monte Carlo simulations. These networks are
   characterized by a connectivity ( or degree) distribution P(k) similar
   to k(-gamma). The disorder present in these complex networks frustrates
   the antiferromagnetic spin ordering, giving rise to a spin-glass (SG)
   phase at low temperature. The paramagnetic-SG transition temperature T-c
   has been studied as a function of the parameter gamma and the minimum
   degree present in the networks. T-c is found to increase when the
   exponent gamma is reduced, in line with a larger frustration caused by
   the presence of nodes with higher degree.}},
