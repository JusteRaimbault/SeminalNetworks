@article{ ISI:000238694200045,
Author = {Bianconi, Ginestra and Marsili, Matteo},
Title = {{Effect of degree correlations on the loop structure of scale-free
   networks}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2006}},
Volume = {{73}},
Number = {{6, 2}},
Month = {{JUN}},
Abstract = {{In this paper we study the impact of degree correlations in the subgraph
   statistics of scale-free networks. In particular we consider loops,
   simple cases of network subgraphs which encode the redundancy of the
   paths passing through every two nodes of the network. We provide an
   understanding of the scaling of the clustering coefficient in modular
   networks in terms of the maximal eigenvector of the average adjacency
   matrix of the ensemble. Furthermore we show that correlations affect in
   a relevant way the average number of Hamiltonian paths in a three-core
   of real world networks. We prove our results in the two-vertex
   correlated hidden variable ensemble and we check the results with exact
   counting of small loops in real graphs.}},
