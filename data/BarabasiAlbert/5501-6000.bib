@article{ ISI:000248198800035,
Author = {Xu, Degang and Li, Yanjun and Wu, Tie-Jun},
Title = {{Improving consensus and synchronizability of networks of coupled systems
   via adding links}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2007}},
Volume = {{382}},
Number = {{2}},
Pages = {{722-730}},
Month = {{AUG 15}},
Abstract = {{The problem of improving the consensus and synchronizability for
   weighted networks of dynamic systems via adding links among them is
   investigated in this paper. A weighted network entropy is introduced to
   measure the homogeneity of the weighted networks. The relationship
   between the homogeneity and the consensus and synchronizability is
   analyzed. A solution scheme is proposed to improve the consensus and
   synchronizability of the weighted networks through maximizing their
   homogeneity via adding as few links as possible. Weighted networks
   created from the Barrat model are tested by using the proposed scheme,
   and two commonly used link-adding approaches are selected to compare
   with this scheme. Computer simulation results show that the proposed
   link-adding scheme can enhance the consensus and the synchronizability
   of the weighted networks effectively and efficiently. (C) 2007 Elsevier
   B.V. All rights reserved.}},
