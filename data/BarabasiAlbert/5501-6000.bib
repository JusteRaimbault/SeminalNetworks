@article{ ISI:000355209800017,
Author = {Marjanovic, Goran and Hero, III, Alfred O.},
Title = {{l(0) Sparse Inverse Covariance Estimation}},
Journal = {{IEEE TRANSACTIONS ON SIGNAL PROCESSING}},
Year = {{2015}},
Volume = {{63}},
Number = {{12}},
Pages = {{3218-3231}},
Month = {{JUN 15}},
Abstract = {{Recently, there has been focus on penalized log-likelihood covariance
   estimation for sparse inverse covariance (precision) matrices. The
   penalty is responsible for inducing sparsity, and a very common choice
   is the convex l(1) norm. However, the best estimator performance is not
   always achieved with this penalty. The most natural sparsity promoting
   ``norm{''} is the nonconvex l(0) penalty but its lack of convexity has
   deterred its use in sparse maximum likelihood estimation. In this paper,
   we consider nonconvex l(0) penalized log-likelihood inverse covariance
   estimation and present a novel cyclic descent algorithm for its
   optimization. Convergence to a local minimizer is proved, which is
   highly nontrivial, and we demonstrate via simulations the reduced bias
   and superior quality of the l(0) penalty as compared to the l(1)
   penalty.}},
