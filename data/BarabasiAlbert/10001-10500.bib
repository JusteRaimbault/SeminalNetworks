Title = {{Efficient Equilibria in a Public Goods Game}},
Booktitle = {{2015 IEEE/WIC/ACM INTERNATIONAL CONFERENCE ON WEB INTELLIGENCE AND
   INTELLIGENT AGENT TECHNOLOGY (WI-IAT), VOL 2}},
Year = {{2015}},
Pages = {{214-219}},
