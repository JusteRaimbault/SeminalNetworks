@inproceedings{ ISI:000302331000006,
Author = {Palmieri, Francesco},
Editor = {{Doulamis, A and Mambretti, J and Tomkos, I and Varvarigou, T}},
Title = {{Percolation-Based Replica Discovery in Peer-to-Peer Grid Infrastructures}},
