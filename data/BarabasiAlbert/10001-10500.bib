@article{ ISI:000361578400002,
Author = {Li, Chun-Hsien and Yang, Suh-Yuh},
Title = {{Effects of network structure on the synchronizability of nonlinearly
   coupled Hindmarsh-Rose neurons}},
Journal = {{PHYSICS LETTERS A}},
Year = {{2015}},
Volume = {{379}},
Number = {{40-41}},
Pages = {{2541-2548}},
Month = {{OCT 23}},
Abstract = {{This work is devoted to investigate the effects of network structure on
   the synchronizability of nonlinearly coupled dynamical network of
   Hindmarsh-Rose neurons with a sigmoidal coupling function. We mainly
   focus on the networks that exhibit the small-world character or
   scale-free property. By checking the first nonzero eigenvalue of the
   outer-coupling matrix, which is closely related to the synchronization
   threshold, the synchronizabilities of three specific network ensembles
   with prescribed network structures are compared. Interestingly, we find
   that networks with more connections will not necessarily result in
   better synchronizability. (C) 2015 Elsevier B.V. All rights reserved.}},
Publisher = {{ELSEVIER SCIENCE BV}},
Address = {{PO BOX 211, 1000 AE AMSTERDAM, NETHERLANDS}},
