@article{ ISI:000311865500020,
Author = {Chelminiak, Przemyslaw},
Title = {{Emergence of complex networks from diffusion on fractal lattices. A
   special case of the Sierpinski gasket and tetrahedron}},
Journal = {{PHYSICS LETTERS A}},
Year = {{2012}},
Volume = {{376}},
Number = {{45}},
Pages = {{2917-2921}},
Month = {{OCT 1}},
Abstract = {{A new approach to the assemblage of complex networks displaying the
   scale-free architecture is proposed. While the growth and the
   preferential attachment of incoming nodes assure an emergence of such
   networks according to the Barabasi-Albert model, it is argued here that
   the preferential linking condition needs not to be a principal rule. To
   assert this statement a simple computer model based on random walks on
   fractal lattices is introduced. It is shown that the model successfully
   reproduces the degree distributions, the ultra-small-worldness and the
   high clustering arising from the topology of scale-free networks. (C)
   2012 Elsevier B.V. All rights reserved.}},
