@article{ ISI:000356910600002,
Author = {Bhamidi, Shankar and Goodman, Jesse and van der Hofstad, Remco and
   Komjathy, Julia},
Title = {{DEGREE DISTRIBUTION OF SHORTEST PATH TREES AND BIAS OF NETWORK SAMPLING
   ALGORITHMS}},
