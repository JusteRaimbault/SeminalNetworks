@article{ ISI:000318437000004,
Author = {Wang, Xiaohan and Chen, Zhaoqun and Liu, Pengfei and Gu, Yuantao},
Title = {{Edge Balance Ratio: Power Law From Vertices to Edges in Directed Complex
   Network}},
Journal = {{IEEE JOURNAL OF SELECTED TOPICS IN SIGNAL PROCESSING}},
Year = {{2013}},
Volume = {{7}},
Number = {{2}},
Pages = {{184-194}},
Month = {{APR}},
Abstract = {{Power law distribution is common in real-world networks including online
   social networks. Many studies on complex networks focus on the
   characteristics of vertices, which are always proved to follow the power
   law. However, few researches have been done on edges in directed
   networks. In this paper, edge balance ratio is firstly proposed to
   measure the balance property of edges in directed networks. Based on
   edge balance ratio, balance profile and positivity are put forward to
   describe the balance level of the whole network. Then the distribution
   of edge balance ratio is theoretically analyzed. In a directed network
   whose vertex in-degree follows the power law with scaling exponent, it
   is proved that the edge balance ratio follows a piecewise power law,
   with the scaling exponent of each section linearly dependent on. The
   theoretical analysis is verified by numerical simulations. Moreover, the
   theoretical analysis is confirmed by statistics of real-world online
   social networks, including Twitter network with 35 million users and
   Sina Weibo network with 110 million users.}},
