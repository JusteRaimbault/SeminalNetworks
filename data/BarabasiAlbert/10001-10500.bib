@inproceedings{ ISI:000371471001045,
Author = {Rong, Zhihai and Wu, Zhi-Xi and Tse, Chi Kong},
Book-Group-Author = {{IEEE}},
Title = {{Community Structure Promotes the Emergence of Persistence Behavior in
   Social Networks}},
