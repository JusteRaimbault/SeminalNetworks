@article{ ISI:000325158300005,
Author = {Gong, Yubing and Xu, Bo and Wu, Ya'nan},
Title = {{Adaptive coupling optimized spiking coherence and synchronization in
   Newman-Watts neuronal networks}},
Journal = {{CHAOS}},
Year = {{2013}},
Volume = {{23}},
Number = {{3}},
Month = {{SEP}},
Abstract = {{In this paper, we have numerically studied the effect of adaptive
   coupling on the temporal coherence and synchronization of spiking
   activity in Newman-Watts Hodgkin-Huxley neuronal networks. It is found
   that random shortcuts can enhance the spiking synchronization more
   rapidly when the increment speed of adaptive coupling is increased and
   can optimize the temporal coherence of spikes only when the increment
   speed of adaptive coupling is appropriate. It is also found that
   adaptive coupling strength can enhance the synchronization of spikes and
   can optimize the temporal coherence of spikes when random shortcuts are
   appropriate. These results show that adaptive coupling has a big
   influence on random shortcuts related spiking activity and can enhance
   and optimize the temporal coherence and synchronization of spiking
   activity of the network. These findings can help better understand the
   roles of adaptive coupling for improving the information processing and
   transmission in neural systems. (C) 2013 AIP Publishing LLC.}},
