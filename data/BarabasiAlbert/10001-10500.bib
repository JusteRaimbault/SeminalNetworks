@inproceedings{ ISI:000406864300032,
Author = {Gomez, Florian and Lorimer, Tom and Stoop, Ruedi},
Editor = {{Mladenov, VM and Ivanov, PC}},
Title = {{Complex Networks of Harmonic Structure in Classical Music}},
Booktitle = {{NONLINEAR DYNAMICS OF ELECTRONIC SYSTEMS}},
