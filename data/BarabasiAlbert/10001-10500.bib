@article{ ISI:000274157600025,
Author = {Liu Chao and Li Rong and Duan Zhi-Sheng and Chen Guan-Rong},
Title = {{Average Range and Network Synchronizability}},
Journal = {{COMMUNICATIONS IN THEORETICAL PHYSICS}},
Year = {{2010}},
Volume = {{53}},
Number = {{1}},
Pages = {{115-120}},
Month = {{JAN}},
Abstract = {{The influence of structural properties of a network on the network
   synchronizability is studied by introducing a new concept of average
   range of edges. For both small-world and scale-free networks, the effect
   of average range on the synchronizability of networks with bounded or
   unbounded synchronization regions is illustrated through numerical
   simulations. The relations between average range, range distribution,
   average distance, and maximum betweenness are also explored, revealing
   the effects of these factors on the network synchronizability of the
   small-world and scale-free networks, respectively.}},
