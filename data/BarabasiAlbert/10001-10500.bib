@article{ ISI:000331162500001,
Author = {Zhang, Wei and He, Ming-sheng and Jin, Rui},
Title = {{OPINION DYNAMICS IN COMPLEX NETWORKS}},
Journal = {{LITHUANIAN JOURNAL OF PHYSICS}},
Year = {{2013}},
Volume = {{53}},
Number = {{4}},
Pages = {{185-194}},
Abstract = {{Based on the modification of the convergence parameter mu in the
   Weisbuch-Deffuant (WD) model, we investigated the influence of the
   network structure on opinion dynamics by comparing the processes of
   opinion dynamics in the Watts-Strogatz (WS) small-world network and
   Barabasi-Albert (BA) scale-free network. The simulation results present
   that the time evolution of opinions does not always end up with a
   consensus; the final number of opinion clusters depends on the value of
   the bounded confidence but compared with the situation in the original
   WD model the effect of the bounded confidence is different when dynamics
   happens in the small-world network and BA network. Furthermore, the
   structural cohesion of the network is strengthened by the rich nodes in
   the BA network, which make the opinions evolve at a much faster rate
   than those in the small-world network.}},
