@article{ ISI:000307654400001,
Author = {Wang, Tianbo and Zhou, Wuneng and Zhao, Dejun and Zhao, Shouwei},
Title = {{Exponential Synchronization Analysis and Control for Discrete-Time
   Uncertain Delay Complex Networks with Stochastic Effects}},
Journal = {{MATHEMATICAL PROBLEMS IN ENGINEERING}},
Year = {{2012}},
Abstract = {{The exponential synchronization for a class of discrete-time uncertain
   complex networks with stochastic effects and time delay is investigated
   by using the Lyapunov stability theory and discrete Halanay inequality.
   The uncertainty arises from the difference of the nodes' reliability in
   the complex network. Through constructing an appropriate Lyapunov
   function and applying inequality technique, some synchronization
   criteria and two control methods are obtained to ensure the considered
   complex network being exponential synchronization. Finally, a numerical
   example is provided to show the effectiveness of our proposed methods.}},
