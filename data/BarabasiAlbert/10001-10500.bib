@article{ ISI:000327734800031,
Author = {Menniti, Salvatore and Castagna, Emanuele and Mazza, Tommaso},
Title = {{Estimating the global density of graphs by a sparseness index}},
Journal = {{APPLIED MATHEMATICS AND COMPUTATION}},
Year = {{2013}},
Volume = {{224}},
Pages = {{346-357}},
Month = {{NOV 1}},
Abstract = {{Computation of sparse matrix is key in a wide range of applications of
   science and engineering. Matrix is tightly bound to the graph data
   structure and frequently used as an effective alternative: the
   complexity of fairly complicated operations on graphs can be measured as
   the computer time required to execute a number of arithmetic operations
   on nonzero quantities of a matrix. In this perspective, sparse matrices
   are computationally advantageous.
   We present a rigorous refinement of the completeness index, a
   mathematical function designed to quantify the sparsity of matrices. We
   prove its mathematical properties as well as its usefulness in the
   biological realm. (C) 2013 Elsevier Inc. All rights reserved.}},
