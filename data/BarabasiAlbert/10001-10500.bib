@inproceedings{ ISI:000380559900015,
Author = {Javarone, Marco Alberto and Galam, Serge},
Editor = {{Aiello, LM and McFarland, D}},
Title = {{Emergence of Extreme Opinions in Social Networks}},
Booktitle = {{Social Informatics}},
Series = {{Lecture Notes in Computer Science}},
