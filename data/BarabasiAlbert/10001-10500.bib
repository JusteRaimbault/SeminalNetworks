@inproceedings{ ISI:000317939900008,
Author = {Djidjev, Hristo and Onus, Melih},
Editor = {{Bader, DA and Meyerhenke, H and Sanders, P and Wagner, D}},
Title = {{Using graph partitioning for efficient network modularity optimization}},
