@article{ ISI:000352993300005,
Author = {Nian, Fuzhong and Wang, Ke},
Title = {{Multi-granularity immunization strategy based on SIRS model in
   scale-free network}},
Journal = {{MODERN PHYSICS LETTERS B}},
Year = {{2015}},
Volume = {{29}},
Number = {{8}},
Month = {{MAR 30}},
Abstract = {{In this paper, a new immunization strategy was established to prevent
   the epidemic spreading based on the principle of ``Multi-granularity{''}
   and ``Pre-warning Mechanism{''}, which send different pre-warning signal
   with the risk rank of the susceptible node to be infected. The
   pre-warning means there is a higher risk that the susceptible node is
   more likely to be infected. The multi-granularity means the susceptible
   node is linked with multi-infected nodes. In our model, the effect of
   the different situation of the multi-granularity immunizations is
   compared and different spreading rates are adopted to describe the
   epidemic behavior of nodes. In addition the threshold value of epidemic
   outbreak is investigated, which makes the result more convincing. The
   theoretical analysis and the simulations indicate that the proposed
   immunization strategy is effective and it is also economic and feasible.}},
