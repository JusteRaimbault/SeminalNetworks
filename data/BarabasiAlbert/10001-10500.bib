@inproceedings{ ISI:000325220500003,
Author = {Ascoli, Giorgio A. and Samsonovich, Alexei V.},
Editor = {{Samsonovich, AV and Johannsdottir, KR and Chella, A and Goertzel, B}},
Title = {{NeuroNavigator: A Biologically Inspired Universal Cognitive Microcircuit}},
Booktitle = {{BIOLOGICALLY INSPIRED COGNITIVE ARCHITECTURES 2010}},
Series = {{Frontiers in Artificial Intelligence and Applications}},
Year = {{2010}},
Volume = {{221}},
