@article{ ISI:000318208400011,
Author = {Shen, Yi},
Title = {{Detect local communities in networks with an outside rate coefficient}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2013}},
Volume = {{392}},
Number = {{12}},
Pages = {{2821-2829}},
Month = {{JUN 15}},
Abstract = {{In this paper, we present a local method for detecting communities in
   networks. We define an outside rate coefficient psi(out) in our method.
   psi(out) has a very simple form and is easy to calculate. The local
   community enclosing a starting node can be detected by agglomerating the
   node with the smallest psi(out) at each time step. When there are two or
   more nodes having the same smallest outside rate coefficient psi(out),
   we agglomerate the node with the largest k(in). This operation is
   remarkably beneficial to the accuracy of our method, and simulations on
   benchmark networks and real networks demonstrate that our local method
   is efficient to detect communities in networks. (C) 2013 Elsevier B.V.
   All rights reserved.}},
