@article{ ISI:000276270200002,
Author = {Li, Xueping and Yang, Dengfeng and Sawhney, Rapinder},
Title = {{Key establishment for layered group-based Wireless Sensor Networks}},
Journal = {{INTERNATIONAL JOURNAL OF AD HOC AND UBIQUITOUS COMPUTING}},
Year = {{2010}},
Volume = {{5}},
Number = {{3}},
Pages = {{150-162}},
Abstract = {{Establishing pairwise keys for sensors is crucial to secure
   communications in Wireless Sensor Networks (WSNs). Several key
   pre-distribution schemes have been proposed to establish pairwise keys,
   but they are not scalable and suffer from a dramatic degradation of
   security when the number of compromised sensor exceeds a threshold. In
   this paper, we propose a Layered Group-based Key Establishment (LGKE)
   scheme, in which a logic top layer ensures the scalability through an
   Exclusion Basic System (EBS) technique while a logic low layer adopts a
   group-based key preload scheme to ensure the security. The security
   analysis and quantitative evaluation show the superiority of LGKE with
   regard to scalability, resilience, robustness and communication
   overhead.}},
