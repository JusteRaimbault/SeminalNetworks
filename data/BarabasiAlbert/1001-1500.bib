@article{ ISI:000272807700002,
Author = {Tsai, Chung-Jung and Ma, Buyong and Nussinov, Ruth},
Title = {{Protein-protein interaction networks: how can a hub protein bind so many
   different partners?}},
Journal = {{TRENDS IN BIOCHEMICAL SCIENCES}},
Year = {{2009}},
Volume = {{34}},
Number = {{12}},
Pages = {{594-600}},
Month = {{DEC}},
Abstract = {{How can a single hub protein bind so many different partners? Numerous
   studies have sought differences between hubs and non-hubs to explain
   what makes a protein a hub and how a shared hub-binding site can be
   promiscuous, yet at the same time be specific. Here, we suggest that the
   problem is largely non-existent and resides in the popular
   representation of protein interaction networks: protein products derived
   from a single gene, even if different, are clustered in maps into a
   single node. This leads to the impression that a single protein binds to
   a very large number of partners. In reality, it does not; rather,
   protein networks reflect the combination of multiple proteins, each with
   a distinct conformation.}},
