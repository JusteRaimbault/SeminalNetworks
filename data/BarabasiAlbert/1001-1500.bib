@article{ ISI:000241200000057,
Author = {Zhao, Ming and Zhou, Tao and Wang, Bing-Hong and Yan, Gang and Yang,
   Hui-Jie and Bai, Wen-Jie},
Title = {{Relations between average distance, heterogeneity and network
   synchronizability}},
