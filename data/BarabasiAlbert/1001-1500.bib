@article{ ISI:000280580100013,
Author = {Hunt, D. and Korniss, G. and Szymanski, B. K.},
Title = {{Network Synchronization in a Noisy Environment with Time Delays:
   Fundamental Limits and Trade-Offs}},
Journal = {{PHYSICAL REVIEW LETTERS}},
Year = {{2010}},
Volume = {{105}},
Number = {{6}},
Month = {{AUG 4}},
Abstract = {{We study the effects of nonzero time delays in stochastic
   synchronization problems with linear couplings in an arbitrary network.
   Using the known exact threshold value from the theory of differential
   equations with delays, we provide the synchronizability threshold for an
   arbitrary network. Further, by constructing the scaling theory of the
   underlying fluctuations, we establish the absolute limit of
   synchronization efficiency in a noisy environment with uniform time
   delays, i.e., the minimum attainable value of the width of the
   synchronization landscape. Our results also have strong implications for
   optimization and trade-offs in network synchronization with delays.}},
