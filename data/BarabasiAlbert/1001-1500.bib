@article{ ISI:000373648800006,
Author = {Hu, Jun and Wang, Zidong and Chen, Dongyan and Alsaadi, Fuad E.},
Title = {{Estimation, filtering and fusion for networked systems with
   network-induced phenomena: New progress and prospects}},
