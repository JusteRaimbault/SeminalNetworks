@article{ ISI:000264870100004,
Author = {Zhang, Guo-Qing and Zhang, Guo-Qiang and Yang, Qing-Feng and Cheng,
   Su-Qi and Zhou, Tao},
Title = {{Evolution of the Internet and its cores}},
Journal = {{NEW JOURNAL OF PHYSICS}},
Year = {{2008}},
Volume = {{10}},
Month = {{DEC 18}},
Abstract = {{In this paper, we empirically study the evolution of large scale
   Internet topology at the autonomous system (AS) level. The network size
   grows in an exponential form, obeying the famous Moore's law. We
   theoretically predict that the size of the AS-level Internet will double
   every 5.32 years. We apply the k-core decomposition method on the real
   Internet, and find that the size of a k-core with larger k is nearly
   stable over time. In addition, the maximal coreness is very stable after
   2003. In contrast to the predictions of most previous models, the
   maximal degree of the Internet is also relatively stable versus time. We
   use the edge-exchange operation to obtain the randomized networks with
   the same degree sequence. A systematical comparison is drawn, indicating
   that the real Internet is more loosely connected, and both the full
   Internet and the nucleus are more disassortative than their randomized
   versions.}},
