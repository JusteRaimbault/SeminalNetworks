@article{ ISI:000224722000003,
Author = {Li, L and Alderson, D and Willinger, W and Doyle, J},
Title = {{A first-principles approach to understanding the Internet's router-level
   topology}},
Journal = {{ACM SIGCOMM COMPUTER COMMUNICATION REVIEW}},
Year = {{2004}},
