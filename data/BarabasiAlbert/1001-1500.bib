@article{ ISI:000301845100011,
Author = {Tanimoto, Jun and Brede, Markus and Yamauchi, Atsuo},
Title = {{Network reciprocity by coexisting learning and teaching strategies}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2012}},
Volume = {{85}},
Number = {{3, 1}},
Month = {{MAR 21}},
Abstract = {{We propose a network reciprocity model in which an agent
   probabilistically adopts learning or teaching strategies. In the
   learning adaptation mechanism, an agent may copy a neighbor's strategy
   through Fermi pairwise comparison. The teaching adaptation mechanism
   involves an agent imposing its strategy on a neighbor. Our simulations
   reveal that the reciprocity is significantly affected by the frequency
   with which learning and teaching agents coexist in a network and by the
   structure of the network itself.}},
