@article{ ISI:000236467700016,
Author = {Chen, ZY and Wang, XF},
Title = {{Effects of network structure and routing strategy on network capacity}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2006}},
Volume = {{73}},
Number = {{3, 2}},
Month = {{MAR}},
Abstract = {{The capacity of maximum end-to-end traffic flow the network is able to
   handle without overloading is an important index for network performance
   in real communication systems. In this paper, we estimate the variations
   of network capacity under different routing strategies for three
   different topologies. Simulation results reveal that the capacity
   depends on the underlying network structure and the capacity increases
   as the network becomes more homogeneous. It is also observed that the
   network capacity is greatly enhanced when the new traffic awareness
   routing strategy is adopted in each network structure.}},
