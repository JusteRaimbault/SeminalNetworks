@article{ ISI:000383706800012,
Author = {Hu, Jessica Xin and Thomas, Cecilia Engel and Brunak, Soren},
Title = {{Network biology concepts in complex disease comorbidities}},
Journal = {{NATURE REVIEWS GENETICS}},
Year = {{2016}},
Volume = {{17}},
Number = {{10}},
Pages = {{615-629}},
Month = {{OCT}},
Abstract = {{The co-occurrence of diseases can inform the underlying network biology
   of shared and multifunctional genes and pathways. In addition,
   comorbidities help to elucidate the effects of external exposures, such
   as diet, lifestyle and patient care. With worldwide health transaction
   data now often being collected electronically, disease co-occurrences
   are starting to be quantitatively characterized. Linking network
   dynamics to the real-life, non-ideal patient in whom diseases co-occur
   and interact provides a valuable basis for generating hypotheses on
   molecular disease mechanisms, and provides knowledge that can facilitate
   drug repurposing and the development of targeted therapeutic strategies.}},
