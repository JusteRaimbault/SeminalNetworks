@article{ ISI:000385334800002,
Author = {Wang, Chengjiang and Wang, Li and Wang, Juan and Sun, Shiwen and Xia,
   Chengyi},
Title = {{Inferring the reputation enhances the cooperation in the public goods
   game on interdependent lattices}},
