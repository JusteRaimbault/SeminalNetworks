@article{ ISI:000282997600008,
Author = {Gatti, Domenico Delli and Gallegati, Mauro and Greenwald, Bruce and
   Russo, Alberto and Stiglitz, Joseph E.},
Title = {{The financial accelerator in an evolving credit network}},
Journal = {{JOURNAL OF ECONOMIC DYNAMICS \& CONTROL}},
Year = {{2010}},
Volume = {{34}},
Number = {{9, SI}},
Pages = {{1627-1650}},
Month = {{SEP}},
Note = {{14th International Conference on Computing in Economics and Finance (CEF
   2008), Univ Sorbonne, Inst Higher Learn, Paris, FRANCE, JUN 26-28, 2008}},
Organization = {{Soc Comp Econ}},
Abstract = {{We model a credit network characterized by credit relationships
   connecting (i) downstream (D) and upstream (U) firms and (ii) firms and
   banks. The net worth of D firms is the driver of fluctuations. The
   production of D firms and of their suppliers (U firms) in fact, is
   constrained by the availability of internal finance-proxied by net
   worth-to the D firms. The structure of credit interlinkages changes over
   time due to an endogeneous process of partner selection, which leads to
   the polarization of the network At the aggregate level, the distribution
   of growth rates exhibits negative skewness and excess kurtosis. When a
   shock hits the macroeconomy or a significant group of agents in the
   credit network a bankruptcy avalanche can follow if agents' leverage is
   critically high. In a nutshell we want to explore the properties of a
   network-based financial accelerator. (C) 2010 Elsevier B.V. All rights
   reserved.}},
