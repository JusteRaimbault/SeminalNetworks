@article{ ISI:000250276900006,
Author = {Astolfi, L. and Fallani, F. De Vico and Cincotti, F. and Mattia, D. and
   Marciani, M. G. and Bufalari, S. and Salinari, S. and Colosimo, A. and
   Ding, L. and Edgar, J. C. and Heller, W. and Miller, G. A. and He, B.
   and Babiloni, F.},
Title = {{Imaging functional brain connectivity patterns from high-resolution EEG
   and fMRI via graph theory}},
Journal = {{PSYCHOPHYSIOLOGY}},
Year = {{2007}},
Volume = {{44}},
Number = {{6}},
Pages = {{880-893}},
Month = {{NOV}},
Abstract = {{We describe a set of computational tools able to estimate cortical
   activity and connectivity from high- resolution EEG and fMRI recordings
   in humans. These methods comprise the estimation of cortical activity
   using realistic geometry head volume conductor models and distributed
   cortical source models, followed by the evaluation of cortical
   connectivity between regions of interest coincident with the Brodmann
   areas via the use of Partial Directed Coherence. Connectivity patterns
   estimated on the cortical surface in different frequency bands are then
   imaged and interpreted with measures based on graph theory. These
   computational tools were applied on a set of EEG and fMRI data from a
   Stroop task to demonstrate the potential of the proposed approach. The
   present findings suggest that the methodology is able to identify
   differences in functional connectivity patterns elicited by different
   experimental tasks or conditions.}},
