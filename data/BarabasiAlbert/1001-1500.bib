@article{ ISI:000273208000006,
Author = {Nielsen, Jens},
Title = {{Systems biology of lipid metabolism: From yeast to human}},
Journal = {{FEBS LETTERS}},
Year = {{2009}},
Volume = {{583}},
Number = {{24}},
Pages = {{3905-3913}},
Month = {{DEC 17}},
Note = {{146th Nobel Symposium on Systems Biology, Drobak, NORWAY, JUN 25-27,
   2009}},
Abstract = {{Lipid metabolism is highly relevant as it plays a central role in a
   number of human diseases. Due to the highly interactive structure of
   lipid metabolism and its regulation, it is necessary to apply a holistic
   approach, and systems biology is therefore well suited for integrated
   analysis of lipid metabolism. In this paper it is demonstrated that the
   yeast Saccharomyces cerevisiae serves as an excellent model organism for
   studying the regulation of lipid metabolism in eukaryotes as most of the
   regulatory structures in this part of the metabolism are conserved
   between yeast and mammals. Hereby yeast systems biology can assist to
   improve our understanding of how lipid metabolism is regulated.
   (c) 2009 Federation of European Biochemical Societies. Published by
   Elsevier B.V. All rights reserved.}},
