@article{ ISI:000268060100034,
Author = {Zhu, Hailin and Luo, Hong and Peng, Haipeng and Li, Lixiang and Luo, Qun},
Title = {{Complex networks-based energy-efficient evolution model for wireless
   sensor networks}},
Journal = {{CHAOS SOLITONS \& FRACTALS}},
