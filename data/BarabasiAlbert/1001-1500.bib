@article{ ISI:000344467500020,
Author = {Wen, Guanghui and Yu, Wenwu and Chen, Michael Z. Q. and Yu, Xinghuo and
   Chen, Guanrong},
Title = {{H-infinity Pinning Synchronization of Directed Networks With Aperiodic
   Sampled-Data Communications}},
