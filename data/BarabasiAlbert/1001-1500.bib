@article{ ISI:000235355800003,
Author = {Belykh, T and Hasler, M and Lauret, M and Nijmeijer, H},
Title = {{Synchronization and graph topology}},
Journal = {{INTERNATIONAL JOURNAL OF BIFURCATION AND CHAOS}},
Year = {{2005}},
Volume = {{15}},
Number = {{11}},
Pages = {{3423-3433}},
Month = {{NOV}},
Abstract = {{This paper clarifies the relation between synchronization and graph
   topology. Applying the Connection Graph Stability method developed by
   Belykh et al. {[}2004a] to the study of synchronization in networks of
   coupled oscillators, we show which graph properties matter for
   synchronization. In particular, while we explicitly link the stability
   of synchronization with the average path length for a wide class of
   coupling graphs, we prove by a simple argument that the average path
   length is not always the crucial quantity for synchronization.,W also
   show that synchronization in scale-free networks can be described by
   means of regular networks with a star-like coupling structure. Finally,
   by considering an example of coupled Hindmarsh-Rose neuron models, we
   demonstrate how global stability of synchronization depends on the
   parameters of the individual oscillator.}},
