@article{ ISI:000228818200028,
Author = {Krapivsky, PL and Redner, S},
Title = {{Network growth by copying}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2005}},
Volume = {{71}},
Number = {{3, 2}},
Month = {{MAR}},
Abstract = {{We introduce a growing network model in which a new node attaches to a
   randomly selected node, as well as to all ancestors of the target node.
   This mechanism produces a sparse, ultrasmall network where the average
   node degree grows logarithmically with network size while the network
   diameter equals 2. We determine basic geometrical network properties,
   such as the size dependence of the number of links and the in- and
   out-degree distributions. We also compare our predictions with real
   networks where the node degree also grows slowly with time-the Internet
   and the citation network of all Physical Review papers.}},
