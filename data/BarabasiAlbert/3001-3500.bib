@article{ ISI:000089778900008,
Author = {Ben-Naim, E and Krapivsky, PL},
Title = {{Fragmentation with a steady source}},
Journal = {{PHYSICS LETTERS A}},
Year = {{2000}},
Volume = {{275}},
Number = {{1-2}},
Pages = {{48-53}},
Month = {{OCT 2}},
Abstract = {{We investigate fragmentation processes with a steady input of fragments.
   We find that the size distribution approaches a stationary form which
   exhibits a power law divergence in the small size limit, P-x(x) similar
   to x(-3). This algebraic behavior is robust as it is independent of the
   details of the input as well as the spatial dimension. The full time
   dependent behavior is obtained analytically for arbitrary inputs, and is
   found to exhibit a universal scaling behavior. (C) 2000 Elsevier Science
   B.V. All rights reserved.}},
