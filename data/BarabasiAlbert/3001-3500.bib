@article{ ISI:000266116900016,
Author = {Yachie, Nozomu and Saito, Rintaro and Sugahara, Junichi and Tomita,
   Masaru and Ishihama, Yasushi},
Title = {{In Silico Analysis of Phosphoproteome Data Suggests a Rich-get-richer
   Process of Phosphosite Accumulation over Evolution}},
