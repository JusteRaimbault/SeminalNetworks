@inproceedings{ ISI:000381621400002,
Author = {McLaughlin, Adam and Bader, David A.},
Book-Group-Author = {{IEEE}},
Title = {{Fast Execution of Simultaneous Breadth-First Searches on Sparse Graphs}},
Booktitle = {{2015 IEEE 21ST INTERNATIONAL CONFERENCE ON PARALLEL AND DISTRIBUTED
   SYSTEMS (ICPADS)}},
Series = {{International Conference on Parallel and Distributed Systems -
   Proceedings}},
