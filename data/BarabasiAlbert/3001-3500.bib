@article{ ISI:000320954900063,
Author = {Deng, Liping and Wu, Zhaoyan and Wu, Qingchu},
Title = {{Pinning synchronization of complex network with non-derivative and
   derivative coupling}},
Journal = {{NONLINEAR DYNAMICS}},
Year = {{2013}},
Volume = {{73}},
Number = {{1-2}},
Pages = {{775-782}},
Month = {{JUL}},
Abstract = {{This paper investigates synchronization of a complex network with
   non-derivative and derivative coupling. For achieving the pinning
   synchronization, the corresponding controllers are designed and applied
   to only a small fraction of nodes. Both linear and adaptive feedback
   control methods are used to design controllers. Based on Lyapunov
   stability theory, several simple and useful criteria for pinning
   synchronization are derived. Finally, numerical simulations are given to
   verify the effectiveness of the derived results.}},
