@article{ ISI:000328454900028,
Author = {Anzo, A. and Barajas-Ramirez, J. G.},
Title = {{Synchronization in complex networks under structural evolution}},
Journal = {{JOURNAL OF THE FRANKLIN INSTITUTE-ENGINEERING AND APPLIED MATHEMATICS}},
Year = {{2014}},
Volume = {{351}},
Number = {{1}},
Pages = {{358-372}},
Month = {{JAN}},
Abstract = {{We investigate the effects of structural evolution on the stability of
   synchronized behavior in complex networks. By structural evolution we
   mean processes that change the topology of the network. In particular,
   we consider structural evolution as two simultaneous processes: on one
   hand, the topology changes according to an arbitrary switching law among
   a set of admissible patterns of connection; on the other hand, the
   strength of connection evolves according to an adaptive law. Our results
   show that by constraining the admissible patterns of connection, and
   using an adaptive law based on the difference between the nodes, we can
   guarantee the stability of the synchronized solution of the network
   despite structural changes. Additionally, we extend our results by
   considering alternative structural evolution processes, namely, a
   node-based adaptive strategy and a resetting switching law. We
   illustrate our results with numerical simulation. (C) 2013 The Franklin
   Institute. Published by Elsevier Ltd. All rights reserved.}},
