@inproceedings{ ISI:000413483400033,
Author = {Osaka, Kengo and Toriumi, Fujio and Sugawara, Toshiharu},
Editor = {{Cherifi, H and Gaito, S and Quattrociocchi, W and Sala, A}},
Title = {{Effect of Direct Reciprocity on Continuing Prosperity of Social
   Networking Services}},
