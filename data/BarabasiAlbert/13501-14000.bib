Title = {{An Approximate Ripple-Spreading Algorithm to Identify the k Best Paths}},
Booktitle = {{2017 13TH INTERNATIONAL CONFERENCE ON NATURAL COMPUTATION, FUZZY SYSTEMS
   AND KNOWLEDGE DISCOVERY (ICNC-FSKD)}},
Year = {{2017}},
