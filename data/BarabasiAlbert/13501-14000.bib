@article{ ISI:000395127400014,
Author = {Cai, Kai-Quan and Yu, Lu and Zhu, Yan-Bo},
Title = {{A hybrid queuing strategy for network traffic on scale-free networks}},
Journal = {{MODERN PHYSICS LETTERS B}},
Year = {{2017}},
Volume = {{31}},
Number = {{5}},
Month = {{FEB 20}},
Abstract = {{In this paper, a hybrid queuing strategy (HQS) is proposed in traffic
   dynamics model on scale-free networks, where the delivery priority of
   packets in the queue is related to their distance to destination and the
   queue length of next jump. We compare the performance of the proposed
   HQS with that of the traditional first-in-first-out (FIFO) queuing
   strategy and the shortest-remaining-path-first (SRPF) queuing strategy
   proposed by Du et al. It is observed that the network traffic efficiency
   utilizing HQS with suitable value of parameter h can be further improved
   in the congestion state. Our work provides new insights for the
   understanding of the networked-traffic systems.}},
