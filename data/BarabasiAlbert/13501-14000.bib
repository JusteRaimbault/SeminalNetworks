@inproceedings{ ISI:000427123500090,
Author = {Chen, Guang and Wen, Guanghui and Yu, Wenwu and Cao, Jinde},
Book-Group-Author = {{IEEE}},
Title = {{Routing with Distributed Multiple Paths in Networks}},
Booktitle = {{2017 13TH IEEE INTERNATIONAL CONFERENCE ON CONTROL \& AUTOMATION (ICCA)}},
Series = {{IEEE International Conference on Control and Automation ICCA}},
Year = {{2017}},
Pages = {{519-523}},
Note = {{13th IEEE International Conference on Control \& Automation (ICCA),
   Ohrid, MACEDONIA, JUL 03-06, 2017}},
Organization = {{IEEE}},
Abstract = {{In this paper, a new routing strategy named as Distributed Multiple Path
   (DMP) routing strategy is proposed. To yield a high-efficiency
   transmission performance, the proposed DMP routing strategy utilizes the
   information of static network topology, as well as that of dynamic
   network transmission status. Specifically, for each of the routing
   Origin-Destination (O-D) node pairs, the DMP routing strategy first
   computes out and stores multiple paths which possess short length and
   meanwhile overlap less between each other from the static network
   topology perspective. During the transmission process, for each
   transmission task, the information of dynamic network transmission
   status is utilized to choose a favorable routing path from the stored
   multiple paths. Computer simulation tested on the scale-free networks
   shows that, the DMP routing strategy can improve the anti-congestion
   transmission performance of networks significantly, and can provide
   better routing robustness for networks subject to random partial
   failures.}},
