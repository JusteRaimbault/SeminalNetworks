@article{ ISI:000418343000011,
Author = {Nokkala, Johannes and Maniscalco, Sabrina and Piilo, Jyrki},
Title = {{Non-Markovianity over Ensemble Averages in Quantum Complex Networks}},
Journal = {{OPEN SYSTEMS \& INFORMATION DYNAMICS}},
Year = {{2017}},
Volume = {{24}},
Number = {{4}},
Month = {{DEC}},
Note = {{48th Symposium on Mathematical Physics on
   Gorini-Kossakowski-Lindblad-Sudarshan Master Equation - 40 Years After,
   Nicolaus Copernicus Univ, Inst Phys, Torun, POLAND, JUN 10-12, 2016}},
Abstract = {{We consider bosonic quantum complex networks as structured finite
   environments for a quantum harmonic oscillator and investigate the
   interplay between the network structure and its spectral density,
   excitation transport properties and non-Markovianity. After a review of
   the formalism used, we demonstrate how even small changes to the network
   structure can have a large impact on the transport of excitations. We
   then consider the non-Markovianity over ensemble averages of several
   different types of random networks of identical oscillators and uniform
   coupling strength. Our results show that increasing the number of
   interactions in the network tends to suppress the average
   non-Markovianity. This suggests that tree networks are the random
   networks optimizing this quantity.}},
