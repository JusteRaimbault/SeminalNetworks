@article{ ISI:000392284900002,
Author = {Oliveira, Rafael S. and Andrade, Roberto F. S.},
Title = {{Ferromagnetic model on the Apollonian packing}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2017}},
Volume = {{95}},
Number = {{1}},
Month = {{JAN 17}},
Abstract = {{This work investigates the influence of geometrical features of the
   Apollonian packing (AP) on the behavior of magnetic models. The proposed
   model differs from previous investigations on the Apollonian network
   (AN), where the magnetic coupling constants depend only on the
   properties of the network structure defined by the packing, but not on
   quantitative aspects of its geometry. In opposition to the exact scale
   invariance observed in the AN, the circle's sizes in the AP are scaled
   by different factors when one goes from one generation to the next,
   requiring a different approach for the evaluation of the model's
   properties. If the nearest-neighbors coupling constants are defined by
   J(i,j) similar to 1/(r(i)+ r(j)) a, where r(i) indicates the radius of
   the circle i containing the node i, the results for the correlation
   length. indicate that the model's behavior depend on a. In the
   thermodynamic limit, the uniform model (alpha = 0) is characterized by
   xi -> 8 for all T > 0. Our results indicate that, on increasing a, the
   system changes to an uncorrelated pattern, with finite. at all T > 0, at
   a value alpha(c) similar or equal to 0.743. For any fixed value of a, no
   finite temperature singularity in the specific heat is observed,
   indicating that changes in the magnetic ordering occur only when a is
   changed. This is corroborated by the results for the magnetization and
   magnetic susceptibility.}},
