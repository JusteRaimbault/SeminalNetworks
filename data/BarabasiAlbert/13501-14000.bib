@inproceedings{ ISI:000428438100162,
Author = {Gan, Lingqing and Djuric, Petar M.},
Book-Group-Author = {{IEEE}},
Title = {{BAYESIAN SELECTION OF MODELS OF NETWORK FORMATION}},
Booktitle = {{2017 IEEE 7TH INTERNATIONAL WORKSHOP ON COMPUTATIONAL ADVANCES IN
   MULTI-SENSOR ADAPTIVE PROCESSING (CAMSAP)}},
Year = {{2017}},
Note = {{7th IEEE International Workshop on Computational Advances in
   Multi-Sensor Adaptive Processing (CAMSAP), Curacao, NETH ANTILLES, DEC
   10-13, 2017}},
Organization = {{IEEE}},
Abstract = {{Models of growing networks have attracted a lot of interest during the
   past few years. An important question about these models is to decide
   which model explains an observed network formation most accurately. In
   this work, we propose a Bayesian model selection scheme which chooses
   the best model based on predictive distributions. The procedure was
   investigated on three types of models, including the random model, the
   preferential attachment model and the hybrid model. With the hybrid
   model, we leverage results on imperfect Bernoulli trial experiments to
   obtain the posterior distribution of the weight parameter, which is
   characterized as a polynomial function on the interval {[}0, 1]. A Beta
   distribution is used to approximate the posterior in order to reduce the
   growing computational and representation complexity. Simulations in
   accordance with the proposed scheme are carried out. They demonstrate
   validity of the proposed approach.}},
