@article{ ISI:000418101500003,
Author = {Wang, Zhiyong and Hall, Benika and Xu, Jinbo and Shi, Xinghua},
Title = {{A Sparse Learning Framework for Joint Effect Analysis of Copy Number
   Variants}},
Journal = {{IEEE-ACM TRANSACTIONS ON COMPUTATIONAL BIOLOGY AND BIOINFORMATICS}},
