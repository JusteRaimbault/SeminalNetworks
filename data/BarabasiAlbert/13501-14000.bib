@article{ ISI:000396904300005,
Author = {Taghawi-Nejad, Davoud and Veetil, Vipin P.},
Title = {{The Complexity of Coordination}},
Journal = {{EASTERN ECONOMIC JOURNAL}},
Year = {{2017}},
Volume = {{43}},
Number = {{2}},
Pages = {{260-270}},
Month = {{MAR}},
Abstract = {{The traditional mechanism of finding Nash equilibria presumes economic
   actors are capable of performing computations that even computers would
   take far too long to complete. A decentralized and parallel process of
   interactions between simple economic actors is presented as a more
   plausible microfoundation of the concept of Nash equilibria. It is found
   that agent interactions on a scale-free network converge to an
   equilibrium within reasonable time. NP computational complexity of Nash
   equilibria does not diminish its empirical relevance.}},
