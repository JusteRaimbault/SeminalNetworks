@inproceedings{ ISI:000437036300013,
Author = {Lu, Youjun and Xu, Daoyun and Zhou, Jincheng},
Editor = {{Du, D and Li, L and Zhu, E and He, K}},
Title = {{Degree Correlations in Two Layer Growth Model with Nonlinear
   Preferential Attachment Rule}},
