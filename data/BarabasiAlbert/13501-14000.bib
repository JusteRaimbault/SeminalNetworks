@inproceedings{ ISI:000419321700015,
Author = {Jakobi, Akos},
Editor = {{Ivan, I and Singleton, A and Horak, J and Inspektor, T}},
Title = {{Proximity-Driven Motives in the Evolution of an Online Social Network}},
