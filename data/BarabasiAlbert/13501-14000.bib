@article{ ISI:000416750500002,
Author = {Inoue, Hiroyasu},
Title = {{Controllability analyses of nation-wide firm networks}},
Journal = {{EVOLUTIONARY AND INSTITUTIONAL ECONOMICS REVIEW}},
