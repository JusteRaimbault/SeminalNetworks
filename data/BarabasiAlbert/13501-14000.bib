@article{ ISI:000382258600074,
Author = {Zhang, Lanhua and Chen, Juan and Wang, Mei and Li, Yujuan and Xue,
   Shaowei and Tang, Yiyuan and Sun, Baoliang},
Title = {{Topology Universality and Dissimilarity in a Class of Scale-Free
   Networks}},
Journal = {{PLOS ONE}},
Year = {{2016}},
Volume = {{11}},
Number = {{8}},
Month = {{AUG 25}},
Abstract = {{We study the effect of subtle changes on the evolution in the scale-free
   (SF) networks. Three extended models are evolved based on competition
   and inner anti-preferential deletion in growth and preferential
   attachment processes. By nonlinear and dynamic controlling on randomness
   and determinacy, three models can self-organize into scale-free
   networks, and diverse scaling exponents appear. Moreover, the model with
   more determinacy has more stringent parameter control than randomness,
   especially in the edge deletion. Our results suggest that the nature of
   the topology universality and dissimilarity in SF networks may be the
   subtle changes of randomness and determinacy.}},
