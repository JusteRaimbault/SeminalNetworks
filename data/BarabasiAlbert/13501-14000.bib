@inproceedings{ ISI:000450919800026,
Author = {Stracqualursi, Luisa and Agati, Patrizia},
Editor = {{Palumbo, F and Montanari, A and Vichi, M}},
Title = {{Predicting the Evolution of a Constrained Network: A Beta Regression
   Model}},
Booktitle = {{DATA SCIENCE: INNOVATIVE DEVELOPMENTS IN DATA ANALYSIS AND CLUSTERING}},
Series = {{Studies in Classification Data Analysis and Knowledge Organization}},
Year = {{2017}},
Pages = {{333-342}},
