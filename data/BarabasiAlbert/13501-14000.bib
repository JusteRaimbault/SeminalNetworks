@article{ ISI:000402804200012,
Author = {Bronski, Jared C. and He, Yizhang and Li, Xinye and Liu, Yue and
   Sponseller, Danielle Rae and Wolbert, Seth},
Title = {{The stability of fixed points for a Kuramoto model with Hebbian
   interactions}},
Journal = {{CHAOS}},
Year = {{2017}},
Volume = {{27}},
Number = {{5}},
Month = {{MAY}},
Abstract = {{We consider a variation of the Kuramoto model with dynamic coupling,
   where the coupling strengths are allowed to evolve in response to the
   phase difference between the oscillators, a model first considered by
   Ha, Noh, and Park. We demonstrate that the fixed points of this model,
   as well as their stability, can be completely expressed in terms of the
   fixed points and stability of the analogous classical Kuramoto problem
   where the coupling strengths are fixed to a constant (the same for all
   edges). In particular, for the ``all-to-all{''} network, where the
   underlying graph is the complete graph, the problem reduces to the
   problem of understanding the fixed points and stability of the
   all-to-all Kuramoto model with equal edge weights, a problem that is
   well understood. Published by AIP Publishing.}},
