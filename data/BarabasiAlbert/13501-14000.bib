@article{ ISI:000461286200005,
Author = {Yuan, Xilaoliang},
Title = {{Stakeholders Identification of Internet Public Opinion in Drug Safety
   Emergencies Based on SNA}},
Journal = {{INVESTIGACION CLINICA}},
