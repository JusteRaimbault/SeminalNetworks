@inproceedings{ ISI:000406412100024,
Author = {Du, Fei and Huang, Hongwei and Zhang, Dongming and Zhang, Fan},
Editor = {{Huang, J and Fenton, GA and Zhang, L and Griffiths, DV}},
Title = {{Resilience Analysis of Metro Networks: A Case Study of Shanghai Metro
   Network}},
Booktitle = {{GEO-RISK 2017: RELIABILITY-BASED DESIGN AND CODE DEVELOPMENTS}},
Series = {{Geotechnical Special Publication}},
Year = {{2017}},
Number = {{283}},
Pages = {{247-258}},
Note = {{Geo-Risk Conference, Denver, CO, JUN 04-07, 2017}},
Organization = {{Amer Soc Civil Engineers, Geo Inst; Amer Soc Civil Engineers}},
Abstract = {{In this paper, the concept of resilience of network topology is defined
   as the ability to sustain the disruptions that can reduce the network
   connectivity and the ability to quickly recover the normal connectivity;
   the metric for quantifying the resilience is proposed based on the
   resilience triangle and network global efficiency. The metric of
   resilience can be used to identify the optimal recovery strategy when
   network is damaged. Shanghai metro network is used to demonstrate the
   applicability of the proposed resilience metric. The resilience of the
   metro network has been measured by assuming breakdown of metro
   station(s), and the optimal recovery sequence has been presented by
   using the proposed metric. The topological characteristics are also
   analyzed and investigated using several parameters. The results of this
   paper indicate that Shanghai metro network is `small-world' network and
   `scale-free' network when represented by L-space graph. The resilience
   metric proposed can be applied to measure the resilience of real world
   metro network and optimize the recovery strategy.}},
