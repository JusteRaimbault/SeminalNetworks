@article{ ISI:000439640500018,
Author = {Dexter, Philip and Liu, Yu David and Chiu, Kenneth},
Title = {{Lazy Graph Processing in Haskell}},
Journal = {{ACM SIGPLAN NOTICES}},
Year = {{2016}},
