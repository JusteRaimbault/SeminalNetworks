@article{ ISI:000401342700020,
Author = {Li, Huaqing and Wang, Yinqiu and Chen, Guo and Xia, Dawen and Xiao, Li},
Title = {{Consensus in networked dynamical systems with event-triggered control
   inputs and random switching topologies}},
