@article{ ISI:000403556100006,
Author = {Gomes, Orlando},
Title = {{Nonlinearities in Behavioral Macroeconomics}},
Journal = {{NONLINEAR DYNAMICS PSYCHOLOGY AND LIFE SCIENCES}},
