@article{ ISI:000411942500017,
Author = {Canals, Agusti and Ortoll, Eva and Nordberg, Markus},
Title = {{COLLABORATION NETWOKS IN BIG SCIENCE: THE ATLAS EXPERIMENT AT CERN}},
