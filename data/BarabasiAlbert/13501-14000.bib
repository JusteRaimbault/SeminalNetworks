@inproceedings{ ISI:000426965800058,
Author = {Borassi, Michele and Crescenzi, Pierluigi and Trevisan, Luca},
Book-Group-Author = {{ACM}},
Title = {{An Axiomatic and an Average-Case Analysis of Algorithms and Heuristics
   for Metric Properties of Graphs}},
Booktitle = {{PROCEEDINGS OF THE TWENTY-EIGHTH ANNUAL ACM-SIAM SYMPOSIUM ON DISCRETE
   ALGORITHMS}},
