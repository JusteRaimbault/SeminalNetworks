@article{ ISI:000423965200254,
Author = {Zeng, Shuai and Li, Juanjuan and Ni, Xiaochun and Yuan, Yong and Wang,
   Fei-Yue},
Title = {{Research on Social Marketing Strategies with An Agent-based Propagation
   Model}},
