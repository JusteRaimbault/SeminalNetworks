@article{ ISI:000426424200002,
Author = {Burstein, David and Kenter, Franklin and Kun, Jeremy and Shi, Feng},
Title = {{Interception in distance-vector routing networks}},
Journal = {{JOURNAL OF COMPLEX NETWORKS}},
