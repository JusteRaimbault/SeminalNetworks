@article{ ISI:000318544400001,
Author = {Shirazi, Amirhossein and Namaki, Ali and Roohi, Amir Ahmad and Jafari,
   Gholamreza},
Title = {{Transparency Effect in the Emergence of Monopolies in Social Networks}},
Journal = {{JASSS-THE JOURNAL OF ARTIFICIAL SOCIETIES AND SOCIAL SIMULATION}},
Year = {{2013}},
Volume = {{16}},
Number = {{1}},
Month = {{JAN}},
Abstract = {{A power law degree distribution is displayed in many complex networks.
   However, in most real social and economic networks, deviation from
   power-law behavior is observed. Such networks also have giant hubs far
   from the tail of the power law distribution. We propose a model based on
   information `transparency (i.e. how much information is visible to
   others), which can explain the power structure in societies with
   non-transparency in information delivery. The emergence of very high
   degree nodes is explained as a direct result of censorship. Based on
   these assumptions, we define four distinct transparency regions:
   perfectly non-transparent, low transparent, perfectly transparent
   regions and regions where information is exaggerated. We observe the
   emergence of some very high degree nodes in low transparency networks.
   We show that the low transparency networks are more vulnerable to attack
   and the controllability of low transparent networks is more difficult
   than for the others. Also, the low transparency networks have a smaller
   mean path length and higher clustering coefficients than the other
   regions.}},
Publisher = {{J A S S S}},
Address = {{UNIV SURREY, DEPT SOCIOLOGY, GUILDFORD GU2 7XH, SURREY, ENGLAND}},
