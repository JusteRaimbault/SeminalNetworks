@article{ ISI:000304891600013,
Author = {Buono, C. and Lagorio, C. and Macri, P. A. and Braunstein, L. A.},
Title = {{Crossover from weak to strong disorder regime in the duration of
   epidemics}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2012}},
Volume = {{391}},
Number = {{16}},
Pages = {{4181-4185}},
Month = {{AUG 15}},
Abstract = {{We study the susceptible-infected-recovered (SIR) model in complex
   networks, considering that not all individuals in the population
   interact in the same way. This heterogeneity between contacts is modeled
   by a continuous disorder. In our model, the disorder represents the
   contact time or the closeness between individuals. We find that the
   duration time of an epidemic has a crossover with the system size, from
   a power-law regime to a logarithmic regime depending on the
   transmissibility related to the strength of the disorder. Using
   percolation theory, we find that the duration of the epidemic scales as
   the average length of the branches of the infection. Our theoretical
   findings, supported by simulations, explains the crossover between the
   two regimes. (C) 2012 Elsevier B.V. All rights reserved.}},
