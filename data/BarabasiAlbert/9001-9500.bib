@article{ ISI:000364539100010,
Author = {Singh, R. K. and Bagarti, Trilochan},
Title = {{Synchronization in an evolving network}},
Journal = {{EPL}},
Year = {{2015}},
Volume = {{111}},
Number = {{5}},
Month = {{SEP}},
Abstract = {{In this work we study the dynamics of Kuramoto oscillators on a
   stochastically evolving network whose evolution is governed by the
   phases of the individual oscillators and degree distribution.
   Synchronization is achieved after a threshold connection density is
   reached. This cumulative effect of topology and dynamics has many
   real-world implications, where synchronization in a system emerges as a
   collective property of its components in a self-organizing manner. The
   synchronous state remains stable as long as the connection density
   remains above the threshold value, with additional links providing
   resilience against network fluctuations. Copyright (C) EPLA, 2015}},
