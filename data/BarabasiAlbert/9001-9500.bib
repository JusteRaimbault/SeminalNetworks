@article{ ISI:000336706000019,
Author = {Ahadpour, Sodeif and Sadra, Yaser and ArastehFard, Zahra},
Title = {{Markov-binary visibility graph: A new method for analyzing complex
   systems}},
Journal = {{INFORMATION SCIENCES}},
Year = {{2014}},
Volume = {{274}},
Pages = {{286-302}},
Month = {{AUG 1}},
Abstract = {{In order to study the dynamics of complex systems has been used
   information recorded in the form of time series. These time series can
   be investigated from a complex network perspective. Using two-state
   Markov chain and the binary visibility graph, we investigate these time
   series. Moreover, several topological aspects of the constructed graph,
   such as degree distribution, clustering coefficient, and mean visibility
   length are studied. Our results show that the Markov-binary visibility
