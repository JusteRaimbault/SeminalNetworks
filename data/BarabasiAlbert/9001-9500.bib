@article{ ISI:000296673400027,
Author = {Guo, Shi-Ze and Lu, Zhe-Ming and Chen, Zhe and Luo, Hao},
Title = {{Strength-Strength and Strength-Degree Correlation Measures for Directed
   Weighted Complex Network Analysis}},
Journal = {{IEICE TRANSACTIONS ON INFORMATION AND SYSTEMS}},
Year = {{2011}},
Volume = {{E94D}},
Number = {{11}},
Pages = {{2284-2287}},
Month = {{NOV}},
Abstract = {{This Letter defines thirteen useful correlation measures for directed
   weighted complex network analysis. First, in-strength and out-strength
   are defined for each node in the directed weighted network. Then, one
   node-based strength-strength correlation measure and four arc-based
   strength-strength correlation measures are defined. In addition,
   considering that each node is associated with in-degree, out-degree,
   in-strength and out-strength, four node-based strength-degree
   correlation measures and four arc-based strength-degree correlation
   measures are defined. Finally, we use these measures to analyze the
   world trade network and the food web. The results demonstrate the
   effectiveness of the proposed measures for directed weighted networks.}},
