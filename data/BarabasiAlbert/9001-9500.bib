@article{ ISI:000322454200009,
Author = {Wang, Yuyi and Ramon, Jan and Fannes, Thomas},
Title = {{An efficiently computable subgraph pattern support measure: counting
   independent observations}},
Journal = {{DATA MINING AND KNOWLEDGE DISCOVERY}},
