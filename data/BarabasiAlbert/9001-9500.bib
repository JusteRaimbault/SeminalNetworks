@article{ ISI:000320845000006,
Author = {Xu, Junqun and Zhang, Jianxiong and Tang, Wansheng},
Title = {{Parameters and structure identification of complex delayed networks via
   pinning control}},
Journal = {{TRANSACTIONS OF THE INSTITUTE OF MEASUREMENT AND CONTROL}},
Year = {{2013}},
Volume = {{35}},
Number = {{5}},
Pages = {{619-624}},
Month = {{JUL}},
Abstract = {{In this paper, an uncertain complex delayed network model with
   non-identical nodes is established. The parameters of each dynamical
   node and the network structure are considered to be uncertain. The goal
   is to identify the unknown system parameters and the unknown network
   structure. By designing an adaptive observer network and applying the
   strategy of pinning control, the uncertain parameters and structure of
   the initial network can be identified. A sufficient condition to
   guarantee the controllability of the adaptive pinning control is
   presented, which shows that the uncertain parameters and the unknown
   coupling structure of the driven network can be identified successfully
   under the designed controllers. Finally, a numerical example is given to
   show the effectiveness of the proposed approach.}},
