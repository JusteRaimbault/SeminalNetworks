@article{ ISI:000309568300019,
Author = {Wang Dan and Jing Yuan-Wei and Hao Bin-Bin},
Title = {{Effect of weighted scheme on synchronizability based on different
   network structures}},
Journal = {{ACTA PHYSICA SINICA}},
Year = {{2012}},
Volume = {{61}},
Number = {{17}},
Month = {{SEP}},
Abstract = {{Motivated by the observations that the weight of a link in realistic
   networks is strongly correlated with the product of the corresponding
   degrees, we present an asymmetrical weighted scheme relating to the
   degrees of starting point and terminal point, and investigate the
   effects of weighted scheme on synchronizability in light of different
   network structures. The results show that the more heterogeneous the
   degree distribution of network, the more remarkable the effect on
   enhanced synchronizability by the weighted method is. However, there is
   little effect on enhanced synchronizability by the weighted method for
   the homogeneous random network. It is shown that no matter whether small
   world networks or scale-free networks, the synchronizability is maximal
   when the total strength of all in-links of every node is equal to one.}},
