@inproceedings{ ISI:000353639700185,
Author = {Kas, Miray and Carley, Kathleen M. and Carley, L. Richard},
Editor = {{Ozyer, T and Carrington, P}},
Title = {{Incremental Closeness Centrality for Dynamically Changing Social
   Networks}},
Booktitle = {{2013 IEEE/ACM INTERNATIONAL CONFERENCE ON ADVANCES IN SOCIAL NETWORKS
   ANALYSIS AND MINING (ASONAM)}},
Year = {{2013}},
Pages = {{1250-1258}},
