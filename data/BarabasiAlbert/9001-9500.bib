@incollection{ ISI:000405604600004,
Author = {Malagon, Nicolas and Larsen, Ellen},
Editor = {{Jeon, KW}},
Title = {{Heredity and Self-Organization: Partners in the Generation and Evolution
   of Phenotypes}},
Booktitle = {{INTERNATIONAL REVIEW OF CELL AND MOLECULAR BIOLOGY, VOL 315}},
Series = {{International Review of Cell and Molecular Biology}},
Year = {{2015}},
Volume = {{315}},
Pages = {{153-181}},
Abstract = {{In this review we examine the role of self-organization in the context
   of the evolution of morphogenesis. We provide examples to show that
   self-organized behavior is ubiquitous, and suggest it is a mechanism
   that can permit high levels of biodiversity without the invention of
   ever-increasing numbers of genes. We also examine the implications of
   self-organization for understanding the ``internal descriptions{''} of
