@article{ ISI:000325561300001,
Author = {Villanueva-Oller, J. and Acedo, L. and Morano, J. A. and
   Sanchez-Sanchez, A.},
Title = {{Epidemic Random Network Simulations in a Distributed Computing
   Environment}},
Journal = {{ABSTRACT AND APPLIED ANALYSIS}},
Year = {{2013}},
Abstract = {{We discuss a computational system following the paradigm of distributed
   computing, which will allow us to simulate the epidemic propagation in
   random networks with large number of nodes up to one million. This
   paradigm consists of a server that delivers tasks to be carried out by
   client computers. When the task is finished, the client sends the
   obtained results to the server to be stored until all tasks are finished
   and then ready to be analysed. Finally, we show that this technique
   allows us to disclose the emergence of seasonal patterns in the
   respiratory syncytial virus transmission dynamics which do not appear
   neither in smaller systems nor in continuous systems.}},
