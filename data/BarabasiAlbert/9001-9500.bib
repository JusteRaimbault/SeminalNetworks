@article{ ISI:000287518600024,
Author = {Yook, Soon-Hyung and Park, Juyong},
Title = {{Topological properties of scale-free networks driven by a graph
   Hamiltonian}},
Journal = {{EPL}},
Year = {{2011}},
Volume = {{93}},
Number = {{3}},
Month = {{FEB}},
Abstract = {{We numerically study an equilibrium network model, generated via a graph
   Hamiltonian, that exhibits a power-law degree distribution P(k)similar
   to k(-gamma) with gamma approximate to 1.5 in the thermodynamic limit.
   While the degree exponent, gamma, is equal to the one found in the
   Merging-Creation (MC) model, we find that the detailed topological
   properties such as degree-degree correlation and the modular structure
   differ significantly. We discuss the possible origin of these
   differences. Copyright (C) EPLA, 2011}},
