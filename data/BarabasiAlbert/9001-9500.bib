@article{ ISI:000336268700008,
Author = {Wu Zhao-Yan},
Title = {{Adaptive Impulsive Outer Synchronization Between Drive-Response
   Dynamical Networks}},
Journal = {{COMMUNICATIONS IN THEORETICAL PHYSICS}},
