@article{ ISI:000295840700002,
Author = {Guo, Dongchao and Liang, Mangui and Li, Dandan and Jiang, Zhongyuan},
Title = {{Effect of random edge failure on the average path length}},
Journal = {{JOURNAL OF PHYSICS A-MATHEMATICAL AND THEORETICAL}},
Year = {{2011}},
Volume = {{44}},
Number = {{41}},
Month = {{OCT 14}},
Abstract = {{We study the effect of random removal of edges on the average path
   length (APL) in a large class of uncorrelated random networks in which
   vertices are characterized by hidden variables controlling the
   attachment of edges between pairs of vertices. A formula for
   approximating the APL of networks suffering random edge removal is
   derived first. Then, the formula is confirmed by simulations for
   classical ER (Erdos and Renyi) random graphs, BA (Barabasi and Albert)
   networks, networks with exponential degree distributions as well as
   random networks with asymptotic power-law degree distributions with
   exponent alpha > 2.}},
