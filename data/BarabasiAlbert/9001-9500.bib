@article{ ISI:000307811800002,
Author = {Zhao, Jichang and Feng, Xu and Dong, Li and Liang, Xiao and Xu, Ke},
Title = {{Performance of local information-based link prediction: a sampling
   perspective}},
Journal = {{JOURNAL OF PHYSICS A-MATHEMATICAL AND THEORETICAL}},
Year = {{2012}},
Volume = {{45}},
Number = {{34}},
Month = {{AUG 31}},
Abstract = {{Link prediction is pervasively employed to uncover the missing links in
   the snapshots of real-world networks, which are usually obtained through
   different kinds of sampling methods. In the previous literature, in
   order to evaluate the performance of the prediction, known edges in the
   sampled snapshot are divided into the training set and the probe set
   randomly, without considering the underlying sampling approaches.
   However, different sampling methods might lead to different missing
   links, especially for the biased ways. For this reason, random
   partition-based evaluation of performance is no longer convincing if we
   take the sampling method into account. In this paper, we try to
   re-evaluate the performance of local information-based link predictions
   through sampling method governed division of the training set and the
