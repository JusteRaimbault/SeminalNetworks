@article{ ISI:000258602600026,
Author = {Yuan Shao-Qian and Zhao Hai and Li Chao and Zhang Xin},
Title = {{A network model with exponential cut-off degree distribution and
   local-cluster property}},
Journal = {{ACTA PHYSICA SINICA}},
Year = {{2008}},
Volume = {{57}},
Number = {{8}},
Pages = {{4805-4811}},
Month = {{AUG}},
Abstract = {{According to the character of local evolving mechanism in real network,
   we propose a new local-clustered network evolving model ( LC model).
   Theoretical analysis and numerical simulations show that the LC model
   obeys a power-law degree distribution with an exponential cut-off, and
   its cluster coefficient is much bigger than the local-world model ( LW
   model) proposed by Li and Chen and is more, close to real network. We
   simulate the performance of LC model against random errors and malice
   attacks, and discover that LC model is more fragile under attacks
   because of its large cluster coefficient.}},
