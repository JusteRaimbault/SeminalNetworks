Editor = {{Battiston, S and DePellegrini, F and Caldarelli, G and Merelli, E}},
Title = {{Nonparametric Estimation of the Preferential Attachment Function in
   Complex Networks: Evidence of Deviations from Log Linearity}},
Booktitle = {{PROCEEDINGS OF ECCS 2014: EUROPEAN CONFERENCE ON COMPLEX SYSTEMS}},
Series = {{Springer Proceedings in Complexity}},
