@inproceedings{ ISI:000401963300097,
Author = {Grando, Felipe and Noble, Diego and Lamb, Luis C.},
Book-Group-Author = {{IEEE}},
Title = {{An Analysis of Centrality Measures for Complex and Social Networks}},
Booktitle = {{2016 IEEE GLOBAL COMMUNICATIONS CONFERENCE (GLOBECOM)}},
