@inproceedings{ ISI:000375221400001,
Author = {Jacob, Emmanuel and Morters, Peter},
Editor = {{Gleich, DF and Komjathy, J and Litvak, N}},
Title = {{Robustness of Spatial Preferential Attachment Networks}},
Booktitle = {{ALGORITHMS AND MODELS FOR THE WEB GRAPH, (WAW 2015)}},
Series = {{Lecture Notes in Computer Science}},
Year = {{2015}},
Volume = {{9479}},
Pages = {{3-14}},
Note = {{12th International Workshop on Algorithms and Models for the Web-Graph
   (WAW), Eindhoven, NETHERLANDS, DEC 10-11, 2015}},
Organization = {{EURANDOM; NETWORKS; Microsoft Res; Google}},
Abstract = {{We study robustness under random attack for a class of networks, in
   which new nodes are given a spatial position and connect to existing
   vertices with a probability favouring short spatial distances and high
   degrees. In this model of a scale-free network with clustering one can
   independently tune the power law exponent tau > 2 of the degree
   distribution and a parameter delta > 1 determining the decay rate of the
   probability of long edges. We argue that the network is robust if omega
   < 2 + 1/delta, but fails to be robust if tau > 2 + 1/delta-1. Hence
   robustness depends not only on the power-law exponent but also on the
   clustering features of the network.}},
