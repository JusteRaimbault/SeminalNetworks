@article{ ISI:000383735800013,
Author = {Malyshkin, Yury and Paquette, Elliot},
Title = {{The power of choice over preferential attachment}},
Journal = {{ALEA-LATIN AMERICAN JOURNAL OF PROBABILITY AND MATHEMATICAL STATISTICS}},
Year = {{2015}},
Volume = {{12}},
Number = {{2}},
Pages = {{903-915}},
Abstract = {{We introduce a new type of preferential attachment tree that includes
   choices in its evolution, like with Achlioptas processes. At each step
   in the growth of the graph, a new vertex is introduced. A fixed number d
   of possible neighbor vertices are selected independently and with
   probability proportional to degree. Between them, the vertex with
   smallest degree is chosen, and a new edge is created. We determine with
   high probability the largest degree of this graph up to some additive
   error term.}},
