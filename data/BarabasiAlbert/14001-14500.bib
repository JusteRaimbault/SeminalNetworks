@article{ ISI:000369550200006,
Author = {Kato, Hideyuki and Ikeguchi, Tohru},
Title = {{Oscillation, Conduction Delays, and Learning Cooperate to Establish
   Neural Competition in Recurrent Networks}},
Journal = {{PLOS ONE}},
Year = {{2016}},
Volume = {{11}},
Number = {{2}},
Month = {{FEB 3}},
Abstract = {{Specific memory might be stored in a subnetwork consisting of a small
   population of neurons. To select neurons involved in memory formation,
   neural competition might be essential. In this paper, we show that
   excitable neurons are competitive and organize into two assemblies in a
   recurrent network with spike timing-dependent synaptic plasticity (STDP)
   and axonal conduction delays. Neural competition is established by the
   cooperation of spontaneously induced neural oscillation, axonal
   conduction delays, and STDP. We also suggest that the competition
   mechanism in this paper is one of the basic functions required to
   organize memory-storing subnetworks into fine-scale cortical networks.}},
