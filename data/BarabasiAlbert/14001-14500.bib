@inproceedings{ ISI:000363781600011,
Author = {Iosup, Alexandru and Capota, Mihai and Hegeman, Tim and Guo, Yong and
   Ngai, Wing Lung and Varbanescu, Ana Lucia and Verstraaten, Merijn},
Editor = {{Rabl, T and Sachs, K and Poess, M and Baru, C and Jacobson, HA}},
Title = {{Towards Benchmarking IaaS and PaaS Clouds for Graph Analytics}},
Booktitle = {{BIG DATA BENCHMARKING, WBDB 2014}},
Series = {{Lecture Notes in Computer Science}},
