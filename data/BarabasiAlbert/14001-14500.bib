@inproceedings{ ISI:000363951600002,
Author = {Cao, Zhigang and Chen, Xujin and Hu, Xiaodong and Wang, Changjun},
Editor = {{Thai, MT and Nguyen, NP and Shen, H}},
Title = {{Pricing in Social Networks with Negative Externalities}},
