Title = {{RPECA-Rumor Propagation Based Eventual Consistency Assessment Algorithm}},
Booktitle = {{ADVANCED PARALLEL PROCESSING TECHNOLOGIES, APPT 2015}},
Series = {{Lecture Notes in Computer Science}},
Year = {{2015}},
Volume = {{9231}},
