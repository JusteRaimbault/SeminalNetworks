@inproceedings{ ISI:000382257200096,
Author = {Saxena, Akrati and Malik, Vaibhav and Iyengar, S. R. S.},
Book-Group-Author = {{IEEE}},
Title = {{Estimating the Degree Centrality Ranking}},
Booktitle = {{2016 8TH INTERNATIONAL CONFERENCE ON COMMUNICATION SYSTEMS AND NETWORKS
   (COMSNETS)}},
Series = {{International Conference on Communication Systems and Networks}},
Year = {{2016}},
Note = {{8th International Conference on Communication Systems and Networks
   (COMSNETS), Bangalore, INDIA, JAN 05-10, 2016}},
Abstract = {{Complex networks have gained more attention from the last few years. The
   size of the real world complex networks, such as online social networks,
   WWW networks, collaboration networks, is exponentially increasing with
   time. It is not feasible to completely collect, store and process these
   networks. In the present work, we propose a method to estimate degree
   centrality ranking of a node without having complete structure of the
   graph. The proposed method uses degree of a node and power law exponent
   of the degree distribution to calculate the ranking. We also study
   simulation results on Barabasi-Albert model. Simulation results show
   that average error in the calculated ranking is approximately 5\% of
   total number of nodes.}},
