@article{ ISI:000362239900012,
Author = {Peterson, Nicholas R. and Pittel, Boris},
Title = {{DISTANCE BETWEEN TWO RANDOM k-OUT DIGRAPHS, WITH AND WITHOUT
   PREFERENTIAL ATTACHMENT}},
