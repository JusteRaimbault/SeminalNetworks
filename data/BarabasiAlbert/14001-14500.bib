@inproceedings{ ISI:000401921400248,
Author = {Ma, Chuan and Ding, Ming and Chen, He and Lin, Zihuai and Mao, Guoqiang
   and Li, Xu},
Book-Group-Author = {{IEEE}},
Title = {{Socially Aware Distributed Caching in Device-to-Device Communication
   Networks}},
Booktitle = {{2016 IEEE GLOBECOM WORKSHOPS (GC WKSHPS)}},
Series = {{IEEE Globecom Workshops}},
Year = {{2016}},
Note = {{IEEE-Communications-Society Global Communications Conference on Trusted
   Communications with Physical Layer Security (IEEE GLOBECOM), Washington,
   DC, DEC 04-08, 2016}},
Organization = {{IEEE Commun Soc; IEEE}},
Abstract = {{Content caching in user devices with device-to-device (D2D)
   communication capacities is becoming a promising technique to address
   the data traffic explosion problem in the next generation mobile
   networks. In this paper, we develop a socially aware distributed caching
   strategy based on a decentralized learning automaton, referred to as the
   Discrete Generalized Pursuit Algorithm (DGPA), to optimize the cache
   placement operation in D2D networks. Different from existing caching
   schemes, the proposed algorithm not only considers the file request
   probability and the closeness of devices as measured by their distance,
   but also takes into account the social relationship between D2D users.
   Furthermore, we characterize the mutual impact between the contents
   cached in different D2D users. Simulation results show that the proposed
   algorithm converges quickly and outperforms its counterparts using
   deterministic caching and random caching. Our work sheds new insights on
   the optimal design of D2D cache placement operations.}},
