@inproceedings{ ISI:000380503600061,
Author = {Huang, Minxue and Wang, Qiyuan and Xiao, Bangming and Wang, Dianwen},
Editor = {{Yang, Z}},
Title = {{Opinion Leadership Evolution in Consumer Advice Network - The Role of
   Expectancy Cues and Network Structure}},
Booktitle = {{PROCEEDINGS OF 2015 CHINA MARKETING INTERNATIONAL CONFERENCE: BIG DATA,
   CULTURAL DIFFERENCE AND MARKETING}},
