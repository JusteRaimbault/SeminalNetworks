@article{ ISI:000367402200009,
Author = {Lachgar, A. and Achahbar, A.},
Title = {{Network growth with preferential attachment and without ``rich get
   richer{''} mechanism}},
Journal = {{INTERNATIONAL JOURNAL OF MODERN PHYSICS C}},
Year = {{2016}},
Volume = {{27}},
Number = {{2}},
Month = {{FEB}},
Abstract = {{We propose a simple preferential attachment model of growing network
   using the complementary probability of Barabasi-Albert (BA) model, i.e.
   Pi(k(i)) proportional to 1 - k(i)/Sigma(kj)(j). In this network, new
   nodes are preferentially attached to not well connected nodes. Numerical
   simulations, in perfect agreement with the master equation solution,
   give an exponential degree distribution. This suggests that the power
   law degree distribution is a consequence of preferential attachment
   probability together with ``rich get richer{''} phenomena. We also
   calculate the average degree of a target node at time t(< k(s)(t)>) and
   its fluctuations, to have a better view of the microscopic evolution of
   the network, and we also compare the results with BA model.}},
