@article{ ISI:000353295500004,
Author = {Wu, Qingchu and Chen, Shufang},
Title = {{Threshold analysis for epidemic models with high-risk immunization on
   networks}},
Journal = {{INTERNATIONAL JOURNAL OF BIOMATHEMATICS}},
Year = {{2015}},
Volume = {{8}},
Number = {{3}},
Month = {{MAY}},
Abstract = {{In this paper, an SIRS epidemic model with high-risk immunization was
   investigated, where a susceptible neighbor of an infected node is
   immunized with rate h. Through analyzing the discrete-time model, we
   found that the epidemic threshold above which an epidemic can prevail
   and persist in a population is inversely proportional to 1 - h value. We
   also studied the continuous-time epidemic model and obtained a different
   result: the epidemic threshold does not depend on the immunization
   parameter h. Our results suggest that the difference between the
   discrete-time epidemic model and the continuous-time epidemic model
   exists in the high-risk immunization.}},
