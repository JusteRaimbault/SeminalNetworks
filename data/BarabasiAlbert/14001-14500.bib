@inproceedings{ ISI:000401906900121,
Author = {Salas, Julian},
Editor = {{Domeniconi, C and Gullo, F and Bonchi, F and DomingoFerrer, J and BaezaYates, R and Zhou, ZH and Wu, X}},
Title = {{Faster univariate microaggregation for power law distributions:
   k-degree-anonymity for big graphs}},
Booktitle = {{2016 IEEE 16TH INTERNATIONAL CONFERENCE ON DATA MINING WORKSHOPS (ICDMW)}},
Series = {{International Conference on Data Mining Workshops}},
