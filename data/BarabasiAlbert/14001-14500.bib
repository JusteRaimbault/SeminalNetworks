@article{ ISI:000352841600003,
Author = {Kavurucu, Yusuf},
Title = {{A comparative study on network motif discovery algorithms}},
Journal = {{INTERNATIONAL JOURNAL OF DATA MINING AND BIOINFORMATICS}},
Year = {{2015}},
Volume = {{11}},
Number = {{2}},
Pages = {{180-204}},
Abstract = {{Subgraphs that occur in complex networks with significantly higher
   frequency than those in randomised networks are called network motifs.
   Such subgraphs often play important roles in the functioning of those
   networks. Finding network motifs is a computationally challenging
   problem. The main difficulties arise from the fact that real networks
   are large and the size of the search space grows exponentially with
   increasing network and motif size. Numerous methods have been developed
   to overcome these challenges. This paper provides a comparative study of
   the key network motif discovery algorithms in the literature and
