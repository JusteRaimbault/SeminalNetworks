@article{ ISI:000369428000024,
Author = {Su Gui-Feng and Li Xiao-Wen and Zhang Xiao-Bing and Zhang Yi and Li Xue},
Title = {{Complete Condensation of Zero Range Process in Fitness Networks}},
Journal = {{COMMUNICATIONS IN THEORETICAL PHYSICS}},
Year = {{2015}},
Volume = {{64}},
Number = {{6}},
Pages = {{753-757}},
Month = {{DEC 1}},
Abstract = {{In current paper we study the so-called ``complete condensation{''} of
   zero range process on the fitness network. It is found that under the
   high temperature limit, the condensation behavior on the fitness model
   converges to that of the scale -free network, as expected. However, at
   some temperatures below the critical temprature of Bose Einstein
