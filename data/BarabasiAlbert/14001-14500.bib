@article{ ISI:000358525800001,
Author = {Sufrauj, Shamnaaz B. and Schiavo, Stefano and Riccaboni, Massimo},
Title = {{Big hits, export concentration and volatility}},
Journal = {{ECONOMIA POLITICA}},
Year = {{2015}},
Volume = {{32}},
Number = {{2}},
Pages = {{135-166}},
Month = {{AUG}},
Abstract = {{Recent empirical work has documented the high concentration of trade
   flows, and the large role played by few ``big hits{''} in each country's
   export. We propose a simple stochastic benchmark against which we assess
   each economy's actual number of ``big hits{''}. We show that most
   European countries underperform the benchmark, while China, the US and
   Germany do better. A low number of ``big hits{''} (relative to our
   prediction) is associated with higher export volatility. Looking at
   possible determinants of ``big hits{''}, we find they depend on the
   actual performance of each country, so that industrial policy needs to
   be country-specific.}},
