@inproceedings{ ISI:000386629900008,
Author = {Summers, Tyler and Shames, Iman},
Book-Group-Author = {{IEEE}},
Title = {{Convex relaxations and Gramian rank constraints for sensor and actuator
   selection in networks}},
Booktitle = {{2016 IEEE INTERNATIONAL SYMPOSIUM ON INTELLIGENT CONTROL (ISIC)}},
Series = {{IEEE International Symposium on Intelligent Control}},
Year = {{2016}},
Pages = {{341-346}},
Note = {{IEEE International Symposium on Intelligent Control (ISIC), Buenos
   Aires, ARGENTINA, SEP 19-22, 2016}},
Organization = {{IEEE}},
Abstract = {{We propose a convex relaxation heuristic for sensor and actuator
   selection problems in dynamical networks using Gramian metrics. We also
   propose heuristic algorithms to enforce a rank constraint on the Gramian
   that can be used in conjunction with combinatorial greedy algorithms and
   the convex relaxation. This allows selection of sensor or actuator sets
   that optimize an objective function while preserving a certain amount of
   observability or controllability throughout the state space, combining
   previous methods that focus exclusively on either rank or Gramian
   metrics. We illustrate and compare the greedy and convex relaxation
   heuristics in several numerical examples involving random and regular
   networks.}},
