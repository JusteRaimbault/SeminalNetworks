@inproceedings{ ISI:000393168500104,
Author = {Kalla, Anshuman and Sharma, Sudhir Kumar},
Editor = {{Astya, PN and Swaroop, A and Sharma, V and Singh, M}},
Title = {{A Constructive Review of In-Network Caching: A Core Functionality of ICN}},
Booktitle = {{2016 IEEE INTERNATIONAL CONFERENCE ON COMPUTING, COMMUNICATION AND
