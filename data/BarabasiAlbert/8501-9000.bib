@article{ ISI:000431987100013,
Author = {Tylianakis, Jason M. and Martinez-Garcia, Laura B. and Richardson, Sarah
   J. and Peltzer, Duane A. and Dickie, Ian A.},
Title = {{Symmetric assembly and disassembly processes in an ecological network}},
Journal = {{ECOLOGY LETTERS}},
