@article{ ISI:000246577900009,
Author = {Sen, Parongama},
Title = {{A novel approach for studying realistic navigations on networks}},
Journal = {{JOURNAL OF STATISTICAL MECHANICS-THEORY AND EXPERIMENT}},
Year = {{2007}},
Month = {{APR}},
Abstract = {{We consider navigation or search schemes on networks which are realistic
   in the sense that not all search chains can be completed. We show that
   the quantity mu = rho/s(d), where sd is the average dynamic shortest
   distance and. the success rate for completion of a search, is a
   consistent measure for the quality of a search strategy. Taking the
   example of realistic searches on scale free networks, we. nd that scales
   with the system size N as N-sigma, where sigma decreases as the
   searching strategy is improved. This measure is also shown to be
   sensitive to the distinguishing characteristics of networks. In this new
   approach, a dynamic small world (DSW) effect is said to exist when delta
   similar to 0. We show that such a DSW does indeed exist in social
   networks in which the linking probability is dependent on social
   distances.}},
