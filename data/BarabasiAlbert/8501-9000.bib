@article{ ISI:000227877600002,
Author = {Egghe, L},
Title = {{A characterization of the law of Lotka in terms of sampling}},
Journal = {{SCIENTOMETRICS}},
Year = {{2005}},
Volume = {{62}},
Number = {{3}},
Pages = {{321-328}},
Month = {{MAR}},
Abstract = {{An incomplete bibliography (or, more generally, an incomplete
   Information Production Process (IPP)) can be considered as a sample from
   a complete one. Sampling can be done in the sources or in the items. The
   simplest sampling technique is the systematic one where every k(th)
   source or k(th) item is taken (alternatively: deleted) (k is an element
   of N).
   In this paper we give a definition of systematic sampling in items and
   sources in the framework of an IPP in which we have continuous
   variables. We prove the theorem that in such IPPs we have a Lotkaian
   size-frequency function (i.e. a decreasing power function) if and only
   if systematic sampling in sources is the same as systematic sampling in
   items. In this proof we use the well-known characterization of power
   functions as scale-free functions.}},
