@article{ ISI:000249392600015,
Author = {Ying, Li and Zeng-Rong, Liu and Jian-Bao, Zhang},
Title = {{Dynamics of network motifs in genetic regulatory networks}},
Journal = {{CHINESE PHYSICS}},
Year = {{2007}},
Volume = {{16}},
Number = {{9}},
Pages = {{2587-2594}},
Month = {{SEP}},
Abstract = {{Network motifs hold a very important status in genetic regulatory
   networks. This paper aims to analyse the dynamical property of the
   network motifs in genetic regulatory networks. The main result we
   obtained is that the dynamical property of a single motif is very simple
   with only an asymptotically stable equilibrium point, but the
   combination of several motifs can make more complicated dynamical
   properties emerge such as limit cycles. The above-mentioned result shows
   that network motif is a stable substructure in genetic regulatory
   networks while their combinations make the genetic regulatory network
   more complicated.}},
