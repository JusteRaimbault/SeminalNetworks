@inproceedings{ ISI:000455787300094,
Author = {Dong, Yuxiao and Johnson, Reid A. and Xu, Jian and Chawla, Nitesh V.},
Book-Group-Author = {{ACM}},
Title = {{Structural Diversity and Homophily: A Study Across More Than One Hundred
   Big Networks}},
