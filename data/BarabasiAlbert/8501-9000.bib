@article{ ISI:000375233700007,
Author = {Zhang, Rui-xia and Jin, Zhen and Li, Shu-ping},
Title = {{Epidemic Spreading with Time Delay on Complex Networks}},
Journal = {{ACTA MATHEMATICAE APPLICATAE SINICA-ENGLISH SERIES}},
Year = {{2016}},
Volume = {{32}},
Number = {{2}},
Pages = {{319-326}},
Month = {{JUN}},
Abstract = {{In this paper, we propose a susceptible-infected-susceptible (SIS) model
   on complex networks, small-world (WS) networks and scale-free (SF)
   networks, to study the epidemic spreading behavior with time delay which
   is added into the infected phase. Considering the uniform delay, the
   basic reproduction number (R) over bar (0) on WS networks and (R) over
   bar (0) on SF networks are obtained respectively. On WS networks, if (R)
   over bar (0) <= 1, there is a disease-free equilibrium and it is locally
   asymptotically stable; if (R) over bar (0) > 1, there is an epidemic
   equilibrium and it is locally asymptotically stable. On SF networks, if
   (R) over bar (0) <= 1, there is a disease-free equilibrium; if (R) over
   bar (0) > 1, there is an epidemic equilibrium. Finally, we carry out
   simulations to verify the conclusions and analyze the effect of the time
   delay tau, the effective rate lambda, average connectivity < k > and the
   minimum connectivity m on the epidemic spreading.}},
