@article{ ISI:000399572300001,
Author = {Hernandez-Urbina, Victor and Herrmann, J. Michael},
Title = {{Neuronal avalanches in complex networks}},
Journal = {{COGENT PHYSICS}},
Year = {{2016}},
Volume = {{3}},
Pages = {{1-13}},
Abstract = {{Brain networks are neither regular nor random. Their structure allows
   for optimal information processing and transmission across the entire
   neural substrate of an organism. However, for topological features to be
   appropriately harnessed, brain networks should implement a dynamical
   regime which prevents phase-locked and chaotic behaviour. Critical
   neural dynamics refer to a dynamical regime in which the system is
   poised at the boundary between regularity and randomness. It has been
   reported that neural systems poised at this boundary achieve maximum
   computational power. In this paper, we review recent results regarding
   critical neural dynamics that emerge from systems whose underlying
   structure exhibits complex network properties.}},
