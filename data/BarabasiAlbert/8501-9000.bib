Author = {Joos, Felix and Perarnau, Guillem and Rautenbach, Dieter and Reed, Bruce},
Book-Group-Author = {{IEEE}},
Title = {{How to determine if a random graph with a fixed degree sequence has a
   giant component}},
Booktitle = {{2016 IEEE 57TH ANNUAL SYMPOSIUM ON FOUNDATIONS OF COMPUTER SCIENCE
   (FOCS)}},
