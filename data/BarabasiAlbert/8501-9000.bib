@article{ ISI:000233176500002,
Author = {Ying, L and Yang, L and Shan, XM and Ren, Y and Jiao, J and Qiu, B},
Title = {{Dynamic properties of epidemic spreading on finite size complex networks}},
Journal = {{CHINESE PHYSICS}},
Year = {{2005}},
Volume = {{14}},
Number = {{11}},
Pages = {{2153-2157}},
Month = {{NOV}},
Abstract = {{The Internet presents a complex topological structure, on which computer
   viruses can easily spread. By using theoretical analysis and computer
   simulation methods, the dynamic process of disease spreading on finite
   size networks with complex topological structure is investigated. On the
   finite size networks, the spreading process of SIS
   (susceptible-infected-susceptible) model is a finite Markov chain with
   an absorbing state. Two parameters, the survival probability and the
   conditional infecting probability, are introduced to describe the
   dynamic properties of disease spreading on finite size networks. Our
   results can help understanding computer virus epidemics and other
   spreading phenomena on communication and social networks. Also,
   knowledge about the dynamic character of virus spreading is helpful for
   adopting immunity policy.}},
