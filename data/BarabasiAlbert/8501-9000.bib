@inproceedings{ ISI:000253367700021,
Author = {Dwyer, Tim and Marriott, Kim},
Editor = {{Hong, SH and Nishizeki, T and Quan, W}},
Title = {{Constrained stress majorization using diagonally scaled gradient
   projection}},
Booktitle = {{GRAPH DRAWING}},
Series = {{LECTURE NOTES IN COMPUTER SCIENCE}},
Year = {{2008}},
Volume = {{4875}},
Pages = {{219-230}},
Note = {{15th International Symposium on Graph Drawing, Sydney, AUSTRALIA, SEP
   24-26, 2007}},
Organization = {{Univ Sydney; Tom Sawyer; ILOG; Natl ICT Australia Ltd; Australian Govt,
   Dept Defence; CSIRO; y Works}},
Abstract = {{Constrained stress majorization is a promising new technique for
   integrating application specific layout constraints into force-directed
   graph layout. We significantly improve the speed and convergence
   properties of the constrained stress-majorization technique for graph
   layout by employing a diagonal scaling of the stress function. Diagonal
   scaling requires the active-set quadratic programming solver used in the
   projection step to be extended to handle separation constraints with
   scaled variables, i.e. of the form s(i)y(i) + g(ij) <= s(j)y(j). The
   changes, although relatively small, are quite subtle and explained in
   detail.}},
