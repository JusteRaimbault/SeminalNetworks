@inproceedings{ ISI:000428586800022,
Author = {Chen, Zesheng and Taylor, Kurtis},
Editor = {{Musaev, A and Ferreira, JE and Higashino, T}},
Title = {{Modeling the Spread of Influence for Independent Cascade Diffusion
   Process in Social Networks}},
Booktitle = {{2017 IEEE 37TH INTERNATIONAL CONFERENCE ON DISTRIBUTED COMPUTING SYSTEMS
   WORKSHOPS (ICDCSW)}},
Series = {{IEEE International Conference on Distributed Computing Systems Workshops}},
