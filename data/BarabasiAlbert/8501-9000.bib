@inproceedings{ ISI:000265327500012,
Author = {Schwind, Michael and Wegmann, Christian},
Book-Group-Author = {{IEEE COMPUTER SOC}},
Title = {{SVNNAT: Measuring Collaboration in Software Development Networks}},
Booktitle = {{IEEE JOINT CONFERENCE ON E-COMMERCE TECHNOLOGY (CEC'08) AND ENTERPRISE
   COMPUTING, E-COMMERCE AND E-SERVICES (EEE'08)}},
