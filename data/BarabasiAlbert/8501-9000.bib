@inproceedings{ ISI:000378833300017,
Author = {Javarone, Marco A. and Interdonato, Roberto and Tagarelli, Andrea},
Editor = {{Cherifi, H and Goncalves, B and Menezes, R and Sinatra, R}},
Title = {{Modeling Evolutionary Dynamics of Lurking in Social Networks}},
