@article{ ISI:000260736300023,
Author = {Zhu, Xiao-Long and Sang, Jian-Ping and Wang, Li-Long and Huang,
   Sheng-You and Zou, Xian-Wu},
Title = {{Structure properties and synchronizability of cobweb-like networks}},
