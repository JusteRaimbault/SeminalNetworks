@article{ ISI:000317350800003,
Author = {Giatsidis, Christos and Thilikos, Dimitrios M. and Vazirgiannis,
   Michalis},
Title = {{D-cores: measuring collaboration of directed graphs based on degeneracy}},
Journal = {{KNOWLEDGE AND INFORMATION SYSTEMS}},
Year = {{2013}},
Volume = {{35}},
Number = {{2}},
Pages = {{311-343}},
Month = {{MAY}},
Abstract = {{Community detection and evaluation is an important task in graph mining.
   In many cases, a community is defined as a subgraph characterized by
   dense connections or interactions between its nodes. A variety of
   measures are proposed to evaluate different quality aspects of such
   communities-in most cases ignoring the directed nature of edges. In this
   paper, we introduce novel metrics for evaluating the collaborative
   nature of directed graphs-a property not captured by the single node
   metrics or by other established community evaluation metrics. In order
   to accomplish this objective, we capitalize on the concept of graph
   degeneracy and define a novel D-core framework, extending the classic
   graph-theoretic notion of -cores for undirected graphs to directed ones.
   Based on the D-core, which essentially can be seen as a measure of the
   robustness of a community under degeneracy, we devise a wealth of novel
   metrics used to evaluate graph collaboration features of directed
   graphs. We applied the D-core approach on large synthetic and real-world
   graphs such as Wikipedia, DBLP, and ArXiv and report interesting results
   at the graph as well at the node level.}},
