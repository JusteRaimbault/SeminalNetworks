@article{ ISI:000265350900017,
Author = {Santiago, A. and Benito, R. M.},
Title = {{Robustness of heterogeneous complex networks}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2009}},
Volume = {{388}},
Number = {{11}},
Pages = {{2234-2242}},
Month = {{JUN 1}},
Abstract = {{In this paper we study the robustness of heterogeneous preferential
   attachment networks. The robustness of a network measures its structural
   tolerance to the random removal of nodes and links. We numerically
   analyze the influence of the affinity parameters on a set of
   ensemble-averaged robustness metrics. We show that the presence of
   heterogeneity does not fundamentally alter the smooth nature of the
   fragmentation process of the models. We also show that a moderate level
   of locality translates into slight improvements in the robustness
   metrics, which prompts us to conjecture an evolutionary argument for the
   existence of real networks with power-law scaling in their connectivity
   and Clustering distributions. (C) 2009 Elsevier B.V. All rights
   reserved.}},
