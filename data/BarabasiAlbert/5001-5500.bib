@article{ ISI:000386698000001,
Author = {Velarde, Manuel G. and Chetverikov, Alexander P. and Ebeling, Werner and
   Dmitriev, Sergey V. and Lakhno, Victor D.},
Title = {{From solitons to discrete breathers}},
Journal = {{EUROPEAN PHYSICAL JOURNAL B}},
Year = {{2016}},
Volume = {{89}},
Number = {{10}},
Pages = {{1-13}},
Month = {{OCT 24}},
Abstract = {{The excitation of solitons and discrete breathers (pinned or otherwise,
   also known as intrinsic localized modes, DB/ILM) in a one-dimensional
   lattice, also denoted as a chain, is considered when both on-site and
   inter-site vibrations, coupled together, are governed by the empirical
   Morse interaction. We focus attention on the transformation of the
   former into the latter as the relative strength of the on-site potential
   to that of the inter-site potential is increased.}},
Publisher = {{SPRINGER}},
Address = {{233 SPRING ST, NEW YORK, NY 10013 USA}},
