@inproceedings{ ISI:000185214000003,
Author = {Barabasi, AL and Ravasz, E and Oltvai, Z},
Editor = {{PastorSatorras, R and Rubi, M and DiazGuilera, A}},
Title = {{Hierarchical organization of modularity in complex networks}},
Booktitle = {{STATISTICAL MECHANICS OF COMPLEX NETWORKS}},
