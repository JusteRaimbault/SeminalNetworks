@article{ ISI:000300258200085,
Author = {Zhao Lin and Zhou Xin and Liang Zhi and Wu Jia-Rui},
Title = {{Integrating the environmental factor into the strategy updating rule to
   promote cooperation in evolutionary games}},
Journal = {{CHINESE PHYSICS B}},
Year = {{2012}},
Volume = {{21}},
Number = {{1}},
Month = {{JAN}},
Abstract = {{Many previous studies have shown that the environment plays an important
   role for social individuals. In this paper, we integrate the
   environmental factor, which is defined as the average payoff of all a
   player's neighbours, with the standard Fermi updating rule by
   introducing a tunable parameter, omega. It is found that the level of
   cooperation increases remarkably, and that the cooperators can better
   resist the invasion of defection with an increase in omega. This
   interesting phenomenon is then explained from a microscopic view. In
   addition, the universality of this mechanism is also proved with the
   help of the small-world network and the random regular graph. This work
   may be helpful in understanding cooperation behaviour in species from
   unicellular organisms up to human beings.}},
