@article{ ISI:000184510600061,
Author = {Kim, JH and Goh, KI and Kahng, B and Kim, D},
Title = {{Probabilistic prediction in scale-free networks: Diameter changes}},
Journal = {{PHYSICAL REVIEW LETTERS}},
Year = {{2003}},
Volume = {{91}},
Number = {{5}},
Month = {{AUG 1}},
Abstract = {{In complex systems, responses to small perturbations are too diverse to
   definitely predict how much they would be, and then such diverse
   responses can be predicted in a probabilistic way. Here we study such a
   problem in scale-free networks, for example, the diameter changes by the
   deletion of a single vertex for various in silico and real-world
   scale-free networks. We find that the diameter changes are indeed
   diverse and their distribution exhibits an algebraic decay with an
   exponent zeta asymptotically. Interestingly, the exponent zeta is robust
   as zetasimilar or equal to2.2(1) for most scale-free networks and
   insensitive to the degree exponents gamma as long as 2<gammaless than or
   equal to3. However, there is another type with zetasimilar or equal
   to1.7(1) and its examples include the Internet and its related in silico
   model.}},
