Language = {{English}},
Affiliation = {{Chau, HF (Reprint Author), Univ Hong Kong, Dept Phys \& Ctr Theoret \& Computat Phys, Pokfulam Rd, Hong Kong, Hong Kong, Peoples R China.
   Chau, H. F.; Wong, C. Y.; Chow, F. K.; Fung, Chi-Hang Fred, Univ Hong Kong, Dept Phys \& Ctr Theoret \& Computat Phys, Hong Kong, Hong Kong, Peoples R China.}},
DOI = {{10.1016/j.physa.2014.07.082}},
ISSN = {{0378-4371}},
