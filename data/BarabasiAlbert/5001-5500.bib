@article{ ISI:000245061700006,
Author = {Liu, Jing and He, Keqing and Peng, Rong and Ma, Yutao},
Title = {{A study on the weight and topology corrlation of object oriented
   software coupling network}},
Journal = {{DYNAMICS OF CONTINUOUS DISCRETE AND IMPULSIVE SYSTEMS-SERIES
   B-APPLICATIONS \& ALGORITHMS}},
Year = {{2006}},
Volume = {{13}},
Number = {{3, S}},
Pages = {{955-959}},
Month = {{DEC}},
Note = {{International Conference on Complex Systems and Applications, Huhhot,
   PEOPLES R CHINA, JUN 16-18, 2006}},
Abstract = {{Software network has been reported by several previous research works to
   demonstrate both small world and scale free characteristics. However the
   property of individual nodes in software network has always been
   ignored. This incomplete characterization of software impedes the
   understanding of the software developing process. To overcome that, we
   introduce software metrics into the software network. The previous
   neglected relationships between classes are also considered. Thus a more
   thorough view of software is formed, namely, the OO software coupling
   network. Results from the real world Object Oriented(OO) software
   reveals that in addition to the power law distribution of the (In/Out)
   degree distribution, the weights of nodes also display power law
   distribution. Furthermore, there is a clear correlation between the
   weight and the out degree. These discoveries will help uncover the
   underlying mechanisms of software evolution.}},
