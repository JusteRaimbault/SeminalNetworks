@article{ ISI:000428749000007,
Author = {Liu, Chengliang and Wang, Jiaqi and Zhang, Hong},
Title = {{Spatial heterogeneity of ports in the global maritime network detected
   by weighted ego network analysis}},
Journal = {{MARITIME POLICY \& MANAGEMENT}},
