@article{ ISI:000431395700002,
Author = {Fosdick, Bailey K. and Larremore, Daniel B. and Nishimura, Joel and
   Ugander, Johan},
Title = {{Configuring Random Graph Models with Fixed Degree Sequences}},
Journal = {{SIAM REVIEW}},
Year = {{2018}},
Volume = {{60}},
