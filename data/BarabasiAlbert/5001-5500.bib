@inproceedings{ ISI:000231441001034,
Author = {Zhong, M and Shen, K and Seiferas, J},
Editor = {{Makki, K and Knightly, E}},
Title = {{Non-uniform random membership management in peer-to-peer networks}},
Booktitle = {{IEEE INFOCOM 2005: THE CONFERENCE ON COMPUTER COMMUNICATIONS, VOLS 1-4,
   PROCEEDINGS}},
Series = {{IEEE INFOCOM}},
Year = {{2005}},
Pages = {{1151-1161}},
Note = {{24th Annual Joint Conference of the IEEE Computer and Communications
   Societies, Miami, FL, MAR 13-17, 2005}},
Organization = {{IEEE Comp Soc; IEEE Commun Soc}},
Abstract = {{Existing random membership management algorithms provide each node with
   a small, uniformly random subset of global participants. However, many
   applications would benefit more from non-uniform random member subsets.
   For instance, non-uniform gossip algorithms can provide distance-based
   propagation bounds and thus information can reach nearby nodes sooner.
   In another example, Kleinberg shows that networks with random long-links
   following distance-based non-uniform distributions exhibit better
   routing performance than those with uniformly randomized topologies.
   In this paper, we propose a scalable non-uniform random membership
   management algorithm, which provides each node with a random membership
   subset with application-specified probability distributions g., with
   probability inversely proportional to distances. Our algorithm is the
   first non-uniform random membership management algorithm with proved
   convergence and bounded convergence time. Moreover, our algorithm does
   not put specific restrictions on the network topologies and thus has
   wide applicability.}},
