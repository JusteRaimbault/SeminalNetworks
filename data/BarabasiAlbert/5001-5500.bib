@article{ ISI:000325842000030,
Author = {Zhang, Yan},
Title = {{The impact of other-regarding tendencies on the spatial vaccination game}},
Journal = {{CHAOS SOLITONS \& FRACTALS}},
Year = {{2013}},
Volume = {{56}},
Number = {{SI}},
Pages = {{209-215}},
Month = {{NOV}},
Abstract = {{Using the evolutionary game framework to describe the vaccination
   behaviors of individuals, this work focuses on the impact of
   other-regarding tendencies on collective decision-making of vaccination.
   With extensive computational simulations, we find that when the
   vaccination cost is small or moderate, the variance of vaccination
   coverage as well as epidemic size is monotonically depending on the
   other-regarding effect. However, with a high price of vaccine, a
   moderate deviation from being entirely self-centered results in a poor
   communal vaccination benefit. In addition, we also find that the
   other-regarding effect may lead to an optimal total social cost. (C)
   2013 Elsevier Ltd. All rights reserved.}},
