@article{ ISI:000247624000042,
Author = {Kwak, Wooseop and Yang, Jae-Suk and Sohn, Jang-il and Kim, In-mook},
Title = {{Critical behavior of the XY model on growing scale-free networks}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2007}},
Volume = {{75}},
Number = {{6, 1}},
Month = {{JUN}},
Abstract = {{Applying the histogram reweighting method, we investigate the critical
   behavior of the XY model on growing scale-free networks with various
   degree exponents lambda. For lambda <= 3, the critical temperature
   diverges as it does for the Ising model on scale-free networks. For
   lambda = 8, on the other hand, we observe a second-order phase
   transition at finite temperature. We obtain the critical temperature
   T-c=3.08(2) and the critical exponents (nu) over tilde =2.62(3),
   gamma/(nu) over tilde =0.127(4), and beta/(nu) over tilde =0.442(2) from
   a finite-size scaling analysis.}},
