@article{ ISI:000406988000001,
Author = {Shang, Ronghua and Liu, Huan and Jiao, Licheng},
Title = {{Multi-objective clustering technique based on k-nodes update policy and
   similarity matrix for mining communities in social networks}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
