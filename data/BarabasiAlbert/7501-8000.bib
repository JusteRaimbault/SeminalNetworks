@article{ ISI:000251326200025,
Author = {Valverde, Sergi and Sole, Ricard V. and Bedau, Mark A. and Packard,
   Norman},
Title = {{Topology and evolution of technology innovation networks}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2007}},
Volume = {{76}},
Number = {{5, 2}},
Month = {{NOV}},
Abstract = {{The web of relations linking technological innovation can be fairly
   described in terms of patent citations. The resulting patent citation
   network provides a picture of the large-scale organization of
   innovations and its time evolution. Here we study the patterns of change
   of patents registered by the U.S. Patent and Trademark Office. We show
   that the scaling behavior exhibited by this network is consistent with a
   preferential attachment mechanism together with a Weibull-shaped aging
   term. Such an attachment kernel is shared by scientific citation
   networks, thus indicating a universal type of mechanism linking ideas
   and designs and their evolution. The implications for evolutionary
   theory of innovation are discussed.}},
