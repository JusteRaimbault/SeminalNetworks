@article{ ISI:000462364200003,
Author = {Yang, Lu-Xing and Li, Pengdeng and Zhang, Yushu and Yang, Xiaofan and
   Xiang, Yong and Zhou, Wanlei},
Title = {{Effective Repair Strategy Against Advanced Persistent Threat: A
   Differential Game Approach}},
