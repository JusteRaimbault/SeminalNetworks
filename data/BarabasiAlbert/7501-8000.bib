@article{ ISI:000303168500007,
Author = {Zhao, Kang and Yen, John and Ngamassi, Louis-Marie and Maitland, Carleen
   and Tapia, Andrea H.},
Title = {{Simulating inter-organizational collaboration network: a
   multi-relational and event-based approach}},
Journal = {{SIMULATION-TRANSACTIONS OF THE SOCIETY FOR MODELING AND SIMULATION
   INTERNATIONAL}},
Year = {{2012}},
Volume = {{88}},
Number = {{5}},
Pages = {{617-633}},
Month = {{MAY}},
Abstract = {{In this research, we study inter-organizational collaboration from the
   perspective of multi-relational networks. We develop an agent-based
   model to simulate how a collaboration network among organizations
   emerges from organizations' interactions through another network: the
   inter-organizational communication network. Our model adds links (or
   edges) into the collaboration network on the basis of events, which
   correspond to organizations' formation of collaborative teams for joint
   projects. The proposed approach also models the competitive yet
   non-exclusive dissemination of information among organizations,
   organizations' dynamic prioritization of candidate projects, and
   network-based influence. Applying the model to a case study of the
   humanitarian sector, we configure and validate the agent-based
   simulation, and use it to analyze how to promote inter-organizational
   humanitarian collaboration by encouraging communication. The simulation
   results suggest that encouraging communication between peripheral
   organizations can better promote collaboration than other strategies.}},
