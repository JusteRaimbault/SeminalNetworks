@article{ ISI:000345204200017,
Author = {Aragao Rego, Henio Henrique and Braunstein, Lidia A. and D'Agostino,
   Gregorio and Stanley, H. Eugene and Miyazima, Sasuke},
Title = {{When a Text Is Translated Does the Complexity of Its Vocabulary Change?
   Translations and Target Readerships}},
Journal = {{PLOS ONE}},
Year = {{2014}},
Volume = {{9}},
Number = {{10}},
Month = {{OCT 29}},
Abstract = {{In linguistic studies, the academic level of the vocabulary in a text
   can be described in terms of statistical physics by using a
   ``temperature'' concept related to the text's word-frequency
   distribution. We propose a ``comparative thermo-linguistic'' technique
   to analyze the vocabulary of a text to determine its academic level and
   its target readership in any given language. We apply this technique to
   a large number of books by several authors and examine how the
   vocabulary of a text changes when it is translated from one language to
   another. Unlike the uniform results produced using the Zipf law, using
   our ``word energy'' distribution technique we find variations in the
   power-law behavior. We also examine some common features that span
   across languages and identify some intriguing questions concerning how
   to determine when a text is suitable for its intended readership.}},
