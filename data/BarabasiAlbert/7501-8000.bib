@inproceedings{ ISI:000389226900021,
Author = {Hartung, Sepp and Komusiewicz, Christian and Nichterlein, Andre},
Editor = {{Boas, PV and Groen, FCA and Italiano, GF and Nawrocki, J and Sack, H}},
Title = {{On Structural Parameterizations for the 2-Club Problem}},
