@article{ ISI:000444827200019,
Author = {Tang, Changbing and Li, Ang and Li, Xiang},
Title = {{Asymmetric Game: A Silver Bullet to Weighted Vertex Cover of Networks}},
Journal = {{IEEE TRANSACTIONS ON CYBERNETICS}},
