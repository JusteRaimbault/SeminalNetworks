@article{ ISI:000443286300030,
Author = {Yang, Junyuan and Xu, Fei},
Title = {{Global stability of two SIS epidemic mean-field models on complex
   networks: Lyapunov functional approach}},
Journal = {{JOURNAL OF THE FRANKLIN INSTITUTE-ENGINEERING AND APPLIED MATHEMATICS}},
Year = {{2018}},
Volume = {{355}},
Number = {{14}},
Pages = {{6763-6779}},
Month = {{SEP}},
Abstract = {{The global stability of the endemic equilibrium is of particular
   interest in characterizing the behaviours of epidemic models on complex
   networks. Due to the specific structures of SIS models, rigorous proof
   of the global property for systems has been challenging. In this paper,
   we propose a general method to calculate the basic reproduction number
   R-0 of some mean-field epidemic models on complex networks by the
   renewal process. This method can be generalized to estimate the basic
   reproduction number R-0 of edge-based compartmental model and pairwise
   model on complex networks. Furthermore, we show an overall results for
   the globally asymptotical stability of the endemic equilibrium compared
   with constructing monotone sequences in existing literatures. (C) 2018
   The Franklin Institute. Published by Elsevier Ltd. All rights reserved.}},
