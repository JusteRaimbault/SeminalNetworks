@article{ ISI:000357704500043,
Author = {Xie, Zheng and Zhu, Jiang and Kong, Dexing and Li, Jianping},
Title = {{A random geometric graph built on a time-varying Riemannian manifold}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2015}},
Volume = {{436}},
Pages = {{492-498}},
Month = {{OCT 15}},
Abstract = {{The theory of random geometric graph enables the study of complex
   networks through geometry. To analyze evolutionary networks,
   time-varying geometries are needed. Solutions of the generalized
   hyperbolic geometric flow are such geometries. Here we propose a
   scale-free network model, which is a random geometric graph on a two
   dimensional disc. The metric of the disc is a Ricci flat solution of the
   flow. The model is used to physically simulate the growth and
   aggregation of a type of cancer cell. (C) 2015 Elsevier B.V. All rights
   reserved.}},
