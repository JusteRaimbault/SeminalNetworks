Title = {{Spin glass behavior of the antiferromagnetic Heisenberg model on scale
   free network}},
Booktitle = {{XXVI IUPAP CONFERENCE ON COMPUTATIONAL PHYSICS (CCP2014)}},
Series = {{Journal of Physics Conference Series}},
