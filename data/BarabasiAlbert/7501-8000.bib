@article{ ISI:000332032800004,
Author = {Li, Long and Sun, Weigang and Wang, Guixiang and Xu, Guanghui},
Title = {{Mean first-passage time on a family of small-world treelike networks}},
Journal = {{INTERNATIONAL JOURNAL OF MODERN PHYSICS C}},
Year = {{2014}},
Volume = {{25}},
Number = {{3}},
Month = {{MAR}},
Abstract = {{In this paper, we obtain exact scalings of mean first-passage time
   (MFPT) of random walks on a family of small-world treelike networks
   formed by two parameters, which includes three kinds. First, we
   determine the MFPT for a trapping problem with an immobile trap located
   at the initial node, which is defined as the average of the
   first-passage times (FPTs) to the trap node over all possible starting
   nodes, and it scales linearly with network size N in large networks. We
   then analytically obtain the partial MFPT (PMFPT) which is the mean of
   FPTs from the trap node to all other nodes and show that it increases
   with N as N ln N. Finally we establish the global MFPT (GMFPT), which is
   the average of FPTs over all pairs of nodes. It also grows with N as N
   ln N in the large limit of N. For these three kinds of random walks, we
   all obtain the analytical expressions of the MFPT and they all increase
   with network parameters. In addition, our method for calculating the
   MFPT is based on the self-similar structure of the considered networks
   and avoids the calculations of the Laplacian spectra.}},
