@inproceedings{ ISI:000252834500004,
Author = {Flaxman, Abraham D. and Frieze, Alan M. and Vera, Juan},
Editor = {{Bonato, A and Chung, FRK}},
Title = {{A geometric preferential attachment model of networks II}},
