@inproceedings{ ISI:000257947900004,
Author = {Gunes, Mehmet Hadi and Sarac, Kamil},
Book-Group-Author = {{IEEE}},
Title = {{Importance of IP alias resolution in sampling Internet topologies}},
Booktitle = {{2007 IEEE GLOBAL INTERNET SYMPOSIUM}},
Year = {{2007}},
Pages = {{19-24}},
Note = {{IEEE Global Internet Symposium, Anchorage, AK, MAY   11, 2007}},
Organization = {{IEEE}},
Abstract = {{Internet measurement studies utilize traceroute-based path traces to
   build representative Internet maps. These maps are then used to analyze
   various topological characteristics of the Internet. IP alias resolution
   is an important step in building a map from a set of collected path
   traces. In this paper, we study the impact of incomplete IP alias
   resolution on Internet measurement studies. Using a set of synthetic
   topologies and a genuine topology map, we experimentally show that the
   accuracy/completeness of alias resolution has an important effect on the
   observed topological characteristics. The results obtained in this work
   point out the importance of IP alias resolution and call for further
   research in alias resolution.}},
