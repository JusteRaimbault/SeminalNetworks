@article{ ISI:000296399300008,
Author = {Liu Yuchao and Zhang Haisu and Ma Yutao and Li Deyi and Chen Guisheng},
Title = {{Collective Intelligence and Uncertain Knowledge Representation in Cloud
   Computing}},
