@article{ ISI:000360065300003,
Author = {Fernandes, F. P. and de Albuquerque, Douglas F. and Lima, F. W. S. and
   Plascak, J. A.},
Title = {{Critical behavior of the spin-1 Blume-Capel model on two-dimensional
   Voronoi-Delaunay random lattices}},
Journal = {{PHYSICAL REVIEW E}},
