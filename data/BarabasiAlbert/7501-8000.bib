@article{ ISI:000271631200025,
Author = {Zheng Jian-Feng and Gao Zi-You and Fu Bai-Bai and Li Feng},
Title = {{Cascading failures in congested complex networks with feedback}},
Journal = {{CHINESE PHYSICS B}},
Year = {{2009}},
Volume = {{18}},
Number = {{11}},
Pages = {{4754-4759}},
Month = {{NOV}},
Abstract = {{In this article, we investigate cascading failures in complex networks
   by introducing a feedback. To characterize the effect of the feedback,
   we define a procedure that involves a self-organization of trip
   distribution during the process of cascading failures. For this purpose,
   user equilibrium with variable demand is used as an alternative way to
   determine the traffic flow pattern throughout the network. Under the
   attack, cost function dynamics are introduced to discuss edge overload
   in complex networks, where each edge is assigned a finite capacity
   (controlled by parameter alpha). We find that scale-free networks
   without considering the effect of the feedback are expected to be very
   sensitive to alpha as compared with random networks, while this
   situation is largely improved after introducing the feedback}},
