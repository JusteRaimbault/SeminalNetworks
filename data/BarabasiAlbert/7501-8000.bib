@article{ ISI:000308020800018,
Author = {Buscarino, Arturo and Frasca, Mattia and Fortuna, Luigi and Fiore,
   Angelo Sarra},
Title = {{A New Model for Growing Social Networks}},
Journal = {{IEEE SYSTEMS JOURNAL}},
Year = {{2012}},
Volume = {{6}},
Number = {{3}},
Pages = {{531-538}},
Month = {{SEP}},
Abstract = {{In this paper, a new model of real social networks is introduced. The
   main characteristic of the model is an attachment mechanism based on the
   existence of network communities, a key feature of real social networks.
   Using numerical simulations, we show how this model shares with these
   networks several peculiarities (such as high levels of clustering, low
   values of the characteristic path length and node betweenness, division
   into communities and heterogeneous degree distribution), not captured in
   other network models.}},
Publisher = {{IEEE-INST ELECTRICAL ELECTRONICS ENGINEERS INC}},
Address = {{445 HOES LANE, PISCATAWAY, NJ 08855-4141 USA}},
