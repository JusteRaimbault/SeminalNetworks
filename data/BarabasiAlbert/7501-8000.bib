@article{ ISI:000250199600010,
Author = {Wang, Lei and Dai, Hua-Ping and Sun, You-Xian},
Title = {{General random pseudofractal networks}},
Journal = {{JOURNAL OF PHYSICS A-MATHEMATICAL AND THEORETICAL}},
Year = {{2007}},
Volume = {{40}},
Number = {{44}},
Pages = {{13279-13289}},
Month = {{NOV 2}},
Abstract = {{We introduce a general random pseudofractal network model by assigning
   fitness to each edge. In this model, continuous growth and attachment,
   determined by their fitness of already existing edges, are the two
   ingredients. We obtain the analytical results that our model exhibits a
   power-law degree distribution with exponent. = 2 + m( 1 + alpha m)(-1),
   where m and a are tunable parameters. We also show that a general random
   pseudofractal network has a large clustering coefficient and a small
   average distance leading to a small-world effect. These theoretical
   results agree well with numerical simulations.}},
