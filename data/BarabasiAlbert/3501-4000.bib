@article{ ISI:000235660300018,
Author = {Noh, JD and Kim, SW},
Title = {{Random-walk and pair-annihilation processes on scale-free networks}},
Journal = {{JOURNAL OF THE KOREAN PHYSICAL SOCIETY}},
Year = {{2006}},
Volume = {{48}},
Number = {{2}},
