Title = {{Spectral Measure of Robustness for Internet Topology}},
Booktitle = {{PROCEEDINGS OF 2010 3RD IEEE INTERNATIONAL CONFERENCE ON COMPUTER
   SCIENCE AND INFORMATION TECHNOLOGY (ICCSIT 2010), VOL 6}},
Series = {{International Conference on Computer Science and Information Technology}},
