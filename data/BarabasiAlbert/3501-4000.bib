@article{ ISI:000241076600006,
Author = {Cattuto, C. and Loreto, V. and Servedio, V. D. P.},
Title = {{A Yule-Simon process with memory}},
Journal = {{EUROPHYSICS LETTERS}},
Year = {{2006}},
Volume = {{76}},
Number = {{2}},
Pages = {{208-214}},
Month = {{OCT}},
Abstract = {{The Yule-Simon model has been used as a tool to describe the growth of
   diverse systems, acquiring a paradigmatic character in many fields of
   research. Here we study a modified Yule-Simon model that takes into
   account the full history of the system by means of a hyperbolic memory
   kernel. We show how the memory kernel changes the properties of
   preferential attachment and provide an approximate analytical solution
   for the frequency distribution density as well as for the frequency-rank
   distribution.}},
