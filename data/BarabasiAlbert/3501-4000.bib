@article{ ISI:000300325800016,
Author = {Yi, Sangyoon and Choi, Jinho},
Title = {{The organization of scientific knowledge: the structural characteristics
   of keyword networks}},
Journal = {{SCIENTOMETRICS}},
Year = {{2012}},
Volume = {{90}},
Number = {{3}},
Pages = {{1015-1026}},
Month = {{MAR}},
Abstract = {{The understanding of scientific knowledge itself may promote further
   advances in science and research on the organization of knowledge may be
   an initiative to this effort. This stream of research, however, has been
   mainly driven by the analysis of citation networks. This study uses, as
   an alternative knowledge element, information on the keywords of papers
   published in business research and examines how they are associated with
   each other to constitute a body of scientific knowledge. The results
   show that, unlike most citation networks, keyword networks are not
   small-word networks but, rather, locally clustered scale-free networks
   with a hierarchic structure. These structural patterns are robust
   against the scope of scientific fields involved. In addition, this paper
   discusses the origins and implications of the identified structural
   characteristics of keyword networks.}},
