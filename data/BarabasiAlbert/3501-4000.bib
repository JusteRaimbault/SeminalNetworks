@article{ ISI:000227019000017,
Author = {Nacher, JC and Yamada, T and Goto, S and Kanehisa, M and Akutsu, T},
Title = {{Two complementary representations of a scale-free network}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2005}},
Volume = {{349}},
Number = {{1-2}},
Pages = {{349-363}},
Month = {{APR 1}},
Abstract = {{Several studies on real complex networks from different fields as
   biology, economy, or sociology have shown that the degree of nodes
   (number of edges connected to each node) follows a scale-free power-law
   distribution like P(k) approximate to k(-gamma), where P(k) denotes the
   frequency of the nodes that are connected to k other nodes. Here we have
   carried out a study on scale-free networks, where a line graph
   transformation (i.e., edges in an initial network are transformed into
   nodes) is applied on a power-law distribution. Our results indicate that
   a power-law distribution as P(k) approximate to k(-gamma+1) is found for
   the transformed network together with a peak for low-degree nodes. In
   the present work, we show a parametrization of this behaviour and
   discuss its application on real networks as metabolic networks,
   protein-protein interaction network and World Wide Web. (C) 2004
   Elsevier B.V. All rights reserved.}},
