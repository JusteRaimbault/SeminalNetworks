@article{ ISI:000416032100011,
Author = {de Souza, Heitor S. P. and Fiocchi, Claudio and Iliopoulos, Dimitrios},
Title = {{The IBD interactome: an integrated view of aetiology, pathogenesis and
   therapy}},
Journal = {{NATURE REVIEWS GASTROENTEROLOGY \& HEPATOLOGY}},
Year = {{2017}},
Volume = {{14}},
Number = {{12}},
Pages = {{739+}},
