@article{ ISI:000288539100002,
Author = {Zhang, Ju-ping and Jin, Zhen},
Title = {{The analysis of an epidemic model on networks}},
Journal = {{APPLIED MATHEMATICS AND COMPUTATION}},
Year = {{2011}},
Volume = {{217}},
Number = {{17}},
Pages = {{7053-7064}},
Month = {{MAY 1}},
Abstract = {{The paper consider an epidemic model with birth and death on networks.
   We derive the epidemic threshold R-0 dependent on birth rate b, death
   rate d (natural death) and mu from the infectious disease and natural
   death, and cure rate gamma. And the stability of the equilibriums (the
   disease-free equilibrium and endemic equilibrium) are analysed. Finally,
   the effects of various immunization schemes are studied and compared. We
   show that both targeted, and acquaintance immunization strategies
   compare favorably to a proportional scheme in terms of effectiveness.
   For active immunization, the threshold is easier to apply practically.
   To illustrate our theoretical analysis, some numerical simulations are
   also included. (c) 2011 Published by Elsevier Inc.}},
