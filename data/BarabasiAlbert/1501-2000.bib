@article{ ISI:000223488400007,
Author = {Saramaki, J and Kaski, K},
Title = {{Scale-free networks generated by random walkers}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2004}},
Volume = {{341}},
Pages = {{80-86}},
Month = {{OCT 1}},
Abstract = {{We present a simple mechanism for generating undirected scale-free
   networks using random walkers, where the network growth is determined by
   choosing parent vertices by sequential random walks. We show that this
   mechanism produces scale-free networks with degree exponent gamma = 3
   and clustering coefficients depending on random walk length. The
   mechanism can be interpreted in terms of preferential attachment without
   explicit knowledge of node degrees. (C) 2004 Elsevier B.V. All rights
   reserved.}},
