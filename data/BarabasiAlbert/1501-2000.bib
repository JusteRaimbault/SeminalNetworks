@article{ ISI:000261141500067,
Author = {Maillart, T. and Sornette, D. and Spaeth, S. and von Krogh, G.},
Title = {{Empirical Tests of Zipf's Law Mechanism in Open Source Linux
   Distribution}},
Journal = {{PHYSICAL REVIEW LETTERS}},
Year = {{2008}},
Volume = {{101}},
Number = {{21}},
Month = {{NOV 21}},
Abstract = {{Zipf's power law is a ubiquitous empirical regularity found in many
   systems, thought to result from proportional growth. Here, we establish
   empirically the usually assumed ingredients of stochastic growth models
   that have been previously conjectured to be at the origin of Zipf's law.
   We use exceptionally detailed data on the evolution of open source
   software projects in Linux distributions, which offer a remarkable
   example of a growing complex self-organizing adaptive system, exhibiting
   Zipf's law over four full decades.}},
