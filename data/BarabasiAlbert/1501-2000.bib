@article{ ISI:000304651400051,
Author = {Wu, Xiangjun and Lai, Darong and Lu, Hongtao},
Title = {{Generalized synchronization of the fractional-order chaos in weighted
   complex dynamical networks with nonidentical nodes}},
Journal = {{NONLINEAR DYNAMICS}},
