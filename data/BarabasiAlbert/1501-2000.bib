@article{ ISI:000176342700016,
Author = {Comellas, F and Sampels, M},
Title = {{Deterministic small-world networks}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2002}},
Volume = {{309}},
Number = {{1-2}},
Pages = {{231-235}},
Month = {{JUN 1}},
Abstract = {{Many real-life networks, such as the World Wide Web, transportation
   systems, biological or social networks, achieve both a strong local
   clustering (nodes have many mutual neighbors) and a small diameter
   (maximum distance between any two nodes). These networks have been
   characterized as small-world networks and modeled by the addition of
   randomness to regular structures. We show that small-world networks can
   be constructed in a deterministic way. This exact approach permits a
   direct calculation of relevant network parameters allowing their
   immediate contrast with real-world networks and avoiding complex
   computer simulations. (C) 2002 Elsevier Science B.V. All rights
   reserved.}},
