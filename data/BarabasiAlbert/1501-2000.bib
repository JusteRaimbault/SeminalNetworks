@article{ ISI:000259460900001,
Author = {Rambaldi, Davide and Giorgi, Federico M. and Capuani, Fabrizio and
   Ciliberto, Andrea and Ciccarelli, Francesca D.},
Title = {{Low duplicability and network fragility of cancer genes}},
Journal = {{TRENDS IN GENETICS}},
Year = {{2008}},
Volume = {{24}},
Number = {{9}},
Pages = {{427-430}},
Month = {{SEP}},
Abstract = {{We identified genomic and network properties of similar to 600 genes
   mutated in different cancer types. These genes tend not to duplicate
   but, unlike most human singletons, they encode central hubs of highly
   interconnected modules within the protein-protein interaction network
   (PIN). We find that cancer genes are fragile components of the human
   gene repertoire, sensitive to dosage modification. Furthermore, other
   nodes of the human PIN with similar properties are rare and probably
   enriched in candidate cancer genes.}},
