@article{ ISI:000236467700003,
Author = {Huang, L and Park, K and Lai, YC},
Title = {{Information propagation on modular networks}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2006}},
Volume = {{73}},
Number = {{3, 2}},
Month = {{MAR}},
Abstract = {{Networks with a community (or modular) structure underlie many social
   and biological phenomena. In such a network individuals tend to form
   sparsely linked local communities, each having dense internal
   connections. We investigate the dynamics of information propagation on
   modular networks by using a three-state epidemic model with a unit
   spreading rate (i.e., the probability for a susceptible individual to be
   ``infected{''} with the information is one). We find a surprising,
   resonancelike phenomenon: the information lifetime on the network can be
   maximized by the number of modules. The result can be useful for
   optimizing or controlling information spread on social or biological
   networks.}},
