@article{ ISI:000349576900003,
Author = {Caccioli, Fabio and Farmer, J. Doyne and Foti, Nick and Rockmore, Daniel},
Title = {{Overlapping portfolios, contagion, and financial stability}},
Journal = {{JOURNAL OF ECONOMIC DYNAMICS \& CONTROL}},
Year = {{2015}},
Volume = {{51}},
Pages = {{50-63}},
Month = {{FEB}},
Abstract = {{We study the problem of interacting channels of contagion in financial
   networks. The first channel of contagion is counterparty failure risk;
   this is captured empirically using data for the Austrian interbank
   network. The second channel of contagion is overlapping portfolio
   exposures; this is studied using a stylized model. We perform stress
   tests according to different protocols. For the parameters we study
   neither channel of contagion results in large effects on its own. In
   contrast, when both channels are active at once, bankruptcies are much
   more common and have large systemic effects. (C) 2014 Elsevier B.V. All
   rights reserved.}},
