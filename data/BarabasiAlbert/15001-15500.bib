@inproceedings{ ISI:000310859800013,
Author = {Grilo, Carlos and Correia, Luis},
Editor = {{Antunes, L and Pinto, HS}},
Title = {{Network Regularity and the Influence of Asynchronism on the Evolution of
   Cooperation}},
Booktitle = {{PROGRESS IN ARTIFICIAL INTELLIGENCE-BOOK}},
Series = {{Lecture Notes in Artificial Intelligence}},
Year = {{2011}},
Volume = {{7026}},
Pages = {{168-181}},
