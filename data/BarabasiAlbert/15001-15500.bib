@inproceedings{ ISI:000317063700004,
Author = {Yoshida, Tetsuya and Yamada, Yuu},
Editor = {{Watada, J and Watanabe, T and PhillipsWren, G and Howlett, RJ and Jain, LC}},
Title = {{Immunization of Networks via Modularity Based Node Representation}},
