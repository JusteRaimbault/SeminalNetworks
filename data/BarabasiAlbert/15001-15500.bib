@inproceedings{ ISI:000307488600056,
Author = {Scholtes, Ingo and Botev, Jean and Esch, Markus and Sturm, Peter},
Editor = {{Zhou, J}},
Title = {{Epidemic Self-synchronization in Complex Networks}},
