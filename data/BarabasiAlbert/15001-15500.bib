@article{ ISI:000273616600011,
Author = {Ni, Y. and Wu, L. and Zhu, S. Q. and Huang, Y.},
Title = {{Geographical effect on weighted network synchronisation}},
Journal = {{EUROPEAN PHYSICAL JOURNAL D}},
Year = {{2010}},
Volume = {{56}},
Number = {{2}},
Pages = {{221-229}},
Month = {{JAN}},
Abstract = {{The propensity for synchronisation of weighted and asymmetrical networks
   is investigated. The weighting scheme is employed when connection
   strengths are weighted according to local information. The weighting
   schemes of different level of information restriction are compared. It
   is found that the optimal network synchronisability is obtained when the
   weighting scheme is based on softly restricted information. It is even
   better than scheme based on global information. This implies that ``more
   knowledge, better network synchronisability{''} is not always true. This
   is mainly due to a partial synchronisation phenomenon taking place at a
   cluster level. The finding may be helpful to gain a better understanding
   of why synchronisation is widely observed in many real large-scale
   systems while each node is aware of limited information about only a
   small part of a network.}},
