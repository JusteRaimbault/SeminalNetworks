@article{ ISI:000293018700008,
Author = {Zhuang, Yong and Shi, Dong-Mei and Du, Wen-Bo and Zhang, Hai-Feng and
   Wang, Bing-Hong},
Title = {{INTEGRATING LOCAL DYNAMIC AND GLOBAL STATIC INFORMATION FOR ROUTING
   TRAFFIC ON NETWORKS}},
