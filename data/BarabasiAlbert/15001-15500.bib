@article{ ISI:000280808100008,
Author = {Li Jie and Gao Song and Wang Jin and Zhang ChenYu},
Title = {{Construction of comprehensive gene network for human mitochondria}},
Journal = {{CHINESE SCIENCE BULLETIN}},
