@inproceedings{ ISI:000325760100042,
Author = {Ye, Qi and Wu, Bin and Wang, Bai},
Editor = {{Cao, L and Feng, Y and Zhong, J}},
Title = {{Multiple Level Views on the Adherent Cohesive Subgraphs in Massive
   Temporal Call Graphs}},
