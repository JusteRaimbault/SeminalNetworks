@inproceedings{ ISI:000392421700510,
Author = {Sakamoto, Yasuaki and Shi, Hongyuan},
Editor = {{Ohlsson, S and Catrambone, R}},
Title = {{The Benefit of Imitating Particular Individuals}},
Booktitle = {{COGNITION IN FLUX}},
