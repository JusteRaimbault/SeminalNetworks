@article{ ISI:000269753700021,
Author = {Geng, Xian-Min and Wen, Guang-Hui and Wan, Shu-Chen and Xiong, Jie-Yu
   and Wang, Ying},
Title = {{The emergence of scale-free networks with a seceding mechanism}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2009}},
Volume = {{388}},
Number = {{20}},
Pages = {{4484-4490}},
Month = {{OCT 15}},
Abstract = {{In order to explore further the underlying mechanism of scale-free
   networks, we study stochastic secession as a mechanism for the creation
   of complex networks. In this evolution the network growth incorporates
   the addition of new nodes, the addition of new links between existing
   nodes, the deleting and rewiring of some existing links, and the
   stochastic secession of nodes. To random growing networks with
   preferential attachment, the model yields scale-free behavior for the
   degree distribution. Furthermore, we obtain an analytical expression of
   the power-law degree distribution with scaling exponent gamma ranging
   from 1.1 to 9. The analytical expressions are in good agreement with the
   numerical simulation results. (C) 2009 Elsevier B.V. All rights
   reserved.}},
