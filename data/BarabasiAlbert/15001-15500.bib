@article{ ISI:000268329100008,
Author = {Ke, J. and Li, P. -P. and Chen, X. -S. and Lin, Z. and Zheng, Y. Z.},
Title = {{Connectivity of growing networks with link constraints}},
Journal = {{EUROPEAN PHYSICAL JOURNAL B}},
Year = {{2009}},
Volume = {{70}},
Number = {{2}},
Pages = {{211-215}},
Month = {{JUL}},
Abstract = {{We propose a growing network model with link constraint, in which new
   nodes are continuously introduced into the system and immediately
   connected to preexisting nodes, and any arbitrary node cannot receive
   new links when it reaches a maximum number of links k(m). The
   connectivity of the network model is then investigated by means of the
   rate equation approach. For the connection kernel A(k)=k(gamma), the
   degree distribution n(k) takes a power law if gamma a parts per thousand
   yen1 and decays stretched exponentially if 0a parts per thousand
   currency sign gamma < 1. We also consider a network system with the
   connection kernel A(k)=k(alpha)(k(m)-k)(beta). It is found that n(k)
   approaches a power law in the alpha > 1 case and has a stretched
   exponential decay in the 0a parts per thousand currency sign alpha < 1
   case, while it can take a power law with exponential truncation in the
   special alpha=beta=1 case. Moreover, n(k) may have a U-type structure if
   alpha > beta.}},
