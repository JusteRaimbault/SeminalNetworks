@inproceedings{ ISI:000280141100060,
Author = {Wang, Li and Guo, Dongchao and Liang, Mangui},
Editor = {{Han, Z and Liang, MG and Cen, YG}},
Title = {{The Research of Network Capacity Based on the Network Edge Congestion}},
