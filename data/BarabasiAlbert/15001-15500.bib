@inproceedings{ ISI:000297039000021,
Author = {Fyson, Nick and De Bie, Tijl and Cristianini, Nello},
Editor = {{Dobnikar, A and Lotric, U and Ster, B}},
Title = {{Reconstruction of Causal Networks by Set Covering}},
Booktitle = {{ADAPTIVE AND NATURAL COMPUTING ALGORITHMS, PT II}},
Series = {{Lecture Notes in Computer Science}},
Year = {{2011}},
Volume = {{6594}},
