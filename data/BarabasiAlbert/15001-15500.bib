@inproceedings{ ISI:000310759600008,
Author = {Furtado, Vasco and Oliveira, Douglas},
Editor = {{Costa, LD and Evsukoff, A and Mangioni, G and Menezes, R}},
Title = {{Generating Power Law Distribution of Spatial Events with Multi-agents}},
