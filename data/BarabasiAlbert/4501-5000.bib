@article{ ISI:000357778600004,
Author = {Miller, Benjamin A. and Beard, Michelle S. and Wolfe, Patrick J. and
   Bliss, Nadya T.},
Title = {{A Spectral Framework for Anomalous Subgraph Detection}},
