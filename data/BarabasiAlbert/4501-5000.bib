@article{ ISI:000250491900030,
Author = {Chen, Mu and Yu, Boming and Xu, Peng and Chen, Jun},
Title = {{A new deterministic complex network model with hierarchical structure}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2007}},
Volume = {{385}},
Number = {{2}},
Pages = {{707-717}},
Month = {{NOV 15}},
Abstract = {{We introduce a new simple pseudo tree-like network model, deterministic
   complex network (DCN). The proposed DCN model may simulate the
   hierarchical structure nature of real networks appropriately and have
   the unique property of,skipping the levels', which is ubiquitous in
   social networks. Our results indicate that the DCN model has a rather
   small average path length and large clustering coefficient, leading to
   the small-world effect. Strikingly, our DCN model obeys a discrete
   power-law degree distribution P(k) proportional to k-gamma, with
   exponent gamma approaching 1.0. We also discover that the relationship
   between the clustering coefficient and degree follows the scaling law
   C(k)similar to k(-1), which quantitatively determines the DCN's
   hierarchical structure. (C) 2007 Elsevier B.V. All rights reserved.}},
