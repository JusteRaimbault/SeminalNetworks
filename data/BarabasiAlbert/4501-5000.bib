@article{ ISI:000364339800005,
Author = {Agreste, Santa and De Meo, Pasquale and Ferrara, Emilio and Piccolo,
   Sebastiano and Provetti, Alessandro},
Title = {{Trust Networks Topology, Dynamics, and Measurements}},
Journal = {{IEEE INTERNET COMPUTING}},
Year = {{2015}},
Volume = {{19}},
Number = {{6}},
Pages = {{26-35}},
Month = {{NOV-DEC}},
Abstract = {{Through online social network analysis, the emergence (over time) of
   ``trusted{''} users is investigated, by studying the evolution of
   topological and centrality measures of the network of trust within the
   overall social network. Large datasets of user activity are studied from
   Ciao and Epinions (two online platforms with an explicit notion of trust
   controlled by users), and Prosper (a micro-lending site where trust
   remains implicit). The implications of such findings are discussed,
   particularly regarding how to enable trust in online platforms and
   interaction, with implications for trust-based activities.}},
