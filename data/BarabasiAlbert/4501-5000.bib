@inproceedings{ ISI:000355411500077,
Author = {Moustafa, Walaa Eldin and Kimmig, Angelika and Deshpande, Amol and
   Getoor, Lise},
Book-Group-Author = {{IEEE}},
Title = {{Subgraph Pattern Matching over Uncertain Graphs with Identity Linkage
   Uncertainty}},
Booktitle = {{2014 IEEE 30TH INTERNATIONAL CONFERENCE ON DATA ENGINEERING (ICDE)}},
