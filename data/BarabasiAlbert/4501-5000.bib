@article{ ISI:000234004800012,
Author = {Wemmenhove, B and Nikoletopoulos, T and Hatchett, JPL},
Title = {{Replica symmetry breaking in the `small world' spin glass}},
Journal = {{JOURNAL OF STATISTICAL MECHANICS-THEORY AND EXPERIMENT}},
Year = {{2005}},
Month = {{NOV}},
Abstract = {{We apply the cavity method to an Ising spin glass model on a `small
   world' lattice, a random bond graph superimposed upon a one-dimensional
   ferromagnetic ring, which allows us to identify the cavity fields in the
   chain with eigenvectors of the replicated transfer matrix for the chain
   using the one-step replica symmetry breaking scheme developed by Mezard
   and Parisi for the Bethe lattice; we evaluate observables for a model
   with fixed connectivity and +/-J long range bonds. We observe that
   ferromagnetic ordering within this model extends into a larger parameter
   region than predicted from replica symmetric calculations. Numerical
   simulations, although suffering non-equilibration effects, seem to
   provide support for this result.}},
