@article{ ISI:000269753700023,
Author = {Kitajima, Hiroyuki and Kurths, Juergen},
Title = {{Bifurcation in neuronal networks with hub structure}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2009}},
Volume = {{388}},
