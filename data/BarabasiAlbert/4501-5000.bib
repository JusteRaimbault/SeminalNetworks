@article{ ISI:000353829800010,
Author = {Yadav, Alok and Jalan, Sarika},
Title = {{Origin and implications of zero degeneracy in networks spectra}},
Journal = {{CHAOS}},
Year = {{2015}},
Volume = {{25}},
Number = {{4}},
Month = {{APR}},
Abstract = {{The spectra of many real world networks exhibit properties which are
   different from those of random networks generated using various models.
   One such property is the existence of a very high degeneracy at the zero
   eigenvalue. In this work, we provide all the possible reasons behind the
   occurrence of the zero degeneracy in the network spectra, namely, the
   complete and partial duplications, as well as their implications. The
   power-law degree sequence and the preferential attachment are the
   properties which enhances the occurrence of such duplications and hence
   leading to the zero degeneracy. A comparison of the zero degeneracy in
   protein-protein interaction networks of six different species and in
   their corresponding model networks indicates importance of the degree
   sequences and the power-law exponent for the occurrence of zero
   degeneracy. (C) 2015 AIP Publishing LLC.}},
