Book-Group-Author = {{IEEE}},
Title = {{Optimizing and Auto-Tuning Scale-Free Sparse Matrix-Vector
   Multiplication on Intel Xeon Phi}},
Booktitle = {{2015 IEEE/ACM INTERNATIONAL SYMPOSIUM ON CODE GENERATION AND
   OPTIMIZATION (CGO)}},
