@article{ ISI:000247900800003,
Author = {Cai, Shi-Min and Yan, Gang and Zhou, Tao and Zhou, Pei-Ling and Fu,
   Zhong-Qian and Wang, Bing-Hong},
Title = {{Scaling behavior of an artificial traffic model on scale-free networks}},
Journal = {{PHYSICS LETTERS A}},
