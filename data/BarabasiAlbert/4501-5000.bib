@article{ ISI:000371048100002,
Author = {Pobiedina, Nataliia and Ichise, Ryutaro},
Title = {{Citation count prediction as a link prediction problem}},
Journal = {{APPLIED INTELLIGENCE}},
Year = {{2016}},
Volume = {{44}},
Number = {{2}},
Pages = {{252-268}},
Month = {{MAR}},
Abstract = {{The citation count is an important factor to estimate the relevance and
   significance of academic publications. However, it is not possible to
   use this measure for papers which are too new. A solution to this
   problem is to estimate the future citation counts. There are existing
   works, which point out that graph mining techniques lead to the best
   results. We aim at improving the prediction of future citation counts by
   introducing a new feature. This feature is based on frequent graph
   pattern mining in the so-called citation network constructed on the
   basis of a dataset of scientific publications. Our new feature improves
   the accuracy of citation count prediction, and outperforms the
   state-of-the-art features in many cases which we show with experiments
   on two real datasets.}},
