@article{ ISI:000269249500011,
Author = {Wang, Jian-Wei and Rong, Li-Li},
Title = {{VULNERABILITY OF EFFECTIVE ATTACK ON EDGES IN SCALE-FREE NETWORKS DUE TO
   CASCADING FAILURES}},
