@article{ ISI:000413946300001,
Author = {Caron, Francois and Fox, Emily B.},
Title = {{Sparse graphs using exchangeable random measures}},
Journal = {{JOURNAL OF THE ROYAL STATISTICAL SOCIETY SERIES B-STATISTICAL
   METHODOLOGY}},
Year = {{2017}},
