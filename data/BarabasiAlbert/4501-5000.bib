@article{ ISI:000297779500045,
Author = {Kovarik, Jaromir and Branas-Garza, Pablo and Cobo-Reyes, Ramon and Paz
   Espinosa, Maria and Jimenez, Natalia and Ponti, Giovanni},
Title = {{Prosocial norms and degree heterogeneity in social networks}},
