@inproceedings{ ISI:000238207800034,
Author = {Memon, Nasrullah and Larsen, Henrik Legind},
Editor = {{Mehrotra, S and Zeng, DD and Chen, H and Thuraisingham, B and Wang, FY}},
Title = {{Practical algorithms for destabilizing terrorist networks}},
