@article{ ISI:000249863300013,
Author = {Zhang, Zhongzhi and Zhou, Shuigeng and Wang, Zhiyong and Shen, Zhen},
Title = {{A geometric growth model interpolating between regular and small-world
   networks}},
Journal = {{JOURNAL OF PHYSICS A-MATHEMATICAL AND THEORETICAL}},
Year = {{2007}},
Volume = {{40}},
Number = {{39}},
Pages = {{11863-11876}},
Month = {{SEP 28}},
Abstract = {{We propose a geometric growth model which interpolates between
   one-dimensional linear graphs and small-world networks. The model
   undergoes a transition from large to small worlds. We study the
