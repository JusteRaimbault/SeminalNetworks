@article{ ISI:000392793500071,
Author = {Zhu, Liang and Wang, Youguo},
Title = {{Rumor spreading model with noise interference in complex social networks}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2017}},
Volume = {{469}},
Pages = {{750-760}},
Month = {{MAR 1}},
Abstract = {{In this paper, a modified susceptible infected removed (SIR) model has
   been proposed to explore rumor diffusion on complex social networks. We
   take variation of connectivity into consideration and assume the
   variation as noise. On the basis of related literature on virus
   networks, the noise is described as standard Brownian motion while
   stochastic differential equations (SDE) have been derived to
   characterize dynamics of rumor diffusion both on homogeneous networks
   and heterogeneous networks. Then, theoretical analysis on homogeneous
   networks has been demonstrated to investigate the solution of SDE model
   and the steady state of rumor diffusion. Simulations both on Barabasi
   Albert (BA) network and Watts Strogatz (WS) network display that the
