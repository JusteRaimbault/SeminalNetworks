@article{ ISI:000393679200011,
Author = {Zhang, Hao and Wang, Xing-yuan and Lin, Xiao-hui},
Title = {{Stability and Control of Fractional Chaotic Complex Networks with Mixed
   Interval Uncertainties}},
Journal = {{ASIAN JOURNAL OF CONTROL}},
