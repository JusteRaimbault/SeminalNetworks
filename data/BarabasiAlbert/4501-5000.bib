@article{ ISI:000337183500010,
Author = {Pajouh, Foad Mahdavi and Miao, Zhuqi and Balasundaram, Balabhaskar},
Title = {{A branch-and-bound approach for maximum quasi-cliques}},
Journal = {{ANNALS OF OPERATIONS RESEARCH}},
