@article{ ISI:000268653900021,
Author = {Vilela, Andre L. M. and Moreira, F. G. Brady},
Title = {{Majority-vote model with different agents}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2009}},
Volume = {{388}},
Number = {{19}},
Pages = {{4171-4178}},
Month = {{OCT 1}},
Abstract = {{We consider the majority-vote model with noise in a network of social
   interactions for a system with two classes of individuals, class sigma
   and class tau. For the two-agent model each class has its own dynamics,
   with individuals of sigma class being influenced by neighbors of both
   classes, while the individuals of type tau are influenced only by
   neighbors of that class. We use Monte Carlo Simulations and finite-size
   scaling techniques to estimate the critical properties of the system in
   the stationary state. The Calculated Values of the critical noise
   parameters, q(sigma){*} and q(tau){*}, allow us to identify five
   distinct regions in the phase diagram on the q(tau)-q(sigma) plane. The
   critical exponents for each class are the same and we conclude that the
   present model belongs to the same universality class as the
   two-dimensional lsing model. (C) 2009 Published by Elsevier B.V.}},
