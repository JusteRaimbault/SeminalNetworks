@inproceedings{ ISI:000280123000004,
Author = {Meyer, Christian M. and Gurevych, Iryna},
Editor = {{Gelbukh, A}},
Title = {{Worth Its Weight in Gold or Yet Another Resource - A Comparative Study
   of Wiktionary, OpenThesaurus and GermaNet}},
Booktitle = {{COMPUTATIONAL LINGUISTICS AND INTELLIGENT TEXT PROCESSING}},
Series = {{Lecture Notes in Computer Science}},
