@article{ ISI:000347017300029,
Author = {Wang, Xiaojie and Zhang, Xue and Zhao, Chengli and Xie, Zheng and Zhang,
   Shengjun and Yi, Dongyun},
Title = {{Predicting link directions using local directed path}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2015}},
Volume = {{419}},
Pages = {{260-267}},
Month = {{FEB 1}},
Abstract = {{Link prediction in directed network is attracting growing interest among
   many network scientists. Compared with predicting the existence of a
   link, determining its direction is more complicated. In this paper, we
   propose an efficient solution named Local Directed Path to predict link
   direction. By adding an extra ground node to the network, we solve the
   information loss problem in sparse network, which makes our method
   effective and robust. As a quasi-local method, our method can deal with
   large-scale networks in a reasonable time. Empirical analysis on real
   networks shows that our method can correctly predict link directions,
   which outperforms some local and global methods. (C) 2014 Elsevier B.V.
   All rights reserved.}},
