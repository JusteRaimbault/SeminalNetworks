@article{ ISI:000268987400049,
Author = {Xia, Yongxiang and Liu, Nianjun and Iu, Herbert H. C.},
Title = {{Oscillation and chaos in a deterministic traffic network}},
Journal = {{CHAOS SOLITONS \& FRACTALS}},
Year = {{2009}},
Volume = {{42}},
Number = {{3}},
Pages = {{1700-1704}},
Month = {{NOV 15}},
Abstract = {{Traffic dynamics of regular networks are of importance in theory and
   practice. In this paper, we study such a problem with a regular lattice
   structure. We specify the network structure and traffic protocols so
   that all the random features are removed, When a node is attacked and
   then removed, the traffic redistributes, causing complicated dynamical
   results. With different system redundancy, we observe rich dynamics,
   ranging from stable state to periodic to chaotic oscillation. Since this
   is a completely deterministic system, we can conclude that the nonlinear
   dynamics is purely due to the interior nonlinear feature of the traffic.
