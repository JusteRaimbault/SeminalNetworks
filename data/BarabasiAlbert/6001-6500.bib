@article{ ISI:000261562800048,
Author = {Yang Xu-Hua and Wang Bo and Wang Wan-Liang and Sun You-Xian},
Title = {{Research on Some Bus Transport Networks with Random Overlapping Clique
   Structure}},
Journal = {{COMMUNICATIONS IN THEORETICAL PHYSICS}},
Year = {{2008}},
Volume = {{50}},
Number = {{5}},
Pages = {{1249-1254}},
Month = {{NOV}},
Abstract = {{On the basis of investigating the statistical data of bus transport
   networks of three big cities (BTN) consists of a lot of cliques, which
   intensively connect and overlap with each other. We study the network
   properties, which include the degree distribution, multiple edges'
   overlapping time distribution, distribution of the overlap size between
   any two overlapping cliques, distribution of the number of cliques that
   a node belongs to. Naturally, the cliques also constitute a network,
   with the overlapping nodes being their multiple links. We also research
   its network properties such as degree distribution, clustering, average
   path length, and so on. We propose that a BTN has the properties of
   random clique increment and random overlapping clique, at the same time,
   a BTN is a small-world network with highly clique-clustered and highly
   clique-overlapped. Finally, we introduce a BTN evolution model, whose
   simulation results agree well with the statistical laws that emerge in
   real BTNs.}},
