@article{ ISI:000347602000031,
Author = {Gast, Mikael and Hauptmann, Mathias and Karpinski, Marek},
Title = {{Inapproximability of dominating set on power law graphs}},
Journal = {{THEORETICAL COMPUTER SCIENCE}},
Year = {{2015}},
Volume = {{562}},
Pages = {{436-452}},
Month = {{JAN 11}},
Abstract = {{We prove the first logarithmic lower bounds for the approximability of
   the MINIMUM DOMINATING SET problem for the case of connected (alpha,
   beta)-power law graphs for a being a size parameter and beta the power
   law exponent. We give also a best up to now upper approximation bound
   for this problem in the case of the parameters beta > 2. We develop also
   a new functional method for proving lower approximation bounds and
   display a sharp approximation phase transition area between
   approximability and inapproximability of the underlying problems. Our
   results depend on a method which could be also of independent interest.
   (C) 2014 Elsevier B.V. All rights reserved.}},
