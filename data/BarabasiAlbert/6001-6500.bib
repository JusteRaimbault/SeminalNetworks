@article{ ISI:000405536900045,
Author = {Liu, Hao-Ran and Hu, Yan-Long and Yin, Rong-Rong and Deng, Yu-Jing},
Title = {{Cascading failure model of scale-free topology for avoiding node failure}},
Journal = {{NEUROCOMPUTING}},
Year = {{2017}},
Volume = {{260}},
Pages = {{443-448}},
Month = {{OCT 18}},
Abstract = {{In order to efficiently fortify the robustness of a scale-free network
   against cascading failure, a model of this phenomenon is proposed based
   on defining the load of a node with respect to both degree and
   betweenness centrality. Simultaneously, when the load is redistributed,
   an allocation proportion is ascertained according to both the loads and
   the energies of the nodes. After that, we obtain the relationship
   between the model parameters and the robustness of the network against
   the cascading failure with theoretical analysis, which also helps us
   determine the influence of the average network degree on cascading
   failure. Finally, the results of theoretical analysis are verified with
   simulation experiments. (C) 2017 Elsevier B.V. All rights reserved.}},
