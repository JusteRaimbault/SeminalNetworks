@article{ ISI:000248835600003,
Author = {Kimura, Takayuki and Ikeguchi, Tohru},
Title = {{An optimum strategy for dynamic and stochastic packet routing problems
   by chaotic neurodynamics}},
Journal = {{INTEGRATED COMPUTER-AIDED ENGINEERING}},
Year = {{2007}},
