@article{ ISI:000296633700015,
Author = {Bauke, H. and Moore, C. and Rouquier, J. B. and Sherrington, D.},
Title = {{Topological phase transition in a network model with preferential
   attachment and node removal}},
Journal = {{EUROPEAN PHYSICAL JOURNAL B}},
Year = {{2011}},
Volume = {{83}},
Number = {{4}},
Pages = {{519-524}},
Month = {{OCT}},
Abstract = {{Preferential attachment is a popular model of growing networks. We
   consider a generalized model with random node removal, and a combination
   of preferential and random attachment. Using a high-degree expansion of
   the master equation, we identify a topological phase transition
   depending on the rate of node removal and the relative strength of
   preferential vs. random attachment, where the degree distribution goes
   from a power law to one with an exponential tail.}},
