@article{ ISI:000361611300013,
Author = {Duan, Wei and Fan, Zongchen and Zhang, Peng and Guo, Gang and Qiu,
   Xiaogang},
Title = {{Mathematical and computational approaches to epidemic modeling: a
   comprehensive review}},
Journal = {{FRONTIERS OF COMPUTER SCIENCE}},
