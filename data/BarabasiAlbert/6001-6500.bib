@article{ ISI:000366785900042,
Author = {Yin, Rong-Rong and Liu, Bin and Liu, Hao-Ran and Li, Ya-Qian},
Title = {{Research on invulnerability of the random scale-free network against
   cascading failure}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2016}},
Volume = {{444}},
Pages = {{458-465}},
Month = {{FEB 15}},
Abstract = {{The effect of structure parameters of random scale-free network on the
   network invulnerability for cascading failure is investigated by
   establishing a cascading failure model of random scale-free network
   based on node degree and analyzing the effect of node capacity on the
   cascading failure. The node capacity threshold is thus obtained.
   Furthermore, the relationship between the threshold of node capacity and
   the structure parameters of the network (the number of added edges per
   time slot and the power exponent) is established. The experimental
   results show that the structure parameters of the network are positively
   correlated with the network invulnerability for cascading failure. The
   more the number of added edges at a time and higher the power exponent,
   the stronger the network invulnerability for cascading failure. (C) 2015
   Published by Elsevier B.V.}},
