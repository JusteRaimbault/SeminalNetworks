@article{ ISI:000469284500031,
Author = {Ping, Pengyao and Wang, Lei and Kuang, Linai and Ye, Songtao and Iqbal,
   Muhammad Faisal Buland and Pei, Tingrui},
Title = {{A Novel Method for LncRNA-Disease Association Prediction Based on an
   lncRNA-Disease Association Network}},
Journal = {{IEEE-ACM TRANSACTIONS ON COMPUTATIONAL BIOLOGY AND BIOINFORMATICS}},
