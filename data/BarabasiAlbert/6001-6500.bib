@article{ ISI:000289622200026,
Author = {Zhong, Lixin and Qiu, Tian and Ren, Fei and Li, Pingping and Chen, Bihui},
Title = {{Time scales of epidemic spread and risk perception on adaptive networks}},
Journal = {{EPL}},
Year = {{2011}},
Volume = {{94}},
Number = {{1}},
Month = {{APR}},
Abstract = {{Incorporating dynamic contact networks and delayed awareness into a
   contagion model with memory, we study the spreading patterns of
   infectious diseases in connected populations. It is found that the
   spread of an infectious disease is not only related to the past
   exposures of an individual to the infected but also the time scales of
   risk perception reflected in the social-network adaptation. The epidemic
   threshold pc is found to decrease with the rise of the time scale
   parameter s and the memory length T, which satisfies the equation pc =
   1/T + omega T/< k > a(s)(1-e(-omega T2)/a(s)). Both the lifetime of the
   epidemic and the topological property of the evolved network are
   considered. The standard deviation sigma(d) of the degree distribution
   increases with the rise of the absorbing time t(c), a power-law relation
   sigma(d) = mt(c)(gamma) is found. Copyright (C) EPLA, 2011}},
