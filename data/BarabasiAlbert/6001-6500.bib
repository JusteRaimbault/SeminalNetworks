@article{ ISI:000323458500004,
Author = {Chami, Goylette F. and Molyneux, David H. and Kontoleon, Andreas A. and
   Dunne, David W.},
Title = {{Exploring network theory for mass drug administration}},
Journal = {{TRENDS IN PARASITOLOGY}},
