@article{ ISI:000224475600026,
Author = {Chan, KP and Zheng, DF and Hui, PM},
Title = {{Effects of aging and links removal on epidemic dynamics in scale-free
   networks}},
Journal = {{INTERNATIONAL JOURNAL OF MODERN PHYSICS B}},
Year = {{2004}},
Volume = {{18}},
Number = {{17-19}},
Pages = {{2534-2539}},
Month = {{JUL 30}},
