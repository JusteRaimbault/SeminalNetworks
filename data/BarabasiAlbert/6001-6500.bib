@article{ ISI:000341267000010,
Author = {Foschi, Rachele and Riccaboni, Massimo and Schiavo, Stefano},
Title = {{Preferential attachment in multiple trade networks}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2014}},
Volume = {{90}},
Number = {{2}},
Month = {{AUG 27}},
Abstract = {{In this paper we develop a model for the evolution of multiple networks
   which is able to replicate the concentrated and sparse nature of world
   trade data. Our model is an extension of the preferential attachment
   growth model to the case of multiple networks. Countries trade a variety
   of goods of different complexity. Every country progressively evolves
   from trading less sophisticated to high-tech goods. The probabilities of
   capturing more trade opportunities at a given level of complexity and of
   starting to trade more complex goods are both proportional to the number
   of existing trade links. We provide a set of theoretical predictions and
   simulative results. A calibration exercise shows that our model
   replicates the same concentration level of world trade as well as the
   sparsity pattern of the trade matrix. We also discuss a set of numerical
   solutions to deal with large multiple networks.}},
