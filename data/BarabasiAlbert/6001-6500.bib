@article{ ISI:000266904100021,
Author = {Wu, Ye and Li, Ping and Chen, Maoyin and Xiao, Jinghua and Kurths,
   Juergen},
Title = {{Response of scale-free networks with community structure to external
   stimuli}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2009}},
Volume = {{388}},
Number = {{14}},
Pages = {{2987-2994}},
Month = {{JUL 15}},
Abstract = {{The response of scale-free networks with community structure to external
   stimuli is studied. By disturbing some nodes with different strategies,
   it is shown that the robustness of this kind of network can be enhanced
   due to the existence of communities in the networks. Some of the
   response patterns are found to coincide with topological communities. We
   show that such phenomena also occur in the cat brain network which is an
   example of a scale-free like network with community structure. Our
   results provide insights into the relationship between network topology
   and the functional organization in complex networks from another
   viewpoint. (C) 2009 Elsevier B.V. All rights reserved.}},
