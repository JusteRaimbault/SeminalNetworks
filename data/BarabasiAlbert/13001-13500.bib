@article{ ISI:000441548800003,
Author = {Cecchini, Flavio Massimiliano and Riedl, Martin and Fersini, Elisabetta
   and Biemann, Chris},
Title = {{A comparison of graph-based word sense induction clustering algorithms
   in a pseudoword evaluation framework}},
Journal = {{LANGUAGE RESOURCES AND EVALUATION}},
