Title = {{RaRE: Social Rank Regulated Large-scale Network Embedding}},
Booktitle = {{WEB CONFERENCE 2018: PROCEEDINGS OF THE WORLD WIDE WEB CONFERENCE
   (WWW2018)}},
Year = {{2018}},
