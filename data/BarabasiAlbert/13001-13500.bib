@article{ ISI:000445924900006,
Author = {Lin, Chongze and Zheng, Yi},
Title = {{MFSC: Mean-Field-Theory and Spreading-Coefficient Based Degree
   Distribution Analysis in Social Network}},
Journal = {{KSII TRANSACTIONS ON INTERNET AND INFORMATION SYSTEMS}},
Year = {{2018}},
Volume = {{12}},
Number = {{8}},
Pages = {{3630-3656}},
Month = {{AUG 31}},
Abstract = {{Degree distribution can provide basic information for structural
   characteristics and internal relationship in social network. It is a
   critical procedure for social network topology analysis. In this paper,
   based on the mean-field theory, we study a special type of social
   network with exponential distribution of time intervals. First of all,
   in order to improve the accuracy of analysis, we propose a spreading
   coefficient algorithm based on intimate relationship, which determines
   the number of the joined members through the intimacy among members.
   Then, simulation show that the degree distribution of follows the
   power-law distribution and has small-world characteristics. Finally, we
   compare the performance of our algorithm with the existing algorithms,
   and find that our algorithm improves the accuracy of degree distribution
   as well as reducing the time complexity significantly, which can
   complete 29.04\% higher precision and 40.94\% lower implementation time.}},
