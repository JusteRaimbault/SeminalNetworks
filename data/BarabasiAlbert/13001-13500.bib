@article{ ISI:000442362100005,
Author = {Feng, Minyu and Qu, Hong and Yi, Zhang and Kurths, Juergen},
Title = {{Subnormal Distribution Derived From Evolving Networks With Variable
   Elements}},
Journal = {{IEEE TRANSACTIONS ON CYBERNETICS}},
Year = {{2018}},
Volume = {{48}},
Number = {{9}},
Pages = {{2556-2568}},
Month = {{SEP}},
Abstract = {{During the past decades, power-law distributions have played a
   significant role in analyzing the topology of scale-free networks.
   However, in the observation of degree distributions in practical
   networks and other nonuniform distributions such as the wealth
   distribution, we discover that, there exists a peak at the beginning of
   most real distributions, which cannot be accurately described by a
   monotonic decreasing power-law distribution. To better describe the real
   distributions, in this paper, we propose a subnormal distribution
   derived from evolving networks with variable elements and study its
   statistical properties for the first time. By utilizing this
   distribution, we can precisely describe those distributions commonly
   existing in the real world, e.g., distributions of degree in social
   networks and personal wealth. Additionally, we fit connectivity in
   evolving networks and the data observed in the real world by the
   proposed subnormal distribution, resulting in a better performance of
   fitness.}},
