@article{ ISI:000449027600051,
Author = {Zhang, Chengjun and Liu, Jin and Qu, Yanzhen and Han, Tianqi and Ge,
   Xujun and Zeng, An},
Title = {{Enhancing the robustness of recommender systems against spammers}},
Journal = {{PLOS ONE}},
Year = {{2018}},
Volume = {{13}},
Number = {{11}},
Month = {{NOV 1}},
Abstract = {{The accuracy and diversity of recommendation algorithms have always been
   the research hotspot of recommender systems. A good recommender system
   should not only have high accuracy and diversity, but also have adequate
   robustness against spammer attacks. However, the issue of recommendation
   robustness has received relatively little attention in the literature.
   In this paper, we systematically study the influences of different
   spammer behaviors on the recommendation results in various
   recommendation algorithms. We further propose an improved algorithm by
   incorporating the inner-similarity of user's purchased items in the
   classic KNN approach. The new algorithm effectively enhances the
   robustness against spammer attacks and thus outperforms traditional
   algorithms in recommendation accuracy and diversity when spammers exist
   in the online commercial systems.}},
