@article{ ISI:000431721800002,
Author = {Bhamidi, Shankar and Jin, Jimmy and Nobel, Andrew},
Title = {{CHANGE POINT DETECTION IN NETWORK MODELS: PREFERENTIAL ATTACHMENT AND
   LONG RANGE DEPENDENCE}},
Journal = {{ANNALS OF APPLIED PROBABILITY}},
Year = {{2018}},
Volume = {{28}},
Number = {{1}},
Pages = {{35-78}},
Month = {{FEB}},
Abstract = {{Inspired by empirical data on real world complex networks, the last few
   years have seen an explosion in proposed generative models to understand
   and explain observed properties of real world networks, including power
   law degree distribution and ``small world{''} distance scaling. In this
   context, a natural question is how to understand the effect of change
   points-how abrupt changes in parameters driving the network model change
   structural properties of the network. We study this phenomenon in one
   popular class of dynamically evolving networks: preferential attachment
   models. We derive asymptotic properties of various functionals of the
   network including the degree distribution as well as maximal degree
   asymptotics, in essence showing that the change point does effect the
   degree distribution but does not change the degree exponent. This
   provides evidence for long range dependence and sensitive dependence of
   the evolution of the network on the initial evolution of the process. We
   propose an estimator for the change point and prove consistency
   properties of this estimator. The methodology developed highlights the
   effect of the nonergodic nature of the evolution of the network on
   classical change point estimators.}},
Publisher = {{INST MATHEMATICAL STATISTICS}},
Address = {{3163 SOMERSET DR, CLEVELAND, OH 44122 USA}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Bhamidi, S (Reprint Author), Univ N Carolina, Dept Stat \& Operat Res, 304 Hanes Hall, Chapel Hill, NC 27599 USA.
   Bhamidi, Shankar; Jin, Jimmy; Nobel, Andrew, Univ N Carolina, Dept Stat \& Operat Res, 304 Hanes Hall, Chapel Hill, NC 27599 USA.}},
DOI = {{10.1214/17-AAP1297}},
ISSN = {{1050-5164}},
Keywords = {{Branching process; Jagers-Nerman stable age distribution; point process;
