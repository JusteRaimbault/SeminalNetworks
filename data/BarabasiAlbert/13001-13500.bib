@inproceedings{ ISI:000453049600007,
Author = {Kazawa, Yui and Tsugawa, Sho},
Editor = {{Cornelius, S and Coronges, K and Goncalves, B and Sinatra, R and Vespignani, A}},
Title = {{Proposal of Strategic Link Addition for Improving the Robustness of
   Multiplex Networks}},
