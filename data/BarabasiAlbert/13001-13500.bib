@article{ ISI:000441652500001,
Author = {Barbero, Javier and Behrens, Kristian and Zofio, Jose L.},
Title = {{Industry location and wages: The role of market size and accessibility
   in trading networks}},
Journal = {{REGIONAL SCIENCE AND URBAN ECONOMICS}},
Year = {{2018}},
Volume = {{71}},
Pages = {{1-24}},
Month = {{JUL}},
Abstract = {{We analyze the effects of local market size and accessibility on the
   spatial distribution of economic activity and wages in general
   equilibrium trade models with many asymmetric countries and costly trade
   for all goods. In models with a homogeneous sector, local market size is
   generally more strongly correlated with a country's industry share,
   whereas accessibility better explains a country's wage. We analytically
   show that result in a simplified case and then confirm it using
   simulations with random trading networks. In models with only
   differentiated sectors, both local market size and accessibility are
   highly correlated with wages. The impact of local market size on
   industry location is more robust than the impact of local market size on
   wages in economic geography models.}},
