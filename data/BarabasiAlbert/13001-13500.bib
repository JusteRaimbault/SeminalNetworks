Title = {{The effect of social proximity on degree distribution of Scale-free
   network}},
Booktitle = {{PROCEEDINGS OF THE INTERNATIONAL ACADEMIC CONFERENCE ON FRONTIERS IN
   SOCIAL SCIENCES AND MANAGEMENT INNOVATION (IAFSM 2018)}},
Series = {{AEBMR-Advances in Economics Business and Management Research}},
