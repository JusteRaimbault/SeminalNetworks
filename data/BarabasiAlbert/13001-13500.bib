@article{ ISI:000449079000007,
Author = {Yoghourdjian, Vahan and Dwyer, Tim and Klein, Karsten and Marriott, Kim
   and Wybrow, Michael},
Title = {{Graph Thumbnails: Identifying and Comparing Multiple Graphs at a Glance}},
Journal = {{IEEE TRANSACTIONS ON VISUALIZATION AND COMPUTER GRAPHICS}},
Year = {{2018}},
Volume = {{24}},
Number = {{12}},
Pages = {{3081-3095}},
Month = {{DEC}},
Abstract = {{We propose Graph Thumbnails, small icon-like visualisations of the
   high-level structure of network data. Graph Thumbnails are designed to
   be legible in small multiples to support rapid browsing within large
   graph corpora. Compared to existing graph-visualisation techniques our
   representation has several advantages: (1) the visualisation can be
   computed in linear time; (2) it is canonical in the sense that
   isomorphic graphs will always have identical thumbnails; and (3) it
   provides precise information about the graph structure. We report the
   results of two user studies. The first study compares Graph Thumbnails
   to node-link and matrix views for identifying similar graphs. The second
   study investigates the comprehensibility of the different
   representations. We demonstrate the usefulness of this representation
   for summarising the evolution of protein-protein interaction networks
   across a range of species.}},
Publisher = {{IEEE COMPUTER SOC}},
Address = {{10662 LOS VAQUEROS CIRCLE, PO BOX 3014, LOS ALAMITOS, CA 90720-1314 USA}},
Type = {{Article}},
Language = {{English}},
