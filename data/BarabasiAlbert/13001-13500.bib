@inproceedings{ ISI:000458680500005,
Author = {Kong Xiao-dan and Zhang Dan},
Editor = {{Tao, M}},
Title = {{The Influence of Knowledge Cooperation on Enterprise Innovation
   Performance in Innovation Network-The Moderating Effect of Network
   Characteristics}},
Booktitle = {{2018 25TH ANNUAL INTERNATIONAL CONFERENCE ON MANAGEMENT SCIENCE \&
   ENGINEERING}},
Series = {{International Conference on Management Science and Engineering-Annual
   Conference Proceedings}},
Year = {{2018}},
Pages = {{31-39}},
Note = {{25th Annual International Conference on Management Science and
   Engineering, Frankfurt, GERMANY, AUG 17-20, 2018}},
Organization = {{Harbin Inst Technol, Sch Management; Chinese Assoc Econ \& Culture
   Commun Germany; IEEE Technol Management Council; Harbin Inst Technol;
   IEEE}},
Abstract = {{Knowledge cooperation based on network platform is an important approach
   for enterprises to gain competitive advantage. This paper based on the
   social network theory and value creation theory, explores the influence
   of knowledge cooperation on enterprise innovation performance, and
   introduces network characteristics as situational variables. The network
   characteristics are divided into two dimensions: network position and
   network relationship. This paper studies network position, network
   relationship and the interaction of both how to moderates the knowledge
   cooperation and innovation performance. Using 53 high-end equipment
   manufacturing enterprises in Shanghai as a sample, the empirical study
   is carried out by hierarchical regression analysis and three-way
   interaction analysis. The results show that, knowledge cooperation has
   the positive effect on enterprise innovation performance. Network
   position moderates the relationship between knowledge cooperation and
   enterprise innovation performance in a flat inverted-U function. Network
   relationship positively moderates the relationship between knowledge
   cooperation and enterprise innovation performance. The interaction
   between network position and network relationship moderates the
   relationship between knowledge cooperation and firm innovation
   performance in a sharp inverted-U function.}},
