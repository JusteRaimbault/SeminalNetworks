@article{ ISI:000450206400001,
Author = {Lin, Zhiwei and Ye, Fanghua and Chen, Chuan and Zheng, Zibin},
Title = {{BGN: Identifying Influential Nodes in Complex Networks via Backward
   Generating Networks}},
