@article{ ISI:000455509900001,
Author = {Elsner, Wolfram},
Title = {{COMPLEXITY AND INNOVATION. WHY BENEFICIAL EFFECTS OF INNOVATION HIGHLY
   DEPEND}},
Journal = {{JOURNAL OF INSTITUTIONAL STUDIES}},
