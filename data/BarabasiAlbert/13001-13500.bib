@inproceedings{ ISI:000455640600204,
Author = {Pasta, Qasim and Zaidi, Faraz},
Editor = {{Brandes, U and Reddy, C and Tagarelli, A}},
Title = {{Model to Generate Benchmark Graphs Based on Evolution Dynamics}},
