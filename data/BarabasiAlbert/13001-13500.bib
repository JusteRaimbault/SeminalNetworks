@article{ ISI:000456681800009,
Author = {Zeng, Jin and Shao, Chenxi and Wang, Xingfu and Miao, Fuyou},
Title = {{Evaluation method for node importance based on attraction between nodes}},
Journal = {{INTERNATIONAL JOURNAL OF MODERN PHYSICS C}},
Year = {{2018}},
Volume = {{29}},
Number = {{12}},
Month = {{DEC}},
Abstract = {{Vital node, which has some special functions, plays an important role
   compared to other nodes in complex networks. Recently, the discovery of
   vital nodes in complex networks has captured increasing attention due to
   their important theoretical significance and great practicability. By
   defining the confidence of the node and the inter-node attraction, the
   significance of the node is measured by the product of the confidence of
   the node and the aggregation of attractions of the node on other nodes
   in the network. The experimental results illustrate that the proposed
   method has higher precision and performs well on various networks with
   different structures.}},
