@article{ ISI:000451897100007,
Author = {Bogner, Kristina and Mueller, Matthias and Schlaile, Michael P.},
Title = {{Knowledge diffusion in formal networks: the roles of degree distribution
   and cognitive distance}},
Journal = {{INTERNATIONAL JOURNAL OF COMPUTATIONAL ECONOMICS AND ECONOMETRICS}},
Year = {{2018}},
Volume = {{8}},
Number = {{3-4}},
Pages = {{388-407}},
Abstract = {{Social networks provide a natural infrastructure for knowledge creation
   and exchange. In this paper, we study the effects of a skewed degree
   distribution within formal networks on knowledge exchange and diffusion
   processes. To investigate how the structure of networks affects
   diffusion performance, we use an agent-based simulation model of four
   theoretical networks as well as an empirical network. Our results
   indicate an interesting effect: neither path length nor clustering
   coefficient is the decisive factor determining diffusion performance but
   the skewness of the link distribution is. Building on the concept of
   cognitive distance, our model shows that even in networks where
   knowledge can diffuse freely, poorly connected nodes are excluded from
   joint learning in networks.}},
