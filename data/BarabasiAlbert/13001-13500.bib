@article{ ISI:000429301500012,
Author = {Tian, Zhanwei and Zhang, Zhuo and Wang, Hongfei and Ma, Li},
Title = {{Complexity analysis on public transport networks of 97 large- and
   medium-sized cities in China}},
Journal = {{INTERNATIONAL JOURNAL OF MODERN PHYSICS B}},
Year = {{2018}},
Volume = {{32}},
Number = {{9}},
Month = {{APR 10}},
Abstract = {{The traffic situation in Chinese urban areas is continuing to
   deteriorate. To make a better planning and designing of the public
   transport system, it is necessary to make profound research on the
   structure of urban public transport networks (PTNs). We investigate 97
   large-and medium-sized cities' PTNs in China, construct three types of
   network models bus stop network, bus transit network and bus line
   network, then analyze the structural characteristics of them. It is
   revealed that bus stop network is small-world and scale-free, bus
   transit network and bus line network are both small-world. Betweenness
   centrality of each city's PTN shows similar distribution pattern,
   although these networks' size is various. When classifying cities
   according to the characteristics of PTNs or economic development level,
   the results are similar. It means that the development of cities'
   economy and transport network has a strong correlation, PTN expands in a
   certain model with the development of economy.}},
