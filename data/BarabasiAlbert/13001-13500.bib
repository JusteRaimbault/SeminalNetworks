@inproceedings{ ISI:000443490600003,
Author = {Iskhakov, Lenar and Kaminski, Bogumil and Mironov, Maksim and Pralat,
   Pawel and Prokhorenkova, Liudmila},
Editor = {{Bonato, A and Pralat, P and Raigorodskii, A}},
Title = {{Clustering Properties of Spatial Preferential Attachment Model}},
Booktitle = {{ALGORITHMS AND MODELS FOR THE WEB GRAPH (WAW 2018)}},
