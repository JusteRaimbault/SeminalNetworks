@article{ ISI:000448164300001,
Author = {Meng, Qing-Kuan and Feng, Dong-Tai and Sun, Yu-Ping and Zhou, Ai-Ping
   and Sun, Yan and Tan, Shu-Gang and Gao, Xu-Tuan},
Title = {{Study on the phase transition of the fractal scale-free networks}},
Journal = {{CHINESE PHYSICS B}},
Year = {{2018}},
Volume = {{27}},
Number = {{10}},
Month = {{OCT}},
Abstract = {{Based on the Ising spin, the phase transition on fractal scale-free
   networks with tree-like skeletons is studied, where the loops are
   generated by local links The degree distribution of the tree-like
   skeleton satisfies the power-law form P(k) similar to k(-delta) It is
   found that when delta >= 3, the renormalized scale-free network will
   have the same degree distribution as the original network. For a special
   case of delta = 4.5, a ferromagnetic to paramagnetic transition is found
   and the critical temperature is determined by the box-covering
   renormalization method. By keeping the structure of the fractal
   scale-free network constant, the numerical relationship between the
   critical temperature and the network size is found, which is the form of
   power law.}},
