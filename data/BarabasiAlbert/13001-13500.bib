@article{ ISI:000430343100005,
Author = {Wang, Qian-Yue and Li, Ya-Qian and Deng, Yu-Jing and Zhao, He-Yao and
   Liu, Bin},
Title = {{A Cascading Failure Model of Directed Scale-Free Topology with
   Hierarchical Structure}},
Journal = {{AD HOC \& SENSOR WIRELESS NETWORKS}},
Year = {{2018}},
Volume = {{40}},
Number = {{3-4, SI}},
Pages = {{237-254}},
Abstract = {{To study cascading failure of a directed hierarchical network, the
   difference between a directed network cascading failure process and an
   undirected network cascading failure process is analyzed. And the
   directed scale-free cascading failure model based on the hierarchical
   structure is proposed. Considering the node in-degree, neighbor-node
   in-degrees and node layers, the initial node load in the directed
   hierarchical network is defined and the proportional relation between
   the node capacity and initial load is established. Hierarchical
   heterogeneity is taken into account for load redistribution after node
   failure so that the additional load is allocated to the same or higher
   layer nodes. Then the relationship between network parameters and
   network robustness is obtained from the analyses of network cascading
   failure. Finally, the model is simulated using Matlab. Through the
   numerical and simulation analyses, the effectiveness of the proposed
   method in this paper is proved and this offers a foundation for further
   research on cascading failures of directed networks.}},
