@article{ ISI:000447318800007,
Author = {Kashyap, G. and Ambika, G.},
Title = {{Mechanisms for tuning clustering and degree-correlations in directed
   networks}},
Journal = {{JOURNAL OF COMPLEX NETWORKS}},
