@article{ ISI:000437208100008,
Author = {Meghanathan, Natarajan},
Title = {{delta-Space for real-world networks: A correlation analysis of decay
   centrality vs. degree centrality and closeness centrality}},
Journal = {{JOURNAL OF KING SAUD UNIVERSITY-COMPUTER AND INFORMATION SCIENCES}},
Year = {{2018}},
Volume = {{30}},
Number = {{3}},
Pages = {{391-403}},
Month = {{JUL}},
Abstract = {{We analyze a suite of 48 real-world networks and compute the decay
   centrality (DEC) of the vertices for the complete range of values for
   the decay parameter delta epsilon (0, 1) as well as determine the
   Pearson's correlation coefficient (PCC) between the DEC delta values and
   degree centrality (DEG) and closeness centrality (CLC). We observe
   PCC(DEC delta, DEG) to decrease with increase in delta and PCC(DEC
   delta, CLC) to decrease with decrease in delta. We define the
   delta-space(r) for a real-world network with respect to the DEG, DEC,
   CLC correlation as the difference between the maximum and minimum delta
   values under which we observe a particular level of correlation (r)
   between the DEC, DEG and DEC, CLC metrics respectively. We show that the
   PCC(DEG, CLC) values for the real-world networks exhibit a very strongly
   positive correlation with the delta-space(r) values and demonstrate that
   one could predict the delta-space(r) value for a real-world network
   using the PCC(DEG, CLC) value for that network. We also analyze the
