@article{ ISI:000428826700011,
Author = {Wu, Jianshe and Dang, Ni and Jiao, Yang},
Title = {{Reconstruction of networks from one-step data by matching positions}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2018}},
Volume = {{497}},
Pages = {{118-125}},
Month = {{MAY 1}},
Abstract = {{It is a challenge in estimating the topology of a network from short
   time series data. In this paper, matching positions is developed to
   reconstruct the topology of a network from only one-step data. We
   consider a general network model of coupled agents, in which the phase
   transformation of each node is determined by its neighbors. From the
   phase transformation information from one step to the next, the
   connections of the tail vertices are reconstructed firstly by the
   matching positions. Removing the already reconstructed vertices, and
   repeatedly reconstructing the connections of tail vertices, the topology
   of the entire network is reconstructed. For sparse scale-free networks
   with more than ten thousands nodes, we almost obtain the actual topology
   using only the one-step data in simulations. (C) 2018 Elsevier B.V. All
   rights reserved.}},
