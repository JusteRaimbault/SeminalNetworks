@article{ ISI:000448013400001,
Author = {Dong, Jian and Chen, Bin and Liu, Liang and Ai, Chuan and Zhang, Fang},
Title = {{The Analysis of Influencing Factors of Information Dissemination on
   Cascade Size Distribution in Social Networks}},
