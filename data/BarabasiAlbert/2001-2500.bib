@article{ ISI:000300413500004,
Author = {Tumminello, Michele and Lillo, Fabrizio and Piilo, Jyrki and Mantegna,
   Rosario N.},
Title = {{Identification of clusters of investors from their real trading activity
   in a financial market}},
Journal = {{NEW JOURNAL OF PHYSICS}},
Year = {{2012}},
Volume = {{14}},
Month = {{JAN 20}},
Abstract = {{We use statistically validated networks, a recently introduced method of
   validating links in a bipartite system, to identify clusters of
   investors trading in a financial market. Specifically, we investigate a
   special database allowing us to track the trading activity of individual
   investors of Nokia stock. We find that many statistically detected
   clusters of investors show a very high degree of synchronization in time
   when they decide to trade and in the trading action taken. We
   investigate the composition of these clusters and find that several of
   them show an over-expression of specific categories of investors.}},
