@article{ ISI:000224302300024,
Author = {Gronlund, A and Holme, P},
Title = {{Networking the seceder model: Group formation in social and economic
   systems}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2004}},
Volume = {{70}},
Number = {{3, 2}},
Month = {{SEP}},
Abstract = {{The seceder model illustrates how the desire to be different from the
   average can lead to formation of groups in a population. We turn the
   original, agent based, seceder model into a model of network evolution.
   We find that the structural characteristics of our model closely match
   empirical social networks. Statistics for the dynamics of group
   formation are also given. Extensions of the model to networks of
   companies are also discussed.}},
