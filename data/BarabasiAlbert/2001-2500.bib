@article{ ISI:000242408800015,
Author = {Leyva, I. and Sendina-Nadal, I. and Almendral, J. A. and Sanjuan, M. A.
   F.},
Title = {{Sparse repulsive coupling enhances synchronization in complex networks}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2006}},
Volume = {{74}},
Number = {{5, 2}},
Month = {{NOV}},
Abstract = {{Through the last years, different strategies to enhance synchronization
   in complex networks have been proposed. In this work, we show that
   synchronization of nonidentical dynamical units that are attractively
   coupled in a small-world network is strongly improved by just making
   phase-repulsive a tiny fraction of the couplings. By a purely
   topological analysis that does not depend on the dynamical model, we
   link the emerging dynamical behavior with the structural properties of
   the sparsely coupled repulsive network.}},
