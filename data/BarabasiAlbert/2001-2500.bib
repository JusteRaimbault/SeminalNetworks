@article{ ISI:000279238400008,
Author = {Arbesman, Samuel and Strogatz, Steven H. and Vitevitch, Michael S.},
Title = {{THE STRUCTURE OF PHONOLOGICAL NETWORKS ACROSS MULTIPLE LANGUAGES}},
Journal = {{INTERNATIONAL JOURNAL OF BIFURCATION AND CHAOS}},
Year = {{2010}},
Volume = {{20}},
Number = {{3}},
Pages = {{679-685}},
Month = {{MAR}},
Abstract = {{The network characteristics based on the phonological similarities in
   the lexicons of several languages were examined. These languages
   differed widely in their history and linguistic structure, but
   commonalities in the network characteristics were observed. These
   networks were also found to be different from other networks studied in
   the literature. The properties of these networks suggest explanations
   for various aspects of linguistic processing and hint at deeper
   organization within the human language.}},
