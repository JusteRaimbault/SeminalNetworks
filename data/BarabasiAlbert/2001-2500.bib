@article{ ISI:000366419400009,
Author = {Wang, Yonggong and Li, Zhenyu and Tyson, Gareth and Uhlig, Steve and
   Xie, Gaogang},
Title = {{Design and Evaluation of the Optimal Cache Allocation for
   Content-Centric Networking}},
