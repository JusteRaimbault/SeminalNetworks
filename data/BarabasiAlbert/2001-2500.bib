@article{ ISI:000180297700002,
Author = {Mirza, BJ and Keller, BJ and Ramakrishnan, N},
Title = {{Studying recommendation algorithms by graph analysis}},
Journal = {{JOURNAL OF INTELLIGENT INFORMATION SYSTEMS}},
Year = {{2003}},
Volume = {{20}},
Number = {{2}},
Pages = {{131-160}},
Month = {{MAR}},
Abstract = {{We present a novel framework for studying recommendation algorithms in
   terms of the `jumps' that they make to connect people to artifacts. This
   approach emphasizes reachability via an algorithm within the implicit
   graph structure underlying a recommender dataset and allows us to
   consider questions relating algorithmic parameters to properties of the
   datasets. For instance, given a particular algorithm `jump,' what is the
   average path length from a person to an artifact? Or, what choices of
   minimum ratings and jumps maintain a connected graph? We illustrate the
   approach with a common jump called the `hammock' using movie recommender
   datasets.}},
