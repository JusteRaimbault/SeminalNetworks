@article{ ISI:000233650000006,
Author = {Rodgers, GJ and Austin, K and Kahng, B and Kim, D},
Title = {{Eigenvalue spectra of complex networks}},
Journal = {{JOURNAL OF PHYSICS A-MATHEMATICAL AND GENERAL}},
Year = {{2005}},
Volume = {{38}},
Number = {{43}},
Pages = {{9431-9437}},
Month = {{OCT 28}},
Abstract = {{We examine the eigenvalue spectrum, rho(mu), of the adjacency matrix of
   a random scale-free network with an average of p edges per vertex using
   the replica method. We show how in the dense limit, when p -> infinity,
   one can obtain two relatively simple coupled equations whose solution
   yields p (4) for an arbitrary complex network. For scale-free graphs,
   with degree distribution exponent lambda, we obtain an exact expression
   for the eigenvalue spectrum when lambda = 3 and show that rho(mu)
   similar to 1/mu(2 lambda-1) for large mu. In the limit lambda ->
   infinity we recover known results for the Erdos-Renyi random graph.}},
