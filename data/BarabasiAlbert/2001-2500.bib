@article{ ISI:000299186500004,
Author = {Bickel, Peter J. and Chen, Aiyou and Levina, Elizaveta},
Title = {{THE METHOD OF MOMENTS AND DEGREE DISTRIBUTIONS FOR NETWORK MODELS}},
