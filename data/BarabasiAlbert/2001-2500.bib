@article{ ISI:000233658700026,
Author = {Valverde, S and Sole, RV},
Title = {{Logarithmic growth dynamics in software networks}},
Journal = {{EUROPHYSICS LETTERS}},
Year = {{2005}},
Volume = {{72}},
Number = {{5}},
Pages = {{858-864}},
Month = {{DEC}},
Abstract = {{In a recent paper, Krapivsky and Redner (Phys. Rev. E, 71 (2005) 036118)
   proposed a new growing network model with new nodes being attached to a
   randomly selected node, as well to all ancestors of the target node. The
   model leads to a sparse graph with an average degree growing
   logarithmically with the system size. Here we present compeling evidence
   for software networks being the result of a similar class of growing
   dynamics. The predicted pattern of network growth, as well as the
   stationary in- and out-degree distributions are consistent with the
   model. Our results confirm the view of large-scale software topology
   being generated through duplication-rewiring mechanisms. Implications of
   these findings are outlined.}},
