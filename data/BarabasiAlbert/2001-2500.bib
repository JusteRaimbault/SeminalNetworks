@article{ ISI:000310977600002,
Author = {Dal'Maso Peron, Thomas Kaue and Rodrigues, Francisco A.},
Title = {{Determination of the critical coupling of explosive synchronization
   transitions in scale-free networks by mean-field approximations}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2012}},
Volume = {{86}},
Number = {{5, 2}},
Month = {{NOV 12}},
Abstract = {{An explosive synchronization can be observed in scale-free networks when
   Kuramoto oscillators have natural frequencies equal to their number of
   connections. The present paper reports on mean-field approximations to
   determine the critical coupling of such explosive synchronization. It
   has been verified that the equation obtained for the critical coupling
   has an inverse dependence on the network average degree. This expression
   differs from those whose frequency distributions are unimodal and even.
   In this case, the critical coupling depends on the ratio between the
   first and second statistical moments of the degree distribution.
   Numerical simulations were also conducted to verify our analytical
   results.}},
