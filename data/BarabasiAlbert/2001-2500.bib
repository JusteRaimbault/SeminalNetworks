@article{ ISI:000224495900018,
Author = {Vega, YM and Vazquez-Prada, M and Pacheco, AF},
Title = {{Fitness for synchronization of network motifs}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2004}},
Volume = {{343}},
Pages = {{279-287}},
Month = {{NOV 15}},
Abstract = {{We study the synchronization of Kuramoto's oscillators in small parts of
   networks known as motifs. We first report on the system dynamics for the
   case of a scale-free network and show the existence of a non-trivial
   critical point. We compute the probability that network motifs
   synchronize, and find that the fitness for synchronization correlates
   well with motif's interconnectedness and structural complexity. Possible
   implications for present debates about network evolution in biological
   and other systems are discussed. (C) 2004 Elsevier B.V. All rights
   reserved.}},
