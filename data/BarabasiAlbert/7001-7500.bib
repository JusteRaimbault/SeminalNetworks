@article{ ISI:000281449800015,
Author = {Pu Cun-Lai and Pei Wen-Jiang and Miao Rui-Hua and Zhou Si-Yuan and Wang
   Kai},
Title = {{Study on queue resource allocation in scale-free networks}},
Journal = {{ACTA PHYSICA SINICA}},
Year = {{2010}},
Volume = {{59}},
Number = {{9}},
Pages = {{6009-6013}},
Month = {{SEP}},
Abstract = {{In this article, information traffic dynamics in scale-free networks
   with limited queue resource is investigated firstly. It is found that
   the network transits from a free flow state to full congestion state
   with the increase in the packet generation rate. Then, a queue resource
   allocation model based on node degree is proposed. The core of this
   model is to make the queue length of node i proportional to k(i)(beta)
   (k(i) being the degree of node i and beta being an allocation
   parameter). Simulations show that the capacity of the scale-free network
   is maximal when beta is about 1.25 for the shortest path algorithm used
   to transfer information. Furthermore, the optimal value of allocation
   parameter p is irrelevant to the network size and the amount of queue
   resource.}},
