@inproceedings{ ISI:000401813700060,
Author = {Marinho, Vanessa Queiroz and Hirst, Graeme and Amancio, Diego Raphael},
Book-Group-Author = {{IEEE}},
Title = {{Authorship attribution via network motifs identification}},
Booktitle = {{PROCEEDINGS OF 2016 5TH BRAZILIAN CONFERENCE ON INTELLIGENT SYSTEMS
   (BRACIS 2016)}},
