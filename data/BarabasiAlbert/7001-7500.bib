@article{ ISI:000398723600001,
Author = {Wehmuth, Klaus and Fleury, Eric and Ziviani, Artur},
Title = {{MultiAspect Graphs: Algebraic Representation and Algorithms}},
Journal = {{ALGORITHMS}},
