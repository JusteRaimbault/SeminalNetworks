@article{ ISI:000267991600038,
Author = {Zhang Hai-Feng and Small, Michael and Fu Xin-Chu},
Title = {{Different Epidemic Models on Complex Networks}},
Journal = {{COMMUNICATIONS IN THEORETICAL PHYSICS}},
