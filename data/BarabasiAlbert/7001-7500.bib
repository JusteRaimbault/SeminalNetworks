@article{ ISI:000298296200005,
Author = {Smaragdakis, Georgios and Laoutaris, Nikolaos and Lekakis, Vassilis and
   Bestavros, Azer and Byers, John W. and Roussopoulos, Mema},
Title = {{Selfish Overlay Network Creation and Maintenance}},
Journal = {{IEEE-ACM TRANSACTIONS ON NETWORKING}},
