@inproceedings{ ISI:000252834500009,
Author = {Volkovich, Yana and Litvak, Nelly and Donato, Debora},
Editor = {{Bonato, A and Chung, FRK}},
Title = {{Determining factors behind the PageRank log-log plot}},
Booktitle = {{ALGORITHMS AND MODELS FOR THE WEB-GRAPH}},
Series = {{Lecture Notes in Computer Science}},
Year = {{2007}},
Volume = {{4863}},
Pages = {{108+}},
Note = {{5th International Workshop on Algorithms and Models for the Web-Graph,
   San Diego, CA, DEC 11-12, 2007}},
Organization = {{Calif Inst Telecommun \& Informat Technol; Google; Yahoo Res; Natl Sci
   Fdn; Springer, Lecture Notes Comp Sci; Univ Calif, San Diego}},
Abstract = {{We study the relation between PageRank and other parameters of
   information networks such as in-degree, out-degree, and the fraction of
   dangling nodes. We model this relation through a stochastic equation
   inspired by the original definition of PageRank. Further, we use the
   theory of regular variation to prove that PageRank and in-degree follow
   power laws with the same exponent. The difference between these two
   power laws is in a multiplicative constant, which depends mainly on the
   fraction of dangling nodes, average in-degree, the power law exponent,
   and the damping factor. The out-degree distribution has a minor effect,
   which we explicitly quantify. Finally, we propose a ranking scheme which
   does not depend on out-degrees.}},
