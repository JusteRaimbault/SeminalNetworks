@article{ ISI:000324268500005,
Author = {Li, Yan and Han, Dong},
Title = {{A Two-Stage Contact Process on Scale-Free Networks}},
Journal = {{JOURNAL OF STATISTICAL PHYSICS}},
Year = {{2013}},
Volume = {{153}},
Number = {{2}},
Pages = {{312-324}},
Month = {{OCT}},
Abstract = {{We study a two-stage contact process on scale-free networks as a model
   for the spread of epidemics. We show that any virus starting from a
   single vertex with arbitrarily small infection rates can last for a
   super-polynomial time with positive probability if the power law
   exponent alpha > 2. This is in sharp contrast with the mean-field
   analysis. The estimation of the metastable density is also provided.}},
