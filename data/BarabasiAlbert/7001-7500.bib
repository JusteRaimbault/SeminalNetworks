@article{ ISI:000237495400021,
Author = {Gu, Zhi-Ming and Zhou, Tao and Wang, Bing-Hong and Yan, Gang and Zhu,
   Chen-Ping and Fu, Zhong-Qian},
Title = {{Simplex triangulation induced scale-free networks}},
