@article{ ISI:000318408100031,
Author = {Ling, Xiang and Hu, Mao-Bin and Ding, Jian-Xun and Shi, Qing and Jiang,
   Rui},
Title = {{Effects of target routing model on the occurrence of extreme events in
   complex networks}},
Journal = {{EUROPEAN PHYSICAL JOURNAL B}},
Year = {{2013}},
Volume = {{86}},
Number = {{4}},
Month = {{APR}},
Abstract = {{This paper investigates the effect of routing protocol on the occurrence
   of extreme events (EE) in complex networks, as an extension of {[}V.
   Kishore, M. S. Santhanam, R. E. Amritkar, Phys. Rev. Lett. 106, 188701
   (2011)]. The target routing model {[}W. X. Wang, B. H. Wang, C.Y. Yin,
   Y.B. Xie, T. Zhou, Phys. Rev. E 73, 026111 (2006)] is considered. In the
   model, a tunable power parameter alpha controls the packets' preference
   of forwarding direction. We derive exact expressions for the stationary
   distribution probability of packets and estimate the occurrence
   probability of EE on the nodes. The occurrence of EE strongly depends on
   the routing parameter. For Barabasi-Albert scale-free network,
   Erdos-Renyi random network and Newman-Watts small-world network, it is
   shown that the minimal occurrence of EE is achieved at alpha = -1.}},
