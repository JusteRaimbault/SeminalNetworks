@article{ ISI:000267061000011,
Author = {Yang, Renhuan and Song, Aiguo},
Title = {{SCALING PROPERTIES OF AN EVOLVING NETWORK MODEL WITH AGING MECHANISM}},
Journal = {{INTERNATIONAL JOURNAL OF MODERN PHYSICS C}},
