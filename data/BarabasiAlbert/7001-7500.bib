@article{ ISI:000371087000004,
Author = {Zhang, Xiaojun and He, Zheng and Rayman-Bacchus, Lez},
Title = {{Random Birth-and-Death Networks}},
Journal = {{JOURNAL OF STATISTICAL PHYSICS}},
