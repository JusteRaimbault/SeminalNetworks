@article{ ISI:000287920400003,
Author = {Hu, T. -M. and Hayton, W. L.},
Title = {{Architecture of the drug-drug interaction network}},
Journal = {{JOURNAL OF CLINICAL PHARMACY AND THERAPEUTICS}},
