@article{ ISI:000269353300008,
Author = {Avetisov, V. A. and Chertovich, A. V. and Nechaev, S. K. and Vasilyev,
   O. A.},
Title = {{On scale-free and poly-scale behaviors of random hierarchical networks}},
Journal = {{JOURNAL OF STATISTICAL MECHANICS-THEORY AND EXPERIMENT}},
Year = {{2009}},
Month = {{JUL}},
Abstract = {{In this paper the question of statistical properties of
   block-hierarchical random matrices is raised for the first time in
   connection with structural characteristics of random hierarchical
   networks obtained by a `mipmapping' procedure. In particular, we compute
   numerically the spectral density of large random adjacency matrices de
   fined via a hierarchy of the Bernoulli distributions \{q(1), q(2), ...\}
