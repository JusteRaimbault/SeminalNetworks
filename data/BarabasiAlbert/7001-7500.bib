@article{ ISI:000389354200003,
Author = {Dadlani, Aresh and Kumar, Muthukrishnan Senthil and Murugan, Suvi and
   Kim, Kiseon},
Title = {{System Dynamics of a Refined Epidemic Model for Infection Propagation
   Over Complex Networks}},
Journal = {{IEEE SYSTEMS JOURNAL}},
