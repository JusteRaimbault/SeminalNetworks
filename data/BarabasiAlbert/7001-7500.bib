@inproceedings{ ISI:000393395800273,
Author = {Goodwine, Bill},
Book-Group-Author = {{IEEE}},
Title = {{Fractional-Order Dynamics in a Random, Approximately Scale-Free Network
   of Agents}},
Booktitle = {{2014 13TH INTERNATIONAL CONFERENCE ON CONTROL AUTOMATION ROBOTICS \&
