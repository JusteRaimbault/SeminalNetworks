@article{ ISI:000249374700005,
Author = {Rodriguez, Victor and Janssens, Frizo and Debackere, Koenraad and De
   Moor, Bart},
Title = {{Material transfer agreements and collaborative publication activity: The
   case of a biotechnology network}},
Journal = {{RESEARCH EVALUATION}},
Year = {{2007}},
Volume = {{16}},
Number = {{2}},
Pages = {{123-136}},
Month = {{JUN}},
Abstract = {{Although material transfer agreements may be useful to exchange research
   materials between laboratories, academics and policymakers have
   suggested that the trend towards their standardisation might impede the
   progress of science by constraining one type of research collaboration:
   the co-publication activity of organisations. For that reason, we
   examine whether involvement in those agreements affects the pattern of
   co-publication activity. The construct co-publication activity is
   thereby operationalised on the basis of a bibliometric approach. The
   organisations and their collaboration are described with the help of
   graph theory. According to our interpretation of findings, material
   transfer agreements might not have interfered in such a way to limit
   co-publication activity of research organisations in the network.}},
