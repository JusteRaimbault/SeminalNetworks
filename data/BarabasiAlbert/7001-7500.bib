@article{ ISI:000179176300026,
Author = {Kim, JW and Hunt, B and Ott, E},
Title = {{Evolving networks with multispecies nodes and spread in the number of
   initial links}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2002}},
Volume = {{66}},
Number = {{4, 2}},
Month = {{OCT}},
Abstract = {{We consider models for growing networks incorporating two effects not
   previously considered: (i) different species of nodes, with each species
   having different properties (such as different attachment probabilities
   to other node species) and (ii) when a new node is created, its number
   of links to old nodes is random with a given probability distribution.
   Our numerical simulations show good agreement with analytic solutions.
   As an application of our model, we investigate the movie-actor network
   with movies considered as nodes and actors as links.}},
