@article{ ISI:000270146600006,
Author = {Zheng, Zhigang and Feng, Xiaoqin and Ao, Bin and Cross, Michael C.},
Title = {{Synchronization of groups of coupled oscillators with sparse connections}},
Journal = {{EPL}},
Year = {{2009}},
Volume = {{87}},
Number = {{5}},
Month = {{SEP}},
Abstract = {{Synchronization of groups of coupled oscillators with sparse connections
   are explored. It is found that different topologies of intergroup
   couplings may lead to different synchronizability. In the
   strong-coupling limit, an analytical treatment and criterion is proposed
   to judge the synchronization between communities of oscillators, and an
   optimal connection scheme for the group synchronization is given. By
   varying the intergroup and intragroup coupling strengths, different
   synchronous phases, i.e., the unsynchronized state, intragroup
   synchronization, intergroup synchronization, and global synchronization
   are revealed. The present discussions and results can be applied to
   study the pattern formation and synchronization of coupled
   spatiotemporal systems. Copyright (C) EPLA, 2009}},
