@inproceedings{ ISI:000305337100027,
Author = {Guns, Raf},
Editor = {{Noyons, E and Ngulube, P and Leta, J}},
Title = {{Bipartite networks for link prediction: Can they improve prediction
   performance?}},
Booktitle = {{PROCEEDINGS OF ISSI 2011: THE 13TH CONFERENCE OF THE INTERNATIONAL
   SOCIETY FOR SCIENTOMETRICS AND INFORMETRICS, VOLS 1 AND 2}},
Series = {{Proceedings of the International Conference on Scientometrics and
   Informetrics}},
