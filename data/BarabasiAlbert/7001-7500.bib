@article{ ISI:000368529100046,
Author = {Ouyang, Bo and Jiang, Lurong and Teng, Zhaosheng},
Title = {{A Noise-Filtering Method for Link Prediction in Complex Networks}},
Journal = {{PLOS ONE}},
Year = {{2016}},
Volume = {{11}},
Number = {{1}},
Month = {{JAN 20}},
Abstract = {{Link prediction plays an important role in both finding missing links in
   networked systems and complementing our understanding of the evolution
   of networks. Much attention from the network science community are paid
   to figure out how to efficiently predict the missing/future links based
   on the observed topology. Real-world information always contain noise,
   which is also the case in an observed network. This problem is rarely
   considered in existing methods. In this paper, we treat the existence of
   observed links as known information. By filtering out noises in this
   information, the underlying regularity of the connection information is
   retrieved and then used to predict missing or future links. Experiments
   on various empirical networks show that our method performs noticeably
   better than baseline algorithms.}},
