@article{ ISI:000229904900016,
Author = {Pulkkinen, O and Merikoski, J},
Title = {{Phase transitions on Markovian bipartite graphs - an application of the
   zero-range process}},
Journal = {{JOURNAL OF STATISTICAL PHYSICS}},
