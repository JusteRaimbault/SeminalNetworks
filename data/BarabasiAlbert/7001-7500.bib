@article{ ISI:000371223000002,
Author = {Morita, Satoru},
Title = {{Six Susceptible-Infected-Susceptible Models on Scale-free Networks}},
Journal = {{SCIENTIFIC REPORTS}},
Year = {{2016}},
Volume = {{6}},
Month = {{MAR 3}},
Abstract = {{Spreading phenomena are ubiquitous in nature and society. For example,
   disease and information spread over underlying social and information
   networks. It is well known that there is no threshold for spreading
   models on scale-free networks; this suggests that spread can occur on
   such networks, regardless of how low the contact rate may be. In this
   paper, I consider six models with different contact and propagation
   mechanisms, which include models studied so far, but are apt to be
   confused. To compare these six models, I analyze them by degree-based
   mean-field theory. I find that the result depends on the details of
   contact and propagation mechanism.}},
