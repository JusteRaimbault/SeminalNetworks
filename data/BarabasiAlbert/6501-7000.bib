@article{ ISI:000376693600013,
Author = {Nie, Tingyuan and Guo, Zheng and Zhao, Kun and Lu, Zhe-Ming},
Title = {{The dynamic correlation between degree and betweenness of complex
   network under attack}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2016}},
Volume = {{457}},
Pages = {{129-137}},
Month = {{SEP 1}},
Abstract = {{Complex networks are often subjected to failure and attack. Recent work
   has addressed the resilience of complex networks to either random or
   intentional deletion of nodes or links. Here we simulate the breakdown
   of the small-world network and the scale-free network under node failure
