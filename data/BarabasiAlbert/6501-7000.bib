@article{ ISI:000402733100015,
Author = {Li, Zhepeng and Fang, Xiao and Bai, Xue and Sheng, Olivia R. Liu},
Title = {{Utility-Based Link Recommendation for Online Social Networks}},
Journal = {{MANAGEMENT SCIENCE}},
Year = {{2017}},
Volume = {{63}},
