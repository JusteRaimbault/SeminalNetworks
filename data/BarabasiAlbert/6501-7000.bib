@article{ ISI:000305401000003,
Author = {Zhang, Lanhua and Li, Yujuan and Wang, Mei and Wang, Xiujuan and Xue,
   Shaowei and Cao, Chen},
Title = {{A novel deterministic hybrid complex network model created by
   inner-outer iteration}},
Journal = {{NONLINEAR DYNAMICS}},
