@article{ ISI:000262914700002,
Author = {Cont, Rama and Tanimura, Emily},
Title = {{SMALL-WORLD GRAPHS: CHARACTERIZATION AND ALTERNATIVE CONSTRUCTIONS}},
Journal = {{ADVANCES IN APPLIED PROBABILITY}},
Year = {{2008}},
Volume = {{40}},
Number = {{4}},
Pages = {{939-965}},
Month = {{DEC}},
Abstract = {{Small-world graphs are examples of random graphs which mimic empirically
   observed features of social networks. We propose an intrinsic definition
   of small-world graphs, based on a probabilistic formulation of scaling
   properties of the graph, which does not rely on any particular
   construction. Our definition is shown to encompass existing models of
   small world graphs, proposed by Watts (1999) and studied by Barbour and
   Reinert (2001), which are based on random perturbations of a regular
   lattice. We also propose alternative constructions ofst-nall-world
   graphs which are not based on lattices and Study their scaling
   properties.}},
