@article{ ISI:000301819100003,
Author = {Castelucio, Andre and Gomes, Antonio Tadeu A. and Ziviani, Artur and
   Salles, Ronaldo M.},
Title = {{Intra-domain IP traceback using OSPF}},
Journal = {{COMPUTER COMMUNICATIONS}},
Year = {{2012}},
Volume = {{35}},
Number = {{5}},
Pages = {{554-564}},
Month = {{MAR 1}},
Abstract = {{Denial of service (DoS) attacks are a serious threat to the appropriate
   operation of services within network domains. In this paper, we propose
   a system called OsTraS (OSPF-based Traceback System) that helps network
   operators to deal with this threat by creating an overlay network for
   intra-domain IF traceback. The main contribution of our proposal with
   respect to previous work is its ability to provide partial and
   progressive deployment of the traceback system throughout a monitored
   network domain. The OsTraS system builds its overlay network using the
   OSPF routing protocol through the definition of an Opaque LSA (Link
   State Advertisement) specially conceived for this purpose. We
   investigate and evaluate the performance of partial and progressive
   deployment of the proposed system, showing its suitability even for
   large network domains. (C) 2010 Elsevier B.V. All rights reserved.}},
