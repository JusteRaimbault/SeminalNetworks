@inproceedings{ ISI:000273957000008,
Author = {di Nitto, Elisabetta and Dubois, Daniel J. and Mirandola, Raffaela},
Book-Group-Author = {{IEEE}},
Title = {{On Exploiting Decentralized Bio-inspired Self-organization Algorithms to
   Develop Real Systems}},
