@article{ ISI:000409094000014,
Author = {Lau, Matthew K. and Borrett, Stuart R. and Baiser, Benjamin and Gotelli,
   Nicholas J. and Ellison, Aaron M.},
Title = {{Ecological network metrics: opportunities for synthesis}},
Journal = {{ECOSPHERE}},
Year = {{2017}},
Volume = {{8}},
Number = {{8}},
Month = {{AUG}},
Abstract = {{Network ecology provides a systems basis for approaching ecological
   questions, such as factors that influence biological diversity, the role
   of particular species or particular traits in structuring ecosystems,
   and long-term ecological dynamics (e.g., stability). Whereas the
   introduction of network theory has enabled ecologists to quantify not
   only the degree, but also the architecture of ecological complexity,
   these advances have come at the cost of introducing new challenges,
   including new theoretical concepts and metrics, and increased data
   complexity and computational intensity. Synthesizing recent developments
   in the network ecology literature, we point to several potential
   solutions to these issues: integrating network metrics and their
   terminology across sub-disciplines; benchmarking new network algorithms
   and models to increase mechanistic understanding; and improving tools
