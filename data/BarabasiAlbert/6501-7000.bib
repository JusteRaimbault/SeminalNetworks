@article{ ISI:000320838600004,
Author = {Lemmouchi, Slimane and Haddad, Mohammed and Kheddouci, Hamamache},
Title = {{Robustness study of emerged communities from exchanges in peer-to-peer
   networks}},
Journal = {{COMPUTER COMMUNICATIONS}},
Year = {{2013}},
Volume = {{36}},
Number = {{10-11}},
Pages = {{1145-1158}},
Month = {{JUN}},
Abstract = {{The study of emerged community structure is an important challenge in
   networks analysis. In fact, several methods have been proposed in the
   literature to statistically determine the significance of discovered
   structures. Nevertheless, most of existing analysis models consider only
   the structural aspect of emerged communities. We are interested in
   studying the robustness of emerged communities in peer-to-peer (P2P)
   networks. More precisely, we consider the emerged communities in the
   induced graph by all the exchanges in these networks. Hence, rather than
   examining the robustness only on the structural properties of the graph,
   we will focus on the parameters that allow the emergence of community
   structures. In fact, perturbing these parameters might destroy most of
   the obtained properties at the emerged level. To the best of our
   knowledge, robustness of networks has never been considered from this
   angle before. In this paper, we study the impact of perturbing the
   content and the profile of nodes on the emerged communities in P2P
   networks. We show how these alterations affect both structure and
   information supported by the emerged structures. (C) 2013 Elsevier B.V.
   All rights reserved.}},
