@article{ ISI:000396045900021,
Author = {Fedotov, Sergei and Stage, Helena},
Title = {{Anomalous Metapopulation Dynamics on Scale-Free Networks}},
Journal = {{PHYSICAL REVIEW LETTERS}},
Year = {{2017}},
Volume = {{118}},
Number = {{9}},
Month = {{MAR 3}},
Abstract = {{We model transport of individuals across a heterogeneous scale-free
   network where a few weakly connected nodes exhibit heavy-tailed
   residence times. Using the empirical law of the axiom of cumulative
   inertia and fractional analysis, we show that ``anomalous cumulative
   inertia{''} overpowers highly connected nodes in attracting network
   individuals. This fundamentally challenges the classical result that
   individuals tend to accumulate in high-order nodes. The derived
   residence time distribution has a nontrivial U shape which we encounter
   empirically across human residence and employment times.}},
