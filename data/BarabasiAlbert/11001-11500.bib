@inproceedings{ ISI:000440865800001,
Author = {van der Hofstad, Remco},
Editor = {{Bodlaender, HL and Woeginger, GJ}},
Title = {{Counting Graphs and Null Models of Complex Networks: Configuration Model
   and Extensions}},
Booktitle = {{GRAPH-THEORETIC CONCEPTS IN COMPUTER SCIENCE (WG 2017)}},
