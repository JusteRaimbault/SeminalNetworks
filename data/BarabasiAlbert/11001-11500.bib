@article{ ISI:000442799000001,
Author = {Tomimori, Masashi and Sugawara, Shinji},
Title = {{Content sharing method using expected acquisition rate in hybrid
   peer-to-peer networks with cloud storages}},
Journal = {{INTERNATIONAL JOURNAL OF SPACE-BASED AND SITUATED COMPUTING}},
Year = {{2017}},
Volume = {{7}},
Number = {{4}},
Pages = {{187-196}},
Abstract = {{Recently, various content sharing methods that are available on
   peer-to-peer (P2P) networks, have been actively researched; a P2P
   network generally consists of a number of peers and evenly connected to
   each other. In most of previous studies, shared content items are
   supposed to be kept only in the storages of the peers, and effective
   ways of content sharing under the environment where not only the
   storages of peers, but also ones of a cloud system are available have
   not sufficiently researched yet. In this paper, we propose an efficient
   content sharing method in which an appropriate content acquisition rate
   can be set preliminarily according to the payable storage cost, and
   actually the rate can be maintained by keeping sufficient number of
   content replicas under the environment where storages of both peers and
   a cloud system are available.}},
