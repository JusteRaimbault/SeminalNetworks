@inproceedings{ ISI:000443490600008,
Author = {Blaesius, Thomas and Friedrich, Tobias and Katzmann, Maximilian and
   Krohmer, Anton and Striebel, Jonathan},
Editor = {{Bonato, A and Pralat, P and Raigorodskii, A}},
Title = {{Towards a Systematic Evaluation of Generative Network Models}},
Booktitle = {{ALGORITHMS AND MODELS FOR THE WEB GRAPH (WAW 2018)}},
Series = {{Lecture Notes in Computer Science}},
Year = {{2018}},
Volume = {{10836}},
Pages = {{99-114}},
Note = {{15th International Workshop on Algorithms and Models for the Web Graph
   (WAW), Moscow Inst Phys \& Technol, Moscow, RUSSIA, MAY 17-18, 2018}},
Organization = {{Microsoft Res New England; Google Res; Yandex; Internet Math}},
Abstract = {{Generative graph models play an important role in network science.
   Unlike real-world networks, they are accessible for mathematical
   analysis and the number of available networks is not limited. The
   explanatory power of results on generative models, however, heavily
   depends on how realistic they are. We present a framework that allows
   for a systematic evaluation of generative network models. It is based on
   the question whether real-world networks can be distinguished from
   generated graphs with respect to certain graph parameters.
   As a proof of concept, we apply our framework to four popular random
   graph models (Erdos-Renyi, Barabasi-Albert, Chung-Lu, and hyperbolic
   random graphs). Our experiments for example show that all four models
   are bad representations for Facebook's social networks, while Chung-Lu
   and hyperbolic random graphs are good representations for other
   networks, with different strengths and weaknesses.}},
