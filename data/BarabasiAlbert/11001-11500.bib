@article{ ISI:000418370200022,
Author = {Lebichot, Bertrand and Saerens, Marco},
Title = {{A bag-of-paths node criticality measure}},
Journal = {{NEUROCOMPUTING}},
Year = {{2018}},
Volume = {{275}},
Pages = {{224-236}},
Month = {{JAN 31}},
Abstract = {{This work compares several node (and network) criticality measures
   quantifying to which extend each node is critical with respect to the
   communication flow between nodes of the network, and introduces a new
   measure based on the Bag-of-Paths (BoP) framework. Network disconnection
   simulation experiments show that the new BoP measure outperforms all the
   other tested measures on a sample of Erdos-Renyi and Albert-Barabasi
   graphs. Furthermore, a faster (but still O (n(3))), approximate, BoP
