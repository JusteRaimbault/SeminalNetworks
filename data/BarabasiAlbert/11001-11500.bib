@inproceedings{ ISI:000390760100046,
Author = {Morzy, Mikolaj and Kazienko, Przemyslaw and Kajdanowicz, Tomasz},
Editor = {{Kumar, R and Caverlee, J and Tong, H}},
Title = {{Priority Rank Model for Social Network Generation}},
Booktitle = {{PROCEEDINGS OF THE 2016 IEEE/ACM INTERNATIONAL CONFERENCE ON ADVANCES IN
   SOCIAL NETWORKS ANALYSIS AND MINING ASONAM 2016}},
Year = {{2016}},
Pages = {{315-318}},
