@article{ ISI:000407925400001,
Author = {Cao, Chunjie and Zhang, Zhiqiang and Sun, Jingzhang and Wang, Xianpeng
   and Huang, Mengxing},
Title = {{Vulnerability Analysis of Interdependent Scale-Free Networks with
   Complex Coupling}},
Journal = {{JOURNAL OF ELECTRICAL AND COMPUTER ENGINEERING}},
Year = {{2017}},
Abstract = {{Recent studies have shown that random nodes are vulnerable in
   interdependent networks with simple coupling. However, relationships in
   actual networks are interrelated and complex coupling. This paper
   analyzes the vulnerability of interdependent scale-free networks with
   complex coupling based on the BA model. The results indicate that these
   networks have the same vulnerability against themaximum node attack, the
   load of the maximum node attack, and the random node attack, which
   explain that the coupling relationship between network nodes is an
   important factor in network design.}},
