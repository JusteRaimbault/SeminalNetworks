Language = {{English}},
Affiliation = {{Jalili, M (Reprint Author), RMIT Univ, Sch Engn, Melbourne, Vic, Australia.
   Hamedmoghadam, Homayoun, Monash Univ, Fac Engn, Melbourne, Vic, Australia.
   Jalili, Mandi; Yu, Xinghuo, RMIT Univ, Sch Engn, Melbourne, Vic, Australia.}},
DOI = {{10.1016/j.physa.2017.08.048}},
ISSN = {{0378-4371}},
EISSN = {{1873-2119}},
