@article{ ISI:000453725200001,
Author = {Si, Shuaizong and Wang, Jinfa and Yu, Chong and Zhao, Hai},
Title = {{Ensgy-Efficient and Fault-Tolerant Evolution Models Based on Link
   Prediction for Large-Scale Wireless Sensor Networks}},
Journal = {{IEEE ACCESS}},
