Title = {{GRM: Group Regularity Mobility Model}},
Booktitle = {{PROCEEDINGS OF THE 20TH ACM INTERNATIONAL CONFERENCE ON MODELLING,
   ANALYSIS AND SIMULATION OF WIRELESS AND MOBILE SYSTEMS (MSWIM'17)}},
Year = {{2017}},
