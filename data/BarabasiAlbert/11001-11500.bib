@article{ ISI:000411311600001,
Author = {Antonioni, Alberto and Tomassini, Marco},
Title = {{A growing social network model in geographical space}},
Journal = {{JOURNAL OF STATISTICAL MECHANICS-THEORY AND EXPERIMENT}},
Year = {{2017}},
Month = {{SEP}},
Abstract = {{In this work we propose a new model for the generation of social
   networks that includes their often ignored spatial aspects. The model is
   a growing one and links are created either taking space into account, or
   disregarding space and only considering the degree of target nodes.
   These two effects can be mixed linearly in arbitrary proportions through
   a parameter. We numerically show that for a given range of the
   combination parameter, and for given mean degree, the generated network
   class shares many important statistical features with those observed in
   actual social networks, including the spatial dependence of connections.
   Moreover, we show that the model provides a good qualitative fit to some
   measured social networks.}},
