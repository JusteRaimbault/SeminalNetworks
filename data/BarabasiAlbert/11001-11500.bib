@article{ ISI:000402046200017,
Author = {Jiang, Nan and Li, Bin and Pan, Pingan and Wan, Tao and Liu, Lingfeng},
Title = {{C-POEM: comprehensive performance optimization evaluation model for
   wireless sensor networks}},
