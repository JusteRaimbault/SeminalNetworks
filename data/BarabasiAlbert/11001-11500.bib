@inproceedings{ ISI:000437040400024,
Author = {Xu, Shuaishuai and Zhou, Yinzuo and Zhang, Zike},
Editor = {{Cheng, X and Ma, W and Liu, H and Shen, H and Feng, S and Xie, X}},
Title = {{Estimating the Origin of Diffusion in Complex Networks with Limited
   Observations}},
Booktitle = {{SOCIAL MEDIA PROCESSING, SMP 2017}},
Series = {{Communications in Computer and Information Science}},
Year = {{2017}},
Volume = {{774}},
Pages = {{296-307}},
Note = {{6th National Conference on Social Media Processing (SMP), Beijing,
   PEOPLES R CHINA, SEP 14-17, 2017}},
Organization = {{Chinese Informat Proc Soc China}},
Abstract = {{A disease propagating in a community or a rumor spreading in a social
   network can be described by a contact network whose nodes are persons or
   centers of contagion and links heterogeneous relations among them.
   Suppose that a disease or a rumor originating from a single source among
   a set of suspects spreads in a network, how to locate this disease/rumor
   source based on a limited set of observations? We study the problem of
   estimating the origin of a disease/rumor outbreak: given a contact
   network and a snapshot of epidemic spread at a certain time, root out
   the infection source. Assuming that the epidemic spread follows the
   usual susceptible-infected (SI) model, we introduce an inference
   algorithm based on sparsely placed observers. We present an algorithm
   which utilizes the correlated information between the network structure
   (shortest paths) and the diffusion dynamics (time sequence of
   infection). The numerical results of artificial and empirical networks
   show that it leads to significant improvement of performance compared to
   existing approaches. Our analysis sheds insight into the behavior of the
   disease/rumor spreading process not only in the local particular regime
   but also for the whole general network.}},
