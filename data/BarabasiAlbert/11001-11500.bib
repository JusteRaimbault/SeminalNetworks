@article{ ISI:000386212000002,
Author = {Norrenbrock, Christoph},
Title = {{Percolation threshold on planar Euclidean Gabriel graphs}},
Journal = {{EUROPEAN PHYSICAL JOURNAL B}},
Year = {{2016}},
Volume = {{89}},
Number = {{5}},
Month = {{MAY 2}},
Abstract = {{In the present article, numerical simulations have been performed to
   find the bond and site percolation thresholds on two-dimensional Gabriel
   graphs (GG) for Poisson point processes. GGs belong to the family of
   ``proximity graphs{''} and are discussed, e.g., in context of the
   construction of backbones for wireless ad-hoc networks. Finite-size
   scaling analyses have been performed to find the critical points and
   critical exponents nu, beta and gamma. The critical exponents obtained
   this way verify that the associated universality class is that of
   standard 2D percolation.}},
