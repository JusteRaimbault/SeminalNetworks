@article{ ISI:000395251600018,
Author = {Khan, Gohar Feroz and Wood, Jacob},
Title = {{Knowledge Networks of the Information Technology Management Domain: A
   Social Network Analysis Approach}},
Journal = {{COMMUNICATIONS OF THE ASSOCIATION FOR INFORMATION SYSTEMS}},
Year = {{2016}},
Volume = {{39}},
Pages = {{367-397}},
Abstract = {{Using the social network analysis technique, we decomposed the knowledge
   networks of the information technology management (ITM) domain. We
   included a total of 893 papers published during the 1995-2014 period in
   the network analysis. From this domain, the network and ego level
   properties-such as, degree centralities, density, components, structural
   holes, and degree distribution-suggest that, unlike the other
   information systems communities, the ITM is a community with a unique
   character and distinct collaboration patterns. The results show that the
   ITM knowledge networks are fragmented and exhibit a power law
   distribution in which incoming nodes and links prefer to attach to the
   nodes that are already well connected. We discuss several implications
   that arise from the network configuration that could aid the future
   development of the ITM domain.}},
