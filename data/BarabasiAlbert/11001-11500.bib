@article{ ISI:000418389500069,
Author = {Jaeger, Georg and Hofer, Christian and Kapeller, Marie and Fuellsack,
   Manfred},
Title = {{Hidden early-warning signals in scale-free networks}},
Journal = {{PLOS ONE}},
Year = {{2017}},
Volume = {{12}},
Number = {{12}},
Month = {{DEC 18}},
Abstract = {{Critical transitions of complex systems can often be predicted by
   so-called early-warning signals (EWS). In some cases, however, such
   signals cannot be detected although a critical transition is imminent.
   Observing a relation of EWS-detectability and the network topology in
   which the system is implemented, we simulate and investigate scale-free
   networks and identify which networks show, and which do not show EWS in
   the framework of a two state system that exhibits critical transitions.
   Additionally, we adapt our approach by examining the effective state of
   the system, rather than its natural state, and conclude that this
   transformation can reveal hidden EWS in networks where those signals are
   otherwise obscured by a complex topology.}},
