@article{ ISI:000395168700008,
Author = {Sumour, Muneer A. and Lima, F. W. S.},
Title = {{A systematic investigation of the gap in various Barabasi-Albert and
   Erdos-Renyi networks}},
Journal = {{INTERNATIONAL JOURNAL OF MODERN PHYSICS C}},
Year = {{2017}},
Volume = {{28}},
Number = {{2}},
Month = {{FEB}},
Abstract = {{On Barabasi-Albert networks ( BA) and variations as well as on
   Erdos-Renyi ( ER) random graphs, we study the occurrence of a gap in the
   neighbor numbers k(i) versus node index i ( with i = 1; 2;...; N) at m =
   2, 4, 6, 10, 50, 100 and 150 and with up to N = 1 000 000 nodes. Here,
   we call ``gap{''} a jump in the neighbor numbers k(i) when i equals the
   initial number m of neighbors; m is also the number of neighbors
   randomly selected by a newly added node. The size of the gap depends on
   the value of m and causes a deformation in the structure k(i) of the
   networks studied here. We give a systematic investigation of the gap in
   all types of BA networks known to us, and only the undirected BA ( UBA)
   network and the ER graphs show no gap for m > 4.}},
Publisher = {{WORLD SCIENTIFIC PUBL CO PTE LTD}},
Address = {{5 TOH TUCK LINK, SINGAPORE 596224, SINGAPORE}},
Type = {{Article}},
