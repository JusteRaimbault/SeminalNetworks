@article{ ISI:000387002100007,
Author = {Ganis, Mary and Minnery, John and Mateo-Babiano, Iderlina},
Title = {{Planning people-places: A small world network paradigm for
   masterplanning with people in mind}},
Journal = {{ENVIRONMENT AND PLANNING B-PLANNING \& DESIGN}},
Year = {{2016}},
Volume = {{43}},
Number = {{6}},
Pages = {{1075-1095}},
Month = {{NOV}},
Abstract = {{The research is about planning urban places in a way that is relevant to
   the changing people-place phenomenon. In particular, the masterplanning
   of large-scale inner city redevelopment sites are discussed because such
   places have higher concentrations of people and are where change is most
   rapid and profound. Complexity theory of cities (CTC) potentially offers
   an adaptive and resilient masterplanning paradigm for urban change.
   However, some CTC simulation models of urban change have been criticized
   because they are devoid of the input of human cognition and perception.
   This research examines human cognition and perception of place and its
   role in masterplanning from a tripartite perspective of placemaking:
   conceived space; perceived space; and adaptive space. The case study
   area for the research is South Bank, a 42-hectare inner city
   redevelopment site in Brisbane, Australia.}},
