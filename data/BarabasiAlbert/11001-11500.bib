@article{ ISI:000385434600003,
Author = {Darrasse, Alexis and Hwang, Hsien-Kuei and Soria, Michele},
Title = {{Shape Measures of Random Increasing k-trees}},
Journal = {{COMBINATORICS PROBABILITY \& COMPUTING}},
Year = {{2016}},
Volume = {{25}},
Number = {{5}},
Pages = {{668-699}},
Month = {{SEP}},
Abstract = {{Random increasing k-trees represent an interesting and useful class of
   strongly dependent graphs that have been studied widely, including being
   used recently as models for complex networks. In this paper we study an
   informative notion called BFS-profile and derive, by several analytic
   means, asymptotic estimates for its expected value, together with the
   limiting distribution in certain cases; some interesting consequences
   predicting more precisely the shapes of random k-trees are also given.
   Our methods of proof rely essentially on a bijection between k-trees and
   ordinary trees, the resolution of linear systems, and a specially framed
   notion called Flajolet-Odlyzko admissibility.}},
