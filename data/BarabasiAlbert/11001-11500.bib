@inproceedings{ ISI:000467925800001,
Author = {Shainline, Jeffrey M.},
Book-Group-Author = {{IEEE}},
Title = {{The largest cognitive systems will be optoelectronic}},
Booktitle = {{2018 IEEE INTERNATIONAL CONFERENCE ON REBOOTING COMPUTING (ICRC)}},
Year = {{2018}},
Pages = {{2-11}},
Note = {{3rd IEEE International Conference on Rebooting Computing (ICRC), Tysons,
   VA, NOV 07-09, 2018}},
Organization = {{IEEE; IEEE Rebooting Comp; AMD}},
Abstract = {{Electrons and photons offer complimentary strengths for information
   processing. Photons are excellent for communication, while electrons are
   superior for computation and memory. Cognition requires distributed
   computation to be communicated across the system for information
   integration. We present reasoning from neuroscience, network theory, and
   device physics supporting the conjecture that large-scale cognitive
   systems will benefit from electronic devices performing synaptic,
   dendritic, and neuronal information processing operating in conjunction
   with photonic communication. On the chip scale, integrated dielectric
   waveguides enable fan-out to thousands of connections. On the system
   scale, fiber and free-space optics can be employed. The largest
   cognitive systems will be limited by the distance light can travel
   during the period of a network oscillation. We calculate that
   optoelectronic networks the area of a large data center (10(5) m(2))
   will be capable of system-wide information integration at 1 MHz. At
   frequencies of cortex-wide integration in the human brain (4 Hz, theta
   band), optoelectronic systems could integrate information across the
   surface of the earth.}},
