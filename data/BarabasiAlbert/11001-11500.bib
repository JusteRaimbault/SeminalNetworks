@inproceedings{ ISI:000427033305011,
Author = {Nozari, Erfan and Pasqualetti, Fabio and Cortes, Jorge},
Book-Group-Author = {{IEEE}},
Title = {{Time-Invariant versus Time-Varying Actuator Scheduling in Complex
   Networks}},
Booktitle = {{2017 AMERICAN CONTROL CONFERENCE (ACC)}},
