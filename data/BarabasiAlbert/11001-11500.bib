@article{ ISI:000394267100017,
Author = {Jacob, Rinku and Harikrishnan, K. P. and Misra, R. and Ambika, G.},
Title = {{Cross over of recurrence networks to random graphs and random geometric
   graphs}},
Journal = {{PRAMANA-JOURNAL OF PHYSICS}},
Year = {{2017}},
Volume = {{88}},
Number = {{2}},
Month = {{FEB}},
Abstract = {{Recurrence networks are complex networks constructed from the time
   series of chaotic dynamical systems where the connection between two
   nodes is limited by the recurrence threshold. This condition makes the
   topology of every recurrence network unique with the degree distribution
   determined by the probability density variations of the representative
   attractor from which it is constructed. Here we numerically investigate
   the properties of recurrence networks from standard low-dimensional
   chaotic attractors using some basic network measures and show how the
   recurrence networks are different from random and scale-free networks.
   In particular, we show that all recurrence networks can cross over to
   random geometric graphs by adding sufficient amount of noise to the time
   series and into the classical random graphs by increasing the range of
   interaction to the system size. We also highlight the effectiveness of a
   combined plot of characteristic path length and clustering coefficient
   in capturing the small changes in the network characteristics.}},
