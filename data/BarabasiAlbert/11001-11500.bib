@inproceedings{ ISI:000379296100026,
Author = {Feng, Shuo and Shen, Derong and Kou, Yue and Nie, Tiezheng and Yu, Ge},
Editor = {{Cui, B and Zhang, N and Xu, J and Lian, X and Liu, D}},
Title = {{Anchor Link Prediction Using Topological Information in Social Networks}},
