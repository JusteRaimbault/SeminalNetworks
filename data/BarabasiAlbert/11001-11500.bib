@article{ ISI:000383907100007,
Author = {Li, Hao and Huang, Jian and Le, Anbo and Wang, Qin and Xi, Lifeng},
Title = {{SCALE-FREE AND SMALL-WORLD PROPERTIES OF VAF FRACTAL NETWORKS}},
