@article{ ISI:000445875000004,
Author = {Gao, Ping and Xi, Jianchao},
Title = {{Network characteristics of tourism destinations: a case from the Yesanpo
   tourism destination in China}},
Journal = {{JOURNAL OF SPATIAL SCIENCE}},
Year = {{2018}},
Volume = {{63}},
Number = {{2, SI}},
Pages = {{245-263}},
Abstract = {{This study considers the Yesanpo tourism destination as a case, using
   network and spatial analysis methods to reveal its structure
   characteristics from the perspective of node centrality, connection
   hierarchy and space cluster. The results are as follows. (1) Core scenic
   spots/central towns have higher centrality than surrounding rural
   communities and characteristic parks, and occupy core positions. (2)
   Network connections show obvious hierarchical characteristics and
   spatial heterogeneity; each hierarchy is based on the core scenic spots
   and the central town, existing spatial difference between the south and
   north and spatial dependence on the river and main road. (3) Spatial
   elements tend to cluster to form several tourism cluster units, which
   centre on core scenic spots, composed of central towns, rural
   communities and characteristic parks, and show the dual features of
   `adjacent connection' and `preferential connection'. The study also
   suggests each tourism node should be equipped with appropriate
   facilities and services based on their structural characteristics and
   functions to promote sustainable development of tourism destinations.}},
