@article{ ISI:000230869200028,
Author = {Chassin, DP and Posse, C},
Title = {{Evaluating North American electric grid reliability using the
   Barabasi-Albert network model}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2005}},
Volume = {{355}},
Number = {{2-4}},
Pages = {{667-677}},
Month = {{SEP 15}},
Abstract = {{The reliability of electric transmission systems is examined using a
   scale-free model of network topology and failure propagation. The
   topologies of the North American eastern and western electric grids are
   analyzed to estimate their reliability based oil the Barabasi-Albert
   network model. A commonly used power system reliability index is
   computed using a simple failure propagation model. The results are
   compared to the values of power system reliability indices previously
   obtained using standard power engineering methods, and they suggest that
   scale-free network models are usable to estimate aggregate electric grid
   reliability. (c) 2005 Elsevier B.V. All rights reserved.}},
