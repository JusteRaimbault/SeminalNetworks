@article{ ISI:000322419500010,
Author = {Baronchelli, Andrea and Ferrer-i-Cancho, Ramon and Pastor-Satorras,
   Romualdo and Chater, Nick and Christiansen, Morten H.},
Title = {{Networks in Cognitive Science}},
Journal = {{TRENDS IN COGNITIVE SCIENCES}},
