@article{ ISI:000280766100032,
Author = {Feingold, Graham and Koren, Ilan and Wang, Hailong and Xue, Huiwen and
   Brewer, Wm. Alan},
Title = {{Precipitation-generated oscillations in open cellular cloud fields}},
