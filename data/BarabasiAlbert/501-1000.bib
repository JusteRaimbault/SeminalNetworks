Author = {Autant-Bernard, Corinne and Billand, Pascal and Frachisse, David and
   Massard, Nadine},
Title = {{Social distance versus spatial distance in R\&D cooperation: Empirical
   evidence from European collaboration choices in micro and
   nanotechnologies}},
