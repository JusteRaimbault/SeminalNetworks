@article{ ISI:000249785900076,
Author = {Liu, Zhe and Hu, Mao-Bin and Jiang, Rui and Wang, Wen-Xu and Wu,
   Qing-Song},
Title = {{Method to enhance traffic capacity for scale-free networks}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2007}},
Volume = {{76}},
Number = {{3, 2}},
Month = {{SEP}},
Abstract = {{In this paper, a method is proposed to enhance the traffic handling
   capacity of scale-free networks by closing or cutting some links between
   some large-degree nodes, for both local routing strategy and global
   shortest-path routing strategy. The traffic capacity of networks is
   found to be considerably improved after applying the link-closing
   strategy, especially in the case of global routing. Due to the strongly
   improved network capacity, easy realization on networks, and low cost,
   the strategy may be useful for modern communication networks.}},
