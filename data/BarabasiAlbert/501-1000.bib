@inproceedings{ ISI:000371251300003,
Author = {Chai, Wei Koong and He, Diliang and Psaras, Ioannis and Pavlou, George},
Editor = {{Bestak, R and Kencl, L and Li, LE and Widmer, J and Yin, H}},
Title = {{Cache ``Less for More{''} in Information-Centric Networks}},
Booktitle = {{NETWORKING 2012, PT I}},
