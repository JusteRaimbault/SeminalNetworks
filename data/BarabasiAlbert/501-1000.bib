@article{ ISI:000302576900001,
Author = {Wu, Qingchu and Fu, Xinchu and Small, Michael and Xu, Xin-Jian},
Title = {{The impact of awareness on epidemic spreading in networks}},
Journal = {{CHAOS}},
Year = {{2012}},
Volume = {{22}},
Number = {{1}},
Month = {{MAR}},
Abstract = {{We explore the impact of awareness on epidemic spreading through a
   population represented by a scale-free network. Using a network
   mean-field approach, a mathematical model for epidemic spreading with
   awareness reactions is proposed and analyzed. We focus on the role of
   three forms of awareness including local, global, and contact awareness.
   By theoretical analysis and simulation, we show that the global
   awareness cannot decrease the likelihood of an epidemic outbreak while
   both the local awareness and the contact awareness can. Also, the
   influence degree of the local awareness on disease dynamics is closely
   related with the contact awareness. (C) 2012 American Institute of
   Physics. {[}doi:10.1063/1.3673573]}},
