@article{ ISI:000221447300019,
Author = {Madar, N and Kalisky, T and Cohen, R and ben-Avraham, D and Havlin, S},
Title = {{Immunization and epidemic dynamics in complex networks}},
Journal = {{EUROPEAN PHYSICAL JOURNAL B}},
