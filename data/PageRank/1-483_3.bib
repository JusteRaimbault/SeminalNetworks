@article{ ISI:000412780100006,
Author = {Recanati, Antoine and Bruls, Thomas and d'Aspremont, Alexandre},
Title = {{A spectral algorithm for fast de novo layout of uncorrected long
   nanopore reads}},
Journal = {{BIOINFORMATICS}},
Year = {{2017}},
Volume = {{33}},
Number = {{20}},
Pages = {{3188-3194}},
Month = {{OCT 15}},
Abstract = {{Motivation: New long read sequencers promise to transform sequencing and
   genome assembly by producing reads tens of kilobases long. However,
   their high error rate significantly complicates assembly and requires
   expensive correction steps to layout the reads using standard assembly
   engines.
   Results: We present an original and efficient spectral algorithm to
   layout the uncorrected nanopore reads, and its seamless integration into
   a straightforward overlap/layout/consensus (OLC) assembly scheme. The
   method is shown to assemble Oxford Nanopore reads from several bacterial
   genomes into good quality (similar to 99\% identity to the reference)
   genome-sized contigs, while yielding more fragmented assemblies from the
   eukaryotic microbe Sacharomyces cerevisiae.}},
