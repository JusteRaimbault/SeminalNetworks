@article{ ISI:000310945000030,
Author = {Lee, Sangkeun and Park, Sungchan and Kahng, Minsuk and Lee, Sang-goo},
Title = {{PathRank: Ranking nodes on a heterogeneous graph for flexible hybrid
   recommender systems}},
Journal = {{EXPERT SYSTEMS WITH APPLICATIONS}},
Year = {{2013}},
Volume = {{40}},
Number = {{2}},
Pages = {{684-697}},
Month = {{FEB 1}},
Abstract = {{We present a flexible hybrid recommender system that can emulate
   collaborative-filtering, content-based filtering, context-aware
   recommendation, and combinations of any of these recommendation
   semantics. The recommendation problem is modeled as a problem of finding
   the most relevant nodes for a given set of query nodes on a
   heterogeneous graph. However, existing node ranking measures cannot
   fully exploit the semantics behind the different types of nodes and
   edges in a heterogeneous graph. To overcome the limitation, we present a
   novel random walk based node ranking measure, PathRank, by extending the
   Personalized PageRank algorithm. The proposed measure can produce node
   ranking results with varying semantics by discriminating the different
   paths on a heterogeneous graph. The experimental results show that our
   method can produce more diverse and effective recommendation results
   compared to existing approaches. (c) 2012 Elsevier Ltd. All rights
   reserved.}},
