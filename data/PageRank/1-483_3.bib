@inproceedings{ ISI:000307024300036,
Author = {Li, Xuefei and Yu, Weiren and Yang, Bo and Le, Jiajin},
Editor = {{Wang, H and Li, S and Oyama, S and Hu, XH and Qian, T}},
Title = {{ASAP: Towards Accurate, Stable and Accelerative Penetrating-Rank
   Estimation on Large Graphs}},
