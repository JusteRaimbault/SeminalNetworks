Author = {Mirkin, Boris and Orlov, Michael},
Editor = {{Migdalas, A and Karakitsiou, A}},
Title = {{Three Aspects of the Research Impact by a Scientist: Measurement Methods
   and an Empirical Evaluation}},
Booktitle = {{OPTIMIZATION, CONTROL, AND APPLICATIONS IN THE INFORMATION AGE: IN HONOR
   OF PANOS M. PARDALOS'S 60TH BIRTHDAY}},
Series = {{Springer Proceedings in Mathematics \& Statistics}},
