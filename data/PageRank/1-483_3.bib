@inproceedings{ ISI:000458175600107,
Author = {Simchowitz, Max and El Alaoui, Ahmed and Recht, Benjamin},
Editor = {{Diakonikolas, I and Kempe, D and Henzinger, M}},
Title = {{Tight Query Complexity Lower Bounds for PCA via Finite Sample Deformed
   Wigner Law}},
Booktitle = {{STOC'18: PROCEEDINGS OF THE 50TH ANNUAL ACM SIGACT SYMPOSIUM ON THEORY
   OF COMPUTING}},
Series = {{Annual ACM Symposium on Theory of Computing}},
