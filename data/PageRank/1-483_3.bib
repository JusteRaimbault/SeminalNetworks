@inproceedings{ ISI:000460583300026,
Author = {Ohsaka, Naoto and Sonobe, Tomohiro and Kakimura, Naonori and Fukunaga,
   Takuro and Fujita, Sumio and Kawarabayashi, Ken-ichi},
Editor = {{Hartmann, S and Ma, H and Hameurlain, A and Pernul, G and Wagner, RR}},
Title = {{Boosting PageRank Scores by Optimizing Internal Link Structure}},
Booktitle = {{DATABASE AND EXPERT SYSTEMS APPLICATIONS, DEXA 2018, PT I}},
Series = {{Lecture Notes in Computer Science}},
Year = {{2018}},
Volume = {{11029}},
Number = {{I}},
Pages = {{424-439}},
Note = {{29th International Conference on Database and Expert Systems
   Applications (DEXA), Regensburg, GERMANY, SEP 03-06, 2018}},
Abstract = {{We consider and formulate problems of PageRank score boosting motivated
   by applications such as effective web advertising. More precisely, given
   a graph and target vertices, one is required to find a fixed-size set of
   missing edges that maximizes the minimum PageRank score among the
   targets. We provide theoretical analyses to show that all of them are
   NP-hard. To overcome the hardness, we develop heuristic-based algorithms
   for them. We finally perform experiments on several real-world networks
   to verify the effectiveness of the proposed algorithms compared to
   baselines. Specifically, our algorithm achieves 100 times improvements
