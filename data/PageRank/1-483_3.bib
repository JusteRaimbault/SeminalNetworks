@article{ ISI:000312652800009,
Author = {Marres, Noortje},
Title = {{The redistribution of methods: on intervention in digital social
   research, broadly conceived}},
Journal = {{SOCIOLOGICAL REVIEW}},
Year = {{2012}},
Volume = {{60}},
