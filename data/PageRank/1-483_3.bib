@inproceedings{ ISI:000391253600220,
Author = {Goswami, Anshuman and Young, Jeffrey and Schwan, Karsten and Farooqui,
   Naila and Gavrilovska, Ada and Wolf, Matthew and Eisenhauer, Greg},
Book-Group-Author = {{IEEE}},
Title = {{GPUShare: Fair-sharing Middleware for GPU Clouds}},
Booktitle = {{2016 IEEE 30TH INTERNATIONAL PARALLEL AND DISTRIBUTED PROCESSING
   SYMPOSIUM WORKSHOPS (IPDPSW)}},
Series = {{IEEE International Symposium on Parallel and Distributed Processing
   Workshops}},
