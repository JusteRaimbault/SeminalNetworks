@inproceedings{ ISI:000428141800038,
Author = {Wang, Shuai and Zhao, Xiang and Li, Bo and Ge, Bin and Tang, Daquan},
Editor = {{Karypis, G and Zhang, J}},
Title = {{Integrating Extractive and Abstractive Models for Long Text
   Summarization}},
