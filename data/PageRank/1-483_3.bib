Journal = {{JOURNAL OF SUPERCOMPUTING}},
Year = {{2014}},
Volume = {{70}},
Number = {{2}},
Pages = {{637-648}},
Month = {{NOV}},
Abstract = {{In this paper, parallel Relaxed and Extrapolated algorithms based on the
