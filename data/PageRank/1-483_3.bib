@article{ ISI:000387525300004,
Author = {Sun, Zhuo and Zheng, Jianfeng},
Title = {{Finding potential hub locations for liner shipping}},
Journal = {{TRANSPORTATION RESEARCH PART B-METHODOLOGICAL}},
Year = {{2016}},
Volume = {{93}},
Number = {{B, SI}},
Pages = {{750-761}},
Month = {{NOV}},
Abstract = {{The current models for hub location problems are unable to find
   potential hub locations in uncharted areas that currently have no port.
   To explore hub locations that are not selected from the present ports,
   this study proposes a two-stage method to address this gap in knowledge.
   A concave cost multicommodity network flow model is solved in the first
   stage to obtain container traffic in waterways. Then, in the second
   stage, a hubbing probability is evaluated for each node to indicate
   potential hub locations. A case study including emerging Arctic routes
   is provided to demonstrate this method. (C) 2016 Elsevier Ltd. All
   rights reserved.}},
Publisher = {{PERGAMON-ELSEVIER SCIENCE LTD}},
