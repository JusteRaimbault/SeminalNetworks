Author = {Jiang, Zhengshen and Liu, Hongzhi and Fu, Bin and Wu, Zhonghai and
   Zhang, Tao},
Book-Group-Author = {{ACM}},
Title = {{Recommendation in Heterogeneous Information Networks Based on
   Generalized Random Walk Model and Bayesian Personalized Ranking}},
Booktitle = {{WSDM'18: PROCEEDINGS OF THE ELEVENTH ACM INTERNATIONAL CONFERENCE ON WEB
   SEARCH AND DATA MINING}},
Year = {{2018}},
Pages = {{288-296}},
