@article{ ISI:000437440500002,
Author = {Kim, Hyunuk and You, Taekho and Ahn, Sang-Jin and Jung, Woo-Sung},
Title = {{Embracing networks of national policy research in future foresight}},
Journal = {{INTERNATIONAL JOURNAL OF KNOWLEDGE-BASED DEVELOPMENT}},
Year = {{2016}},
