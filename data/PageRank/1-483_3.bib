@inproceedings{ ISI:000452535700089,
Author = {Perez, Yonathan and Sosic, Rok and Banerjee, Arijit and Puttagunta,
   Rohan and Raison, Martin and Shah, Pararth and Leskovec, Jure},
Book-Group-Author = {{ACM SIGMOD}},
Title = {{Ringo: Interactive Graph Analytics on Big-Memory Machines}},
Booktitle = {{SIGMOD'15: PROCEEDINGS OF THE 2015 ACM SIGMOD INTERNATIONAL CONFERENCE
