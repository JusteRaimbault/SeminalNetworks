Title = {{X-Rank: Explainable Ranking in Complex Multi-Layered Networks}},
Booktitle = {{CIKM'18: PROCEEDINGS OF THE 27TH ACM INTERNATIONAL CONFERENCE ON
   INFORMATION AND KNOWLEDGE MANAGEMENT}},
Year = {{2018}},
