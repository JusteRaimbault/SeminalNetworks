@article{ ISI:000475736400013,
Author = {Hadjilambrou, Zacharias and Kleanthous, Marios and Antoniou, Georgia and
   Portero, Antoni and Sazeides, Yiannakis},
Title = {{Comprehensive Characterization of an Open Source Document Search Engine}},
