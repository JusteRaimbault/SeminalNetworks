Title = {{Probabilistic Topic and Role Model for Information Diffusion in Social
   Network}},
Booktitle = {{ADVANCES IN KNOWLEDGE DISCOVERY AND DATA MINING, PAKDD 2018, PT II}},
Series = {{Lecture Notes in Artificial Intelligence}},
Year = {{2018}},
