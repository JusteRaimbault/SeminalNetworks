@inproceedings{ ISI:000354534900017,
Author = {Liu, Zhiyu and Herlihy, Maurice},
Editor = {{Kuhn, F}},
Title = {{Approximate Local Sums and Their Applications in Radio Networks}},
Booktitle = {{DISTRIBUTED COMPUTING (DISC 2014)}},
