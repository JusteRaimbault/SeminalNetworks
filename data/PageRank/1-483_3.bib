@article{ ISI:000381540600014,
Author = {Riquelme, Fabian and Gonzalez-Cantergiani, Pablo},
Title = {{Measuring user influence on Twitter: A survey}},
Journal = {{INFORMATION PROCESSING \& MANAGEMENT}},
