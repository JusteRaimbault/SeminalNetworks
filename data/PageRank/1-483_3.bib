@inproceedings{ ISI:000410187400088,
Author = {Taduri, Siddharth and Lau, Gloria T. and Law, Kincho H. and Kesan, Jay
   P.},
Book-Group-Author = {{IEEE Comp Soc}},
Title = {{Retrieval of Patent Documents from Heterogeneous Sources using
   Ontologies and Similarity Analysis}},
Booktitle = {{FIFTH IEEE INTERNATIONAL CONFERENCE ON SEMANTIC COMPUTING (ICSC 2011)}},
