Author = {Tomassen, Stein L. and Strasunskas, Darijus},
Editor = {{BaezaYates, R and Berendt, B and Bertino, E and Lim, EP and Pasi, G}},
Title = {{Construction of Ontology based Semantic-Linguistic Feature Vectors for
   Searching: the Process and Effect}},
Booktitle = {{2009 IEEE/WIC/ACM INTERNATIONAL JOINT CONFERENCES ON WEB INTELLIGENCE
   (WI) AND INTELLIGENT AGENT TECHNOLOGIES (IAT), VOL 3}},
