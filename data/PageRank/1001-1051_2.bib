@inproceedings{ ISI:000266369400009,
Author = {Alani, Harith and Noy, Natalya F. and Shah, Nigam and Shadbolt, Nigel
   and Musen, Mark A.},
Book-Group-Author = {{ACM}},
Title = {{Searching Ontologies Based on Content: Experiments in the Biomedical
   Domain}},
Booktitle = {{K-CAP'07: PROCEEDINGS OF THE FOURTH INTERNATIONAL CONFERENCE ON
   KNOWLEDGE CAPTURE}},
Year = {{2007}},
Pages = {{55-62}},
Note = {{4th International Conference on Knowledge Capture, Whistler, CANADA, OCT
   28-31, 2007}},
Organization = {{ACM SIGART; Boeing, Assoc Advancement Artificial Intelligence; Vulcan \&
   Knowledge Web}},
Abstract = {{As more ontologies become publicly available, finding the ``right{''}
   ontologies becomes much harder. In this paper, we address the problem of
   ontology search: finding a collection of ontologies from an ontology
   repository that are relevant to the user's query. In particular, we look
   at the case when users search for ontologies relevant to a particular
   topic (e.g., an ontology about anatomy). Ontologies that axe most
   relevant to such query often do not have the query term in the names of
   their concepts (e.g., the Foundational Model of Anatomy ontology does
   not have the term ``anatomy{''} in any of its concepts' names). Thus, we
   present a new ontology-search technique that helps users in these types
   of searches. When looking for ontologies on a particular topic (e.g.,
   anatomy), we retrieve from the Web a collection of terms that represent
   the given domain (e.g., terms such as body, brain, skin, etc. for
   anatomy). We then use these terms to expand the user query. We evaluate
   our algorithm on queries for topics in the biomedical domain against a
   repository of biomedical ontologies. We use the results obtained from
   experts in the biomedical-ontology domain as the gold standard. Our
   experiments demonstrate that using our method for query expansion
   improves retrieval results by a 113\%, compared to the tools that search
   only for the user query terms and consider only class and property names
   (like Swoogle). We show 43\% improvement for the case where not only
   class and property names but also property values are taken into
   account.}},
