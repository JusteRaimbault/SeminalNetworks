@inproceedings{ ISI:000290886500012,
Author = {Saloun, Petr and Velart, Zdenek},
Editor = {{Mylonas, P and Wallace, M and Anagnostopoulos, I}},
Title = {{Concept Space Rating for Personalization of Learning Materials Based on
   Relations}},
Booktitle = {{PROCEEDINGS 2009 FOURTH INTERNATIONAL WORKSHOP ON SEMANTIC MEDIA
   ADAPTATION AND PERSONALIZATION}},
Year = {{2009}},
Pages = {{67+}},
Note = {{4th International Workshop on Semantic Media Adaptation and
   Personalization, San Sebastian, SPAIN, DEC 14-15, 2009}},
Organization = {{IEEE Comp Intelligence Soc; IEEE Syst, Man \& Cybernet Soc (SMCS);
   Visual Interact Commun Technol}},
Abstract = {{The problem of navigation of the student over learning objects can be
   solved either by creating relations between learning objects or by using
   metadata of learning objects stored in concepts space of the domain.
   Every learning object has such defined metadata in a form of relation to
   several concepts. Concepts space contains information about structural
   and dependency relations between concepts. We base on the fact that
   relations which exist in the learning object space also exist in the
   concepts space between corresponding concepts. We present an algorithm
   for calculating a rating of a concept. The rating is based on the
   concept's relations and dependencies to other concepts. A navigation
   scheme which uses concept's rating for calculating learning object
   rating with regard to actual student's model is presented.}},
