@article{ ISI:000399118600012,
Author = {Tsvetkova, Milena and Yasseri, Taha and Meyer, Eric T. and Pickering, J.
   Brian and Engen, Vegard and Walland, Paul and Luders, Marika and
   Folstad, Asbjorn and Bravos, George},
Title = {{Understanding Human-Machine Networks: A Cross-Disciplinary Survey}},
