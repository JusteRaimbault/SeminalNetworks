@inproceedings{ ISI:000404430900060,
Author = {Armaly, Ameer and Klaczynski, John and McMillan, Collin},
Book-Group-Author = {{IEEE}},
Title = {{A Case Study of Automated Feature Location Techniques for Industrial
   Cost Estimation}},
Booktitle = {{32ND IEEE INTERNATIONAL CONFERENCE ON SOFTWARE MAINTENANCE AND EVOLUTION
   (ICSME 2016)}},
Series = {{Proceedings-IEEE International Conference on Software Maintenance}},
