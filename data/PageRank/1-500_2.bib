@article{ ISI:000414836000004,
Author = {Holladay, Patrick and Skibins, Jeffrey C. and Zach, Florian J. and Arze,
   Marcelo},
Title = {{Exploratory Social Network Analysis of Stakeholder Organizations Along
   the Illinois and Michigan Canal National Heritage Corridor}},
Journal = {{JOURNAL OF PARK AND RECREATION ADMINISTRATION}},
