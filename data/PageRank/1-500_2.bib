@inproceedings{ ISI:000450867700022,
Author = {Levchuk, Georgiy and Colona-Romano, John},
Editor = {{Kadar, I}},
Title = {{Optimizing Collaborative Computations for Scalable Distributed Inference
   in Large Graphs}},
Booktitle = {{SIGNAL PROCESSING, SENSOR/INFORMATION FUSION, AND TARGET RECOGNITION
   XXVII}},
Series = {{Proceedings of SPIE}},
Year = {{2018}},
Volume = {{10646}},
Note = {{Conference on Signal Processing, Sensor/Information Fusion, and Target
   Recognition XXVII, Orlando, FL, APR 16-19, 2018}},
Organization = {{SPIE}},
Abstract = {{In this paper, we study two methods to optimize distributed
   collaborative computations: (a) data partitioning, which exploits
   locality to reduce data dependencies between local computations, and (b)
   computation aggregation, which reduces communication load between local
   partitions. We analyze the benefits of such optimizations and their
   utility for message-passing processing model. This is a class of
   general-purpose graph analytics widely used in a range of domains and
