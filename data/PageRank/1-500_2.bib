@inproceedings{ ISI:000458522300001,
Author = {Jarke, Matthias},
Editor = {{Schoop, M and Kilgour, DM}},
Title = {{Data Spaces: Combining Goal-Driven and Data-Driven Approaches in
   Community Decision and Negotiation Support}},
Booktitle = {{GROUP DECISION AND NEGOTIATION: A SOCIO-TECHNICAL PERSPECTIVE}},
Series = {{Lecture Notes in Business Information Processing}},
