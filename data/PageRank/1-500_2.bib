@article{ ISI:000368560200006,
Author = {Liu, Li and Chen, Xiwei and Liu, Ming and Jia, Yongpo and Zhong, Jun and
   Gao, Rongmin and Zhao, Yu},
Title = {{An influence power-based clustering approach with PageRank-like model}},
Journal = {{APPLIED SOFT COMPUTING}},
Year = {{2016}},
Volume = {{40}},
Pages = {{17-32}},
Month = {{MAR}},
Abstract = {{In this paper, we present a clustering method called clustering by
   sorting influence power, which incorporates the concept of influence
   power as measurement among points. In our method, clustering is
   performed in an efficient tree-growing fashion exploiting both the
   hypothetical influence powers of data points and the distances among
   data points. Since influence powers among data points evolve over time,
   we adopt a PageRank-like algorithm to calculate them iteratively to
   avoid the issue of improper initial exemplar preference. The
   experimental results show that our proposed method outperforms four
   well-known clustering methods across seven complex and non-isotropic
   datasets. Moreover, our simple clustering method can be easily applied
   to several practical clustering problems. We evaluate the effectiveness
   of our algorithm on two real-world datasets, i.e. an open dataset of
   Alzheimers disease protein-protein interaction network and a dataset for
   race walking recognition collected by ourselves, and we find our method
   outperforms other methods reported in the literature. (C) 2015 Elsevier
   B.V. All rights reserved.}},
