@article{ ISI:000430121000018,
Author = {Master, Lawrence},
Title = {{Improving document ranking with genetic and optimization algorithms}},
Journal = {{INTERNATIONAL JOURNAL OF NUMERICAL MODELLING-ELECTRONIC NETWORKS DEVICES
   AND FIELDS}},
Year = {{2018}},
Volume = {{31}},
Number = {{3}},
Month = {{MAY-JUN}},
Abstract = {{There are many applications to ranking including page searching,
   question answering, recommender systems, sentiment analysis, and
   collaborative filtering, to name a few. In the past several years,
   machine learning and information retrieval techniques have been used to
   develop ranking algorithms, and several listwise approaches to learning
   to rank have been developed. We propose a new method, which we call
   GeneticListMLE++ and GeneticListNet++, which builds on the original
   ListMLE and ListNet algorithms. Our method substantially improves on the
   original ListMLE and ListNet ranking approaches by incorporating genetic
   optimization of hyperparameters, a nonlinear neural network ranking
   model, and a regularization technique.}},
