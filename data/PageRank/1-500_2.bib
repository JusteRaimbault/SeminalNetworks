Editor = {{Joshi, J and Karypis, G and Liu, L and Hu, X and Ak, R and Xia, Y and Xu, W and Sato, AH and Rachuri, S and Ungar, L and Yu, PS and Govindaraju, R and Suzumura, T}},
Title = {{Change Detection and Classification of Digital Collections}},
Booktitle = {{2016 IEEE INTERNATIONAL CONFERENCE ON BIG DATA (BIG DATA)}},
