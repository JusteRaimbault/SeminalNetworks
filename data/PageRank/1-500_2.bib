@article{ ISI:000474377300001,
Author = {Lee, Sang-Chul and Kim, Sang-Wook and Park, Sunju and Chae, Dong-Kyu},
Title = {{A Tripartite-Graph Based Recommendation Framework for Price-Comparison
   Services}},
