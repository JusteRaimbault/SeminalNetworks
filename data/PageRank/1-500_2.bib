@article{ ISI:000444360500020,
Author = {Gomez-Romero, Juan and Molina-Solana, Miguel and Oehmichen, Axel and
   Guo, Yike},
Title = {{Visualizing large knowledge graphs: A performance analysis}},
Journal = {{FUTURE GENERATION COMPUTER SYSTEMS-THE INTERNATIONAL JOURNAL OF ESCIENCE}},
Year = {{2018}},
Volume = {{89}},
Pages = {{224-238}},
Month = {{DEC}},
Abstract = {{Knowledge graphs are an increasingly important source of data and
   context information in Data Science. A first step in data analysis is
   data exploration, in which visualization plays a key role. Currently,
   Semantic Web technologies are prevalent for modeling and querying
   knowledge graphs; however, most visualization approaches in this area
   tend to be overly simplified and targeted to small-sized
   representations. In this work, we describe and evaluate the performance
   of a Big Data architecture applied to large-scale knowledge graph
   visualization. To do so, we have implemented a graph processing pipeline
   in the Apache Spark framework and carried out several experiments with
   real-world and synthetic graphs. We show that distributed
   implementations of the graph building, metric calculation and layout
   stages can efficiently manage very large graphs, even without applying
   partitioning or incremental processing strategies. (C) 2018 Elsevier
   B.V. All rights reserved.}},
