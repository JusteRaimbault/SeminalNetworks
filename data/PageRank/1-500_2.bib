@inproceedings{ ISI:000426794900157,
Author = {Zhang, Xuchen and Ding, Daizong and Huang, Chudi},
Book-Group-Author = {{IEEE}},
Title = {{A User Intention Modeling Algorithm for Friend Recommendation}},
Booktitle = {{2017 IEEE 2ND INTERNATIONAL CONFERENCE ON BIG DATA ANALYSIS (ICBDA)}},
Year = {{2017}},
Pages = {{789-795}},
Note = {{2nd IEEE International Conference on Big Data Analysis (ICBDA), Beijing,
   PEOPLES R CHINA, MAR 10-12, 2017}},
Organization = {{IEEE; Xian Jiaotong Liverpool Univ, Res Inst Big Data Analyt}},
Abstract = {{Friend recommendation has been one of the most challenging problems as
   the social networks grow rapidly, due to the needs of seeking people who
   are acquaintances in real life or share the common interests. In this
   paper, we tackle the problem by treating it as a link prediction task
   and propose a hybrid algorithm that exploits the existing friendship
   links, users' history ratings and the tags annotated to items.
   Specifically, we identify the underlying groups in the social network
   based on different aspects and draw the features that express the
   involvement degree of the users to the groups. The features are then
   incorporated into a user intention model, in order to learn some general
   characteristics about how much users inside(between) a group like to
   associate with others and how much users like to associate with
   common-interest people. The proposed algorithm is evaluated on a
   real-world dataset called LibraryThing. The experimental results show
   that prediction accuracy of our algorithm is improved remarkably
   compared to the standard algorithms.}},
