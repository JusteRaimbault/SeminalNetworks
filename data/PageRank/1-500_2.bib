@inproceedings{ ISI:000426984300002,
Author = {Ma, Ge and Wang, Zhi and Chen, Minghua and Zhu, Wenwu},
Book-Group-Author = {{IEEE}},
Title = {{APRANK: JOINT MOBILITY AND PREFERENCE-BASED MOBILE VIDEO PREFETCHING}},
Booktitle = {{2017 IEEE INTERNATIONAL CONFERENCE ON MULTIMEDIA AND EXPO (ICME)}},
Series = {{IEEE International Conference on Multimedia and Expo}},
