@inproceedings{ ISI:000402739900019,
Author = {Oguntebi, Tayo and Olukotun, Kunle},
Book-Group-Author = {{ACM}},
Title = {{GraphOps: A Dataflow Library for Graph Analytics Acceleration}},
Booktitle = {{PROCEEDINGS OF THE 2016 ACM/SIGDA INTERNATIONAL SYMPOSIUM ON
   FIELD-PROGRAMMABLE GATE ARRAYS (FPGA'16)}},
Year = {{2016}},
Pages = {{111-117}},
