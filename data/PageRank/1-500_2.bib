@inproceedings{ ISI:000426978100045,
Author = {Zeng, Xianhua and Hu, Meng and Zhu, Suwen},
Editor = {{Wu, XD and Ozsu, T and Hendler, J and Lu, R}},
Title = {{Image Retrieval Based on Hierarchical Locally Constrained Diffusion
   Process}},
Booktitle = {{2017 IEEE INTERNATIONAL CONFERENCE ON BIG KNOWLEDGE (IEEE ICBK 2017)}},
Year = {{2017}},
Pages = {{290-296}},
Note = {{IEEE International Conference on Big Knowledge (IEEE ICBK), Hefei,
   PEOPLES R CHINA, AUG 09-10, 2017}},
Organization = {{IEEE; IEEE Comp Soc}},
Abstract = {{To address the problem of high matrix computational cost in traditional
   diffusion process methods for large scale image retrieval, we propose a
   novel image retrieval algorithm based on hierarchical locally
   constrained diffusion process (HLCDP) by combining algebraic multigrid
   and diffusion ranking on manifold. In HLCDP, all retrieved images of the
   image database are built into a bottom-to-top hierarchical structure by
   selecting the representative images. Then the similarity among images on
   the top layer is diffused by using locally constrained diffusion
   process, and the affinities (i.e. ranking scores) between query images
   and top-layer representative images are interpolated to all images on
   the bottom layer to obtain the affinities between the query images and
   all of the images in database. Our method is evaluated on image
