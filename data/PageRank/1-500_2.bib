@article{ ISI:000442400700015,
Author = {Zhu, Zhibo and Peng, Qinke and Li, Zhi and Guan, Xinyu and Muhammad,
   Owais},
Title = {{Fast PageRank Computation Based on Network Decomposition and DAG
   Structure}},
