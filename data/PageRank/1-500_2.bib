@article{ ISI:000404542100005,
Author = {Drakopoulos, Georgios and Kanavos, Andreas and Tsakalidis, Konstantinos},
Title = {{Fuzzy Random Walkers with Second Order Bounds: An Asymmetric Analysis}},
