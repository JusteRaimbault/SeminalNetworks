@article{ ISI:000408621500009,
Author = {Becker, Martin and Lemmerich, Florian and Singer, Philipp and
   Strohmaier, Markus and Hotho, Andreas},
Title = {{MixedTrails: Bayesian hypothesis comparison on heterogeneous sequential
   data}},
Journal = {{DATA MINING AND KNOWLEDGE DISCOVERY}},
Year = {{2017}},
