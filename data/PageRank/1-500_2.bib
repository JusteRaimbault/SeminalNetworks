@article{ ISI:000401402200009,
Author = {Zia, Muhammad Azam and Zhang, Zhongbao and Li, Ximing and Ahmad, Haseeb
   and Su, Sen},
Title = {{ComRank: Joint Weight Technique for the Identification of Influential
   Communities}},
Journal = {{CHINA COMMUNICATIONS}},
