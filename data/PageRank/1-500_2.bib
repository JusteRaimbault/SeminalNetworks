@inproceedings{ ISI:000425232200260,
Author = {Cheng, Yongli and Jiang, Hong and Wang, Fang and Hua, Yu and Feng, Dan},
Book-Group-Author = {{IEEE}},
Title = {{BlitzG: Exploiting High-Bandwidth Networks for Fast Graph Processing}},
Booktitle = {{IEEE INFOCOM 2017 - IEEE CONFERENCE ON COMPUTER COMMUNICATIONS}},
Series = {{IEEE INFOCOM}},
Year = {{2017}},
Note = {{IEEE Conference on Computer Communications (IEEE INFOCOM), Atlanta, GA,
   MAY 01-04, 2017}},
Organization = {{IEEE}},
Abstract = {{Nowadays, high-bandwidth networks are easily accessible in data centers.
   However, existing distributed graph-processing frameworks fail to
   efficiently utilize the additional bandwidth capacity in these networks
   for higher performance, due to their inefficient computation and
   communication models, leading to very long waiting times experienced by
   users for the graph-computing results. The root cause lies in the fact
   that the computation and communication models of these frameworks
   generate, send and receive messages so slowly that only a small fraction
   of the available network bandwidth is utilized. In this paper, we
   propose a high-performance distributed graph-processing framework,
   called BlitzG, to address this problem. This framework fully exploits
   the available network bandwidth capacity for fast graph processing. Our
   approach aims at significant reduction in (i) the computation workload
   of each vertex for fast message generation by using a new slimmeddown
   vertex-centric computation model and (ii) the average message overhead
   for fast message delivery by designing a lightweight message-centric
   communication model. Evaluation on a 40Gbps Ethernet, driven by
   real-world graph datasets, shows that BlitzG outperforms the
   state-of-the-art distributed graph-processing frameworks by up to 27x
   with an average of 20.7x.}},
Publisher = {{IEEE}},
Address = {{345 E 47TH ST, NEW YORK, NY 10017 USA}},
Type = {{Proceedings Paper}},
