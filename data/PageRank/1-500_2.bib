@article{ ISI:000379004200012,
Author = {Gui, Xiaolin and Liu, Jun and Lv, Qiujian and Dong, Chao and Lei,
   Zhenming},
Title = {{Probabilistic Top-k Query: Model and Application on Web Traffic Analysis}},
Journal = {{CHINA COMMUNICATIONS}},
