Title = {{Graph Ranking Guarantees for Numerical Approximations to Katz Centrality}},
Booktitle = {{INTERNATIONAL CONFERENCE ON COMPUTATIONAL SCIENCE (ICCS 2017)}},
Series = {{Procedia Computer Science}},
Year = {{2017}},
