@article{ ISI:000345482300016,
Author = {Sangers, Alex and van Gijzen, Martin B.},
Title = {{The eigenvectors corresponding to the second eigenvalue of the Google
   matrix and their relation to link spamming}},
Journal = {{JOURNAL OF COMPUTATIONAL AND APPLIED MATHEMATICS}},
Year = {{2015}},
Volume = {{277}},
Pages = {{192-201}},
Month = {{MAR 15}},
Abstract = {{Google uses the PageRank algorithm to determine the relative importance
   of a website. Link spamming is the name for putting links between
   websites with no other purpose than to increase the PageRank value of a
   website. To give a fair result to a search query it is important to
   detect whether a website is link spammed so that it can be filtered out
   of the search result.
   While the dominant eigenvector of the Google matrix determines the
   PageRank value, the second eigenvector can be used to detect a certain
   type of link spamming. We will describe an efficient algorithm for
   computing a complete set of independent eigenvectors for the second
   eigenvalue, and explain how this algorithm can be used to detect link
   spamming. We will illustrate the performance of the algorithm on web
   crawls of millions of pages. (C) 2014 Elsevier B.V. All rights reserved.}},
