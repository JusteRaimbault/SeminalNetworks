@article{ ISI:000314277100001,
Author = {Shakery, Azadeh and Zhai, ChengXiang},
Title = {{Leveraging comparable corpora for cross-lingual information retrieval in
   resource-lean language pairs}},
Journal = {{INFORMATION RETRIEVAL}},
