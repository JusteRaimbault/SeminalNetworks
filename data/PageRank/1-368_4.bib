@article{ ISI:000434679100001,
Author = {Khanh Nguyen and Wang, Kai and Bu, Yingyi and Fang, Lu and Xu, Guoqing},
Title = {{Understanding and Combating Memory Bloat in Managed Data-Intensive
   Systems}},
