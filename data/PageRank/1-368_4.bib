@article{ ISI:000342265700041,
Author = {Agryzkov, Taras and Oliver, Jose L. and Tortosa, Leandro and Vicent,
   Jose},
Title = {{A new betweenness centrality measure based on an algorithm for ranking
   the nodes of a network}},
Journal = {{APPLIED MATHEMATICS AND COMPUTATION}},
Year = {{2014}},
Volume = {{244}},
Pages = {{467-478}},
Month = {{OCT 1}},
Abstract = {{We propose and discuss a new centrality index for urban street patterns
   represented as networks in geographical space. This centrality measure,
   that we call ranking-betweenness centrality, combines the idea behind
   the random-walk betweenness centrality measure and the idea of ranking
   the nodes of a network produced by an adapted PageRank algorithm. We
   initially use a PageRank algorithm in which we are able to transform
   some information of the network that we want to analyze into numerical
   values. Numerical values summarizing the information are associated to
   each of the nodes by means of a data matrix. After running the adapted
   PageRank algorithm, a ranking of the nodes is obtained, according to
   their importance in the network. This classification is the starting
   point for applying an algorithm based on the random-walk betweenness
   centrality. A detailed example of a real urban street network is
   discussed in order to understand the process to evaluate the
   ranking-betweenness centrality proposed, performing some comparisons
   with other classical centrality measures. (C) 2014 Elsevier Inc. All
   rights reserved.}},
