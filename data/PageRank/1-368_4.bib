@inproceedings{ ISI:000333267200053,
Author = {Wang, Dong and Abdelzaher, Tarek and Kaplan, Lance and Aggarwal, Charu
   C.},
Book-Group-Author = {{IEEE}},
Title = {{Recursive Fact-finding: A Streaming Approach to Truth Estimation in
   Crowdsourcing Applications}},
Booktitle = {{2013 IEEE 33RD INTERNATIONAL CONFERENCE ON DISTRIBUTED COMPUTING SYSTEMS
   (ICDCS)}},
