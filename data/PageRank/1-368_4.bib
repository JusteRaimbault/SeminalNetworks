@article{ ISI:000305833900026,
Author = {Salehi, Mostafa and Rabiee, Hamid R. and Rajabi, Arezo},
Title = {{Sampling from complex networks with high community structures}},
Journal = {{CHAOS}},
Year = {{2012}},
Volume = {{22}},
Number = {{2}},
Month = {{JUN}},
Abstract = {{In this paper, we propose a novel link-tracing sampling algorithm, based
   on the concepts from PageRank vectors, to sample from networks with high
   community structures. Our method has two phases; (1) Sampling the
   closest nodes to the initial nodes by approximating personalized
   PageRank vectors and (2) Jumping to a new community by using PageRank
   vectors and unknown neighbors. Empirical studies on several synthetic
   and real-world networks show that the proposed method improves the
   performance of network sampling compared to the popular link-based
