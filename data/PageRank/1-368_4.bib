@article{ ISI:000411976600031,
Author = {Nathan, Eisha and Zakrzewska, Anita and Riedy, Jason and Bader, David A.},
Title = {{Local Community Detection in Dynamic Graphs Using Personalized
   Centrality}},
Journal = {{ALGORITHMS}},
Year = {{2017}},
Volume = {{10}},
Number = {{3}},
Month = {{SEP}},
Abstract = {{Analyzing massive graphs poses challenges due to the vast amount of data
   available. Extracting smaller relevant subgraphs allows for further
   visualization and analysis that would otherwise be too computationally
   intensive. Furthermore, many real data sets are constantly changing, and
   require algorithms to update as the graph evolves. This work addresses
   the topic of local community detection, or seed set expansion, using
   personalized centrality measures, specifically PageRank and Katz
   centrality. We present a method to efficiently update local communities
   in dynamic graphs. By updating the personalized ranking vectors, we can
   incrementally update the corresponding local community. Applying our
   methods to real-world graphs, we are able to obtain speedups of up to
   60x compared to static recomputation while maintaining an average recall
   of 0.94 of the highly ranked vertices returned. Next, we investigate how
   approximations of a centrality vector affect the resulting local
   community. Specifically, our method guarantees that the vertices
   returned in the community are the highly ranked vertices from a
   personalized centrality metric.}},
