@inproceedings{ ISI:000345592100004,
Author = {Lee, Jooyoung and Oh, Jae C.},
Editor = {{Dam, HK and Pitt, J and Xu, Y and Governatori, G and Ito, T}},
Title = {{Estimating the Degrees of Neighboring Nodes in Online Social Networks}},
Booktitle = {{PRIMA 2014: PRINCIPLES AND PRACTICE OF MULTI-AGENT SYSTEMS}},
