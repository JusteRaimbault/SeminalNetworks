Title = {{A PageRank-Based Method to Extract Fuzzy Expressions as Features in
   Supervised Classification Problems}},
Booktitle = {{ADVANCES IN ARTIFICIAL INTELLIGENCE, CAEPIA 2018}},
Series = {{Lecture Notes in Artificial Intelligence}},
Year = {{2018}},
Volume = {{11160}},
