@inproceedings{ ISI:000349444700153,
Author = {Walsh, Toby},
Editor = {{Schaub, T and Friedrich, G and OSullivan, B}},
Title = {{The PeerRank Method for Peer Assessment}},
Booktitle = {{21ST EUROPEAN CONFERENCE ON ARTIFICIAL INTELLIGENCE (ECAI 2014)}},
