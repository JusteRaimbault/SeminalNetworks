@article{ ISI:000361837300005,
Author = {Liu, Zifan and Emad, Nahid and Ben Amor, Soufian and Lamure, Michel},
Title = {{PageRank Computation Using a Multiple Implicitly Restarted Arnoldi
   Method for Modeling Epidemic Spread}},
