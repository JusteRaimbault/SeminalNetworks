@inproceedings{ ISI:000386620800024,
Author = {Nargundkar, Ashish and Rao, Y. S.},
Book-Group-Author = {{IEEE}},
Title = {{InfluenceRank: A machine learning approach to measure influence of
   Twitter users}},
Booktitle = {{2016 5TH INTERNATIONAL CONFERENCE ON RECENT TRENDS IN INFORMATION
   TECHNOLOGY (ICRTIT)}},
Series = {{International Conference on Recent Trends in Information Technology}},
Year = {{2016}},
Note = {{5th International Conference on Recent Trends in Information Technology
   (ICRTIT), Chennai, INDIA, APR 08-09, 2016}},
Abstract = {{We devise a system for measuring influence of Twitter users, which we
   call InfluenceRank, based on certain features extracted from their
   Twitter profiles and tweets authored over the duration of two months. As
   in the real world, influence of a user on social media may be judged by
   the engagement they drive through the content they publish. For a tweet,
   engagement can be most obviously measured by the number of retweets
   (RTs), favourites and replies it gets. Our system comprises of a
   regression based machine learning approach with InfluenceRank as the
   predictor variable against the set of our proposed features. The
   regression model has shown reasonable accuracy despite being fit on
   limited data.}},
