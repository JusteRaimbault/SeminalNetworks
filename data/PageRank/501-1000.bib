@article{ ISI:000261206600004,
Author = {Awerbuch, Baruch and Kleinberg, Robert},
Title = {{Competitive collaborative learning}},
Journal = {{JOURNAL OF COMPUTER AND SYSTEM SCIENCES}},
Year = {{2008}},
Volume = {{74}},
