@inproceedings{ ISI:000241104900017,
Author = {Huang, Jiayuan and Zhu, Tingshao and Greiner, Russell and Zhou, Dengyong
   and Schuurmans, Dale},
Editor = {{Furnkranz, J and Scheffer, T and Spiliopoulou, M}},
Title = {{Information marginalization on subgraphs}},
Booktitle = {{KNOWLEDGE DISCOVERY IN DATABASES: PKDD 2006, PROCEEDINGS}},
Series = {{LECTURE NOTES IN ARTIFICIAL INTELLIGENCE}},
Year = {{2006}},
Volume = {{4213}},
Pages = {{199-210}},
Note = {{10th European Conference on Principle and Practice of Knowledge
   Discovery in Databases, Berlin, GERMANY, SEP 18-22, 2006}},
Organization = {{Google; Humboldt Univ Berlin; KD ubiq; STRATO AG; IBM; Deutsch Forsch
   Gemeinsch; PASCAL; Mach Learning; AFOSR; EOARD}},
Abstract = {{Real-world data often involves objects that exhibit multiple
   relationships; for example, `papers' and `authors' exhibit both
   paper-author interactions and paper-paper citation relationships. A
   typical learning problem requires one to make inferences about a
   subclass of objects (e.g. `papers'), while using the remaining objects
   and relations to provide relevant information. We present a simple,
   unified mechanism for incorporating information from multiple object
   types and relations when learning on a targeted subset. In this scheme,
   all sources of relevant information are marginalized onto the target
   subclass via random walks. We show that marginalized random walks can be
   used as a general technique for combining multiple sources of
   information in relational data. With this approach, we formulate new
   algorithms for transduction and ranking in relational data, and quantify
   the performance of new schemes on real world data-achieving good results
   in many problems.}},
