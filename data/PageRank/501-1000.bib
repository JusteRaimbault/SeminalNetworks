@inproceedings{ ISI:000270597400006,
Author = {Skopik, Florian and Truong, Hong-Linh and Dustdar, Schahram},
Editor = {{Gaedke, M and Grossniklaus, M and Diaz, O}},
Title = {{Trust and Reputation Mining in Professional Virtual Communities}},
