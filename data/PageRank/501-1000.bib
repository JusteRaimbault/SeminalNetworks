@inproceedings{ ISI:000252728700051,
Author = {Wang, Yufeng and Hori, Yoshiaki and Sakurai, Kouichi},
Editor = {{Washio, T}},
Title = {{Study on trust inference and emergence of economical small-world
   phenomena in P2P environment}},
Booktitle = {{EMERGING TECHNOLOGIES IN KNOWLEDGE DISCOVERY AND DATA MINING}},
