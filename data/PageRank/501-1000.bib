@inproceedings{ ISI:000267113700017,
Author = {Yildirim, Hilmi and Krishnamoorthy, Mukkai S.},
Book-Group-Author = {{ACM}},
Title = {{A Random Walk Method for Alleviating the Sparsity Problem in
   Collaborative Filtering}},
