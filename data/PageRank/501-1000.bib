@inproceedings{ ISI:000254598700008,
Author = {Olsen, Martin},
Editor = {{Aiello, W and Broder, A and Janssen, J and Milios, E}},
Title = {{Communities in large networks: identification and ranking}},
Booktitle = {{ALGORITHMS AND MODELS FOR THE WEB-GRAPH}},
Series = {{Lecture Notes in Computer Science}},
Year = {{2008}},
Volume = {{4936}},
Pages = {{84-96}},
Note = {{MEETING ON SOCIAL SCIENCE HISTORY, CHICAGO, IL, NOV 21-24, 1985}},
Abstract = {{We study the problem of identifying and ranking the members of a
   community in a very large network with link analysis only, given a set
   of representatives of the community. We define the concept of a
   community justified by a formal analysis of a simple model of the
   evolution of a directed graph. We show that the problem of deciding
   whether a non trivial community exists is NP complete. Nevertheless,
   experiments show that a very simple greedy approach can identify members
   of a community in the Danish part of the web graph with time complexity
   only dependent on the size of the found community and its immediate
   surroundings. The members are ranked with a ``local{''} variant of the
   PageRank algorithm. Results are reported from successful experiments on
   identifying and ranking Danish Computer Science sites and Danish Chess
   pages using only a few representatives.}},
