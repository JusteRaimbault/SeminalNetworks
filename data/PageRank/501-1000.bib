@inproceedings{ ISI:000225309500083,
Author = {Taskar, B and Wong, MF and Abbeel, P and Koller, D},
Editor = {{Thrun, S and Saul, K and Scholkopf, B}},
Title = {{Link prediction in relational data}},
Booktitle = {{ADVANCES IN NEURAL INFORMATION PROCESSING SYSTEMS 16}},
