@article{ ISI:000232355300002,
Author = {Rolleke, T and Tsikrika, T and Kazai, G},
Title = {{A general matrix framework for modelling Information Retrieval}},
Journal = {{INFORMATION PROCESSING \& MANAGEMENT}},
Year = {{2006}},
