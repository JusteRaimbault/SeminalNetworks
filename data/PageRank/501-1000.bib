@inproceedings{ ISI:000270976500026,
Author = {Konstas, Ioannis and Stathopoulos, Vassilios and Jose, Joemon M.},
Editor = {{Sanderson, M and Zhai, CX and Zobel, J and Allan, J and Aslam, JA}},
Title = {{On Social Networks and Collaborative Recommendation}},
Booktitle = {{PROCEEDINGS 32ND ANNUAL INTERNATIONAL ACM SIGIR CONFERENCE ON RESEARCH
   AND DEVELOPMENT IN INFORMATION RETRIEVAL}},
Year = {{2009}},
Pages = {{195-202}},
