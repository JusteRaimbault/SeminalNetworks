@inproceedings{ ISI:000262462900002,
Author = {Sawilla, Reginald E. and Ou, Xinming},
Editor = {{Jajodia, S and Lopez, J}},
Title = {{Identifying Critical Attack Assets in Dependency Attack Graphs}},
