@inproceedings{ ISI:000223999500035,
Author = {Saerens, M and Fouss, F and Yen, L and Dupont, P},
Editor = {{Boulicaut, JF and Esposito, F and Giannoti, F and Pedreschi, D}},
Title = {{The principal components analysis of a graph, and its relationships to
   spectral clustering}},
Booktitle = {{MACHINE LEARNING: ECML 2004, PROCEEDINGS}},
Series = {{Lecture Notes in Computer Science}},
Year = {{2004}},
Volume = {{3201}},
Pages = {{371-383}},
Note = {{15th European Conference on Machine Learning/8th European Conference on
   Principles and Practice of Knowledge Discovery in Databases, Pisa,
   ITALY, SEP 20-24, 2004}},
Organization = {{KDNet; Pascal Network; Kluwer \& Mach Learning Journal; Springer;
   Municipal Pisa; Microsoft Res; COOP; Exeura; INSA-Lyon; ISTI-Cnr; Univ
   Pisa; Univ Bari; Reg Toscana}},
Abstract = {{This work presents a novel procedure for computing (1) distances between
   nodes of a weighted, undirected, graph, called the Euclidean Commute
   Time Distance (ECTD), and (2) a subspace projection of the nodes of the
   graph that preserves as much variance as possible, in terms of the ECTD
   - a principal components analysis of the graph. It is based on a
   Markov-chain model of random walk through the graph. The model assigns
   transition probabilities to the links between nodes, so that a random
   walker can jump from node to node. A quantity, called the average
   commute time, computes the average time taken by a random walker for
   reaching node j for the first time when starting from node i, and coming
   back to node i. The square root of this quantity, the ECTD, is a
   distance measure between any two nodes, and has the nice property of
   decreasing when the number of paths connecting two nodes increases and
   when the ``length{''} of any path decreases. The ECTD can be computed
   from the pseudoinverse of the Laplacian matrix of the graph, which is a
   kernel. We finally define the Principal Components Analysis (PCA) of a
   graph as the subspace projection that preserves as much variance as
   possible, in terms of the ECTD. This graph PCA has some interesting
   links with spectral graph theory, in particular spectral clustering.}},
