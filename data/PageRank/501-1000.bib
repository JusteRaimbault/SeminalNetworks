@inproceedings{ ISI:000250423200006,
Author = {Mousavi, Hamid and Rafiei, Mohammad E. and Movaghar, Ali},
Book-Group-Author = {{IEEE}},
Title = {{Characterizing the web using a new uniform sampling approach}},
