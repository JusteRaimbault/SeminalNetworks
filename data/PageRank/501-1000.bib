@inproceedings{ ISI:000252040600010,
Author = {Castillo, Carlos and Donato, Debora and Gionis, Aristides},
Editor = {{Ziviani, N and BaezaYates, R}},
Title = {{Estimating number of citations using author reputation}},
Booktitle = {{STRING PROCESSING AND INFORMATION RETRIEVAL, PROCEEDINGS}},
