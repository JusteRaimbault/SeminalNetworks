@inproceedings{ ISI:000241594700008,
Author = {Chirita, Paul-Alexandru and Nejdl, Wolfgang},
Editor = {{Crestani, F and Ferragina, P and Sanderson, M}},
Title = {{Analyzing user behavior to rank desktop items}},
