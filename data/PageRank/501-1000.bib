@inproceedings{ ISI:000256351100038,
Author = {Pavlovic, Dusko},
Editor = {{Hirsch, EA and Razborov, AA and Semenov, A and Slissenko, A}},
Title = {{Network as a computer: Ranking paths to find flows}},
