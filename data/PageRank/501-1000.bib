@inproceedings{ ISI:000252834500016,
Author = {Gollapudi, Sreenivas and Najork, Marc and Panigrahy, Rina},
Editor = {{Bonato, A and Chung, FRK}},
Title = {{Using bloom filters to speed up HITS-like ranking algorithms}},
