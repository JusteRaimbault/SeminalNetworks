@inproceedings{ ISI:000228784700058,
Author = {Li, ZS and Du, YJ and Xu, Y and Wang, YK and Qi, DM},
Editor = {{Ruan, D and DHondt, P and DeCock, M and Nachtegael, M and Kerre, EE}},
Title = {{The personalized PageRank based on user behaviors}},
Booktitle = {{APPLIED COMPUTATIONAL INTELLIGENCE}},
Year = {{2004}},
Pages = {{318-323}},
Note = {{6th International Conference on Fuzzy Logic and Intelligent Technologies
   in Nuclear Science, Blankenberge, BELGIUM, SEP 01-03, 2004}},
Abstract = {{The PageRank algorithm takes advantage of the link structure of the Web
   to improve the ranking of query results, independent of any particular
   users. Obviously, the PageRank score reflects a ``democratic{''}
   importance that has no preference for any particular pages. In fact,
   users always browse those Web pages in which they have more interest.
   So, we propose a new personalized ranking vector--UserRank (UR) based on
   user behaviors of browsing pages to estimate the importance of Web
   pages. In order to yield more accurate query results, we take the UR
   vector combined with PageRank vector to generate a new ranking
   vector--Personalized PageRank ((PR)-R-2). Experimental results
   demonstrate that our p(2)R algorithm can more closely reflect user
   preference and more largely improve the precision of ranking results.}},
