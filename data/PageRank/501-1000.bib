@article{ ISI:000227178800004,
Author = {Agosti, M and Pretto, L},
Title = {{Theoretical study of a generalized version of Kleinberg's HITS algorithm}},
Journal = {{INFORMATION RETRIEVAL}},
Year = {{2005}},
