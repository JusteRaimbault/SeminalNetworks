@inproceedings{ ISI:000263677100008,
Author = {Zhang, Jie and Cohen, Robin and Larson, Kate},
Editor = {{Falcone, R and Barber, SK and SabaterMir, J and Singh, MP}},
Title = {{A Trust-Based Incentive Mechanism for E-Marketplaces}},
Booktitle = {{TRUST IN AGENT SOCIETIES}},
