@inproceedings{ ISI:000245348101063,
Author = {Kim, YounHee and Shin, HyeYeon and Chong, KyunRak and Lim, HaeChull},
Book-Group-Author = {{IEEE}},
Title = {{Indexing scheme for keyword search over semantic web documents}},
Booktitle = {{9TH INTERNATIONAL CONFERENCE ON ADVANCED COMMUNICATION TECHNOLOGY:
   TOWARD NETWORK INNOVATION BEYOND EVOLUTION, VOLS 1-3}},
Year = {{2007}},
Pages = {{1205+}},
Note = {{9th International Conference on Advanced Communication Technology (ICACT
   2007), Phoenix Pk, SOUTH KOREA, FEB 12-14, 2007}},
Organization = {{Minist Informat \& Commun; Elect \& Telecommun Res Inst; Natl Informat
   Soc Agcy; IEEE Commun Soc; IEEE Reg 10; Open Stand \& Internet Assoc;
   Korean Inst Commun Sci; IEEK Commun Soc; Inst Informat Advancement;
   Korea Tourism Org; Gangwondo Prov; IEEE Daejeon Sect}},
Abstract = {{The Semantic Web is an extension of the current Web in which information
   are given well-defined meaning to enable computers and people to work in
   cooperation. Many ontology languages such as RDF and OWL describe
   metadata on the Semantic Web. Metadata can be used to retrieve related
   information through the inference more accurately and simply. RDF is a
   language for representing metadata. An enormous number of keywords on
   the Semantic Web are very important to make practical applications of
   the Semantic Web because most users prefer to search with keywords. In
   this paper, we classify queries with keyword conditions into three,
   patterns and propose indexing techniques for keyword-search considering
   both metadata and ontology, The proposed indexing techniques aim at
   efficient process of keyword-search for RDF/S documents. There are many
   studies about keyword-search techniques based on XML. RDF is different
   from XML on the Web. So, there is a need for query processing which is
   appropriate to RDF. And we propose a structure of table for storing RDF
   schema information. We propose a few ranking schemes for RDF/S
   documents. The schemes can be used to determine a sequence of query
   results according to predefined importance of resources.}},
