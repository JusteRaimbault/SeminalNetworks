@article{ ISI:000260999100009,
Author = {Lamberti, Fabrizio and Sanna, Andrea and Demartini, Claudio},
Title = {{A Relation-Based Page Rank Algorithm for Semantic Web Search Engines}},
Journal = {{IEEE TRANSACTIONS ON KNOWLEDGE AND DATA ENGINEERING}},
Year = {{2009}},
Volume = {{21}},
