@inproceedings{ ISI:000239915100003,
Author = {Arch-int, Somjit},
Editor = {{Mohammadian, M}},
Title = {{Web document clustering using semantic link analysis}},
Booktitle = {{International Conference on Computational Intelligence for Modelling,
   Control \& Automation Jointly with International Conference on
   Intelligent Agents, Web Technologies \& Internet Commerce, Vol 2,
   Proceedings}},
Year = {{2006}},
Pages = {{13-18}},
