@inproceedings{ ISI:000239588900030,
Author = {Miyazaki, Syuji},
Editor = {{Washio, T and Sakurai, A and Nakajima, K and Takeda, H and Tojo, S and Yokoo, M}},
Title = {{Directed network as a chaotic dynamical system}},
Booktitle = {{NEW FRONTIERS IN ARTIFICIAL INTELLIGENCE}},
Series = {{LECTURE NOTES IN ARTIFICIAL INTELLIGENCE}},
Year = {{2006}},
Volume = {{4012}},
Pages = {{261-270}},
Note = {{19th Annual Conference of the
   Japanese-Society-for-Artificial-Intelligence, Kitakyushu City, JAPAN,
   JUN, 2005}},
Organization = {{Japanese Soc Artificial Intelligence}},
Abstract = {{A directed network such as the WWW can be represented by a transition
   matrix. Comparing this matrix to a Frobenius-Perron matrix of a chaotic
   piecewise-linear one-dimensional map whose domain can be divided into
   Markov subintervals, we are able to relate the network structure itself
   to chaotic dynamics. Just like various large-deviation properties of
   local expansion rates (finite-time Lyapunov exponents) related to
   chaotic dynamics, we can also discuss those properties of network
   structure.}},
