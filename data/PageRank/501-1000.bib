@inproceedings{ ISI:000231800000007,
Author = {Jones, GJF},
Editor = {{Tanaka, K and Kidawara, Y and Zettsu, K}},
Title = {{Challenges and opportunities of context-aware information access}},
Booktitle = {{INTERNATIONAL WORKSHOP ON UBIQUITOUS DATA MANAGEMENT, PROCEEDINGS}},
Year = {{2005}},
Pages = {{53-60}},
Note = {{International Workshop on Ubiquitous Data Management (UDM2005), Tokyo,
   JAPAN, APR 04, 2005}},
Organization = {{IEEE Comp Soc; Kyoto Univ 21st Century Ctr Excellence Program; Knowledge
   Soc Infrastructure, Informat Res Ctr Dev; Natl Inst Informat \& Commun
   Technol; Database Soc Japan; Informat Proc Soc Japan; Inst Elect,
   Informat \& Commun Engn}},
Abstract = {{Ubiquitous computing environments embedding a wide range of pervasive
   computing technologies provide a challenging and exciting new domain for
   information access. Individuals working in these environments are
   increasingly permanently connected to rich information resources. An
   appealing opportunity of these environments is the potential to deliver
   useful information to individuals either from their previous information
   experiences or external sources. This information should enrich their
   life experiences or make them more effective in their endeavours.
   Information access in ubiquitous computing environments can be made
   ``context-aware{''} by exploiting the wide range context data available
   describing the environment, the searcher and the information itself.
   Realising such a vision of reliable, timely and appropriate
   identification and delivery of information in this way poses numerous
   challenges. A central theme in achieving context-aware information
   access is the combination of information retrieval with multiple
   dimensions of available context data. Potential context data sources,
   include the user's current task, inputs from environmental and biometric
   sensors, associated with the user's current context, previous contexts,
   and document context, which can be exploited using a variety of
   technologies to create new and exciting possibilities for information
   access.}},
