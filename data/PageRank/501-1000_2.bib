@inproceedings{ ISI:000392617100035,
Author = {Chhaya, Meraj M. A. and Coetzee, Marijke},
Editor = {{Kommers, P and Isaias, P and Gauzente, C and Nunes, MB and Peng, GC and Macedo, M}},
Title = {{ENHANCING A QUESTION-ANSWER MEDICAL SOCIAL NETWORK WITH REPUTATION}},
Booktitle = {{PROCEEDINGS OF THE INTERNATIONAL CONFERENCES ON ICT, SOCIETY AND HUMAN
   BEINGS 2014, WEB BASED COMMUNITIES AND SOCIAL MEDIA 2014, E-COMMERCE
   2014, INFORMATION SYSTEMS POST-IMPLEMENTATION AND CHANGE MANAGEMENT 2014
   AND E-HEALTH 2014}},
Year = {{2014}},
Pages = {{287-294}},
Note = {{Int Conf on ICT, Soc and Human Beings / Int Conf on Web Based
   Communities and Social Media / Int Conf on e-Commerce / Int Conf on
   Informat Syst Post-Implementat and Change Management / Int Conf on
   e-Health, Lisbon, PORTUGAL, JUL 17-19, 2014}},
Abstract = {{It is difficult for inexperienced doctors to diagnose patients who
   demonstrate complex symptoms, especially if the doctor is located in a
   rural area and does not have access to a second opinion. The medical
   social network proposed by this research solves this problem by allowing
   inexperienced doctors to pose questions to expert doctors. The system
   relies on a graph-based reputation management system with a
   knowledge-base. Doctors can ask questions, answer each other's
   questions, and contribute medical articles. Content from well-reputed
   doctors is prioritized and reputation scores of doctors who contribute
   constructively to the knowledge base is adjusted. Doctors can vote on
   each other's questions, answers and knowledge-base articles. Using these
   ratings, the social graph ranks questions, answers and articles
   according to their rating, and at the same time increases doctors'
   reputation based on the quality of content they have contributed to the
   system. Questions submitted by doctors with higher reputation rank
   higher than others, and therefore receive more replies, and this
   motivates doctors to further contribute to the system. The resulting
   social network helps in reducing misdiagnoses, and thus decreasing loss
   of life and the possibility of litigation against medical professionals.}},
