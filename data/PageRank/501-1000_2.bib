@inproceedings{ ISI:000458973703018,
Author = {Lindgren, Erik M. and Wu, Shanshan and Dimakis, Alexandros G.},
Editor = {{Lee, DD and Sugiyama, M and Luxburg, UV and Guyon, I and Garnett, R}},
Title = {{Leveraging Sparsity for Efficient Submodular Data Summarization}},
Booktitle = {{ADVANCES IN NEURAL INFORMATION PROCESSING SYSTEMS 29 (NIPS 2016)}},
Series = {{Advances in Neural Information Processing Systems}},
Year = {{2016}},
Volume = {{29}},
Note = {{30th Conference on Neural Information Processing Systems (NIPS),
   Barcelona, SPAIN, 2016}},
Abstract = {{The facility location problem is widely used for summarizing large
   datasets and has additional applications in sensor placement, image
   retrieval, and clustering. One difficulty of this problem is that
   submodular optimization algorithms require the calculation of pairwise
   benefits for all items in the dataset. This is infeasible for large
   problems, so recent work proposed to only calculate nearest neighbor
   benefits. One limitation is that several strong assumptions were invoked
   to obtain provable approximation guarantees. In this paper we establish
   that these extra assumptions are not necessary-solving the sparsified
   problem will be almost optimal under the standard assumptions of the
   problem. We then analyze a different method of sparsification that is a
   better model for methods such as Locality Sensitive Hashing to
   accelerate the nearest neighbor computations and extend the use of the
   problem to a broader family of similarities. We validate our approach by
   demonstrating that it rapidly generates interpretable summaries.}},
