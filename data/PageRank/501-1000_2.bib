Title = {{Efficient Communications in Wireless Sensor Networks Based on Biological
   Robustness}},
Booktitle = {{PROCEEDINGS 12TH ANNUAL INTERNATIONAL CONFERENCE ON DISTRIBUTED
   COMPUTING IN SENSOR SYSTEMS (DCOSS 2016)}},
Series = {{IEEE International Conference on Distributed Computing in Sensor Systems}},
Year = {{2016}},
Pages = {{161-168}},
