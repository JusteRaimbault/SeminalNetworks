@article{ ISI:000346620500003,
Author = {Qin, Guimin and Zhao, Xing-Ming},
Title = {{A survey on computational approaches to identifying disease biomarkers
   based on molecular networks}},
Journal = {{JOURNAL OF THEORETICAL BIOLOGY}},
Year = {{2014}},
Volume = {{362}},
Number = {{SI}},
Pages = {{9-16}},
Month = {{DEC 7}},
Abstract = {{The disease biomarkers can help make accurate diagnosis and therefore
   give appropriate interventions. In the past years, the accumulation of
   various kinds of `omics' data, e.g. genomics and transcriptomics, makes
   it possible to identify disease biomarkers in a more efficient way. In
   particular, the molecular networks that describe the functional
   relationships among molecules enable the identification of disease
   biomarkers from a systematic perspective. In this paper, we surveyed the
