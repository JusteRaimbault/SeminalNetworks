Author = {Canon, Daniel E. and Lopez, Diego M. and Blobel, Bernd},
Editor = {{Blobel, B and Sauermann, S and Mense, A}},
Title = {{Towards Assisted Moderation in Online Healthcare Social Networks:
   Improving Trust in YouTube Searches}},
