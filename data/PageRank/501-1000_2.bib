@inproceedings{ ISI:000380569400008,
Author = {Zhang, Hao and Zhou, Wei and Wan, Xiaoyu and Fu, Ge and Xu, Zhiyong and
   Han, Jizhong},
Book-Group-Author = {{IEEE}},
Title = {{A Novel Clustering Algorithm on Large-Scale Graph Data}},
