@inproceedings{ ISI:000358429100003,
Author = {Hua Shen and Liu, Xinyue},
Editor = {{Jiao, F}},
Title = {{Propagating Anti-TrustRank with Relationship Strength for Fighting Link
   Farming on Twitter}},
Booktitle = {{PROCEEDINGS OF THE INTERNATIONAL SEMINAR ON COMPUTATION, COMMUNICATION
   AND CONTROL}},
Series = {{ACSR-Advances in Comptuer Science Research}},
Year = {{2015}},
Volume = {{4}},
