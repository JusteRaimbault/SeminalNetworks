@inproceedings{ ISI:000362450300029,
Author = {Luo, Qingjun and Zhang, Shiliang and Huang, Tiejun and Gao, Wen and
   Tian, Qi},
Editor = {{Cremers, D and Reid, I and Saito, H and Yang, MH}},
Title = {{Hybrid-Indexing Multi-type Features for Large-Scale Image Search}},
Booktitle = {{COMPUTER VISION - ACCV 2014, PT I}},
Series = {{Lecture Notes in Computer Science}},
Year = {{2015}},
Volume = {{9003}},
Pages = {{446-460}},
Note = {{12th Asian Conference on Computer Vision (ACCV), Singapore, SINGAPORE,
   NOV 01-05, 2014}},
Organization = {{Singapore Tourism Board; Omron; Nvidia; Garena; Samsung; Adobe; ViSenze;
   Lee Fdn; Morpx; Microsoft Res; NICTA}},
Abstract = {{Indexing local features with a vocabulary tree and indexing holistic
   features by compact hashing codes are two successful but separated lines
   of research. Both of the two indexing models are suited for specific
   features and are limited to certain scenarios like partial-duplicate
   search and similar image search, respectively. To conquer such
   limitations, we propose a novel hybrid-indexing strategy, which
   incorporates multiple similarity metrics into one inverted index file
   during off-line indexing. Hybrid-Indexing only requires the
   Bag-of-visual Words (BoWs) model as input for online query, but could
   obtain more satisfying retrieval results because the index file conveys
   hybrid similarities among images. Moreover, hybrid-indexing does not
   degrade the efficiency of classic BoWs based image search. Experiments
   on several public datasets manifest the effectiveness and efficiency of
   our proposed method.}},
