@inproceedings{ ISI:000363951600009,
Author = {Sun, Bing-Jie and Shen, Hua-Wei and Cheng, Xue-Qi},
Editor = {{Thai, MT and Nguyen, NP and Shen, H}},
Title = {{Improve Network Clustering via Diversified Ranking}},
Booktitle = {{COMPUTATIONAL SOCIAL NETWORKS, CSONET 2015}},
Series = {{Lecture Notes in Computer Science}},
Year = {{2015}},
Volume = {{9197}},
Pages = {{104-115}},
Note = {{4th International Conference on Computational Social Network (CSoNet),
   Beijing, PEOPLES R CHINA, AUG 04-06, 2015}},
Abstract = {{Clustering is one fundamental task in network analysis. A widely-used
   clustering method is k-means clustering, where clustering is iteratively
   refined by minimizing the distance between each data point and its
   cluster center. For k-means clustering, one key issue is initialization,
   which heavily affects its accuracy and computational cost. This issue is
   particularly critical when applying k-means clustering to graph data
   where nodes are not embedded in a metric space. In this paper, we
   propose to use diversified ranking method to initialize k-means
   clustering, i.e., finding a set of seed nodes. In diversified ranking,
   seed nodes are figured out by considering their centrality and diversity
   in a unified manner. With seed nodes as starting points, k-means
   clustering is used to cluster nodes into groups. We apply the proposed
   method to detect communities in synthetic network and real-world
   network. Results indicate that the proposed method exhibits high
   effectiveness and efficiency.}},
Publisher = {{SPRINGER-VERLAG BERLIN}},
Address = {{HEIDELBERGER PLATZ 3, D-14197 BERLIN, GERMANY}},
Type = {{Proceedings Paper}},
