@inproceedings{ ISI:000383738200006,
Author = {Geigl, Florian and Lerman, Kristina and Walk, Simon and Strohmaier,
   Markus and Helic, Denis},
Book-Group-Author = {{ACM}},
Title = {{Assessing the Navigational Effects of Click Biases and Link Insertion on
   the Web}},
