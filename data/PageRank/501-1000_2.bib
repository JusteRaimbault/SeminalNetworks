@inproceedings{ ISI:000369890800010,
Author = {Liu, Yuechang and Tong, Hanghang and Xie, Lei and Tang, Yong},
Editor = {{Zhang, X and Sun, M and Wang, Z and Huang, X}},
Title = {{Supervised Link Prediction Using Random Walks}},
Booktitle = {{SOCIAL MEDIA PROCESSING, SMP 2015}},
Series = {{Communications in Computer and Information Science}},
