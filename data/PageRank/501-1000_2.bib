@inproceedings{ ISI:000387861300015,
Author = {Liu, Shaowei and Cui, Peng and Zhu, Wenwu and Yang, Shiqiang},
Book-Group-Author = {{ACM}},
Title = {{Learning Socially Embedded Visual Representation from Scratch}},
