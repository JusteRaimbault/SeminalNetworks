@inproceedings{ ISI:000387959201105,
Author = {Wu, Xiao-Ming and Lie, Zhenguo and Chang, Shih-Fu},
Book-Group-Author = {{IEEE}},
Title = {{New Insights into Laplacian Similarity Search}},
