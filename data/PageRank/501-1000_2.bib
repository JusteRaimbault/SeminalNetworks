@inproceedings{ ISI:000392157900003,
Author = {Plantikow, Stefan and Jorra, Maik},
Editor = {{Xu, J and Yu, G and Zhou, S and Unland, R}},
Title = {{Latency-Optimal Walks in Replicated and Partitioned Graphs}},
