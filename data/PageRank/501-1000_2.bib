@article{ ISI:000360003900012,
Author = {Sohn, Bong-Soo and Jung, Jai E.},
Title = {{A Novel Ranking Model for a Large-Scale Scientific Publication}},
Journal = {{MOBILE NETWORKS \& APPLICATIONS}},
