@inproceedings{ ISI:000389411500027,
Author = {Zhang, Chuxu and Yu, Lu and Lu, Jie and Zhou, Tao and Zhang, Zi-Ke},
Editor = {{Cui, B and Zhang, N and Xu, J and Lian, X and Liu, D}},
Title = {{AdaWIRL: A Novel Bayesian Ranking Approach for Personal Big-Hit Paper
   Prediction}},
