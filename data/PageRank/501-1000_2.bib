@article{ ISI:000320427100003,
Author = {Grisi-Filho, J. H. H. and Amaku, M. and Ferreira, F. and Dias, R. A. and
   Ferreira Neto, J. S. and Negreiros, R. L. and Ossada, R.},
Title = {{Detecting livestock production zones}},
Journal = {{PREVENTIVE VETERINARY MEDICINE}},
Year = {{2013}},
Volume = {{110}},
Number = {{3-4}},
Pages = {{304-311}},
Month = {{JUL 1}},
Abstract = {{Communities are sets of nodes that are related in an important way, most
   likely sharing common properties and/or playing similar roles within a
   network. Unraveling a network structure, and hence the trade preferences
   and pathways, could be useful to a researcher or a decision maker. We
   implemented a community detection algorithm to find livestock
   communities, which is consistent with the definition of a livestock
   production zone, assuming that a community is a group of farm premises
   in which an animal is more likely to stay during its lifetime than
   expected by chance. We applied this algorithm to the network of animal
   movements within the state of Mato Grosso for 2007. This database holds
   information concerning 87,899 premises and 521,431 movements throughout
   the year, totaling 15,844,779 animals moved. The community detection
   algorithm achieved a network partition that shows a clear geographical
   and commercial pattern, two crucial features for preventive veterinary
   medicine applications; this algorithm provides also a meaningful
   interpretation to trade networks where links emerge based on trader node
   choices. (C) 2013 Elsevier B.V. All rights reserved.}},
