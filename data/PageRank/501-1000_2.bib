@inproceedings{ ISI:000375227000024,
Author = {Baqer, Khaled and Anderson, Ross},
Editor = {{Christianson, B and Svenda, P and Matyas, V and Malcolm, J and Stajano, F and Anderson, J}},
Title = {{Do You Believe in Tinker Bell? The Social Externalities of Trust}},
