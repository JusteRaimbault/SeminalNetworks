@inproceedings{ ISI:000389935800045,
Author = {Gimenez-Garcia, Jose M. and Thakkar, Harsh and Zimmermann, Antoine},
Editor = {{Sack, H and Rizzo, G and Steinmetz, N and Mladenic, D and Auer, S and Lange, C}},
