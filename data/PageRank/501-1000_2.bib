@inproceedings{ ISI:000389503800021,
Author = {Salucci, Luca and Bonetta, Daniele and Binder, Walter},
Editor = {{Dutot, PF and Trystram, D}},
Title = {{Lightweight Multi-language Bindings for Apache Spark}},
Booktitle = {{EURO-PAR 2016: PARALLEL PROCESSING}},
