Title = {{Integrating Ontologies and Fuzzy Logic to Represent User-Trustworthiness
   in Recommender Systems}},
Booktitle = {{3RD INTERNATIONAL CONFERENCE ON INFORMATION TECHNOLOGY AND QUANTITATIVE
   MANAGEMENT, ITQM 2015}},
Series = {{Procedia Computer Science}},
Year = {{2015}},
Volume = {{55}},
