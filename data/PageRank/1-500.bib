@inproceedings{ ISI:000281568800026,
Author = {Yu, Weiren and Lin, Xuemin and Le, Jiajin},
Editor = {{Chen, L and Tang, CJ and Yang, J and Gao, YJ}},
Title = {{Taming Computational Complexity: Efficient and Parallel SimRank
   Optimizations on Undirected Graphs}},
