@article{ ISI:000453795300004,
Author = {Merkurjev, Ekaterina and Bertozzi, Andrea L. and Chung, Fan},
Title = {{A SEMI-SUPERVISED HEAT KERNEL PAGERANK MBO ALGORITHM FOR DATA
   CLASSIFICATION}},
Journal = {{COMMUNICATIONS IN MATHEMATICAL SCIENCES}},
Year = {{2018}},
Volume = {{16}},
Number = {{5}},
Pages = {{1241-1265}},
Abstract = {{We present an accurate and efficient graph-based algorithm for
   semi-supervised classification that is motivated by recent successful
   threshold dynamics approaches and derived using heat kernel pagerank.
   Two different techniques are proposed to compute the pagerank, one of
   which proceeds by simulating random walks of bounded length. The
   algorithm produces accurate results even when the number of labeled
   nodes is very small, and avoids computationally expensive linear algebra
   routines. Moreover, the accuracy of the procedure is comparable with or
   better than that of state-of-the-art methods and is demonstrated on
