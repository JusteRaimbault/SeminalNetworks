@inproceedings{ ISI:000296364500046,
Author = {Okamoto, Hiroshi},
Editor = {{Honkela, T and Duch, W and Girolami, M and Kaski, S}},
Title = {{Topic-Dependent Document Ranking: Citation Network Analysis by Analogy
   to Memory Retrieval in the Brain}},
Booktitle = {{ARTIFICIAL NEURAL NETWORKS AND MACHINE LEARNING - ICANN 2011, PT I}},
Series = {{Lecture Notes in Computer Science}},
Year = {{2011}},
Volume = {{6791}},
Pages = {{371-378}},
Note = {{21st International Conference on Artificial Neural Networks, ICANN 2011,
   Aalto Univ Sch Sci, Espoo, FINLAND, JUN 14-17, 2011}},
Organization = {{Aalto Univ Sch Sci, Dept Informat \& Comp Sci}},
Abstract = {{We propose a method of citation analysis for evaluating the
   topic-dependent importance of individual scientific papers. This method
   assumes spreading activation in citation networks with a
   multi-hysteretic input/output relationship for each node (paper). The
   multi-hysteretic property renders the steady state of spreading
   activation continuously dependent on the initial state. Given a topic
   represented by the initial state, the importance of individual papers
   can be defined by the activities they have in the steady state. We have
   devised this method inspired by memory retrieval in the brain, where the
   multi-hysteretic property of single cells or neuronal networks is
   considered to play an essential role for cue-dependent retrieval of
   memory. Quantitative evaluation using a restoration problem has revealed
   that the performance of the proposed method is considerably higher than
   that of the benchmark method. We demonstrate the practical usefulness of
   the proposed method by applying it to a citation network of neuroscience
   papers.}},
