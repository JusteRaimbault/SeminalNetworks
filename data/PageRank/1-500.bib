@inproceedings{ ISI:000311551800029,
Author = {Wang, Hua and Ding, Chris and Huang, Heng},
Editor = {{Balcazar, JL and Bonchi, F and Gionis, A and Sebag, M}},
Title = {{Directed Graph Learning via High-Order Co-linkage Analysis}},
Booktitle = {{MACHINE LEARNING AND KNOWLEDGE DISCOVERY IN DATABASES, PT III}},
Series = {{Lecture Notes in Artificial Intelligence}},
Year = {{2010}},
Volume = {{6323}},
Pages = {{451-466}},
