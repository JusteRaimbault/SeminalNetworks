Title = {{An Improved Concept Vector Space Model for Ontology Based Classification}},
Booktitle = {{2015 11TH INTERNATIONAL CONFERENCE ON SIGNAL-IMAGE TECHNOLOGY \&
   INTERNET-BASED SYSTEMS (SITIS)}},
Year = {{2015}},
Pages = {{240-245}},
