@article{ ISI:000274499500002,
Author = {Milicevic, Aleksandra Klasnja and Nanopoulos, Alexandros and Ivanovic,
   Mirjana},
Title = {{Social tagging in recommender systems: a survey of the state-of-the-art
   and possible extensions}},
Journal = {{ARTIFICIAL INTELLIGENCE REVIEW}},
Year = {{2010}},
Volume = {{33}},
Number = {{3}},
Pages = {{187-209}},
Month = {{MAR}},
Abstract = {{Social tagging systems have grown in popularity over the Web in the last
   years on account of their simplicity to categorize and retrieve content
   using open-ended tags. The increasing number of users providing
   information about themselves through social tagging activities caused
   the emergence of tag-based profiling approaches, which assume that users
   expose their preferences for certain contents through tag assignments.
   Thus, the tagging information can be used to make recommendations. This
   paper presents an overview of the field of social tagging systems which
   can be used for extending the capabilities of recommender systems.
   Various limitations of the current generation of social tagging systems
   and possible extensions that can provide better recommendation
   capabilities are also considered.}},
