@inproceedings{ ISI:000286691600032,
Author = {Malik, Zaki and Medjahed, Brahim},
Editor = {{Maglio, PP and Weske, M and Yang, J and Fantinato, M}},
Title = {{Trust Assessment for Web Services under Uncertainty}},
