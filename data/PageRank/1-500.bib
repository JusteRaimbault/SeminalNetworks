@article{ ISI:000279362300007,
Author = {Altman, Alon and Tennenholtz, Moshe},
Title = {{An Axiomatic Approach to Personalized Ranking Systems}},
Journal = {{JOURNAL OF THE ACM}},
Year = {{2010}},
Volume = {{57}},
Number = {{4}},
Month = {{APR}},
Abstract = {{Personalized ranking systems and trust systems are an essential tool for
   collaboration in a multi-agent environment. In these systems, trust
   relations between many agents are aggregated to produce a personalized
   trust rating of the agents. In this article, we introduce the first
   extensive axiomatic study of this setting, and explore a wide array of
   well-known and new personalized ranking systems. We adapt several axioms
   ( basic criteria) from the literature on global ranking systems to the
   context of personalized ranking systems, and fully classify the set of
   systems that satisfy all of these axioms. We further show that all these
   axioms are necessary for this result.}},
