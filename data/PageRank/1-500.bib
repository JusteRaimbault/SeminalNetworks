@article{ ISI:000307892900009,
Author = {Qi, Guo-Jun and Tsai, Min-Hsuan and Tsai, Shen-Fu and Cao, Liangliang
   and Huang, Thomas S.},
Title = {{Web-Scale Multimedia Information Networks}},
