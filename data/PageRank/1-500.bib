@inproceedings{ ISI:000426901200114,
Author = {Farahani, Hamideh Shahrabi and Bagheri, Alireza and Saraf, Ebrahim Haj
   Khan Mirzaye},
Book-Group-Author = {{IEEE}},
Title = {{Characterizing Behavior of Topical Authorities in Twitter}},
Booktitle = {{2017 INTERNATIONAL CONFERENCE ON INNOVATIVE MECHANISMS FOR INDUSTRY
   APPLICATIONS (ICIMIA)}},
Year = {{2017}},
Pages = {{581-586}},
Note = {{IEEE International Conference on Innovative Mechanisms for Industry
   Applications (ICIMIA, Bangalore, INDIA, FEB 21-23, 2017}},
Organization = {{IEEE; IEEE Bangalore Sect; Dayananda Sagar Coll Engn}},
Abstract = {{In recent years, social media such as Twitter have encountered a
   significant growth because of the ceaseless increase in the volume of
   contents and the number of users. The level of influence among social
   media users is significantly different from one user to the other; and
   as such, it has become more vital to find the most influential users on
   social media. There are lots of mysteries about behaviours of
   influential users; surprisingly, the effects of different measures which
   are used to examine the user influence in different dimensions have not
   been closely examined; knowing the influence level of the metrics helps
   to use more efficient metrics. Therefore, as a viable research area,
   this study, aims to determine relationships among various metrics of
   influential users. According to observations, the dominant feature of
   effective (influential) users is their authoring; which denotes the
   linear relationship between the number of original tweets and influence
   of the user. Also, the minimum number of original tweets that a user
   should write before leading to proliferation of other influential
   metrics is calculated. This research applies Mean Absolute Percentage
   Error (MAPE) and Mean Absolute Error (MAE) measures towards assessing
   the regression model and achieving results accurate to a large
   proportion.}},
