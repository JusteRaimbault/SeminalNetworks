@article{ ISI:000291122800015,
Author = {Cao, Yunbo and Duan, Huizhong and Lin, Chin-Yew and Yu, Yong},
Title = {{Re-Ranking Question Search Results by Clustering Questions}},
Journal = {{JOURNAL OF THE AMERICAN SOCIETY FOR INFORMATION SCIENCE AND TECHNOLOGY}},
