@article{ ISI:000279378300004,
Author = {Seo, Yangjin and Han, Sangyong},
Title = {{Local Scalar Trust Metrics with a Fuzzy Adjustment Method}},
Journal = {{KSII TRANSACTIONS ON INTERNET AND INFORMATION SYSTEMS}},
