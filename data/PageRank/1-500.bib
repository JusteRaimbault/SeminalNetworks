@inproceedings{ ISI:000304129300016,
Author = {Mikami, Nozomi and Pichl, Lukas},
Editor = {{Kikuchi, S and Madaan, A and Sachdeva, S and Bhalla, S}},
Title = {{Summary Extraction from Chinese Text for Data Archives of Online News}},
Booktitle = {{DATABASES IN NETWORKED INFORMATION SYSTEMS}},
