@inproceedings{ ISI:000306392000021,
Author = {Schall, Daniel and Skopik, Florian},
Editor = {{Eder, J and Bielikova, M and Tjoa, AM}},
Title = {{An Analysis of the Structure and Dynamics of Large-Scale Q/A Communities}},
