@inproceedings{ ISI:000299009900065,
Author = {Chao, WenHan and Li, ZhouJun},
Book-Group-Author = {{IEEE}},
Title = {{Improved Graph-based Bilingual Corpus Selection with Sentence Pair
   Ranking for Statistical Machine Translation}},
Booktitle = {{2011 23RD IEEE INTERNATIONAL CONFERENCE ON TOOLS WITH ARTIFICIAL
   INTELLIGENCE (ICTAI 2011)}},
Series = {{Proceedings-International Conference on Tools With Artificial
   Intelligence}},
