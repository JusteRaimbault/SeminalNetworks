@inproceedings{ ISI:000279595200011,
Author = {Kleb, Joachim and Abecker, Andreas},
Editor = {{Aroyo, L and Antoniou, G and Hyvonen, E and TenTeije, A and Stuckenschmidt, H and Crabral, L and Tudorache, T}},
Title = {{Entity Reference Resolution via Spreading Activation on RDF-Graphs}},
Booktitle = {{SEMANTIC WEB: RESEARCH AND APPLICATIONS, PT 1, PROCEEDINGS}},
Series = {{Lecture Notes in Computer Science}},
Year = {{2010}},
